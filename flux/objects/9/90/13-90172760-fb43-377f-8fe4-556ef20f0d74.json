  "rdfs:comment": {
    "de": [
      "Landwirtschaftlicher Familienbetrieb. Obstbau (Erdbeeren, Kirschen, Aprikosen, Pfirsiche, Nektarinen, Pflaumen, Birnen, Äpfel, Fruchtsaft, Kiwis)und Weinbau (Landweine, AOP Saint-Joseph). Verkauf und Verkostung auf dem Bauernhof."
    ],
    "en": [
      "Family agricultural farm: fruit farming  (strawberries, cherries, nectarines, plums, apples and kiwi). Vineyard: (local wine, AOC St. Joseph). Farm shop and onsite tasting."
    ],
    "it": [
      "Azienda agricola di famiglia. Produzione arborea (fragole, ciliegie, albicocche, pesche, nettarine, prugne, pere, mele, succhi di frutta, kiwi) e vinicola (vini locali, AOP Saint-Joseph). Vendita e degustazione in azienda."
    ],
    "fr": [
      "Exploitation familiale agricole. Production arboricole (fraises, cerises, abricots, pêches, nectarines, prunes, poires, pommes, jus de fruits, kiwis)et viticole (vins de pays, AOP Saint-Joseph). Vente et dégustation à la ferme."
    ],
    "es": [
      "Granja familiar. Producción de árboles (fresas, cerezas, albaricoques, melocotones, nectarinas, ciruelas, peras, manzanas, zumos de fruta, kiwis) y producción de vino (vinos locales, AOP Saint-Joseph). Venta y degustación en la granja."
    ],
    "nl": [
      "Familieboerderij. Boomproductie (aardbeien, kersen, abrikozen, perziken, nectarines, pruimen, peren, appels, vruchtensap, kiwi's) en wijnproductie (lokale wijnen, AOP Saint-Joseph). Verkoop en proeverij op de boerderij."
    ]
  },
