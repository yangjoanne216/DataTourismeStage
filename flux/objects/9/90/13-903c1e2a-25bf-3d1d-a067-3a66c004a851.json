  "rdfs:comment": {
    "de": [
      "Führungen mit Eve Vernice (Produzentin von essbaren Blumen und Referentin) von Juni bis September. Besuchergruppe zwischen 10 und 25 Personen. Eve teilt mit Ihnen all ihr Wissen über das Thema Frischblumen, vom Anbau bis zum Teller."
    ],
    "en": [
      "Guided tour by Eve Vernice (Edible Flower Producer and Speaker) from June to September. Group of visitors between 10 and 25. Eve will share all her knowledge on the subject of fresh flowers, from culture to plate."
    ],
    "it": [
      "Visita guidata di Eve Vernice (Produttrice di fiori commestibili e conferenziera) da giugno a settembre. Gruppo di visitatori compreso tra 10 e 25. Eve condividerà con voi tutte le sue conoscenze sul tema dei fiori freschi, dalla cultura al piatto."
    ],
    "fr": [
      "Visite guidée par Eve Vernice (Productrice de fleurs comestibles et conférencière) de juin à septembre. Groupe de visiteurs compris entre 10 et 25. Eve vous partagera toutes ses connaissances sur le sujet des fleurs fraîches, de la culture à l'assiette."
    ],
    "es": [
      "Visita guiada por Eve Vernice (productora de flores comestibles y conferenciante) de junio a septiembre. Grupos de entre 10 y 25 visitantes. Eve compartirá sus conocimientos sobre las flores frescas, desde su cultivo hasta el plato."
    ],
    "nl": [
      "Rondleiding door Eve Vernice (eetbare bloemenproducent en docent) van juni tot september. Groepen van 10 tot 25 bezoekers. Eve zal haar kennis van verse bloemen delen, van teelt tot bord."
    ]
  },
  "rdfs:label": {
