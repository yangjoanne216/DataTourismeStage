      "dc:description": {
        "de": [
          "David Darricau, mineralogischer Prospektor, bietet: Mineralien, Natursteinschmuck, Steinhandwerk, Lithotherapie. Geöffnet während der Schulferien in allen Zonen."
        ],
        "en": [
          "David Darricau, mineral prospector, offers: collectible minerals, jewelry natural stones, stone craftsmanship, lithotherapy. Open during school holidays in all areas."
        ],
        "it": [
          "David Darricau, cercatore mineralogico, propone: collezioni di minerali, gioielli in pietra naturale, artigianato in pietra, litoterapia. Aperto durante le vacanze scolastiche in tutte le zone."
        ],
        "fr": [
          "David Darricau, prospecteur minéralogique, propose: minéraux de collection, bijoux pierres naturelles, artisanat de la pierre, lithothérapie. Ouverture vacances scolaires toutes zones."
        ],
        "es": [
          "David Darricau, prospector mineralógico, ofrece: colecciones de minerales, joyas de piedras naturales, artesanía en piedra, litoterapia. Abierto durante las vacaciones escolares en todas las zonas."
        ],
        "nl": [
          "David Darricau, mineralogisch onderzoeker, biedt aan: mineraalcollecties, natuursteenjuwelen, natuursteenambachten, lithotherapie. Open tijdens schoolvakanties in alle zones."
        ]
      },
