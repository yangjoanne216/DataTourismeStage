  "rdfs:comment": {
    "de": [
      "Repas-concert, mit dem Trio Rapsody im Luz d'Olt, ab 20:30 Uhr.\n\nTeller für eine Mahlzeit :\n- entenwurst mit Sommertrüffel, Pommes frites, Salat, warmer Ziegenkäse mit Honig."
    ],
    "en": [
      "Meal-concert, with the trio Rapsody at Luz d'Olt, from 8:30 pm.\n\nMeal plate :\n- duck sausage with summer truffle, French fries, salad, hot goat cheese with honey."
    ],
    "it": [
      "Pasto-concerto, con il trio Rapsody alla Luz d'Olt, dalle 20.30.\n\nPiatto da pranzo :\n- salsiccia d'anatra con tartufo estivo, patatine, insalata, formaggio di capra caldo con miele."
    ],
    "fr": [
      "Repas-concert, avec le trio Rapsody au Luz d'Olt, à partir de 20h30.\n\nAssiette-repas :\n- saucisse de canard à la truffe d'été, frites, salade, chèvre chaud au miel."
    ],
    "nl": [
      "Maaltijd-concert, met het trio Rapsody in de Luz d'Olt, vanaf 20.30 uur.\n\nMaaltijdbord:\n- eendenworst met zomertruffel, frietjes, salade, warme geitenkaas met honing."
    ],
    "es": [
      "Comida-concierto, con el trío Rapsody en la Luz d'Olt, a partir de las 20.30 horas.\n\nPlato de comida :\n- salchicha de pato con trufa de verano, patatas fritas, ensalada, queso de cabra caliente con miel."
    ]
  },
