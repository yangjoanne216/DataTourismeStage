  "rdfs:comment": {
    "de": [
      "Die Weinbar befindet sich nur wenige Schritte von den Plätzen Madeleine, Prêcheurs und Verdun entfernt im Herzen des Aachener Lebens."
    ],
    "en": [
      "The wine bar is located near the Madeleine, Prêcheurs and Verdun squares in the heart of Aix-en-Provence."
    ],
    "it": [
      "L'enoteca si trova a pochi passi da Place Madeleine, Place Prêcheurs e Place Verdun, nel cuore di Aix-en-Provence."
    ],
    "fr": [
      "Le bar à vin est situé à deux pas des places Madeleine, Prêcheurs et Verdun au coeur de la vie aixoise."
    ],
    "es": [
      "El bar de vinos está situado a un paso de la plaza Madeleine, la plaza Prêcheurs y la plaza Verdun, en el corazón de Aix-en-Provence."
    ],
    "nl": [
      "De wijnbar ligt op een steenworp afstand van de Place Madeleine, Place Prêcheurs en Place Verdun in het hart van Aix-en-Provence."
    ]
  },
