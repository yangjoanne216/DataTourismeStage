  "rdfs:comment": {
    "de": [
      "Traditionelles Restaurant, Pizzeria und regionale Küche , im Herzen von Faulquemont. In einem gemütlichen Dekor, mit einer ruhigen Terrasse auf einem wunderschönen Platz, gegenüber der Kirche aus dem 18."
    ],
    "en": [
      "Traditional restaurant, pizzeria and local cuisine, in the heart of Faulquemont. In a warm setting, with a quiet terrace on a magnificent square, opposite the 18th century church."
    ],
    "it": [
      "Ristorante tradizionale, pizzeria e cucina locale, nel cuore di Faulquemont. In un ambiente accogliente, con una tranquilla terrazza su una magnifica piazza, di fronte alla chiesa del XVIII secolo."
    ],
    "fr": [
      "Restaurant traditionnel, pizzeria et cuisine du terroir , au cœur de Faulquemont. Dans un décor chaleureux, avec une terrasse au calme sur une magnifique place, en face de l'église du 18ème siècle."
    ],
    "nl": [
      "Traditioneel restaurant, pizzeria en lokale keuken, in het hart van Faulquemont. In een warme omgeving, met een rustig terras op een prachtig plein, tegenover de 18e eeuwse kerk."
    ],
    "es": [
      "Restaurante tradicional, pizzería y cocina local, en el corazón de Faulquemont. En un entorno cálido, con una tranquila terraza en una magnífica plaza, frente a la iglesia del siglo XVIII."
    ]
  },
