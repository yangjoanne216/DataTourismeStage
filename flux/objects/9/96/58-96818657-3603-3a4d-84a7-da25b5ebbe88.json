  "rdfs:comment": {
    "de": [
      "Entdecken Sie das Le Farniente in Fos-sur-Mer, das sieben Tage die Woche mittags und abends geöffnet ist. Auf der Speisekarte stehen Vorspeisen, Salate, frisches Fleisch und Fisch sowie hausgemachte Desserts."
    ],
    "en": [
      "Discover Le Farniente in Fos-sur-Mer, open 7 days a week for lunch and dinner, with à la carte starters, salads, fresh meat and fish and homemade desserts."
    ],
    "it": [
      "Scoprite Le Farniente a Fos-sur-Mer, aperto 7 giorni su 7 per pranzo e cena, con un menu à la carte di antipasti, insalate, carne e pesce freschi e dessert fatti in casa."
    ],
    "fr": [
      "Découvrez Le Farniente à Fos-sur-Mer, ouvert 7j/7 midi et soir, avec à la carte entrées, salades, viandes et poissons frais et desserts fait-maison."
    ],
    "nl": [
      "Ontdek Le Farniente in Fos-sur-Mer, 7 dagen per week geopend voor lunch en diner, met een à la carte menu van voorgerechten, salades, vers vlees en vis en huisgemaakte desserts."
    ],
    "es": [
      "Descubra Le Farniente en Fos-sur-Mer, abierto los 7 días de la semana para el almuerzo y la cena, con un menú a la carta de entrantes, ensaladas, carnes y pescados frescos y postres caseros."
    ]
  },
