  "rdfs:comment": {
    "de": [
      "Relais für die Flussschifffahrt.\nSanitäranlagen für Menschen mit Behinderungen. Spielplatz für Kinder. Imbiss, Getränke, Eis in der Hochsaison\nGeöffnet vom 1. Mai bis 30. September."
    ],
    "en": [
      "River relay.\nSanitary facilities for the disabled. Playground for children. Snack bar, drinks, ice cream, in high season\nOpen from May 1st to September 30th."
    ],
    "it": [
      "Staffetta fluviale.\nServizi igienici per disabili. Parco giochi per bambini. Snack bar, bevande, gelati, in alta stagione\nAperto dal 1° maggio al 30 settembre."
    ],
    "fr": [
      "Relais fluvial.\nSanitaires pour les personnes handicapés. Aire de jeux pour enfants. Snack, boissons, glaces, en pleine saison. \nOuvert du 1er mai au 30 septembre."
    ],
    "nl": [
      "Rivier estafette.\nSanitaire voorzieningen voor gehandicapten. Speeltuin voor kinderen. Snackbar, drankjes, ijs, in het hoogseizoen\nGeopend van 1 mei tot 30 september."
    ],
    "es": [
      "Relevo en el río.\nInstalaciones sanitarias para discapacitados. Zona de juegos para niños. Snack bar, bebidas, helados, en temporada alta\nAbierto del 1 de mayo al 30 de septiembre."
    ]
  },
