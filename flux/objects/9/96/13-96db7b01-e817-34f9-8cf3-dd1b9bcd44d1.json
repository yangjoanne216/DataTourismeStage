  "rdfs:comment": {
    "de": [
      "Ehemaliger Familienbauernhof, gegenüber der Kirche: Speisekarte, savoyische Spezialitäten, Salate, Mahlzeiten und Gourmetdesserts.\r\nIm Sommer: Grillen am Holzfeuer auf der Terrasse mit Blick auf den Mont-Blanc."
    ],
    "en": [
      "Ancient family farm, opposite to the church : local specialities, salads, meals and gourmet desserts. In summer, wood-fire grill on the terrace, facing Mont-Blanc..."
    ],
    "it": [
      "Vecchia fattoria di famiglia, di fronte alla chiesa: piatti à la carte, specialità savoiarde, insalate, pasti e dessert gourmet.\r\nIn estate, grigliate sul fuoco di legna sulla terrazza, di fronte al Monte Bianco."
    ],
    "fr": [
      "Ancienne ferme de famille, face à l'église : carte, spécialités savoyardes, salades, repas et desserts gourmands .\r\nL'été, grillade au feu de bois sur la terrasse, face au Mont-Blanc.."
    ],
    "nl": [
      "Voormalige familieboerderij, tegenover de kerk: menu, Savoyaardse specialiteiten, salades, maaltijden en gastronomische desserts.\r\nIn de zomer, houtgestookte grill op het terras, met uitzicht op de Mont-Blanc."
    ],
    "es": [
      "Antigua granja familiar, frente a la iglesia: carta, especialidades saboyanas, ensaladas, comidas y postres gourmet .\r\nEn verano, parrillada en la chimenea en la terraza, frente al Mont-Blanc.."
    ]
  },
