  "rdfs:comment": {
    "de": [
      "Kunsthandwerk aus Bali\r\nShowroom für balinesische Kunsthandwerker.\r\nAus indonesischen Steinen geschnitzte Statuen, Gazebo komplett aus Bambus, Möbel aus massiver Teakwurzel, geschnitzte Leuchten aus rekonstituiertem Stein, Innen- und Außendekoration."
    ],
    "en": [
      "Bali Handicrafts\r\nShowroom of Balinese Arts and Crafts.\r\nStatues carved in Indonesian stones, Gazebo entirely in Bamboo, furniture in solid Teak Root, lights carved in reconstituted stone, interior and exterior decoration."
    ],
    "it": [
      "Artigianato di Bali\r\nShowroom di artigianato artistico balinese.\r\nStatue scolpite in pietra indonesiana, Gazebo interamente in Bambù, mobili in massello di Teak Root, luci scolpite in pietra ricomposta, decorazioni interne ed esterne."
    ],
    "fr": [
      "Artisanats Bali \r\nShowroom d’Artisanats d’Art Balinais.\r\nStatues sculptées en pierres  Indonésiennes , Gazebo entièrement en Bambou , mobilier en Racine de Teck massif , luminaires sculptés  en pierre reconstituée , décoration intérieure et extérieure."
    ],
    "nl": [
      "Bali Ambachten\r\nShowroom van Balinese Kunsthandwerk.\r\nBeelden gesneden in Indonesische stenen, Gazebo geheel in Bamboe, meubels in massief Teakhout, lampen gesneden in gereconstrueerde steen, binnen- en buitendecoratie."
    ],
    "es": [
      "Artesanía de Bali\r\nExposición de artesanía balinesa.\r\nEstatuas talladas en piedras indonesias, Gazebo totalmente de bambú, muebles de raíz de teca maciza, luces talladas en piedra reconstituida, decoración interior y exterior."
    ]
  },
