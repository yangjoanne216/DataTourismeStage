  "rdfs:comment": {
    "de": [
      "Themenrestaurant, amerikanisches Abendessen, 1950/1960er Jahre, Elvis/Marilyn-Zeit.\nRahmen und Universum dieser Atmosphäre, 14000 Gedecke/Monat, 18? durchschnittliche Tickets.\nGuter Empfang und gute Leistung."
    ],
    "en": [
      "Theme restaurant, American dinner, year 1950/1960 Elvis/Marilyn period.\nSetting and universe of this atmosphere, 14000 place settings/month, 18€ average tickets.\nReception and quality service."
    ],
    "it": [
      "Ristorante a tema, cena americana, anni 1950/1960, epoca Elvis/Marilyn.\nL'ambiente e l'atmosfera di questo ristorante, 14.000 coperti al mese, 18? biglietti medi.\nAccoglienza e servizio di alta qualità."
    ],
    "fr": [
      "Restaurant à thèmes, dîner américain, année 1950/1960 époque Elvis/Marilyn.\nCadre et univers de cette ambiance, 14000 couverts/mois, 18€ tickets moyens.\nAccueil et prestation de qualité."
    ],
    "nl": [
      "Thema restaurant, Amerikaans diner, jaren 1950/1960 Elvis/Marilyn tijdperk.\nHet decor en de sfeer van dit restaurant, 14.000 couverts/maand, 18? gemiddelde kaartjes.\nHoogwaardige ontvangst en service."
    ],
    "es": [
      "Restaurante temático, cena americana, época de Elvis/Marilyn de los años 50/1960.\nEl entorno y el ambiente de este restaurante, 14.000 cubiertos/mes, 18? de media.\nAcogida y servicio de alta calidad."
    ]
  },
