  "rdfs:comment": {
    "de": [
      "La Borde, Erlebnisbauernhof, wo die Tiere in guter Nachbarschaft leben und auf Ihren Besuch und Ihre beste Aufmerksamkeit warten."
    ],
    "en": [
      "La Borde, a discovery farm where the animals live peacefully side by side and await your best intentions."
    ],
    "it": [
      "La Borde, una fattoria scoperta dove gli animali vivono in armonia e attendono la vostra visita e le vostre migliori attenzioni."
    ],
    "fr": [
      "La Borde, ferme découverte où les animaux se côtoient en bonne intelligence et attendent votre visite et vos meilleures attentions."
    ],
    "nl": [
      "La Borde, een ontdekkingsboerderij waar de dieren in harmonie samenleven en wachten op uw bezoek en uw beste aandacht."
