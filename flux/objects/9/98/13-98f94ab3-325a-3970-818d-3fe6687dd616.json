  "rdfs:comment": {
    "de": [
      "Shuttlebus zur Ile Sainte-Marguerite. Kommentierte Bootsfahrten nach Monaco, Saint-Tropez und zur Corniche d'Or von April bis September. Kartenverkauf vor Ort und im Office de Tourisme de Vallauris Golfe-Juan."
    ],
    "en": [
      "Shuttle to Ile Sainte-Marguerite. Guided cruise to Monaco, Saint-Tropez and the Corniche d'Or from April to September. Tickets on site and at the Vallauris Golfe-Juan Tourist Office."
    ],
    "it": [
      "Navette per Ile Sainte-Marguerite. Commentate le crociere a Monaco, Saint-Tropez e la Corniche d'Or da aprile a settembre. Biglietti in loco e presso l'Ufficio del Turismo di Vallauris Golfe-Juan."
    ],
    "fr": [
      "Navettes vers l'Ile Sainte-Marguerite. Croisières commentées vers Monaco, Saint-Tropez et la Corniche d'Or d'avril à septembre. Billetterie sur place et à l'Office de Tourisme de Vallauris Golfe-Juan."
    ],
    "es": [
      "Traslados a la isla Sainte-Marguerite. Cruceros guiados a Mónaco, Saint-Tropez y la Corniche d'Or de abril a septiembre. Taquilla en el lugar y en la Oficina de Turismo de Vallauris Golfe-Juan."
    ],
    "nl": [
      "Pendeldienst naar Sainte-Marguerite Island. Begeleide cruises naar Monaco, Saint-Tropez en de Corniche d'Or van april tot september. Loket ter plaatse en bij het toeristenbureau van Vallauris Golfe-Juan."
    ]
  },
  "rdfs:label": {
