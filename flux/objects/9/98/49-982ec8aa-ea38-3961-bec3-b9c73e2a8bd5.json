  "rdfs:comment": {
    "de": [
      "2 Gästezimmer in Mazé in einem restaurierten Bauernhaus"
    ],
    "en": [
      "2 guest rooms in Mazé in a restored farmhouse"
    ],
    "it": [
      "2 camere per gli ospiti a Mazé in una casa colonica ristrutturata"
    ],
    "fr": [
      "2 chambres d'hôtes à Mazé dans une longère restaurée"
    ],
    "es": [
      "2 habitaciones de huéspedes en Mazé en una granja restaurada"
