  "rdfs:comment": {
    "de": [
      "Traditionelles Restaurant mit authentischer Küche, in dem das einzige Motto hausgemachte und gesellige Gerichte sind. Herzliche Atmosphäre. Vorschlag für regionale Gerichte: l'Estofinade."
    ],
    "en": [
      "Traditional restaurant with an authentic cuisine where the only watchword is home cooking and conviviality. Warm atmosphere. Proposal of regional dishes: the Estofinade."
    ],
    "it": [
      "Ristorante tradizionale con una cucina autentica dove l'unica parola d'ordine è cucina casalinga e convivialità. Atmosfera calda. Piatti regionali disponibili: l'Estofinade."
    ],
    "fr": [
      "Restaurant traditionnel avec une cuisine authentique où le seul mot d'ordre est une cuisine maison et conviviale. Ambiance chaleureuse. Proposition de plats régionaux : l'Estofinade."
    ],
    "nl": [
      "Traditioneel restaurant met een authentieke keuken waar het enige sleutelwoord thuis koken en gezelligheid is. Warme sfeer. Regionale gerechten: l'Estofinade."
    ],
    "es": [
      "Restaurante tradicional con una cocina auténtica donde la única consigna es la cocina casera y la convivencia. Ambiente cálido. Platos regionales disponibles: l'Estofinade."
    ]
  },
