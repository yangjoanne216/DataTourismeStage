  "rdfs:comment": {
    "de": [
      "Möblierte Unterkunft in Cransac-les-Thermes, dem einzigen Kurort im Aveyron und der Stadt des Wohlbefindens. Einfamilienhaus Erdgeschoss und erster Stock."
    ],
    "en": [
      "Furnished let located at the Aveyron spa resort for rheumatism. Two-storey house located in town."
    ],
    "it": [
      "Alloggio turistico ammobiliato situato a Cransac-les-Thermes, unica stazione termale dell'Aveyron e città del benessere. Casa indipendente al piano terra e al primo piano."
    ],
    "fr": [
      "Meublé de tourisme situé à Cransac-les-Thermes, l'unique station thermale aveyronnaise et la ville de bien-être. Maison individuelle rez-de-chaussée et 1er étage."
    ],
    "nl": [
      "Gemeubileerde toeristische accommodatie gelegen in Cransac-les-Thermes, het enige kuuroord in Aveyron en de stad van het welzijn. Vrijstaande woning begane grond en 1e verdieping."
    ],
    "es": [
      "Amueblado de turismo en Cransac-les-Thermes, el unico centro termal en Aveyron y la ciudad del bienestar."
    ]
  },
