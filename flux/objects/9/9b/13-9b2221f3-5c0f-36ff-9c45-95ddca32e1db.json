  "rdfs:comment": {
    "de": [
      "Restaurant in einer Berghütte. Dekoration hauptsächlich aus Holz. Gemütliche Atmosphäre. Terrasse nach Süden ausgerichtet. Jeden Mittag Brasserie-Service. Jeden Abend traditionelle Küche."
    ],
    "en": [
      "Mountain chalet restaurant. Décor mainly wood. Warm, cosy atmosphere. South-facing terrace. Brasserie service every lunchtime. Traditional cuisine la carte service every evening."
    ],
    "it": [
      "Ristorante dello chalet di montagna. Decorata principalmente con il legno. Atmosfera calda. Terrazza esposta a sud. Tutti i pranzi con servizio di brasserie. Ogni sera menù di cucina tradizionale."
    ],
    "fr": [
      "Restaurant chalet de montagne. Décoration principalement de bois. Ambiance chaleureuse. Terrasse plein sud. Tous les midis service brasserie. Tous les soirs carte de cuisine traditionnelle."
    ],
    "es": [
      "Restaurante del chalet de montaña. Decorado principalmente con madera. Ambiente cálido. Terraza orientada al sur. Todos los almuerzos en el servicio de brasserie. Todas las noches menú de cocina tradicional."
    ],
    "nl": [
      "Restaurant bergchalet.  Decoratie hoofdzakelijk in hout.  Warme ambiance.  Terras zuidwaards gericht.  Elke middag brasseriedienst.  Elke avond spijskaart van traditionele keuken."
    ]
  },
