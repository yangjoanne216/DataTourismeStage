  "rdfs:comment": {
    "de": [
      "Entdecken Sie in Saint-Rémy-de-Provence Cave du Mistral, um Weine aus der Region und darüber hinaus zu entdecken."
    ],
    "en": [
      "In Saint-Rémy-de-Provence, discover Cave du Mistral to discover wines from the region and elsewhere."
    ],
    "it": [
      "A Saint-Rémy-de-Provence, scoprite la Cave du Mistral per conoscere i vini della regione e non solo."
    ],
    "fr": [
      "À Saint-Rémy-de-Provence, découvrez Cave du Mistral pour découvrir des vins de la région et d'ailleurs."
    ],
    "nl": [
      "Ontdek in Saint-Rémy-de-Provence de Cave du Mistral om wijnen uit de streek en elders te ontdekken."
    ],
    "es": [
      "En Saint-Rémy-de-Provence, descubra la Cave du Mistral para conocer los vinos de la región y de otros lugares."
    ]
  },
