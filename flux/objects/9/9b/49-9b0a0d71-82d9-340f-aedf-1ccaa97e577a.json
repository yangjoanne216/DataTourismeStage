  "rdfs:comment": {
    "de": [
      "Erleben Sie eine einzigartige Erfahrung inmitten der Natur, indem Sie in unseren Jurten gegenüber den Lamas unserer Farm übernachten."
    ],
    "en": [
      "Come and live a unique experience in the middle of nature by staying in our yurts in front of the llamas of our farm."
    ],
    "it": [
      "Venite a vivere un'esperienza unica in mezzo alla natura soggiornando nelle nostre yurte di fronte ai lama della nostra fattoria."
    ],
    "fr": [
      "Venez vivre une expérience unique en pleine nature en séjournant dans nos yourtes face aux lamas de notre ferme."
    ],
    "es": [
      "Ven a vivir una experiencia única en plena naturaleza alojándote en nuestras yurtas frente a las llamas de nuestra granja."
    ],
    "nl": [
      "Kom en beleef een unieke ervaring midden in de natuur door te verblijven in onze yurts met uitzicht op de lama's van onze boerderij."
    ]
  },
