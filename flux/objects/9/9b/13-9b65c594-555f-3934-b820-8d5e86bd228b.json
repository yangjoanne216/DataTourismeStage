  "rdfs:comment": {
    "de": [
      "Restaurant mit Terrasse im Herzen des Stadtzentrums, in der Nähe des Marktplatzes. Im Holzofen gebackene Pizzas. Bereich für Speisen zum Mitnehmen. Crêperie-Ecke abends und am Wochenende."
    ],
    "en": [
      "Restaurant with terrace in the heart of downtown, near the market place. Pizzas cooked on a wood fire. Take away food area. Pancake corner for evenings and weekends."
    ],
    "it": [
      "Ristorante con terrazza nel cuore del centro città, vicino alla piazza del mercato. Pizze cotte a legna. Area d'asporto. Angolo dei pancake la sera e nei fine settimana."
    ],
    "fr": [
      "Restaurant avec terrasse au cœur du centre-ville, à proximité de la place du marché. Pizzas cuites au feu de bois. Espace de restauration à emporter. Coin crêperie soir et weekend."
    ],
    "es": [
      "Restaurante con terraza en pleno centro de la ciudad, cerca de la plaza del mercado. Pizzas al horno de leña. Zona de comida para llevar. Rincón de panqueques por la noche y los fines de semana."
    ],
    "nl": [
      "Restaurant met terras in het hart van Montrevel, dicht bij de markt. Pizza gebakken in houtvuur. Tevens afhaalrestaurant. Pannekoeken 's-avonds en in het weekend."
    ]
  },
