  "rdfs:comment": {
    "de": [
      "Christelle und Philippe empfangen Sie ab 10:30 bis 11:30 Uhr zum Frühstück, von 12:00 bis 14:30 Uhr zum Mittagessen und von 18:30 bis 20:30 Uhr zum Abendservice. Perigourdine oder italienische Küche zur Auswahl."
    ],
    "en": [
      "Philippe offers you traditional and italian food with fresh products. Open from april to november."
    ],
    "it": [
      "Christelle e Philippe vi accolgono dalle 10:30 alle 11:30 per la colazione, dalle 12:00 alle 14:30 per il pranzo e dalle 18:30 alle 20:30 per la cena. Scelta di cucina perigordiana o italiana."
    ],
    "fr": [
      "Christelle et Philippe vous accueillent dès 10h30 à 11h30 pour le petit déjeuner, de 12h00 à 14h30 pour le déjeuner et de 18h30 à 20h30 pour le service du soir. Cuisine périgourdine ou italienne au choix."
    ],
    "es": [
      "Christelle y Philippe le reciben de 10:30 a 11:30 para el desayuno, de 12:00 a 14:30 para el almuerzo y de 18:30 a 20:30 para la cena. A elegir entre cocina perigordiana o italiana."
    ],
    "nl": [
      "Christelle en Philippe ontvangen u van 10.30 tot 11.30 uur voor het ontbijt, van 12.00 tot 14.30 uur voor de lunch en van 18.30 tot 20.30 uur voor het diner. Keuze uit Perigordiaanse of Italiaanse keuken."
    ]
  },
