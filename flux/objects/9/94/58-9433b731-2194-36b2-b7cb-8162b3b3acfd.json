  "rdfs:comment": {
    "de": [
      "Als einfaches Hotel mit gutem Komfort empfängt Sie das Hotel Gambetta im Stadtzentrum, zwischen dem Bahnhof Saint-Charles und der Canebière, nur wenige Minuten vom Alten Hafen entfernt."
    ],
    "en": [
      "A simple and comfortable hotel, the Gambetta hotel welcomes you in the heart of the city, between the Saint-Charles train station and the Canebière, a few minutes from the Old Port."
    ],
    "it": [
      "Semplice e confortevole, l'Hotel Gambetta vi accoglie nel cuore della città, tra la stazione ferroviaria di Saint-Charles e la Canebière, a pochi minuti dal Porto Vecchio."
    ],
    "fr": [
      "Hôtel simple et de bon confort, l'hôtel Gambetta vous accueille en plein centre ville, entre la gare Saint-Charles et la Canebière, à quelques minutes du Vieux Port."
    ],
    "es": [
      "Hotel sencillo y confortable, el Hotel Gambetta le da la bienvenida en el corazón de la ciudad, entre la estación de Saint-Charles y la Canebière, a pocos minutos del Puerto Viejo."
    ],
    "nl": [
      "Hotel Gambetta, een eenvoudig en comfortabel hotel, verwelkomt u in het hart van de stad, tussen het station Saint-Charles en de Canebière, op enkele minuten van de oude haven."
    ]
  },
