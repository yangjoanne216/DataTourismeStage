  "rdfs:comment": {
    "de": [
      "Unterkunft auf dem Bauernhof, 1 km von der Autobahn A75 entfernt. Bauernhaus aus dem 17. Jahrhundert, 2020 renoviert, mit Stein, Balken und modernen Elementen. Kommen Sie und entdecken Sie unsere Umgebung auf dem Land."
    ],
    "en": [
      "Gîte on a farm, 1km from the A75 motorway. Farmhouse from the 17th century, renovated in 2020, with stones, beams and modernity. Come and discover our environment in the countryside."
    ],
    "it": [
      "Agriturismo a 1 km dall'autostrada A75. Casa colonica del XVII secolo, ristrutturata nel 2020, con pietre, travi e modernità. Venite a scoprire il nostro ambiente di campagna."
    ],
    "fr": [
      "Gîte à la ferme, à 1km de l'Autoroute A75. Corps de ferme du 17ème siècle, rénové en 2020, en pierres, poutres et modernité. Venez découvrir notre environnement à la campagne."
    ],
    "es": [
      "Casa de campo, a 1 km de la autopista A75. Masía del siglo XVII, renovada en 2020, con piedras, vigas y modernidad. Venga a descubrir nuestro entorno rural."
    ],
    "nl": [
      "Boerderij, 1km van de A75. Boerderij uit de 17e eeuw, gerenoveerd in 2020, met stenen, balken en moderniteit. Kom en ontdek onze landelijke omgeving."
    ]
  },
