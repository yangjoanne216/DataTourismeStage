  "rdfs:comment": {
    "de": [
      "Dorf auf dem Gipfel eines Hügels, der in Restanques gestaffelt ist, voller Charakter mit Felsenhäusern."
    ],
    "en": [
      "Village perched at the top of a hilltop terraced hill, full of character with rock houses."
    ],
    "it": [
      "Un villaggio arroccato su una collinetta, pieno di carattere con case di roccia."
    ],
    "fr": [
      "Village perché au sommet d'une butte étagée en restanques, plein de caractère avec des maisons rupestres."
    ],
    "es": [
      "Un pueblo encaramado en lo alto de una colina, lleno de carácter con casas de piedra."
    ],
    "nl": [
      "Een dorp bovenop een heuvel, vol karakter met rotswoningen."
    ]
  },
