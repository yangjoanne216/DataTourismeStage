  "rdfs:comment": {
    "de": [
      "Ô'Crépidon, ein Ort des Teilens! Entdecken Sie diese gesellige Crêperie mit familiärer Atmosphäre und genießen Sie eine Küche mit frischen und hausgemachten Produkten."
    ],
    "en": [
      "Ô'Crépidon, a place to share ! Discover this friendly creperie with a family atmosphere, and enjoy a cuisine with fresh and home-made products."
    ],
    "it": [
      "Ô'Crépidon, un luogo di condivisione! Scoprite questa accogliente creperia dall'atmosfera familiare e gustate una cucina con prodotti freschi e fatti in casa."
    ],
    "fr": [
      "Ô'Crépidon, un lieu de partage ! Découvrez cette crêperie conviviale à l'ambiance familiale, et savourez une cuisine avec des produits frais et fait maison."
    ],
    "nl": [
      "Ô'Crépidon, een plek om te delen! Ontdek deze gezellige crêperie met een familiale sfeer, en geniet van een keuken met verse en zelfgemaakte producten."
    ],
    "es": [
      "Ô'Crépidon, ¡un lugar para compartir! Descubra esta acogedora crepería de ambiente familiar y disfrute de una cocina con productos frescos y caseros."
    ]
  },
