  "rdfs:comment": {
    "de": [
      "Wenn Sie durch den Torbogen des Pont des Lanternes gehen, genießen Sie diesen herrlichen Blick auf Bayeux und seine Kathedrale."
    ],
    "en": [
      "Passing under the archway of the Lanterns Bridge, enjoy the magnificent view of Bayeux and its cathedral."
    ],
    "it": [
      "Passando sotto l'arco del Ponte delle Lanterne, godetevi la magnifica vista di Bayeux e della sua cattedrale."
    ],
    "fr": [
      "En passant sous l’arcade du pont des Lanternes, profitez de cette vue magnifique sur Bayeux et sa cathédrale."
    ],
    "es": [
      "Pasando bajo el arco del Puente de las Linternas, disfrute de la magnífica vista de Bayeux y su catedral."
    ],
    "nl": [
      "Onder de boog van de Lantaarnbrug door geniet u van het prachtige uitzicht op Bayeux en zijn kathedraal."
    ]
  },
