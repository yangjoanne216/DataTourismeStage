  "rdfs:comment": {
    "de": [
      "Unterkunft von 30m2 für 2 bis 6 Personen.\nZu Ihrer Verfügung: Kühlschrank, Mikrowelle, Kaffeemaschine, Wasserkocher, Bettwäsche und Handtücher, Bademäntel, Strom ...\n63.60 €/Person."
    ],
    "en": [
      "Lodging of 30m2 for 2 to 6 people.\nAt your disposal : fridge, microwave, coffee machine, kettle, bed linen and towels, bathrobes, electricity ...\n63.60 €/person."
    ],
    "it": [
      "Alloggio di 30 m2 per 2-6 persone.\nA disposizione: frigorifero, microonde, macchina per il caffè, bollitore, biancheria da letto e da bagno, accappatoi, elettricità...\n63.60 ?/persona."
    ],
    "fr": [
      "Hébergement de 30m2 pour 2 à 6 personnes.\nA votre disposition : frigo, micro-ondes, cafetière, bouilloire, linge de lit et toilette, peignoirs, électricité ...\n63.60 €/personne."
    ],
    "es": [
      "Alojamiento de 30m2 para 2 a 6 personas.\nA su disposición: nevera, microondas, cafetera, hervidor de agua, ropa de cama y toallas, albornoces, electricidad...\n63.60 €/persona."
    ],
    "nl": [
      "Accommodatie van 30m2 voor 2 tot 6 personen.\nTot uw beschikking: koelkast, magnetron, koffiezetapparaat, waterkoker, beddengoed en handdoeken, badjassen, elektriciteit ...\n63.60 €/persoon."
    ]
  },
