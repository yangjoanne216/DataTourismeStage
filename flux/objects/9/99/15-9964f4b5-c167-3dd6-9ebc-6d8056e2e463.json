  "rdfs:comment": {
    "de": [
      "Gesellige Ausflüge auf die Loire mit traditionellen Booten (Boote für 12 Personen). Authentische Begegnung mit dem Fluss, seiner Natur und seiner Geschichte. Starts von Chaumont-sur-Loire und Amboise."
    ],
    "en": [
      "Friendly boat tours on the Loire river in traditional boats (12 seats boats). Authentic meeting with the river, its nature and its history. Departure from Chaumont-sur-Loire."
    ],
    "it": [
      "Gite amichevoli sulla Loira in barche tradizionali (barche da 12 persone). Un incontro autentico con il fiume, la sua natura e la sua storia. Partenze da Chaumont-sur-Loire."
    ],
    "fr": [
      "Promenades conviviales sur la Loire en bateaux traditionnels (embarcations de 12 personnes). Rencontre authentique avec le fleuve, sa nature et son histoire. Départs de Chaumont-sur-Loire."
    ],
    "nl": [
      "Vriendschappelijke tochten op de Loire in traditionele boten (12-persoons). Authentieke ontmoeting met de rivier, haar natuur en haar geschiedenis. Vertrek vanuit Chaumont-sur-Loire."
    ],
    "es": [
      "Viajes amistosos por el Loira en barcos tradicionales (barcos de 12 personas). Un auténtico encuentro con el río, su naturaleza y su historia. Salidas desde Chaumont-sur-Loire."
    ]
  },
  "rdfs:label": {
    "de": [
      "Découverte de la Loire en bateau traditionnel - Au départ d'Amboise et de Chaumont-sur-Loire"
    ],
    "en": [
      "Traditional boat outings from Chaumont or Amboise to discover the River Loire"
    ],
    "fr": [
      "Balade en bateau traditionnel sur la Loire avec Millière Raboton"
    ]
  },
