  "rdfs:comment": {
    "de": [
      "Die Country Pony Ranch empfängt Erwachsene, Jugendliche und Kinder ab 2 Jahren auf Pferden und Ponys für 30-minütige bis 3-stündige Ausritte durch die Bocage Bourbonnais."
    ],
    "en": [
      "The Country Pony Ranch welcomes adults, young people and children, from 2 years old, on horse and pony for rides of 30 minutes to 3 hours through the Bocage Bourbonnais."
    ],
    "it": [
      "Il Country Pony Ranch accoglie adulti, giovani e bambini, a partire dai 2 anni, su cavalli e pony per passeggiate da 30 minuti a 3 ore attraverso il Bocage Bourbonnais."
    ],
    "fr": [
      "Le Country Pony Ranch accueille les adultes, les jeunes et les enfants, dès 2 ans, à cheval et à poney pour des balades de 30 minutes à 3 heures à travers le Bocage Bourbonnais."
    ],
    "es": [
      "El Country Pony Ranch acoge a adultos, jóvenes y niños, a partir de 2 años, en caballo y poni para paseos de 30 minutos a 3 horas por el Bocage Bourbonnais."
    ],
    "nl": [
      "De Country Pony Ranch verwelkomt volwassenen, jongeren en kinderen, vanaf 2 jaar, op paard en pony voor ritten van 30 minuten tot 3 uur door de Bocage Bourbonnais."
    ]
  },
