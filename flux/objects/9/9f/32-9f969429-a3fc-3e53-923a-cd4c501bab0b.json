  "rdfs:comment": {
    "de": [
      "Im Yacht Café können Sie am Hafen von Plaisance im Schatten der Pinien eine südländische Küche genießen."
    ],
    "en": [
      "The Yacht Café offers you a southern cuisine on the marina in the shade of the pine trees."
    ],
    "it": [
      "Lo Yacht Café offre la possibilità di gustare la cucina meridionale all'ombra dei pini del porto turistico."
    ],
    "fr": [
      "Le Yacht Café vous propose de déguster une cuisine méridionale sur le port de Plaisance à l'ombre des pins."
    ],
    "nl": [
      "In het Yacht Café kunt u in de schaduw van de pijnbomen in de jachthaven genieten van de zuiderse keuken."
    ],
    "es": [
      "El Yacht Café le ofrece la posibilidad de disfrutar de la cocina sureña a la sombra de los pinos del puerto deportivo."
    ]
