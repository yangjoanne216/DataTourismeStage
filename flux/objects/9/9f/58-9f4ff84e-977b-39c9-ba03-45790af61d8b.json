      "dc:description": {
        "de": [
          "Konfitüren, Olivenöle, Tapenade, Fruchtfleischessig, Weinkeller, Tees und Kräutertees sowie Trüffelspezialitäten."
        ],
        "en": [
          "Delicatessen, Food supply store.\r\n\r\nJam, tapenade, olive oil, vinegar, wine, teas, truffle specialities and famous recipes from Provence."
        ],
        "it": [
          "Marmellate, oli d'oliva, tapenade, aceto di polpa di frutta, cantina, tè e tisane e specialità al tartufo."
        ],
        "fr": [
          "Confitures, huiles d'olive, tapenade, vinaigre à la pulpe de fruits, cave à vin, thés et tisanes ainsi que des spécialités à base de truffe."
        ],
        "es": [
          "Mermeladas, aceites de oliva, tapenade, vinagre de pulpa de fruta, bodega, tés y tisanas, así como especialidades de trufa."
        ],
        "nl": [
          "Jam, olijfolie, tapenade, vruchtenpulpazijn, wijnkelder, thee en kruidenthee en truffelspecialiteiten."
        ]
      },
