  "rdfs:comment": {
    "de": [
      "Sehr Allier ! Bauernladen in Saint-Pourçain: gut, lokal, direkt von den Erzeugern!\r\nEin Laden, der seinen Erzeugern gehört und Ihnen alles bietet, was Sie brauchen!"
    ],
    "en": [
      "Very Allier ! Producers' store in Saint-Pourçain : good, local, direct producers !\r\nA store that belongs to its producers and that offers you everything you need!"
    ],
    "it": [
      "Molto Allier! Negozio di produttori a Saint-Pourçain: buono, locale, direttamente dai produttori!\r\nUn negozio che appartiene ai suoi produttori e che vi offre tutto ciò di cui avete bisogno!"
    ],
    "fr": [
      "Très Allier ! Magasin de producteurs à Saint-Pourçain : bon, local, direct producteurs ! \r\nUn magasin qui appartient à ses producteurs et qui vous propose tout ce dont vous avez besoin !"
    ],
    "es": [
      "¡Muy Allier! Tienda de productores en Saint-Pourçain: ¡buena, local y directa de los productores!\r\nUna tienda que pertenece a sus productores y que le ofrece todo lo que necesita"
    ],
    "nl": [
      "Heel Allier! Producentenwinkel in Saint-Pourçain: goed, lokaal, rechtstreeks van de producenten!\r\nEen winkel die bij zijn producenten hoort en u alles biedt wat u nodig heeft!"
    ]
  },
