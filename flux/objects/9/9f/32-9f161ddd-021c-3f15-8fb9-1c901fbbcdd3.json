  "rdfs:comment": {
    "de": [
      "Ich drehe, modelliere, skulptiere, knete und wende die Erde, um meine Tiere in Raku oder Steingut versenken zu lassen."
    ],
    "en": [
      "I turn, model, sculpt, knead and turn the earth to immerse my animals in raku or stoneware."
    ],
    "it": [
      "Giro, modello, scolpisco, impasto e rigiro l'argilla per immergere i miei animali nel raku o nel gres."
    ],
    "fr": [
      "Je tourne, je modèle, je sculpte, pétrie et tourne la terre pour faire immerger mes animaux en raku ou en grès."
    ],
    "es": [
      "Vuelvo, modelo, esculpo, amaso y giro la arcilla para sumergir mis animales en el raku o el gres."
    ],
