  "rdfs:comment": {
    "de": [
      "Vermietung in La Roche-Posay - Großes Studio von 20 m² - 1. Stock - sehr sonnig, im Stadtzentrum, 1 km von den Thermen des Connétable entfernt. Voll ausgestattete Küche - Flachbildfernseher und Wifi - Gemeinschaftswaschmaschine - Ruhige Lage mit Terrasse und Gemeinschaftsgarten von 1000 m². Gemeinsamer Grill."
    ],
    "en": [
      "Location à la Roche-Posay - Large 20 m² studio apartment - 1st floor - very sunny, in town center, 1km from the Thermes du Connétable. Fully-equipped kitchen - flat-screen TV and wifi - shared washing machine - Quiet location with terrace and 1000 m² shared garden. Shared barbecue."
    ],
    "it": [
      "Location à la Roche-Posay - Ampio monolocale di 20 m² - 1° piano - molto soleggiato, nel centro del paese, a 1 km dalle Thermes du Connétable. Cucina completamente attrezzata - TV a schermo piatto e Wi-Fi - lavatrice in comune - Posizione tranquilla con terrazza e giardino in comune di 1000 m². Barbecue in comune."
    ],
    "fr": [
      "Location à la Roche-Posay - Grand studio de 20 m²- 1er étage - très ensoleillé, en centre-ville, à 1km des Thermes du Connétable. Cuisine toute équipée - TV écran plat et wifi - Lave linge commun - Au calme disposant d'une terrasse et d'un jardin commun de 1000 m². Barbecue commun."
    ],
    "nl": [
      "Location à la Roche-Posay - Groot studio-appartement van 20 m² - 1e verdieping - zeer zonnig, in het centrum, op 1 km van de Thermen van Connétable. Volledig ingerichte keuken - flatscreen-tv en Wi-Fi - gedeelde wasmachine - Rustige ligging met terras en gedeelde tuin van 1000 m². Gemeenschappelijke barbecue."
    ],
    "es": [
      "Location à la Roche-Posay - Gran estudio de 20 m² - 1er piso - muy soleado, en el centro de la ciudad, a 1 km de las Thermes du Connétable. Cocina totalmente equipada - TV de pantalla plana y Wi-Fi - lavadora compartida - Situación tranquila con terraza y jardín compartido de 1000 m². Barbacoa compartida."
    ]
  },
