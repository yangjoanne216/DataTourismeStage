  "rdfs:comment": {
    "de": [
      "Pfirsichbäume sind diese kleinen Hütten, die sich an der Strandpromenade aneinanderreihen."
    ],
    "en": [
      "Fisheries are those little shacks that line the waterfront."
    ],
    "it": [
      "Le pescherie sono le piccole capanne che costeggiano il lungomare."
    ],
    "fr": [
      "Les pêcheries sont ces petites cabanes qui s'alignent sur le front de mer."
    ],
    "es": [
      "Las pesquerías son las pequeñas cabañas que bordean el paseo marítimo."
    ],
    "nl": [
      "De visserijen zijn de kleine hutjes die langs de waterkant staan."
    ]
  },
