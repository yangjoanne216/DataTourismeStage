  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/383b1b12-a668-3253-8d3e-6b01192bdc55",
      "dc:description": {
        "de": [
          "2 Schlafzimmer (2 Einzelbetten, 1 Doppelbett), Wohnzimmer/Kochecke, Gemeinschaftswaschmaschine, Badezimmer, Gemeinschaftshof, Grundstück, Gartenmöbel, Grill. Im Winter zusätzlich Elektroheizung."
        ],
        "en": [
          "2 bedrooms (2 single beds, 1 double bed), living room/kitchenette, shared washing machine, bathroom, shared courtyard, land, garden furniture, barbecue. Electric heating in addition in winter."
        ],
        "it": [
          "2 camere da letto (2 letti singoli, 1 letto matrimoniale), soggiorno/angolo cottura, lavatrice in comune, bagno, cortile in comune, terreno, mobili da giardino, barbecue. Riscaldamento elettrico non incluso in inverno."
        ],
        "fr": [
          "2 Chambres (2 lits 1 pl., 1 lit 2 pl.), séjour/coin cuisine, lave linge commun, salle de bains, cour commune, terrain, salon de jardin, barbecue. Chauffage électrique en sus l'hiver."
        ],
        "nl": [
          "2 slaapkamers (2 eenpersoonsbedden, 1 tweepersoonsbed), woonkamer/keukenhoek, gemeenschappelijke wasmachine, badkamer, gemeenschappelijke binnenplaats, terrein, tuinmeubels, barbecue. Elektrische verwarming niet inbegrepen in de winter."
        ],
        "es": [
          "2 habitaciones (2 camas individuales, 1 cama doble), salón/cocina, lavadora compartida, baño, patio compartido, terreno, muebles de jardín, barbacoa. Calefacción eléctrica no incluida en invierno."
        ]
      },
