  "rdfs:comment": {
    "de": [
      "Entdecken Sie die Ufer des Flusses Moine bei einem Ausritt, der nur wenige Kilometer von Clisson entfernt stattfindet."
    ],
    "en": [
      "Discover the banks of the Moine River with a horseback ride only a few kilometers from Clisson."
    ],
    "it": [
      "Scoprite le rive del Moine con una passeggiata a cavallo a pochi chilometri da Clisson."
    ],
    "fr": [
      "Partez à la découverte des bords de la Moine avec une balade à cheval à seulement quelques kilomètres de Clisson."
    ],
    "nl": [
      "Ontdek de oevers van de Moine met een tocht te paard op slechts enkele kilometers van Clisson."
    ],
    "es": [
      "Descubra las orillas del Moine con un paseo a caballo a pocos kilómetros de Clisson."
    ]
