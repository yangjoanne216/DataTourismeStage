  "rdfs:comment": {
    "de": [
      "Herstellung in Saint Auban sur l'Ouvèze von Düften aus der Provence: Seifen - Kerzen - Parfüms - Kosmetika für zu Hause (Diffusoren & Parfüms), Lavendel- und Lavandinsäckchen, Kräuter der Provence, ätherische Öle."
    ],
    "en": [
      "Boutique at the Chatelard 1802 Senteur de Provence factory. Production in Saint Auban sur l'Ouvèze of scents from Provence: Hand-crafted soaps - candles - perfumes - cosmetics for the home (diffusers & perfumes), lavender and lavandin sachets, Provencal herbs, essential oils."
    ],
    "it": [
      "Produzione a Saint Auban sur l'Ouvèze di profumi provenzali: saponi - candele - profumi - cosmetici per la casa (diffusori e profumi), sacchetti di lavanda e lavandina, erbe provenzali, oli essenziali."
    ],
    "fr": [
      "Fabrication à Saint Auban sur l'Ouvèze de senteurs de Provence :  savons - bougies - parfums - cosmétiques pour la maison (diffuseurs & parfums), sachets de lavande et lavandin, herbes de Provence, huiles essentielles."
    ],
    "nl": [
      "Productie in Saint Auban sur l'Ouvèze van geuren uit de Provence: zepen - kaarsen - parfums - cosmetica voor thuis (diffusors & parfums), lavendel en lavandin zakjes, Provençaalse kruiden, essentiële oliën."
    ],
    "es": [
      "Producción en Saint Auban sur l'Ouvèze de aromas de la Provenza: jabones - velas - perfumes - cosméticos para el hogar (difusores y perfumes), bolsitas de lavanda y lavandín, hierbas provenzales, aceites esenciales."
    ]
  },
