      "dc:description": {
        "de": [
          "Tagesgericht unter der Woche, abwechslungsreiche traditionelle Speisekarte, Sonntagsmenü. Bar durchgehend ab 11 Uhr geöffnet. Sommerterrasse."
        ],
        "en": [
          "Dish of the day during the week, varied traditional menu, Sunday menu. Bar open continuously from 11am. Summer terrace."
        ],
        "it": [
          "Piatto del giorno durante la settimana, menù tradizionale vario, menù della domenica. Bar aperto ininterrottamente dalle 11.00. Terrazza estiva."
        ],
        "fr": [
          "Plat du jour en semaine, carte variée traditionnelle, menu dominical. Bar ouvert en continue à partir de 11h. Terrasse d'été."
        ],
        "es": [
          "Plato del día entre semana, menú tradicional variado, menú del domingo. Bar abierto continuamente a partir de las 11h. Terraza de verano."
        ],
        "nl": [
          "Dagschotel tijdens de week, gevarieerd traditioneel menu, zondagsmenu. Bar doorlopend open vanaf 11 uur. Zomerterras."
        ]
      },
