  "rdfs:comment": {
    "de": [
      "Italienisches Restaurant in der Einkaufspassage von Auchan. Spezialitäten: Vorspeisen, gegrilltes Gemüse \"Antipast\", Caesar-Salat mit Geflügelbrust; Pasta und Risotto; Desserts, Pana cotta, Schokoladenkekse, Tiramisu."
    ],
    "en": [
      "Italian restaurant located in the shopping mall of Auchan. Specialities: starters, grilled \"antipast\" vegetables, Caesar salad with chicken breast; pasta and risotto; desserts, pana cotta, chocolate fudge cookie, tiramisu."
    ],
    "it": [
      "Ristorante italiano situato nel centro commerciale Auchan. Specialità: antipasti, verdure grigliate \"antipast\", Caesar salad con petto di pollo; pasta e risotti; dolci, pana cotta, biscotto al cioccolato, tiramisù."
    ],
    "fr": [
      "Restaurant italien situé dans la galerie marchande d'Auchan. Spécialités : entrées, légumes grillés \"antipast\", salade César au blanc de volaille ; pasta et risotto ; desserts, pana cotta, biscuit coulant au chocolat, tiramisu."
    ],
    "es": [
      "Restaurante italiano situado en el centro comercial Auchan. Especialidades: entrantes, verduras a la parrilla \"antipastos\", ensalada César con pechuga de pollo; pasta y risotto; postres, pana cotta, galleta de chocolate, tiramisú."
    ],
    "nl": [
      "Italiaans restaurant in het Auchan winkelcentrum. Specialiteiten: voorgerechten, gegrilde groenten \"antipasta\", Caesar salade met kipfilet; pasta en risotto; nagerechten, pana cotta, chocolade biscuit, tiramisu."
    ]
  },
