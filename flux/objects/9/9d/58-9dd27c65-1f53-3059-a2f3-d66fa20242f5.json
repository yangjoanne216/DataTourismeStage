  "rdfs:comment": {
    "de": [
      "5 km vom Stadtzentrum von Aix-en-Provence entfernt öffnet dieses Familienhaus seine Türen für Besucher aus aller Welt, um ihnen den Lavendel und vieles mehr näher zu bringen."
    ],
    "en": [
      "5 km from Aix-en-Provence town centre, this family home opens its doors to visitors from all over the world, introducing them to the world of lavander and much else besides."
    ],
    "it": [
      "A 5 km dal centro di Aix-en-Provence, questa casa di famiglia apre le porte a visitatori provenienti da tutto il mondo per scoprire la lavanda e molto altro."
    ],
    "fr": [
      "A 5km du centre-ville d'Aix-en-Provence, cette maison de famille ouvre ses portes aux visiteurs du monde entier pour faire découvrir la lavande et bien plus encore."
    ],
    "nl": [
      "Op 5 km van het centrum van Aix-en-Provence opent dit familiehuis zijn deuren voor bezoekers uit de hele wereld om lavendel en nog veel meer te ontdekken."
    ],
    "es": [
      "A 5 km del centro de Aix-en-Provence, esta casa familiar abre sus puertas a visitantes de todo el mundo para descubrir la lavanda y mucho más."
    ]
  },
