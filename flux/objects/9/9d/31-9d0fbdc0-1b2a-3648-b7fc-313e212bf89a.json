  "rdfs:comment": {
    "de": [
      "Das Rathaus von Sévérac d'Aveyron startet seine Sommersaison 2023 mit den beiden Musik- und Kunsttruppen \"La Manif'\" und \"La Fanfare d'occasion\"!"
    ],
    "en": [
      "The Town Hall of Sévérac d'Aveyron launches its 2023 summer season with two music and visual arts troupes \"La Manif'\" and \"La Fanfare d'occasion\"!"
    ],
    "it": [
      "La Mairie de Sévérac d'Aveyron lancia la sua stagione estiva 2023 con due troupes di musica e arti visive \"La Manif'\" e \"La Fanfare d'occasion\"!"
    ],
    "fr": [
      "La Mairie de Sévérac d'Aveyron lance sa saison estivale 2023 avec deux troupes de musique et arts visuels \"La Manif'\" et \"La Fanfare d'occasion\" !"
    ],
    "es": [
      "La Mairie de Sévérac d'Aveyron inaugura su temporada de verano 2023 con dos compañías de música y artes visuales: \"La Manif'\" y \"La Fanfare d'occasion\""
    ],
    "nl": [
      "De Mairie de Sévérac d'Aveyron lanceert haar zomerseizoen 2023 met twee muziek- en beeldende kunstgroepen \"La Manif'\" en \"La Fanfare d'occasion\"!"
    ]
  },
