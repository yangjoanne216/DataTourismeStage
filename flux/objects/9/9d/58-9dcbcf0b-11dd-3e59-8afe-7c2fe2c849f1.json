  "rdfs:comment": {
    "de": [
      "Prêt-à-porter-Mode für Frauen, Männer und Kinder im provenzalischen Stil. Außerdem Verkauf von Accessoires, Heimtextilien, Tischkultur und Parfums."
    ],
    "en": [
      "Women's, men's and children's ready-to-wear in Provençal style. Also sale of accessories, household linen, tableware and perfumes."
    ],
    "it": [
      "Abbigliamento prêt-à-porter da donna, uomo e bambino in stile provenzale. Vende anche accessori, biancheria per la casa, stoviglie e profumi."
    ],
    "fr": [
      "Prêt-à-porter féminin, masculin et enfant de style provençal. Également vente d'accessoires, de linge de maison, d'art de la table et de parfums."
    ],
    "es": [
      "Ropa de prêt-à-porter de estilo provenzal para mujeres, hombres y niños. También vende accesorios, ropa de hogar, vajilla y perfumes."
    ],
    "nl": [
      "Dames-, heren- en kinderkleding in Provençaalse stijl. Verkoopt ook accessoires, huishoudlinnen, serviesgoed en parfums."
    ]
  },
