  "rdfs:comment": {
    "de": [
      "Gehen Sie an Bord der Sablésien und genießen Sie eine einzigartige Dinner-Kreuzfahrt durch das Sarthe-Tal, die vom Kapitän kommentiert wird."
    ],
    "en": [
      "Come aboard the Sablésien for a unique dinner cruise in the heart of the Sarthe Valley, commented by the captain."
    ],
    "it": [
      "Salite a bordo del Sablésien per una crociera con cena unica nel cuore della Valle della Sarthe, commentata dal capitano."
    ],
    "fr": [
      "Embarquez à bord du Sablésien pour un diner croisière unique au cœur de la Vallée de la Sarthe, commenté par le capitaine."
    ],
    "es": [
      "Suba a bordo del Sablésien para disfrutar de una cena crucero única en el corazón del valle del Sarthe, comentada por el capitán."
    ],
    "nl": [
      "Kom aan boord van de Sablésien voor een unieke dinercruise in het hart van de Sarthe-vallei, met commentaar van de kapitein."
    ]
  },
