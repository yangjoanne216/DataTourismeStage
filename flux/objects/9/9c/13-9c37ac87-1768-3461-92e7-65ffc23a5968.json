  "rdfs:comment": {
    "de": [
      "Eine Vereinspizzeria nur wenige Schritte von der ViaRhôna entfernt! Aus Rhône-Alpes-Weizen hergestellte Pizzen, langsame Gärung, sorgfältig ausgewählter Belag, großzügige Pizzen mit dünnem Teig."
    ],
    "en": [
      "An associative pizzeria a stone's throw from the ViaRhôna! Pizzas made from Rhône-Alpes wheat, slow fermentation, carefully selected toppings, generous thin-crust pizzas."
    ],
    "it": [
      "Una pizzeria comunitaria a due passi dalla ViaRhôna! Pizze prodotte con grano del Rodano-Alpi, fermentazione lenta, condimenti scelti con cura e pizze generose a crosta sottile."
    ],
    "fr": [
      "Une pizzeria associative à deux pas de la ViaRhôna ! Des pizzas élaborées à partir de blés Rhône-Alpes, une fermentation lente, des garnitures soigneusement choisies, des pizzas généreuses à pâte fine."
    ],
    "es": [
      "Una pizzería comunitaria a dos pasos de la ViaRhôna Pizzas elaboradas con trigo de Rhône-Alpes, fermentación lenta, ingredientes cuidadosamente seleccionados y generosas pizzas de masa fina."
    ],
    "nl": [
      "Een buurtpizzeria op een steenworp afstand van de ViaRhôna! Pizza's gemaakt van tarwe uit Rhône-Alpes, langzame gisting, zorgvuldig gekozen toppings en royale pizza's met dunne korst."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Pizzeria La Traversienne"
    ]
  },
