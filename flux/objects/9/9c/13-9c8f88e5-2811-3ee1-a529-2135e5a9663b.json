  "rdfs:comment": {
    "de": [
      "Karte. Restaurant, Brasserie, Crêperie, Pizzeria. Im Herzen des Place des Arcades gelegen, der als historisches Monument klassifiziert ist. Regionale Küche, Kindermenü, Gegrilltes, frischer Fisch, Salate, Muscheln ... Reservierung empfohlen."
    ],
    "en": [
      "Restaurant, brasserie, crêperie, pizzeria. Situated on Place des Arcades, registered as Historic Monument. Local cuisine, Kids Menu, mixed grill, fresh fish, salads, shellfish... Booking advised."
    ],
    "it": [
      "Menu. Ristorante, brasserie, crêperie, pizzeria. Situato nel cuore di Place des Arcades, monumento storico tutelato. Cucina locale, menù per bambini, grigliate, pesce fresco, insalate, crostacei ... Si consiglia la prenotazione."
    ],
    "fr": [
      "Carte. Restaurant, brasserie, crêperie, pizzeria. Situé au coeur de la Place des Arcades classée Monument Historique. Cuisine du terroir, menu enfant, grillades, poisson frais, salades, coquillages ... Réservation recommandée."
    ],
    "es": [
      "Menú. Restaurante, brasserie, crêperie, pizzería. Situado en el corazón de la Place des Arcades, un monumento histórico protegido. Cocina local, menú infantil, parrilladas, pescado fresco, ensaladas, marisco... Se recomienda reservar."
    ],
    "nl": [
      "A la carte. Restaurant, brasserie, pannenkoeken, pizzeria. Gelegen in het hart van Place des Arcades: geclassificeerd als Historisch Monument. Regionale keuken, kindermenu, gegrilde gerechten, verse vis, salades, schelpdieren... Reservering aanbevolen."
    ]
  },
  "rdfs:label": {
    "en": [
      "Restaurant Les Arcades"
    ],
