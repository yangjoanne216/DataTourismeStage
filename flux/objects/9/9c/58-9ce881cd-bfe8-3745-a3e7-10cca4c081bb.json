  "rdfs:comment": {
    "de": [
      "E Bike Trolib, der Spezialist für begleitete Ausflüge mit dem Elektroroller, dieses Konzept ist einzigartig in Frankreich! Wir sorgen für Ihre Zufriedenheit, indem wir Sie von unseren Profis begleiten lassen!"
    ],
    "en": [
      "E Bike Trolib, specialist in accompanied outings in electric scooters, this concept is unique in France! We ensure your satisfaction, by the rental accompanied by our professionals!"
    ],
    "it": [
      "E Bike Trolib, specialista in uscite accompagnate in scooter elettrico, questo concetto è unico in Francia! Garantiamo la vostra soddisfazione, attraverso il noleggio accompagnato dai nostri professionisti!"
    ],
    "fr": [
      "E Bike Trolib, spécialiste des sorties accompagnées en trottinette électrique, ce concept est unique en France! Nous assurons votre satisfaction, par de la location accompagnée par nos professionnels!"
    ],
    "nl": [
      "E Bike Trolib, specialist in begeleide elektrische scooteruitstapjes, dit concept is uniek in Frankrijk! Wij zorgen voor uw tevredenheid, door verhuur onder begeleiding van onze professionals!"
    ],
    "es": [
      "E Bike Trolib, especialista en salidas acompañadas en scooter eléctrico, ¡este concepto es único en Francia! ¡Garantizamos su satisfacción, mediante el alquiler acompañado por nuestros profesionales!"
    ]
  },
