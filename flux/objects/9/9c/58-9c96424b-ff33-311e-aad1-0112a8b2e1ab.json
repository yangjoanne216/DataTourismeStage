  "rdfs:comment": {
    "de": [
      "Intimes Restaurant im Zentrum des Club-Resorts, nur wenige Schritte vom Hafen entfernt.\r\nDie Küche wird von Michel Portos betreut, einem ehemaligen 2*-Koch aus Bordeaux, der 2012 zum Koch des Jahres gekürt wurde."
    ],
    "en": [
      "Intimate restaurant in the center of the Village, a few steps from the port.\r\nCuisine supervised by Michel Portos, former 2* chef in Bordeaux, chef of the year 2012."
    ],
    "it": [
      "Ristorante intimo nel centro del villaggio, a due passi dal porto.\r\nCucina curata da Michel Portos, ex chef 2* di Bordeaux, chef dell'anno 2012."
    ],
    "fr": [
      "Restaurant Intimiste dans le centre du Village, a deux pas du port.\r\nCuisine supervisée par Michel Portos, ancien chef 2* à Bordeaux, chef de l’année 2012."
    ],
    "es": [
      "Restaurante íntimo en el centro del Village, a dos pasos del puerto.\r\nCocina supervisada por Michel Portos, antiguo chef de 2* en Burdeos, chef del año 2012."
    ],
    "nl": [
      "Intiem restaurant in het centrum van het Dorp, op een steenworp afstand van de haven.\r\nKeuken onder leiding van Michel Portos, voormalig 2* chef in Bordeaux, chef van het jaar 2012."
    ]
  },
