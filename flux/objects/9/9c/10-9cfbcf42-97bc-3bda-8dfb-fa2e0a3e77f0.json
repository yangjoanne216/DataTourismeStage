  "rdfs:comment": {
    "de": [
      "Holzhütte gegenüber dem Fremdenverkehrsamt, am Eingang des Japiot-Parks. Kleiner Imbiss, Getränkestand, Crêpes, Waffeln und Eis. Terrasse und Sonnenschirm."
    ],
    "en": [
      "Wooden chalet located in front of the Tourist Office, at the entrance of the Parc Japiot. Snack bar, refreshment bar, pancakes, waffles and ice creams. Terrace and parasol."
    ],
    "it": [
      "Chalet in legno situato di fronte all'Ufficio del Turismo, all'ingresso del Parco Japiot. Spuntini, rinfreschi, frittelle, cialde e gelati. Terrazza e ombrellone."
    ],
    "fr": [
      "Chalet en bois situé en face de l'Office de Tourisme, à l'entrée du Parc Japiot. Petite restauration, buvette, crêpes, gaufres et glaces. Terrasse et parasol."
    ],
    "nl": [
      "Houten chalet gelegen voor het toeristenbureau, aan de ingang van het Japiot Park. Snacks, versnaperingen, pannenkoeken, wafels en ijs. Terras en parasol."
    ],
    "es": [
      "Chalet de madera situado frente a la Oficina de Turismo, a la entrada del Parque Japiot. Aperitivos, refrescos, tortitas, gofres y helados. Terraza y sombrilla."
    ]
  },
