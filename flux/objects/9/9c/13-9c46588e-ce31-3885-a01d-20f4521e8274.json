  "rdfs:comment": {
    "de": [
      "Angélique Raquin empfängt Sie von Montag bis Freitag für eine Mahlzeit vor Ort oder zum Mitnehmen mit einem täglich wechselnden Tagesgericht, das hausgemacht und mit regionalen Produkten zubereitet wird."
    ],
    "en": [
      "Angélique Raquin welcomes you from Monday to Friday for a meal on the spot or to take away with a dish of the day renewed every day, home-made and with regional products."
    ],
    "it": [
      "Angélique Raquin vi accoglie dal lunedì al venerdì per un pasto sul posto o da asporto con un piatto rinnovato ogni giorno, fatto in casa e con prodotti regionali."
    ],
    "fr": [
      "Angélique Raquin vous accueille du lundi au vendredi pour un repas sur place ou à emporter avec un plat du jour renouvelé chaque jour, fait maison et avec des produits régionaux."
    ],
    "nl": [
      "Angélique Raquin ontvangt u van maandag tot vrijdag voor een maaltijd ter plaatse of om mee te nemen met een dagelijks vernieuwde schotel, huisgemaakt en met streekproducten."
    ],
    "es": [
      "Angélique Raquin le recibe de lunes a viernes para una comida in situ o para llevar con un plato diario renovado, casero y con productos regionales."
    ]
  },
  "rdfs:label": {
    "fr": [
      "La Cantine des 4 chemins"
    ]
  },
