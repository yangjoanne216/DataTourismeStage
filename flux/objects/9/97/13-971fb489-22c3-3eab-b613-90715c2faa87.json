  "rdfs:comment": {
    "de": [
      "Verkauf auf dem Bauernhof von Geflügel, Hühnern, Perlhühnern, Truthähnen, Kapaunen (vorzugsweise auf Bestellung). Auf dem Bauernmarkt in Le Teil vertreten, freitags von Mai bis Oktober von 16.30 bis 19 Uhr."
    ],
    "en": [
      "Direct sales of poultry (chickens, Guinea fowl, turkeys and capons) at the farm. Please place an order beforehand. Cherries during the season. You can find us at the Producers Market in Le Teil every Friday from May to October between 4.30 and 7pm."
    ],
    "it": [
      "Vendita di pollame, polli, faraone, tacchini e capponi in azienda (preferibilmente su ordinazione). Presente al mercato dei produttori di Le Teil, il venerdì da maggio a ottobre dalle 16.30 alle 19.00."
    ],
    "fr": [
      "Vente à la Ferme de volailles, poulets, pintades, dindes, chapons (sur commande de préférence). Présent sur le marché des producteurs du Teil, le vendredi de mai à octobre de 16h30 à 19h."
    ],
    "nl": [
      "Verkoop van pluimvee, kippen, parelhoenders, kalkoenen en kapoenen op de boerderij (bij voorkeur op bestelling). Aanwezig op de producentenmarkt in Le Teil, op vrijdag van mei tot oktober van 16.30 tot 19.00 uur."
    ],
    "es": [
      "Venta de aves de corral, pollos, pintadas, pavos y capones en la granja (preferiblemente por encargo). Presente en el mercado de productores de Le Teil, los viernes de mayo a octubre de 16:30 a 19:00 horas."
    ]
  },
  "rdfs:label": {
    "en": [
      "La Pignatelle farmers collective"
    ],
    "fr": [
      "Gaec de la Pignatelle"
    ]
  },
