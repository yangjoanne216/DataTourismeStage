  "rdfs:comment": {
    "de": [
      "Aus den Niederlanden kommend, haben sich Ron und Ellen in Saint-Palais niedergelassen und einen Campingplatz auf dem Bauernhof eröffnet, der durch Gästezimmer und ein Ferienhaus ergänzt wird."
    ],
    "en": [
      "Coming from the Netherlands, Ron and Ellen settled in Saint-Palais and opened a farm campsite, completed by bed and breakfast and a gîte."
    ],
    "it": [
      "Dai Paesi Bassi, Ron ed Ellen si sono stabiliti a Saint-Palais e hanno aperto un campeggio agricolo con camere per gli ospiti e un gîte."
    ],
    "fr": [
      "En provenance des Pays-Bas, Ron et Ellen se sont installés à Saint-Palais et ont ouvert un camping à la ferme complété par des chambres d'hôtes et un gîte."
    ],
    "nl": [
      "Vanuit Nederland vestigden Ron en Ellen zich in Saint-Palais en openden een boerderijcamping met gastenkamers en een gîte."
    ],
    "es": [
      "Desde los Países Bajos, Ron y Ellen se instalaron en Saint-Palais y abrieron una granja-campamento con habitaciones de huéspedes y una casa rural."
    ]
  },
