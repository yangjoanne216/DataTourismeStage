  "rdfs:comment": {
    "de": [
      "Pizza / Burger / Pommes frites zum Mitnehmen, 5 Sitzplätze drinnen, ganzjährig geöffnet.\r\nPIZZA zum Mitnehmen das ganze Jahr über (Frische Produkte, hausgemacht)\r\nBURGERS / FRITES zum Mitnehmen nur von Ende September bis Ende Juni."
    ],
    "en": [
      "Pizzas / Burgers / French fries to take away, 5 places indoors, open all year round.\r\nSale of PIZZAS to take away all year round (Fresh products, home-made)\r\nSale of BURGERS / FRITES to take away in low season (from the end of September to the end of June)."
    ],
    "it": [
      "Pizze / Hamburger / Patatine da asporto, 5 posti all'interno, aperto tutto l'anno.\r\nVendita di PIZZE da asporto tutto l'anno (prodotti freschi, fatti in casa)\r\nVendita di BURGER / FRIES da asporto in bassa stagione (da fine settembre a fine giugno)."
    ],
    "fr": [
      "Pizzas / Burgers / Frites à emporter, 5 places en intérieur, ouvert à l'année.\r\nVente de PIZZAS à emporter toute l’année (Produits frais, fabrication artisanale)\r\nVente de BURGERS / FRITES à emporter en basse saison (de fin septembre à fin juin)."
    ],
    "es": [
      "Pizzas / Hamburguesas / Patatas fritas para llevar, 5 puestos en el interior, abierto todo el año.\r\nVenta de PIZZAS para llevar todo el año (productos frescos, caseros)\r\nVenta de BURGERS / FRIES para llevar en temporada baja (de finales de septiembre a finales de junio)."
    ],
    "nl": [
      "Pizza's / Burgers / Chips om mee te nemen, 5 plaatsen binnen, het hele jaar geopend.\r\nVerkoop van PIZZAS om mee te nemen het hele jaar door (verse producten, huisgemaakt)\r\nVerkoop van BURGERS/FRITES om mee te nemen in het laagseizoen (van eind september tot eind juni)."
    ]
