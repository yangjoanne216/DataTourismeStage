  "rdfs:comment": {
    "de": [
      "Feriendorf, 45 Zimmer mit Fernseher und 3 Gîtes. Voll- oder Halbpensionsformel. Bedecktes und beheiztes Schwimmbad. Reitsportzentrum ! Räume für Seminare. Das ganze Jahr über geöffnet."
    ],
    "en": [
      "Holiday Village, 45 rooms with television and 3 gîtes. Formula full board or half board. Covered and heated swimming pool. Riding school ! Seminar rooms. Open all year round."
    ],
    "it": [
      "Villaggio turistico, 45 camere con televisione e 3 gîtes. Pensione completa o mezza pensione. Piscina coperta e riscaldata. Centro ippico Sale per seminari. Aperto tutto l'anno."
    ],
    "fr": [
      "Village Vacances, 45 chambres avec télévision et 3 gîtes. Formule pension complète ou 1/2 pension. Piscine couverte et chauffée. Centre équestre ! Salles de séminaires. Ouvert à l'année."
    ],
    "nl": [
      "Vakantiedorp, 45 kamers met televisie en 3 gîtes. Volpension of halfpension. Overdekt en verwarmd zwembad. Manege Seminariezalen. Het hele jaar geopend."
    ],
    "es": [
      "Pueblo de vacaciones, 45 habitaciones con televisión y 3 casas rurales. Pensión completa o media pensión. Piscina cubierta y climatizada. Centro ecuestre Salas de seminarios. Abierto todo el año."
    ]
  },
