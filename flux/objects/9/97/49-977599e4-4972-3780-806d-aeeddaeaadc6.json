  "rdfs:comment": {
    "de": [
      "Entdecken Sie die Biwaks des Campingplatzes: Stellplatz mit einer Holzhütte auf Stelzen. Ideal für Wanderer, Radfahrer und Kajakfahrer. Kapazität für 2 Personen."
    ],
    "en": [
      "Discover the campsite's bivouacs: pitch equipped with a wooden hut on stilts. Ideal for hikers, cyclists and kayakers. Capacity of 2 persons."
    ],
    "it": [
      "Scoprite i bivacchi del campeggio: piazzola attrezzata con capanna in legno su palafitte. Ideale per escursionisti, ciclisti e kayakisti. Capacità di 2 persone."
    ],
    "fr": [
      "Découvrez les bivouacs du camping : emplacement équipé d'une cabane en bois sur pilotis. Idéal pour randonneurs, cyclistes, kayakistes. Capacité de 2 personnes."
    ],
    "nl": [
      "Ontdek de bivakplaatsen van de camping: kampeerplaats uitgerust met een houten hut op palen. Ideaal voor wandelaars, fietsers, kajakkers. Capaciteit van 2 personen."
    ],
    "es": [
      "Descubra los vivacs del camping: parcela equipada con una cabaña de madera sobre pilotes. Ideal para excursionistas, ciclistas y piragüistas. Capacidad para 2 personas."
    ]
  },
