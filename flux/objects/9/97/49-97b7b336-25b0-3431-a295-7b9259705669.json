      "dc:description": {
        "de": [
          "Atypisches Restaurant, neues Konzept in Angers. Bedienung an der Theke, vor Ihnen zubereitete Speisen, musikalisches Ambiente Frankreich.\nAbends bis 23 Uhr geöffnet."
        ],
        "en": [
          "Atypical restaurant, new concept in Angers. Service at the counter, elaborate cuisine in front of you, French musical atmosphere.\nOpen in the evening until 11 pm."
        ],
        "it": [
          "Ristorante atipico, di nuova concezione ad Angers. Servizio al banco, cucina preparata davanti a voi, atmosfera musicale francese.\nAperto la sera fino alle 23:00."
        ],
        "fr": [
          "Restaurant atypique, concept nouveau sur Angers. Service au comptoir, cuisine élaborée devant vous, ambiance musicale France.\nOuvert le soir jusqu'à 23 h."
        ],
        "es": [
          "Restaurante atípico, nuevo concepto en Angers. Servicio en el mostrador, cocina preparada delante de ti, ambiente musical francés.\nAbierto por la noche hasta las 23 horas."
        ],
        "nl": [
          "Atypisch restaurant, nieuw concept in Angers. Service aan de balie, keuken voor je neus bereid, Franse muzikale sfeer.\nS Avonds geopend tot 23.00 uur."
        ]
      },
