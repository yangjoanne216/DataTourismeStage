  "rdfs:comment": {
    "de": [
      "Die Prise d'Alzeau liegt im Herzen der Montagne Noire und ist der Ausgangspunkt für das Versorgungssystem des Canal du Midi."
    ],
    "en": [
      "Located at the heart of the Black Mountain, the Alzeau Stream is the starting point of the water supply system for the Canal du Midi."
    ],
    "it": [
      "Situata nel cuore della Montagne Noire, la Prise d'Alzeau è il punto di partenza della rete idrica del Canal du Midi."
    ],
    "fr": [
      "Située au cœur de la Montagne Noire, la Prise d'Alzeau est le point de départ du système d'alimentation du Canal du Midi."
    ],
    "es": [
      "Situada en el corazón de la Montaña Negra, la Prise d'Alzeau es el punto de partida del sistema de abastecimiento de agua del Canal du Midi."
    ],
    "nl": [
      "Gelegen in het hart van de Montagne Noire, is de Prise d'Alzeau het beginpunt van de watervoorziening van het Canal du Midi."
    ]
  },
  "rdfs:label": {
    "en": [
