  "rdfs:comment": {
    "de": [
      "La Chaumière Sainte-Rose empfängt Sie für einen luxuriösen und entrückenden Aufenthalt in Senantes, weniger als eine Stunde von Paris entfernt (50 Minuten von der Porte de Saint-Cloud)."
    ],
    "en": [
      "Chaumière Sainte-Rose will be happy to welcome you for a luxurious, restful stay in Senantes, less than an hour from Paris (50 minutes from Porte de Saint-Cloud)."
    ],
    "it": [
      "La Chaumière Sainte-Rose vi accoglie per un soggiorno lussuoso e rilassante a Senantes, a meno di un'ora da Parigi (50 minuti dalla Porte de Saint-Cloud)."
    ],
    "fr": [
      "La Chaumière Sainte-Rose vous accueille pour un séjour luxueux et dépaysant à Senantes, à moins d'une heure de Paris (50 minutes de la porte de Saint-Cloud)."
    ],
    "es": [
      "La Chaumière Sainte-Rose le da la bienvenida para una estancia lujosa y relajante en Senantes, a menos de una hora de París (50 minutos de la Porte de Saint-Cloud)."
    ],
    "nl": [
      "La Chaumière Sainte-Rose heet u welkom voor een luxueus en ontspannen verblijf in Senantes, op minder dan een uur van Parijs (50 minuten van de Porte de Saint-Cloud)."
    ]
  },
