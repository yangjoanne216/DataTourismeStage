  "rdfs:comment": {
    "de": [
      "Ätherische Öle, Blütenwasser, Seife auf Lavendelbasis. Brot aus eigener Herstellung. Befindet sich im Prozess der Bio-Zertifizierung. Verkauf nach Absprache auf dem Bauernhof."
    ],
    "en": [
      "Essential oils, floral water, lavender soap. Home-made bread. In the process of being certified organic. Sale by appointment at the farm."
    ],
    "it": [
      "Oli essenziali, acqua floreale, sapone alla lavanda. Pane fatto in casa. In procinto di ottenere la certificazione biologica. Vendita su appuntamento in azienda."
    ],
    "fr": [
      "Huiles essentielles, eau florale, savon à base de lavande. Pain fait maison. En cours de certification bio. Vente sur rendez-vous à la ferme."
    ],
    "nl": [
      "Etherische oliën, bloemenwater, zeep op basis van lavendel. Zelfgemaakt brood. Bezig met het verkrijgen van biologische certificering. Verkoop op afspraak op de boerderij."
    ],
    "es": [
      "Aceites esenciales, agua floral, jabón a base de lavanda. Pan casero. En proceso de obtener la certificación ecológica. Venta con cita previa en la granja."
    ]
  },
