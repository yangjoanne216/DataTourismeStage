  "rdfs:comment": {
    "de": [
      "Zugang: Nehmen Sie die Relaisstraße in Richtung Les Bertins, halten Sie auf der Höhe von Le Châtelard an, steigen Sie zum Waldrand auf (5 Minuten zu Fuß)."
    ],
    "en": [
      "Access : take the road from the relay towards Bertins, stop at the Châtelard, go up to the edge of the forest (5mn on foot)."
    ],
    "it": [
      "Accesso: prendere la strada dal relais verso Les Bertins, fermarsi a Le Châtelard, salire fino al margine della foresta (5 minuti a piedi)."
    ],
    "fr": [
      "Accès : prendre la route du relais en direction des Bertins, s'arrêter au niveau du Châtelard, monter à la lisière de la forêt (5mn à pied)."
    ],
    "nl": [
      "Bereikbaarheid: neem de weg van het relais richting Les Bertins, stop bij Le Châtelard, ga omhoog naar de rand van het bos (5 minuten te voet)."
    ],
    "es": [
      "Acceso: tomar la carretera del relevo hacia Les Bertins, parar en Le Châtelard, subir hasta el borde del bosque (5 minutos a pie)."
    ]
  },
