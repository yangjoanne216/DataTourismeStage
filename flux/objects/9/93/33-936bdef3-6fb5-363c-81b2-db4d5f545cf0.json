  "rdfs:comment": {
    "de": [
      "Handwerklich hergestellter Honig aus biologischem Anbau. Von der Imkerin produziert, geerntet und in Gläser abgefüllt.\nKerzen aus Bienenwachs, handwerkliche Herstellung."
    ],
    "en": [
      "Artisanal honey in organic farming. Produced, harvested and put in jars by the beekeeper.\nCandles handmade with beeswax."
    ],
    "it": [
      "Miele biologico prodotto artigianalmente. Prodotto, raccolto e imbottigliato dall'apicoltore.\nCandele artigianali in cera d'api."
    ],
    "fr": [
      "Miel artisanal en agriculture biologique. Produit, récolté, et mis en pots par l'apicultrice.\nBougies fabrication artisanale à la cire d'abeille."
    ],
    "es": [
      "Miel ecológica elaborada artesanalmente. Producido, cosechado y embotellado por el apicultor.\nVelas artesanales hechas con cera de abeja."
    ],
    "nl": [
      "Handgemaakte biologische honing. Geproduceerd, geoogst en gebotteld door de imker.\nHandgemaakte kaarsen van bijenwas."
    ]
  },
