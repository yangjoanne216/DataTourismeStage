  "rdfs:comment": {
    "de": [
      "Atemberaubender Blick auf den Alten Hafen, Restaurant mit Panoramaterrasse. Zeitgenössisches Ambiente in einem vom Innenarchitekten Claudio Colucci entworfenen Dekor."
    ],
    "en": [
      "Breathtaking view of the Old Port, restaurant with panoramic terrace. A resolutely contemporary atmosphere in a decor designed by the architect and decorator Claudio Colucci."
    ],
    "it": [
      "Vista mozzafiato sul Porto Vecchio, ristorante con terrazza panoramica. Un'atmosfera decisamente contemporanea in un arredamento progettato dall'architetto e decoratore Claudio Colucci."
    ],
    "fr": [
      "Vue imprenable sur le Vieux Port, restaurant avec terrasse panoramique. Ambiance résolument contemporaine dans un décor signé par l'architecte décorateur Claudio Colucci."
    ],
    "es": [
      "Impresionante vista del Puerto Viejo, restaurante con terraza panorámica. Un ambiente decididamente contemporáneo en una decoración diseñada por el arquitecto y decorador Claudio Colucci."
    ],
    "nl": [
      "Adembenemend uitzicht op de Oude Haven, restaurant met panoramisch terras. Een resoluut eigentijdse sfeer in een decor ontworpen door de architect en decorateur Claudio Colucci."
    ]
  },
