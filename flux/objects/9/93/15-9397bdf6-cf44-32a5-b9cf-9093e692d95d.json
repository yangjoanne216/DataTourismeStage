  "rdfs:comment": {
    "de": [
      "Willkommen am öffentlichen Strand von Fougères, wo Sie mit der ganzen Familie baden können, mit Spielen für die Kleinsten und Wassersport für die Größeren."
    ],
    "en": [
      "Welcome to the public beach of Fougères where you can come with your family for a swim. There are games for children and nautical sports for adults."
    ],
    "it": [
      "Benvenuti alla spiaggia pubblica di Fougères per un bagno in famiglia con giochi per i più piccoli e sport acquatici per i più grandi."
    ],
    "fr": [
      "Bienvenue à la plage publique de Fougères pour vous baigner en famille avec des jeux pour les plus petits et sports nautiques pour les plus grands."
    ],
    "nl": [
      "Welkom op het openbare strand van Fougères voor een gezinsbad met spelletjes voor de kleintjes en watersporten voor de groteren."
    ],
    "es": [
      "Bienvenido a la playa pública de Fougères para un baño familiar con juegos para los más pequeños y deportes acuáticos para los mayores."
    ]
  },
