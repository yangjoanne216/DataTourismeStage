  "rdfs:comment": {
    "de": [
      "Restaurant, das sich auf Premium-Pizza spezialisiert hat. Wir bieten Ihnen italienische Spezialitäten: handgemachte Pizzas, Bruschetta, Calzone, Salat, italienische Wurst- und Käsesorten. Handwerklich hergestellte Desserts."
    ],
    "en": [
      "Restaurant specialized in premium pizza. We offer Italian specialties: homemade pizzas, bruschettas, calzone, salads, cold cuts and Italian cheeses. Artisanal desserts."
    ],
    "it": [
      "Ristorante specializzato in pizza di qualità. Offriamo specialità italiane: pizze fatte in casa, bruschette, calzoni, insalate, salumi e formaggi italiani. Dolci artigianali."
    ],
    "fr": [
      "Restaurant spécialisé dans la pizza premium. Nous vous proposons des spécialités italiennes : pizzas artisanales, bruschettas, calzone, salade, charcuterie et fromages italiens. Desserts artisanaux."
    ],
    "es": [
      "Restaurante especializado en pizzas de primera calidad. Ofrecemos especialidades italianas: pizzas caseras, bruschettas, calzone, ensaladas, embutidos y quesos italianos. Postres artesanales."
    ],
    "nl": [
      "Restaurant gespecialiseerd in eersteklas pizza. Wij bieden Italiaanse specialiteiten: huisgemaakte pizza's, bruschettas, calzone, salades, vleeswaren en Italiaanse kazen. Artisanale desserts."
    ]
  },
