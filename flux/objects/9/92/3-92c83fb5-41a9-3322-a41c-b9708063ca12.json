      "dc:description": {
        "de": [
          "La Gerbe d'Or, traditionsreiche Bäckerei und Konditorei.\nRestaurantbereich: Sandwiches, Salate, warme Gerichte... mit frischen und qualitativ hochwertigen Produkten."
        ],
        "en": [
          "La Gerbe d'Or, traditional bakery and pastry shop.\nCatering area: sandwiches, salads, hot dishes... with fresh and quality products."
        ],
        "it": [
          "La Gerbe d'Or, panetteria e pasticceria tradizionale.\nArea catering: panini, insalate, piatti caldi? con prodotti freschi e di qualità."
        ],
        "fr": [
          "La Gerbe d'Or, boulangerie pâtisserie de tradition.\nEspace restauration : sandwichs, salades, plats chauds… avec des produits frais et de qualité."
        ],
        "nl": [
          "La Gerbe d'Or, traditionele bakkerij en banketbakkerij.\nCateringruimte: broodjes, salades, warme gerechten... met verse kwaliteitsproducten."
        ],
        "es": [
          "La Gerbe d'Or, panadería y pastelería tradicional.\nZona de restauración: sándwiches, ensaladas, platos calientes... con productos frescos y de calidad."
        ]
      },
