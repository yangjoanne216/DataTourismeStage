  "rdfs:comment": {
    "de": [
      "Bar-Restaurant-Brasserie und Crêperie de Montagne, am Fuße der Pisten gelegen.\r\nSpezialitäten aus den Bergen, gegrilltes Fleisch, Salate, hausgemachte Pizzas und Burger, Crêpes, Waffeln, Eis und hausgemachte Desserts."
    ],
    "en": [
      "Mountain Bar-Restaurant-Brasserie and Crêperie, located at the foot of the slopes.\r\nMountain Specialties, Grilled Meats, Salads, Homemade Pizzas and Burgers, Pancakes, Waffles, Ice Creams and Homemade Desserts."
    ],
    "it": [
      "Bar-Ristorante-Brasserie e Creperie di montagna, situato ai piedi delle piste.\r\nSpecialità di montagna, carni alla griglia, insalate, pizze e hamburger fatti in casa, crepes, waffle, gelati e dolci fatti in casa."
    ],
    "fr": [
      "Bar-Restaurant-Brasserie et Crêperie de Montagne, situé au pied des pistes.\r\nSpécialités de Montagne, Viandes Grillées, Salades, Pizzas et Burgers Maison, Crêpes, Gaufres, Glaces et Desserts Maison."
    ],
    "nl": [
      "Mountain Bar-Restaurant-Brasserie en Crêperie, gelegen aan de voet van de hellingen.\r\nBergspecialiteiten, gegrild vlees, salades, zelfgemaakte pizza's en hamburgers, pannenkoeken, wafels, ijs en zelfgemaakte desserts."
    ],
    "es": [
      "Bar-Restaurante-Brasería de Montaña y Crepería, situado a pie de pistas.\r\nEspecialidades de montaña, carnes a la brasa, ensaladas, pizzas y hamburguesas caseras, crepes, gofres, helados y postres caseros."
    ]
  },
