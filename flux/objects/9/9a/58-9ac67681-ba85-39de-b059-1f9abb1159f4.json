  "rdfs:comment": {
    "de": [
      "Das Restaurant bietet hausgemachte Gerichte, vegetarische Küche, vegane Küche, Frühstück, Teesalon, Brunch, libanesische und armenische Spezialitäten in Marseille 2e."
    ],
    "en": [
      "The restaurant offers homemade dishes, vegetarian cuisine, vegan cuisine, breakfast, tea room, brunch, Lebanese and Armenian specialties in Marseille 2nd."
    ],
    "it": [
      "Il ristorante offre piatti fatti in casa, cucina vegetariana, cucina vegana, colazione, sala da tè, brunch, specialità libanesi e armene a Marsiglia 2e."
    ],
    "fr": [
      "Le restaurant propose des plats faits maison, cuisine végétarienne, cuisine végétalienne, petit-déjeuner, salon de thé, brunch, spécialités libanaises et arméniennes à Marseille 2e."
    ],
    "nl": [
      "Het restaurant biedt huisgemaakte gerechten, vegetarische keuken, veganistische keuken, ontbijt, theesalon, brunch, Libanese en Armeense specialiteiten in Marseille 2e."
    ],
    "es": [
      "El restaurante ofrece platos caseros, cocina vegetariana, cocina vegana, desayunos, salón de té, brunch, especialidades libanesas y armenias en Marsella 2e."
    ]
  },
