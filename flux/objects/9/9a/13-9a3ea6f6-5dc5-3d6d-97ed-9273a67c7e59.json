  "rdfs:comment": {
    "de": [
      "Restaurant-Pizzeria: Frikassee von Jakobsmuscheln mit Schnittlauch, hausgemachte Entenleber - Rinderfilet mit Morcheln, Froschpfanne - Pizza und Gerichte zum Mitnehmen."
    ],
    "en": [
      "Restaurant-pizzeria : scallop fricassee with chives, homemade duck foie gras - Beef fillet with morels, fried frogs - Pizza and takeaway meals."
    ],
    "it": [
      "Ristorante e pizzeria: fricassea di capesante con erba cipollina, foie gras d'anatra fatto in casa, filetto di manzo con spugnole, rane in padella - Pizza e asporto."
    ],
    "fr": [
      "Restaurant- pizzeria : fricassée de St-Jacques à la Ciboulette, Foie gras de canard maison - Filet de boeuf aux morilles, poêlée de grenouilles - Pizza et plats à emporter."
    ],
    "es": [
      "Restaurante y pizzería: fricasé de vieiras con cebollino, foie gras de pato casero, filete de ternera con colmenillas, ranas a la sartén - Pizza y comida para llevar."
    ],
    "nl": [
      "Restaurant en pizzeria: fricassee van sint-jakobsschelpen met bieslook, huisgemaakte eendenlever, ossenhaas met morilles, gebakken kikkers - Pizza en afhaalmaaltijden."
    ]
  },
  "rdfs:label": {
    "fr": [
      "La Grignotte"
    ]
  },
