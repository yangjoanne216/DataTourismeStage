  "rdfs:comment": {
    "de": [
      "Domaine Les Bastidons : Charmante Ferienhäuser in Gordes, Luberon, Vaucluse. Wohn- und Esszimmer, Küche, 2 Schlafzimmer, Badezimmer, Terrasse und Garten. Gesicherte, baumbestandene Residenz mit Pool. Persönlicher Empfang für einen Aufenthalt voller Entspannung und Entdeckungen."
    ],
    "en": [
      "Domaine Les Bastidons : Charming gites located in Gordes, Luberon, Vaucluse. Living room, kitchen, 2 bedrooms, bathroom, terrace and garden. Residence with trees, secured with swimming pool. Personalized welcome for a stay of relaxation and discovery."
    ],
    "it": [
      "Domaine Les Bastidons : Graziosa casa vacanze situata a Gordes, Luberon, Vaucluse. Soggiorno, cucina, 2 camere, bagno, terrazza e giardino. Residenza alberata, protetta con piscina. Accoglienza personalizzata per un soggiorno di relax e scoperta."
    ],
    "fr": [
      "Domaine Les Bastidons : Gîtes de charme situés à Gordes, Luberon, Vaucluse. Salon-séjour, cuisine, 2 chambres, salle de bain, terrasse et jardin. Résidence arborée, sécurisée avec piscine. Accueil personnalisé pour un séjour de détente, de découvertes."
    ],
    "es": [
      "Domaine Les Bastidons : Encantadoras casas rurales situadas en Gordes, Luberon, Vaucluse. Salón, cocina, 2 dormitorios, cuarto de baño, terraza y jardín. Residencia arbolada, asegurada con piscina. Acogida personalizada para una estancia de descanso y descubrimiento."
    ],
    "nl": [
      "Domaine Les Bastidons : Charmante gites gelegen in Gordes, Luberon, Vaucluse. Woonkamer, keuken, 2 slaapkamers, badkamer, terras en tuin. Residentie met bomen, beveiligd met zwembad. Persoonlijk onthaal voor een verblijf van ontspanning en ontdekking."
    ]
  },
