  "rdfs:comment": {
    "de": [
      "WILLKOMMEN IN DER LIBECHE\nIn Mellecey wird Sie unser altes Winzerhaus mit seinem Empfang und seiner familiären Atmosphäre begeistern."
    ],
    "en": [
      "WELCOME TO LA LIBECHE\nIn Mellecey, our old winegrower's house will seduce you with its welcome and family atmosphere."
    ],
    "it": [
      "BENVENUTO A LA LIBECHE\nA Mellecey, la nostra antica casa di viticoltori vi sedurrà con la sua accoglienza e la sua atmosfera familiare."
    ],
    "fr": [
      "BIENVENUE A LA LIBECHE\nA Mellecey, notre ancienne maison vigneronne saura vous séduire par son accueil et son ambiance familiale."
    ],
    "nl": [
      "WELKOM IN LA LIBECHE\nIn Mellecey zal ons oude wijnbouwershuis u verleiden met zijn onthaal en familiale sfeer."
    ],
    "es": [
      "BIENVENIDO A LA LIBECHE\nEn Mellecey, nuestra antigua casa de viñadores le seducirá por su acogida y su ambiente familiar."
    ]
  },
