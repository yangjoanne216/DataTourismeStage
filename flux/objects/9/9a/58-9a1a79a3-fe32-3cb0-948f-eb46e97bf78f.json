  "rdfs:comment": {
    "de": [
      "Das Le Fantastique bietet eine abwechslungsreiche, schmackhafte und fröhliche Bistroküche in einer warmen und entspannten Atmosphäre."
    ],
    "en": [
      "Le Fantastique offers a varied, tasty and cheerful bistro cuisine in a warm and relaxed atmosphere."
    ],
    "it": [
      "Le Fantastique offre una cucina da bistrot varia, gustosa e allegra in un'atmosfera calda e rilassata."
    ],
    "fr": [
      "Le Fantastique propose une cuisine de bistrot variée, savoureuse et gaie, dans une ambiance chaleureuse et décontractée."
    ],
    "es": [
      "Le Fantastique ofrece una cocina de bistró variada, sabrosa y alegre en un ambiente cálido y relajado."
    ],
    "nl": [
      "Le Fantastique biedt een gevarieerde, smakelijke en vrolijke bistrokeuken in een warme en ontspannen sfeer."
    ]
  },
