  "rdfs:comment": {
    "de": [
      "Themenbar, festliche Abende, Live-Konzert, Musikabende das ganze Jahr über. Während der Ferien täglich ab 14 Uhr. Außerhalb der Ferien dienstags geschlossen."
    ],
    "en": [
      "Theme bar, festive evenings, live concerts, musical evenings all year round. From 2pm every day during the holidays. Closed on Tuesdays outside the holidays."
    ],
    "it": [
      "Bar a tema, serate di festa, concerti dal vivo, serate musicali tutto l'anno. Dalle 14:00 tutti i giorni durante le vacanze. Chiuso il martedì al di fuori delle festività."
    ],
    "fr": [
      "Bar à thème, soirées festives, concert live, soirées musicales toute l'année. A partir de 14h tous les jours pendant les vacances. Fermeture le mardi hors vacances."
    ],
    "nl": [
      "Themabar, feestavonden, live concerten, muziekavonden het hele jaar door. Vanaf 14.00 uur elke dag tijdens de feestdagen. Gesloten op dinsdag buiten de feestdagen."
    ],
    "es": [
      "Bar temático, noches de fiesta, conciertos en directo, veladas musicales durante todo el año. A partir de las 14 horas todos los días durante las vacaciones. Cerrado los martes fuera de las vacaciones."
    ]
  },
