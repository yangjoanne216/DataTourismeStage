  "rdfs:comment": {
    "de": [
      "Restaurant im Zentrum der Altstadt, am kleinen Hafen \"Le Miroir aux Oiseaux\". Terrassen mit Blick auf die Gipfel und Boote, gemütliche Zimmer, Marktküche."
    ],
    "en": [
      "Restaurant nestled in the center of the historic district, on the small port \"Le Miroir aux Oiseaux\". Terraces facing the peaks and boats, cozy rooms, market cuisine."
    ],
    "it": [
      "Ristorante situato nel centro del quartiere storico, sul porticciolo \"Le Miroir aux Oiseaux\". Terrazze affacciate sul pointus e sulle barche, camere accoglienti, cucina di mercato."
    ],
    "fr": [
      "Restaurant niché au centre du quartier historique, sur le petit port \" Le Miroir aux Oiseaux\". Terrasses face aux pointus et aux barques, salles cosy, cuisine du marché."
    ],
    "es": [
      "Restaurante ubicado en el centro del distrito histórico, en el pequeño puerto \"Le Miroir aux Oiseaux\". Terrazas frente a los picos y barcos, acogedores salones, cocina de mercado."
    ],
    "nl": [
      "Restaurant genesteld in het centrum van de historische wijk, aan het haventje \"Le Miroir aux Oiseaux\". Terrassen met uitzicht op de pointus en de boten, gezellige kamers, marktkeuken."
    ]
  },
