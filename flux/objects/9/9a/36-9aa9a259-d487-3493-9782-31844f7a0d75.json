  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/939bce3e-60a6-3e43-8871-7a0829473714",
      "dc:description": {
        "de": [
          "2 Doppelzimmer. Großes Esszimmer, WC, Waschküche, Wohnzimmer im Zwischengeschoss (Hifi-Anlage). 2 überdachte Terrassen (Blick auf die Berge), Gartenmöbel, BBQ."
        ],
        "en": [
          "2 double bedrooms. Vast dining room, WC, laundry room, living room on the mezzanine (hi-fi system). 2 covered terraces (mountain view), garden furniture, BBQ."
        ],
        "it": [
          "2 camere da letto matrimoniali. Ampia sala da pranzo, WC, lavanderia, salotto soppalcato (stereo). 2 terrazze coperte (vista sulle montagne), mobili da giardino, barbecue."
        ],
        "fr": [
          "2 ch doubles. Vaste salle à manger, WC, buanderie, salon en mezzanine (chaîne hifi). 2 terrasses couvertes (vue sur la montagne), salon de jardin, BBQ."
        ],
        "nl": [
          "2 dubbele slaapkamers. Grote eetkamer, WC, wasruimte, mezzanine lounge (stereo). 2 overdekte terrassen (uitzicht op de bergen), tuinmeubelen, barbecue."
        ],
        "es": [
          "2 dormitorios dobles. Gran comedor, WC, lavadero, salón en el entresuelo (equipo de música). 2 terrazas cubiertas (vistas a la montaña), muebles de jardín, barbacoa."
        ]
      },
