  "rdfs:comment": {
    "de": [
      "Zimmer \"Syrah\", 16 m², 2. Stock. 3 km vom Dorfzentrum entfernt, auf dem Land, 14 km vom Meer entfernt. Gemeinsamer Swimmingpool. Privater Parkplatz, Badezimmer, private Sanitäranlagen. Fernseher. 2 Betten 80 cm. Ehemaliger Weinkeller. Frühstück wird im Wohnraum oder auf der Terrasse serviert. Klimatisiertes Zimmer."
    ],
    "en": [
      "Room \"Syrah\", 16 m², 2nd floor. 3km from the village centre, in the countryside, 14km from the sea. Collective swimming pool. Private parking, bathroom, private toilets. Television. 2 beds 80 cm. Old wine cellar. Breakfast served in the living room or on the terrace. Air-conditioned room."
    ],
    "it": [
      "Camera \"Syrah\", 16 m², 2° piano. A 3 km dal centro del paese, in campagna, a 14 km dal mare. Piscina collettiva. Parcheggio privato, bagno, servizi igienici privati. Televisione. 2 letti da 80 cm. Vecchia cantina. Colazione servita nel soggiorno o sulla terrazza. Camera climatizzata."
    ],
    "fr": [
      "Chambre \"Syrah\", 16 m², 2ème étage. À 3km du centre-village, à la campagne, à 14km du bord de mer. Piscine collective. Parking privé, salle de bain, sanitaires privés. Télévision. 2 lits 80 cm. Ancienne cave vinicole. Petit déjeuner servi dans la pièce de vie ou en terrasse. Chambre climatisée."
    ],
    "nl": [
      "Kamer \"Syrah\", 16 m², 2e verdieping. 3 km van het dorpscentrum, op het platteland, 14 km van de zee. Collectief zwembad. Privé parkeerplaats, badkamer, privé toiletten. Televisie. 2 bedden van 80 cm. Oude wijnkelder. Het ontbijt wordt geserveerd in de woonkamer of op het terras. Kamer met airconditioning."
    ],
    "es": [
      "Habitación \"Syrah\", 16 m², 2ª planta. A 3 km del centro del pueblo, en el campo, a 14 km del mar. Piscina colectiva. Aparcamiento privado, baño, aseos privados. La televisión. 2 camas de 80 cm. Antigua bodega. El desayuno se sirve en el salón o en la terraza. Habitación con aire acondicionado."
    ]
  },
