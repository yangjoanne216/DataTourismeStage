  "rdfs:comment": {
    "de": [
      "Wie wäre es, wenn Sie sich eine Pause bei einem frisch gerösteten Kaffee gönnen?"
    ],
    "en": [
      "Why not take a break and enjoy a freshly roasted coffee?"
    ],
    "it": [
      "Perché non fare una pausa e prendere un caffè appena tostato?"
    ],
    "fr": [
      "Et si vous vous accordiez une pause autour d'un bon café fraîchement torréfié ?"
    ],
    "es": [
      "¿Por qué no hacer una pausa y tomar un café recién tostado?"
    ],
    "nl": [
      "Waarom neemt u geen pauze en neemt u een vers gebrande koffie?"
    ]
  },
