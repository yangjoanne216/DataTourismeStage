  "rdfs:comment": {
    "de": [
      "Sportverein nach dem Gesetz 1901, der am 26.09.1968 bei der Präfektur von Annecy angemeldet wurde, Nr. 2684.\r\nSportverein, der der Fédération Française Handisport (als gemeinnützig anerkannt) angehört und vom Ministerium für Jugend und Sport zugelassen ist, Nr. 74.S.156."
    ],
    "en": [
      "Sports association law 1901 declared to the Prefecture of Annecy on 26/09/1968, n°2684.\r\nSports association affiliated to the French Disabled Sports Federation (recognized as a public utility) and approved by the Ministry of Youth and Sports, n°74.S.156."
    ],
    "it": [
      "Associazione sportiva ai sensi della legge del 1901 dichiarata alla Prefettura di Annecy il 26/09/1968, n°2684.\r\nAssociazione sportiva affiliata alla Federazione Francese di Handisport (riconosciuta di pubblica utilità) e approvata dal Ministero della Gioventù e dello Sport, n°74.S.156."
    ],
    "fr": [
      "Association sportive loi 1901 déclarée à la Préfecture d'Annecy le 26/09/1968, n°2684.\r\nAssociation sportive affiliée à la Fédération Française Handisport (reconnue d'utilité publique) et agréée par le Ministère de la Jeunesse et des Sports, n°74.S.156."
    ],
    "nl": [
      "Sportvereniging volgens de wet van 1901 verklaard aan de prefectuur van Annecy op 26/09/1968, nr. 2684.\r\nSportvereniging aangesloten bij de Franse Handisport Federatie (erkend als zijnde van openbaar nut) en erkend door het Ministerie van Jeugd en Sport, n°74.S.156."
    ],
    "es": [
      "Asociación deportiva bajo la ley de 1901 declarada a la Prefectura de Annecy el 26/09/1968, n°2684.\r\nAsociación deportiva afiliada a la Federación Francesa de Deportes de Mano (reconocida de utilidad pública) y aprobada por el Ministerio de Juventud y Deportes, n°74.S.156."
    ]
  },
