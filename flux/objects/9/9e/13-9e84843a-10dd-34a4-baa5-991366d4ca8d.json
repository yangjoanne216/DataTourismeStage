  "rdfs:comment": {
    "de": [
      "Siblu Bonne Anse Plage genießt eine privilegierte Lage inmitten des Forêt de la Coubre und an der Bucht von Bonne Anse mit direktem Zugang zu den Radwegen."
    ],
    "en": [
      "Siblu Bonne Anse Plage enjoys a privileged location in the middle of the Coubre forest and on the edge of the Bonne Anse bay with direct access to the bicycle paths."
    ],
    "it": [
      "Il Siblu Bonne Anse Plage gode di una posizione privilegiata nel mezzo della foresta di Coubre e ai margini della baia di Bonne Anse, con accesso diretto alle piste ciclabili."
    ],
    "fr": [
      "Siblu Bonne Anse Plage profite d’une situation privilégiée au milieu de la foret de la Coubre et au bord de la baie de Bonne Anse avec un accès direct sur les pistes cyclables."
    ],
    "nl": [
      "Siblu Bonne Anse Plage heeft een bevoorrechte ligging midden in het bos van Coubre en aan de rand van de baai van Bonne Anse met directe toegang tot de fietspaden."
    ],
    "es": [
      "Siblu Bonne Anse Plage goza de una ubicación privilegiada en medio del bosque de Coubre y al borde de la bahía de Bonne Anse con acceso directo a los carriles bici."
    ]
  },
