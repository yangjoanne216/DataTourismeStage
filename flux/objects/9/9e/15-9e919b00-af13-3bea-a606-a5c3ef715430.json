  "rdfs:comment": {
    "de": [
      "Möbliert in einem ehemaligen Bauernhof, in dem es noch einige Hühner und eine Stute \"Guérande\" gibt."
    ],
    "en": [
      "Receive a warm welcome at this gite on the way out of the village, with a large tree-lined, enclosed garden, for an enjoyable break in the middle of rolling countryside, at the heart of George Sand country and close to the Creuse Valley and Eguzon Lake."
    ],
    "it": [
      "Arredato in una vecchia casa colonica dove ci sono ancora delle galline e una cavalla \"Guérande\"."
    ],
    "fr": [
      "Meublé dans une ancienne ferme où l'on trouve encore quelques poules et une jument \"Guérande\"."
    ],
    "nl": [
      "Ingericht in een oude boerderij waar nog enkele kippen en een \"Guérande\" merrie zijn."
    ],
    "es": [
      "Amueblado en una antigua granja donde todavía hay algunas gallinas y una yegua \"Guérande\"."
    ]
