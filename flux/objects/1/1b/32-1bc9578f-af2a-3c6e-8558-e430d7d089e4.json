  "rdfs:comment": {
    "de": [
      "Traditionelles Bar-Restaurant in Familienbesitz, herzlicher Empfang, idyllischer Rahmen für Momente der Entspannung und Geselligkeit, mit einer landschaftlich gestalteten Minigolfanlage, die man mit Freunden oder der Familie genießen kann!"
    ],
    "en": [
      "Traditional family bar-restaurant, warm welcome, idyllic setting for moments of relaxation and conviviality, with a landscaped mini-golf course, to be played with friends or family!"
    ],
    "it": [
      "Bar e ristorante tradizionale a conduzione familiare, accoglienza calorosa, ambiente idilliaco per momenti di relax e convivialità, con un campo da minigolf paesaggistico, da giocare con gli amici o la famiglia!"
    ],
    "fr": [
      "Bar restaurant traditionnel familial, accueil chaleureux, cadre idyllique pour des instants de détentes et de convivialité, avec un mini-golf paysager, à faire entre amis ou en famille !"
    ],
    "nl": [
      "Traditionele familiebar en restaurant, warm onthaal, idyllische omgeving voor momenten van ontspanning en gezelligheid, met een aangelegde minigolfbaan, om met vrienden of familie te spelen!"
    ],
    "es": [
      "Bar y restaurante familiar tradicional, cálida acogida, entorno idílico para momentos de relajación y convivencia, con un campo de minigolf ajardinado, ¡para jugar con los amigos o la familia!"
    ]
  },
