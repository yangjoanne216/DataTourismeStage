  "rdfs:comment": {
    "de": [
      "UNVERGESSENE WOCHEN: 18.00 Uhr\r\nVermittlung des Dialekts von Tence und seiner Umgebung.\r\n\r\nGERADE WOCHEN: 20.00 Uhr\r\nErlernen der okzitanischen Sprache von hier und anderswo... und oft mit Musik!"
    ],
    "en": [
      "WEEKS OFF: 6:00 pm\r\nTransmission of the patois of Tence and its surroundings.\r\n\r\nPAIR WEEKS: 20h00\r\nLearning the Occitan language, from here and elsewhere... and often with music!"
    ],
    "it": [
      "SETTIMANE CATTIVE: ore 18.00\r\nTrasmissione del patois di Tence e dintorni.\r\n\r\nPRIMA SETTIMANA: ore 20.00\r\nImparare l'occitano, da qui e altrove... e spesso con la musica!"
    ],
    "fr": [
      "SEMAINES IMPAIRES : 18h00\r\nTransmission du patois de Tence et de ses environs.\r\n\r\nSEMAINES PAIRES : 20h00\r\nApprentissage de l'occitan, d'ici et d'ailleurs... et souvent en musique!"
    ],
    "nl": [
      "Slechte weken: 18.00 uur\r\nOverdracht van het patois van Tence en omgeving.\r\n\r\nVroege weken: 20.00 uur\r\nOccitaans leren, van hier en elders... en vaak met muziek!"
    ],
    "es": [
      "SEMANAS MALAS: 6pm\r\nTransmisión del patois de Tence y sus alrededores.\r\n\r\nPRIMERAS SEMANAS: 20:00 horas\r\nAprender occitano, de aquí y de otros lugares... ¡y a menudo con música!"
    ]
  },
