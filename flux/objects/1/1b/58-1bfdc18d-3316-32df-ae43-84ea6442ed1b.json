  "rdfs:comment": {
    "de": [
      "Zwischen Alpilles und Luberon, auf einem 1 ha großen Grundstück mit Olivenbäumen, 30 m² große Wohnung mit gepflegter Dekoration, die an das Haus der Eigentümer angrenzt."
    ],
    "en": [
      "Between Alpilles and Luberon, located on a land of 1 hectare with olive trees, apartment of 30 m² with careful decoration adjoining the owners' house."
    ],
    "it": [
      "Tra le Alpilles e il Luberon, su un terreno di 1 ettaro coltivato a ulivi, appartamento di 30 m² accuratamente decorato adiacente alla casa dei proprietari."
    ],
    "fr": [
      "Entre Alpilles et Luberon, situé sur un terrain d'1 ha arboré d'oliviers, appartement de 30 m² à la décoration soignée mitoyen à la maison des propriétaires."
    ],
    "nl": [
      "Tussen de Alpilles en de Luberon, op een terrein van 1 ha beplant met olijfbomen, een appartement van 30 m² met zorgvuldige decoratie grenzend aan het huis van de eigenaren."
    ],
    "es": [
      "Entre Alpilles y Luberon, situado en una parcela de 1 ha plantada de olivos, un piso de 30 m² con una cuidada decoración contiguo a la casa de los propietarios."
    ]
  },
