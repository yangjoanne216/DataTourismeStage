  "rdfs:comment": {
    "de": [
      "Bungalow, 2 Schlafzimmer, Kochnische, Waschmaschine. Fernseher. Badezimmer, Dusche, WC. Heizung. Parkplatz, Garten, Terrasse. An der Straße zum See."
    ],
    "en": [
      "Chalet, 2 bedrooms, kitchenette, dishwasher. Television. Bathroom with shower, toilet. Heating. Car park, garden, terrace. Located on the road to the lake."
    ],
    "it": [
      "Chalet, 2 camere da letto, 2 letti singoli, 1 letto matrimoniale, divano letto. Angolo cottura, lavastoviglie. Televisione. Bagno, doccia, WC. Riscaldamento. Parcheggio, giardino, terrazza. Situato sulla strada per il lago\nNon sono ammessi animali."
    ],
    "fr": [
      "Chalet, 2 chambres, 2 lits 90, 1 lit 140, canapé convertible. Coin cuisine, lave vaiselle. Télévision. Salle de bain, douche, WC. Chauffage. Parking, jardin, terrasse. Situé sur la route du Lac. \nAnimaux refusés."
    ],
    "nl": [
      "Chalet, 2 slaapkamers, 2 eenpersoonsbedden, 1 tweepersoonsbed, slaapbank. Kitchenette, vaatwasser. Televisie. Badkamer, douche, WC. Verwarming. Parkeerplaats, tuin, terras. Gelegen aan de weg naar het meer\nDieren zijn niet toegestaan."
    ],
    "es": [
      "Chalet, 2 habitaciones, cocina americana, lavavajillas. Televisión. Cuarto de baño, ducha, WC. Calefacción. Aparcamiento, jardín, terraza. Situado en el camino al lago."
    ]
  },
