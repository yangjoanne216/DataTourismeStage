  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/fa12879b-18db-30d7-91e9-9e772d1a58fc",
      "dc:description": {
        "de": [
          "3 km von Lourdes entfernt, ebenerdige Unterkunft im Herzen des Dorfes Adé, mit privatem, geschlossenem Garten.",
          "Im Erdgeschoss: 1 Zimmer (1 Doppelbett), Küche, Badezimmer, überdachter Unterstand, separates WC, Zusatzheizung, eingezäuntes Grundstück, Gartenmöbel, Grill. Nebenkosten inbegriffen."
        ],
        "en": [
          "3km from Lourdes, single storey gîte, located in the heart of the village of Adé, with private enclosed garden.",
          "Ground floor : 1 Bedroom (1 bed 2 pl.), kitchen, bathroom, covered shelter, independent WC, auxiliary heating, enclosed land, garden furniture, barbecue. Charges included."
        ],
        "it": [
          "A 3 km da Lourdes, gîte a un piano, situato nel cuore del villaggio di Adé, con giardino privato recintato.",
          "Al piano terra: 1 camera (1 letto matrimoniale), cucina, bagno, riparo coperto, WC separato, riscaldamento, terreno recintato, mobili da giardino, barbecue. Spese incluse."
        ],
        "fr": [
          "A 3km de Lourdes, gîte de plain pied, situé au coeur du village d'Adé, avec jardin privatif clos.",
          "En rez-de-chaussée : 1 Chambre (1 lit 2 pl.), cuisine, salle de bains, abri couvert, WC indépendant, chauffage d'appoint, terrain clos, salon de jardin, barbecue. Charges comprises."
        ],
        "nl": [
          "Op 3 km van Lourdes, gelijkvloerse gîte, gelegen in het hart van het dorp Adé, met omheinde privétuin.",
          "Op de begane grond: 1 slaapkamer (1 tweepersoonsbed), keuken, badkamer, overdekt afdak, apart toilet, verwarming, omheind terrein, tuinmeubelen, barbecue. Kosten inbegrepen."
        ],
        "es": [
          "A 3 km de Lourdes, casa rural de una sola planta, situada en el corazón del pueblo de Adé, con jardín privado cerrado.",
          "En la planta baja: 1 dormitorio (1 cama doble), cocina, baño, refugio cubierto, aseo separado, calefacción, terreno cerrado, muebles de jardín, barbacoa. Cargos incluidos."
        ]
      },
