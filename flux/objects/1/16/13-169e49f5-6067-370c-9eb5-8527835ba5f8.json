  "rdfs:comment": {
    "de": [
      "Tennisplatz zur Miete am Ufer des Lignon.\r\nFür den Verleih wenden Sie sich an Mag Presse (rue d'Annonay in Tence) oder während der Turnierzeiten direkt an den Tennisclub."
    ],
    "en": [
      "Tennis court for rent on the banks of the Lignon.\r\nTo rent a court, please contact Mag Presse (rue d'Annonay in Tence) or directly the tennis club during the tournament period."
    ],
    "it": [
      "Campo da tennis a noleggio sulle rive del Lignon.\r\nPer affittare, contattare Mag Presse (rue d'Annonay a Tence) o direttamente il tennis club durante il periodo del torneo."
    ],
    "fr": [
      "Court de tennis en location au bord du Lignon.\r\nPour la location s'adresser à Mag Presse (rue d'Annonay à Tence) ou directement au club de Tennis en période de tournoi."
    ],
    "nl": [
      "Tennisbaan te huur aan de oever van de Lignon.\r\nOm te huren kunt u contact opnemen met Mag Presse (rue d'Annonay in Tence) of rechtstreeks met de tennisclub tijdens de toernooiperiode."
    ],
    "es": [
      "Pista de tenis de alquiler a orillas del Lignon.\r\nPara alquilar, póngase en contacto con Mag Presse (rue d'Annonay en Tence) o directamente con el club de tenis durante el periodo del torneo."
    ]
  },
  "rdfs:label": {
    "fr": [
