  "rdfs:comment": {
    "de": [
      "Das Restaurant von Christian Ernst bietet Gourmetküche, Plancha-Küche auf Keramikgrills, im Saal, im Gang und auf der Terrasse mit Panoramahintergrund..."
    ],
    "en": [
      "Christian Ernst's restaurant offers gastronomic cuisine, cooking on a ceramic barbecue, in the dining room, in the corridor and on the terrace, with a panoramic background..."
    ],
    "it": [
      "Il ristorante di Christian Ernst offre cucina gastronomica, cottura a la plancha su barbecue in ceramica, nella sala da pranzo, sulla terrazza e nel corridoio, con vista panoramica..."
    ],
    "fr": [
      "Le restaurant de Christian Ernst propose une cuisine gastronomique, cuisine à la plancha sur barbecue en céramique, en salle, en coursive et en terrasse, sur fond panoramique..."
    ],
    "nl": [
      "Het restaurant van Christian Ernst biedt een gastronomische keuken, a la plancha koken op een keramische barbecue, in de eetzaal, op het terras en in de gang, met een panoramisch uitzicht..."
    ],
    "es": [
      "El restaurante de Christian Ernst ofrece cocina gourmet, a la plancha en barbacoa de cerámica, en el comedor, en la pasarela y en la terraza, con un fondo panorámico..."
    ]
  },
