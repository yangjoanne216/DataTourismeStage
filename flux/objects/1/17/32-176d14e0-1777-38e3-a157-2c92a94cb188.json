  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/9848eefc-41d6-39a1-8323-848655d08bb1",
      "dc:description": {
        "de": [
          "Wohnung Studio, 23 m², Erdgeschoss. 200m von einem Thermalbad, 300m vom Stadtzentrum, 7km von der Meeresküste entfernt. Parkplatz, Gartenmöbel, Terrasse. Heizung, Gefrierschrank, Mikrowelle, Telefon, Fernseher, Kabel-/Satellitenfernsehen. Bett 140 cm Wohnzimmer."
        ],
        "en": [
          "Studio apartment, 23 m², ground floor. At 200m from a spa, at 300m from the city center, at 7km from the seaside. Parking, garden furniture, terrace. Heating, freezer, microwave oven, telephone, television, cable / satellite. Bed 140 cm living room."
        ],
        "it": [
          "Monolocale, 23 m², piano terra. A 200 m da uno stabilimento termale, a 300 m dal centro città, a 7 km dal mare. Parcheggio, mobili da giardino, terrazza. Riscaldamento, congelatore, forno a microonde, telefono, televisione, cavo/satellite. Letto 140 cm soggiorno."
        ],
        "fr": [
          "Appartement studio, 23 m², RDC. À 200m d'un établissement thermal, à 300m du centre-ville, à 7km du bord de mer. Parking, salon de jardin, terrasse. Chauffage, congélateur, four à micro-ondes, téléphone, télévision, câble / satellite. Lit 140 cm séjour."
        ],
        "nl": [
          "Studio, 23 m², begane grond. Op 200m van een kuuroord, op 300m van het stadscentrum, op 7km van de zee. Parkeerplaats, tuinmeubilair, terras. Verwarming, vriezer, magnetron, telefoon, televisie, kabel/satelliet. Bed 140 cm woonkamer."
        ],
        "es": [
          "Estudio de 23 m², planta baja. A 200 m de un balneario, a 300 m del centro de la ciudad, a 7 km de la playa. Aparcamiento, muebles de jardín, terraza. Calefacción, congelador, microondas, teléfono, televisión, cable/satélite. Cama de 140 cm Sala de estar."
        ]
      },
