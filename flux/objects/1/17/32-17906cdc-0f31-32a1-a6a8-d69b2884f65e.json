  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/c3c59d68-36c6-3924-8e72-fe3d83075e93",
      "dc:description": {
        "de": [
          "Weinkeller (Flaschen und lose Ware), Feinkostladen, Lebensmittelgeschäft, Frühstück auf der Terrasse und alkoholfreie Getränke, Brot, Gebäck, Konditoreiwaren, frische Produkte (Käse, Wurstwaren)"
        ],
        "en": [
          "Wine cellar (bottles and bulk), delicatessen, convenience store, breakfast on terrace and soft drinks, bread, pastries, fresh produce (cheese, charcuterie)"
        ],
        "it": [
          "Cantina (bottiglie e prodotti sfusi), gastronomia, minimarket, colazione in terrazza e bevande analcoliche, pane, dolci, prodotti freschi (formaggi, salumi)"
        ],
        "fr": [
          "Cave à vins (bouteilles et vrac), épicerie fine, épicerie dépannage, petit déjeuner sur terrasse et boissons sans alcool, dépôt de pains, viennoiseries, pâtisseries, produits frais (fromage, charcuterie)"
        ],
        "nl": [
          "Wijnkelder (flessen en bulk), delicatessenwinkel, kruidenierswinkel, ontbijt op het terras en niet-alcoholische dranken, brood, gebak, verse producten (kaas, vleeswaren)"
        ],
        "es": [
          "Bodega (botellas y granel), charcutería, tienda de alimentación, desayuno en la terraza y bebidas sin alcohol, pan, bollería, productos frescos (queso, charcutería)"
        ]
      },
