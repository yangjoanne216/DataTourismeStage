  "rdfs:comment": {
    "de": [
      "Die Bretagne lädt in das Herz der Altstadt mit ihren eleganten Fassaden aus dem 17. und 18. Jahrhundert. Abwechslung garantiert!"
    ],
    "en": [
      "Brittany invites itself into the heart of the old town with its elegant 17th and 18th century facades. A change of scenery guaranteed!"
    ],
    "it": [
      "La Bretagna invita a entrare nel cuore della città vecchia con le sue eleganti facciate del XVII e XVIII secolo. Un cambio di scenario garantito!"
    ],
    "fr": [
      "La Bretagne s'invite au cœur de la vieille ville aux élégantes façades des XVIIème et XVIIIème siècles. Dépaysement garanti !"
    ],
    "nl": [
      "Bretagne nodigt zichzelf uit in het hart van de oude stad met zijn elegante 17e en 18e eeuwse gevels. Een verandering van omgeving gegarandeerd!"
    ],
    "es": [
      "Bretaña se invita a sí misma en el corazón del casco antiguo con sus elegantes fachadas de los siglos XVII y XVIII. ¡Un cambio de escenario garantizado!"
    ]
  },
