  "rdfs:comment": {
    "de": [
      "Am Rande eines Pinienwaldes geniessen Sie die Schönheit der Valdaine-Ebene und die Ruhe des Ortes."
    ],
    "en": [
      "Campsite situated on the edge of a pine-tree forest. Enjoy the calm and beauty of Valdaine plain. A leisure park can be found 3 km from there."
    ],
    "it": [
      "Ai margini di una pineta, potrete apprezzare la bellezza della pianura valdostana e la calma del suo sito."
    ],
    "fr": [
      "A la lisière d'une pinède, vous apprécierez la beauté de la plaine de la Valdaine et le calme de son site."
    ],
    "es": [
      "Al borde de un bosque de pinos, apreciará la belleza de la llanura de Valdaine y la calma de su sitio."
    ],
    "nl": [
      "Aan de rand van een dennenbos, zult u de schoonheid van de vlakte van Valdaine en de rust van deze plek waarderen."
    ]
