  "rdfs:comment": {
    "de": [
      "Das gesellige und gemütliche Restaurant par excellence!  Auf der Speisekarte stehen immer frische Produkte und eine gute Atmosphäre."
    ],
    "en": [
      "The warm and friendly restaurant par excellence!  On the menu, always fresh products and a good atmosphere."
    ],
    "it": [
      "Il ristorante caldo e accogliente per eccellenza!  Nel menu, prodotti sempre freschi e una buona atmosfera."
    ],
    "fr": [
      "Le restaurant convivial et chaleureux par excellence !  Au menu, toujours des produits frais et une bonne ambiance."
    ],
    "es": [
      "¡El restaurante cálido y acogedor por excelencia!  En el menú, siempre productos frescos y un buen ambiente."
    ],
    "nl": [
      "Het warme en vriendelijke restaurant bij uitstek!  Op het menu, altijd verse producten en een goede sfeer."
    ]
  },
