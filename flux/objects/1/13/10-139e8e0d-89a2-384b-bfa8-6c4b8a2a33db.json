  "rdfs:comment": {
    "de": [
      "Dieses italienische Bistronomierestaurant befindet sich auf dem Vorplatz der Tribune Nord und empfängt Sie von Dienstag bis Freitag zum Mittagessen sowie an den Abenden der Auswärtsspiele des FC Metz."
    ],
    "en": [
      "Located on the forecourt of the Tribune Nord, this bistronomic Italian restaurant welcomes you from Tuesday to Friday at noon as well as on match nights away from FC Metz."
    ],
    "it": [
      "Situato di fronte alla Tribuna Nord, questo ristorante bistrot italiano vi accoglie dal martedì al venerdì a pranzo e in occasione delle partite dell'FC Metz in trasferta."
    ],
    "fr": [
      "Situé sur le parvis de la Tribune Nord, ce restaurant italien bistronomique vous accueille du mardi au vendredi à midi ainsi que les soirs de match à l'extérieur du FC Metz."
    ],
    "es": [
      "Situado frente a la tribuna norte, este restaurante bistró italiano le da la bienvenida de martes a viernes a la hora del almuerzo, así como en los partidos del FC Metz fuera de casa."
    ],
    "nl": [
      "Dit Italiaanse bistro-restaurant bevindt zich tegenover de Noordtribune en verwelkomt u van dinsdag tot vrijdag tijdens de lunch en bij uitwedstrijden van FC Metz."
    ]
  },
  "rdfs:label": {
