  "rdfs:comment": {
    "de": [
      "Die Domaine les Cengles liegt am Fuße des Berges Sainte-Victoire, in der Gemeinde Châteauneuf-le-Rouge. Das Anwesen umfasst 12 Hektar Olivenbäume."
    ],
    "en": [
      "The Domaine les Cengles is situated at the foot of Sainte-Victoire Mountain, in Châteauneuf-le-Rouge. The estate comprises 12 hectares of olive groves."
    ],
    "it": [
      "La tenuta Cengles si trova ai piedi della montagna Sainte-Victoire, nel comune di Châteauneuf-le-Rouge. La tenuta comprende 12 ettari di ulivi."
    ],
    "fr": [
      "Le domaine les Cengles se situe au pied de la montagne Sainte-Victoire, sur la commune de Châteauneuf-le-Rouge. Le domaine comprend 12 hectares d'oliviers."
    ],
    "es": [
      "La finca Cengles está situada al pie de la montaña Sainte-Victoire, en el municipio de Châteauneuf-le-Rouge. La finca incluye 12 hectáreas de olivos."
    ],
    "nl": [
      "Het domein van Cengles ligt aan de voet van de berg Sainte-Victoire, in de gemeente Châteauneuf-le-Rouge. Het landgoed omvat 12 hectare olijfbomen."
    ]
  },
