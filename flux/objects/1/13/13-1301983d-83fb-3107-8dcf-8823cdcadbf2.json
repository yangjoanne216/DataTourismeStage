  "rdfs:comment": {
    "de": [
      "Charmantes Haus am Eingang des Dorfes. Komfortable Zimmer mit Aussicht. Große Gemeinschaftsräume. Bewaldeter Garten mit beheiztem Pool, ausgestattete Sommerküche, Privatparkplatz."
    ],
    "en": [
      "Charming house at the entrance of the village. Comfortable bedrooms with view. Large common spaces. Garden, heated swimming pool, summer kitchen, private car park."
    ],
    "it": [
      "Casa di charme all'ingresso del paese. Camere confortevoli con vista. Ampie aree comuni. Giardino con piscina riscaldata, cucina estiva attrezzata, parcheggio privato."
    ],
    "fr": [
      "Maison de charme à l’entrée du village. Chambres confortables avec vue. Grands espaces communs. Jardin arboré avec piscine chauffée, cuisine d’été équipée, parking privé."
    ],
    "es": [
      "Encantadora casa a la entrada del pueblo. Habitaciones confortables con vistas. Amplias zonas comunes. Jardín con piscina climatizada, cocina de verano equipada, aparcamiento privado."
    ],
    "nl": [
      "Charmant huis bij de ingang van het dorp. Comfortabele kamers met uitzicht. Grote gemeenschappelijke ruimtes. Tuin met verwarmd zwembad, ingerichte zomerkeuken, privé parkeerplaats."
    ]
  },
