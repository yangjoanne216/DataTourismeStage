  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/6bd0e52e-d089-321f-95c1-5fd499d70677",
      "dc:description": {
        "de": [
          "Restaurant des Hôtel des Bords de l'Ill. Traditioneller und raffinierter Rahmen. Süßwasserfischspezialitäten - Matelotes, gebratener Karpfen und Forelle, traditionelles Sauerkraut und Fischsauerkraut. Flammkuchen, Terrasse auf der Ill. Tagesmenü."
        ],
        "en": [
          "Restaurant of the Hotel des Bords de l'Ill. Traditional and refined setting. Freshwater fish specialities -matelotes, fried carp and trout, traditional sauerkraut with fish. Tartes flambées, terrace on the river Ill. Menu of the day."
        ],
        "it": [
          "Ristorante dell'Hotel des Bords de l'Ill. Ambiente tradizionale e raffinato. Specialità di pesce d'acqua dolce - matelotes, carpe e trote fritte, crauti tradizionali e crauti di pesce. Tartes flambées, terrazza sull'Ill. Menu giornaliero."
        ],
        "fr": [
          "Restaurant de l'Hôtel des Bords de l'Ill. Cadre traditionnel et raffiné. Spécialités de poissons d'eau douce -matelotes, carpe frite et truite, choucroute traditionnelle et choucroute aux poissons. Tartes flambées, terrasse sur l'Ill. Menu du jour."
        ],
        "es": [
          "Restaurante del Hotel des Bords de l'Ill. Entorno tradicional y refinado. Especialidades de pescado de agua dulce -matelotes, carpas y truchas fritas, chucrut tradicional y chucrut de pescado. Tartes flambées, terraza en el Ill. Menú del día."
        ],
        "nl": [
          "Restaurant van het Hotel des Bords de l'Ill. Traditioneel en verfijnd kader. Zoetwatervisspecialiteiten -mateloten, gebakken karper en forel, traditionele zuurkool en viszuurkool. Tartes flambées, terras op de Ill. Dagmenu."
        ]
      },
