  "rdfs:comment": {
    "de": [
      "Châteauneuf-du-Rhône lädt Sie zu seinem Volksfest ein! Genießen Sie ein Konzert, Boule-Wettbewerbe, einen Jahrmarkt und ein Feuerwerk. Kommen Sie zahlreich!"
    ],
    "en": [
      "Châteauneuf-du-Rhône invites you to its festival! Enjoy a concert, petanque competitions, a fairground but also a fireworks display. Come and enjoy!"
    ],
    "it": [
      "Châteauneuf-du-Rhône vi invita alla sua festa votiva! Godetevi un concerto, gare di bocce, un luna park e anche i fuochi d'artificio. Accorrete numerosi!"
    ],
    "fr": [
      "Châteauneuf-du-Rhône vous invite à sa fête votive! Profitez d'un concert, de concours de pétanque, d'une fête foraine mais aussi d'un feu d'artifice. Venez nombreux!"
    ],
    "nl": [
      "Châteauneuf-du-Rhône nodigt u uit voor zijn fête votive! Geniet van een concert, petanquewedstrijden, een kermis maar ook van vuurwerk. Kom in groten getale!"
    ],
    "es": [
      "Châteauneuf-du-Rhône le invita a su fiesta votiva Concierto, competiciones de petanca, parque de atracciones y fuegos artificiales. Acuda en masa"
    ]
  },
  "rdfs:label": {
    "fr": [
      "Fête votive"
    ]
  },
