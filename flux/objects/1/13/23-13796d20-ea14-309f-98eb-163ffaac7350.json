  "rdfs:comment": {
    "de": [
      "Verkauf von Flaschenweinen aus ganz Frankreich und Wein vom Weingut in loser Schüttung; Bier, Spirituosen und Feinkost, Geschenkartikel. Gelegentliche Weinproben."
    ],
    "en": [
      "Sales of bottled wines from all over France and wine from the property in bulk; beers, spirits and delicatessen, gift items. Occasional tastings."
    ],
    "it": [
      "Vendita di vini in bottiglia provenienti da tutta la Francia e di vini sfusi della tenuta; birra, liquori e specialità gastronomiche, articoli da regalo. Degustazioni occasionali."
    ],
    "fr": [
      "Ventes de vins en bouteilles de toute la France et vin de la propriété en vrac; bières, spiritueux et épicerie fine, objets cadeaux. Dégustation occasionnelle."
    ],
    "nl": [
      "Verkoop van gebottelde wijn uit heel Frankrijk en wijn van het domein in bulk; bier, sterke drank en delicatessen, geschenkartikelen. Incidentele proeverijen."
    ],
    "es": [
      "Venta de vino embotellado de toda Francia y vino de la finca a granel; cerveza, licores y delicatessen, artículos de regalo. Degustaciones ocasionales."
    ]
  },
