  "rdfs:comment": {
    "de": [
      "Pasta, aber nicht nur! Mit Dutzenden von Saucen und zahlreichen Toppings bietet Nooï eine große Auswahl für überraschende Mahlzeiten!"
    ],
    "en": [
      "Pasta but not only! With its dozens of sauces and its numerous toppings, Nooï offers a multitude of assortments for meals that are always surprising!"
    ],
    "it": [
      "Pasta ma non solo! Con le sue decine di salse e i numerosi condimenti, Nooï offre una moltitudine di assortimenti per pasti sempre sorprendenti!"
    ],
    "fr": [
      "Des pâtes mais pas que ! Avec ses douzaines de sauces et ses nombreux topping, Nooï propose une multitude d'assortiment pour des repas toujours surprenants!"
    ],
    "es": [
      "¡Pasta pero no sólo! Con sus decenas de salsas y numerosos aderezos, Nooï ofrece una multitud de surtidos para comidas siempre sorprendentes"
    ],
    "nl": [
      "Pasta maar niet alleen! Met zijn tientallen sauzen en zijn talrijke toppings biedt Nooï een veelheid aan assortimenten voor maaltijden die altijd verrassend zijn!"
    ]
  },
