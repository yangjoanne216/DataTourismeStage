  "rdfs:comment": {
    "de": [
      "Traditionelles Restaurant in einem modernen, elsässischen Weinkeller mit beleuchtetem Brunnen. Mittags (11:30 bis 14:30 Uhr) und abends (18:30 bis 21:00 Uhr): elsässische Spezialitäten und Flammkuchen. Brasserie am Nachmittag (von 15 bis 17 Uhr). Schöne private Terrasse in ruhiger Lage."
    ],
    "en": [
      "Traditional restaurant in a modern Alsatian cellar, with a lighted well. Lunch (11:30 am to 2:30 pm) and evening (6:30 pm to 9 pm): Alsatian specialties and flambéed pies. Brasserie in the afternoon (from 3pm to 5pm). Nice private terrace in a quiet area."
    ],
    "it": [
      "Ristorante tradizionale in una moderna cantina alsaziana con pozzo illuminato. Pranzo (dalle 11.30 alle 14.30) e cena (dalle 18.30 alle 21.00): specialità alsaziane e crostate flambé. Brasserie nel pomeriggio (dalle 15.00 alle 17.00). Bella terrazza privata in una zona tranquilla."
    ],
    "fr": [
      "Restaurant traditionnel dans un caveau moderne et alsacien, avec puits éclairé. Midi (11h30 à 14h30) et soir (18h30 à 21h) : spécialités alsaciennes et tartes flambées. Brasserie l'après-midi (de 15h à 17h). Belle terrasse privative au calme."
    ],
    "nl": [
      "Traditioneel restaurant in een moderne Elzasser kelder met een verlichte waterput. Lunch (11.30 tot 14.30 uur) en diner (18.30 tot 21.00 uur): Elzasser specialiteiten en flammkuchen. Brasserie in de middag (van 15.00 tot 17.00 uur). Mooi privé terras in een rustige omgeving."
    ],
    "es": [
      "Restaurante tradicional en una moderna bodega alsaciana con pozo iluminado. Almuerzo (de 11.30 a 14.30) y cena (de 18.30 a 21.00): especialidades alsacianas y tartas flambeadas. Brasserie por la tarde (de 15:00 a 17:00). Bonita terraza privada en una zona tranquila."
    ]
  },
