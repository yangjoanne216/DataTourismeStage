  "rdfs:comment": {
    "de": [
      "Handwerkliche Korbflechterei: Papierkörbe, Einkaufskörbe ... und Objekte nach Maß. Veranstaltet auch Einführungskurse zum Flechten mit Weide."
    ],
    "en": [
      "Artisanal wicker work : baskets, shopping bags...and any made to measure objects. Free visit of the workshop by appointment. We offer workshops and taster sessions of wicker weaving."
    ],
    "it": [
      "Produzione di cesti tradizionali: cesti, cestini per la spesa, ecc. e tutti gli oggetti su misura. Visita gratuita del laboratorio su appuntamento. Organizzazione di corsi di formazione e workshop introduttivi all'intreccio del vimini"
    ],
    "fr": [
      "Fabrication de vannerie artisanale : corbeilles, paniers à provision… et tous objets sur mesures. Visite de l'atelier gratuite sur rendez-vous. Organisation de stages et d'ateliers d'initiation au tressage de l'osier"
    ],
    "nl": [
      "Vervaardiging van ambachtelijke manden: korven, boodschappenmanden, etc. en allerlei op maat gemaakte voorwerpen.  Organisatie van introductieworkshops voor het vlechten van teenwilg."
    ],
    "es": [
      "Fabricación de cestería tradicional: cestas, cestas de la compra, etc. y todos los objetos a medida. Visita gratuita del taller con cita previa. Organización de cursos de formación y talleres de iniciación al tejido de mimbre"
    ]
  },
  "rdfs:label": {
    "en": [
      "Les Chemins de l'Osier Basket-Weaving"
    ],
    "fr": [
      "Vannerie les Chemins de l'Osier"
