  "rdfs:comment": {
    "de": [
      "Restaurant im ehemaligen Pfarrhaus von Golinhac (auf dem GR 65, dem Jakobsweg), herrliche Aussicht auf die Berge von Aubrac und Cantal, 9 km von Entraygues und 15 km von Conques entfernt"
    ],
    "en": [
      "Restaurant located in the former presbytery of Golinhac (on the GR 65, pathway of St Jacques de Compostelle), magnificent views on the mounts of Aubrac and Cantal at 9 km from Entraygues and 15 km from Conques."
    ],
    "it": [
      "Ristorante situato nell'antico presbiterio di Golinhac (sul GR 65, il cammino di Santiago de Compostela), con magnifica vista sulle montagne dell'Aubrac e del Cantal, a 9 km da Entraygues e a 15 km da Conques"
    ],
    "fr": [
      "Restaurant situé dans l'ancien presbytère de Golinhac (sur le GR 65, chemin de St Jacques de Compostelle), vues magnifiques sur les monts d'Aubrac et du Cantal à 9 km d'Entraygues et 15 km de Conques"
    ],
    "es": [
      "Restaurante situado en el antiguo presbiterio de Golinhac (en el GR 65, el camino de Santiago de Compostela), con magníficas vistas a los montes Aubrac y Cantal, a 9 km de Entraygues y a 15 km de Conques"
    ],
    "nl": [
      "Restaurant gelegen in de oude pastorie van Golinhac (op de GR 65, de route van St Jacques de Compostelle), prachtig uitzicht op de bergen van Aubrac en de Cantal, op 9 km van Entraygues en 15 km van Conques"
    ]
  },
