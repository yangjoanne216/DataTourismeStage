  "rdfs:comment": {
    "de": [
      "Éric Touchaleaume hat eine leidenschaftliche Beziehung zu Marseille, wo er seit seinen Anfängen als junger Antiquitätenhändler treue Freundschaften pflegt."
    ],
    "en": [
      "Éric Touchaleaume maintains a passionate relationship with Marseille where he has cultivated faithful friendships since his beginnings as a young antique dealer."
    ],
    "it": [
      "Éric Touchaleaume ha un rapporto appassionato con Marsiglia, dove ha coltivato amicizie leali fin dagli esordi come giovane antiquario."
    ],
    "fr": [
      "Éric Touchaleaume entretient une relation passionnelle avec Marseille ou il cultive des amitiés fidèles depuis ses débuts de jeune antiquaire."
    ],
    "es": [
      "Éric Touchaleaume mantiene una apasionada relación con Marsella, donde ha cultivado fieles amistades desde sus primeros días como joven anticuario."
    ],
    "nl": [
      "Éric Touchaleaume heeft een gepassioneerde band met Marseille, waar hij sinds zijn eerste dagen als jonge antiquair trouwe vriendschappen heeft opgebouwd."
    ]
  },
