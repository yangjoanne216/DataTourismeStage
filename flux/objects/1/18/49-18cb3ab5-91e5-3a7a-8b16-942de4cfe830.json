  "rdfs:comment": {
    "de": [
      "Im Loire-Tal, im Herzen von Saumur, empfängt Sie Bouvet Ladubay, Brut de Loire, im Herzen seiner Weinkeller."
    ],
    "en": [
      "In the Loire Valley, in the heart of the Saumur region, Bouvet Ladubay, Brut de Loire, welcomes you to the heart of its cellars."
    ],
    "it": [
      "Nella Valle della Loira, nel cuore della regione di Saumur, Bouvet Ladubay, Brut de Loire, vi accoglie nelle sue cantine."
    ],
    "fr": [
      "En Val de Loire, au coeur du Saumurois, Bouvet Ladubay, Brut de Loire, vous reçoit au coeur de ses caves."
    ],
    "nl": [
      "In de Loire-vallei, in het hart van de Saumur-streek, heet Bouvet Ladubay, Brut de Loire, u welkom in zijn kelders."
    ],
    "es": [
      "En el Valle del Loira, en el corazón de la región de Saumur, Bouvet Ladubay, Brut de Loire, le da la bienvenida a sus bodegas."
    ]
  },
