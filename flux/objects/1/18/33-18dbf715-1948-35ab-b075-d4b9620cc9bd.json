  "rdfs:comment": {
    "de": [
      "800 m vom Dorf entfernt, Bastide aus dem Mittelalter, denkmalgeschützte Kirche, Lebensmittelgeschäft, Restaurant mit landwirtschaftlichen Produkten. Cazères: 5 km (Bahnhof), St Girons, St Gaudens, die Pyrenäen: 30 km. Toulouse: 80 km, Spanien 60 km."
    ],
    "en": [
      "At 800 m from the village, medieval bastide, listed church, grocery store, restaurant with farm products. Cazères: 5 km (train station), St Girons, St Gaudens, the Pyrenees: 30 km. Toulouse: 80 km, Spain 60 km."
    ],
    "it": [
      "A 800 m dal villaggio, bastide medievale, chiesa tutelata, negozio di alimentari, ristorante di prodotti agricoli. Cazères: 5 km (stazione ferroviaria), St Girons, St Gaudens, i Pirenei: 30 km. Tolosa: 80 km, Spagna 60 km."
    ],
    "fr": [
      "À 800 m du village, bastide du Moyen-Âge, église classée, épicerie, restaurant de produits fermiers. Cazères : 5 km (gare), St Girons, St Gaudens, les Pyrénées : 30 km. Toulouse: 80 km, Espagne 60 km."
    ],
    "nl": [
      "Op 800 m van het dorp, middeleeuwse bastide, geklasseerde kerk, kruidenierswinkel, restaurant met landbouwproducten. Cazères: 5 km (treinstation), St Girons, St Gaudens, de Pyreneeën: 30 km. Toulouse: 80 km, Spanje 60 km."
    ],
    "es": [
      "A 800 m del pueblo, bastida medieval, iglesia catalogada, tienda de comestibles, restaurante de productos agrícolas. Cazères: 5 km (estación de tren), St Girons, St Gaudens, los Pirineos: 30 km. Toulouse: 80 km, España 60 km."
    ]
  },
