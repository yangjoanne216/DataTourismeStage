  "rdfs:comment": {
    "de": [
      "Entdecken Sie den Hubschrauber in der Provence dank Global Héli Services. Ein Team von Spezialisten, das seine Leidenschaft mit Ihnen teilen und neue Aktivitäten und Eindrücke entdecken wird."
    ],
    "en": [
      "Discover the helicopter in Provence thanks to Global Héli Services. A team of specialists who will share their passion with you and help you discover new activities and new sensations."
    ],
    "it": [
      "Scoprite gli elicotteri in Provenza con Global Héli Services. Un team di specialisti che condividerà con voi la propria passione e vi aiuterà a scoprire nuove attività e nuove sensazioni."
    ],
    "fr": [
      "Découvrez l'hélicoptère en Provence grâce à Global Héli Services. Une équipe de spécialistes qui saura vous faire partager sa passion et découvrir de nouvelles activités, de nouvelles sensations."
    ],
    "nl": [
      "Ontdek helikopters in de Provence met Global Héli Services. Een team van specialisten die hun passie met u delen en u helpen nieuwe activiteiten en sensaties te ontdekken."
    ],
    "es": [
      "Descubra los helicópteros en Provenza con Global Héli Services. Un equipo de especialistas que compartirán su pasión con usted y le ayudarán a descubrir nuevas actividades y nuevas sensaciones."
    ]
  },
