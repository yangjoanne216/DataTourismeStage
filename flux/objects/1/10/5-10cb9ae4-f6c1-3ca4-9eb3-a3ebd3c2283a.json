      "dc:description": {
        "de": [
          "Am Fuße der Ruinen des Schlossbergs gelegen, in der Nähe des Sees von Kruth - Wildenstein. Traditionelle elsässische Küche und Suggestionstafel."
        ],
        "en": [
          "Situated at the foot of the Schlossberg ruins, close to the Kruth - Wildenstein Lake. Traditional Alsatian cuisine and suggestion board."
        ],
        "it": [
          "Situato ai piedi delle rovine dello Schlossberg, vicino al lago Kruth - Wildenstein. Cucina tradizionale alsaziana e tavola dei suggerimenti."
        ],
        "fr": [
          "Située au pied des ruines du Schlossberg, à proximité du Lac de Kruth - Wildenstein. Cuisine traditionnelle alsacienne et tableau de suggestion."
        ],
        "es": [
          "Situado al pie de las ruinas de Schlossberg, cerca del lago Kruth - Wildenstein. Cocina tradicional alsaciana y tabla de sugerencias."
        ],
        "nl": [
          "Gelegen aan de voet van de Schlossberg ruïne, vlakbij het Kruth - Wildenstein meer. Traditionele Elzasser keuken en suggestiebord."
        ]
      },
