  "rdfs:comment": {
    "de": [
      "Le Beau Refuge, 100 m vom legendären Galibier-Pass entfernt, auf 2556 m Höhe gelegen, bietet Ihnen seine Panoramaterrasse einen atemberaubenden Blick auf das schöne Tal von Serre Chevalier."
    ],
    "en": [
      "The Beau Refuge, located 100m from the mythical Galibier pass, at an altitude of 2556m, its panoramic terrace offers a breathtaking view of the beautiful Serre Chevalier valley."
    ],
    "it": [
      "Il Beau Refuge, situato a 100 metri dal mitico passo del Galibier, ad un'altitudine di 2556 metri, la sua terrazza panoramica offre una vista mozzafiato sulla splendida valle di Serre Chevalier."
    ],
    "fr": [
      "Le Beau Refuge, situé à 100m du col mythique du Galibier, à 2556m d'altitude, sa terrasse panoramique vous offre une vue à couper le souffle sur la belle vallée de Serre Chevalier."
    ],
    "es": [
      "El Beau Refuge, situado a 100 m del mítico puerto del Galibier, a 2556 m de altitud, su terraza panorámica ofrece una vista impresionante del hermoso valle de Serre Chevalier."
    ],
    "nl": [
      "De Beau Refuge, gelegen op 100m van de mythische Galibierpas, op 2556m hoogte, biedt met zijn panoramisch terras een adembenemend uitzicht op de prachtige vallei van Serre Chevalier."
    ]
  },
