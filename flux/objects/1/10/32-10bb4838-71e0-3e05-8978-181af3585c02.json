  "rdfs:comment": {
    "de": [
      "Mit Les Canalous entdecken Sie den Hausboot-Tourismus, indem Sie ein Hausboot mieten, ohne Führerschein und erleben Sie einen echten Urlaub ohne Stress!"
    ],
    "en": [
      "With Les Canalous, discover river tourism by renting a houseboat, without a license and enjoy a real holiday without stress!"
    ],
    "it": [
      "Con Les Canalous, scoprite il turismo fluviale noleggiando una casa galleggiante senza licenza e godetevi una vera vacanza senza stress!"
    ],
    "fr": [
      "Avec Les Canalous, découvrez le tourisme fluvial en louant un bateau habitable, sans permis et vivez de vraies vacances sans stress !"
    ],
    "nl": [
      "Ontdek met Les Canalous het riviertoerisme door een woonboot zonder vaarbewijs te huren en geniet van een echte vakantie zonder stress!"
    ],
    "es": [
      "Con Les Canalous, descubra el turismo fluvial alquilando una casa flotante sin licencia y disfrute de unas verdaderas vacaciones sin estrés"
    ]
  },
