  "rdfs:comment": {
    "de": [
      "Private Boat 83 ist ein Unternehmen, das Boote mit Skipper vermietet und Tageskreuzfahrten von Hyères und Umgebung aus anbietet, um seine Halbinsel und seine Inseln zu erkunden."
    ],
    "en": [
      "Private Boat 83 is a company of sea excursions departing from Hyères (Port Saint-Pierre) with the possibility of boarding customers according to their requests (Port du Niel, de la Capte, L'Ayguade, the beaches of La Bergerie or Badine or places of their choice)."
    ],
    "it": [
      "Private Boat 83 è una società di noleggio barche con skipper che offre crociere giornaliere da Hyères e dintorni alla scoperta della penisola e delle isole."
    ],
    "fr": [
      "Private Boat 83 est une entreprise de location de bateau avec skipper proposant des croisières à  la journée au départ de Hyères et ses environs pour découvrir sa presqu'île et ses îles."
    ],
    "nl": [
      "Private Boat 83 is een bootverhuurbedrijf met schipper dat dagtochten aanbiedt vanuit Hyères en omgeving om het schiereiland en de eilanden te ontdekken."
    ],
    "es": [
      "Private Boat 83 es una empresa de alquiler de barcos con patrón que ofrece cruceros de un día desde Hyères y sus alrededores para descubrir su península e islas."
    ]
  },
