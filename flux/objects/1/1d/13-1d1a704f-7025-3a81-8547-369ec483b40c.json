  "rdfs:comment": {
    "de": [
      "Wir kochen traditionell und einfallsreich mit frischen regionalen Erzeugnissen, möglichst aus biologischem Anbau."
    ],
    "en": [
      "Traditional and creative French cuisine. Only fesh products. All dishes homemade. Shady terrace. Located right in the heart of the old city. 2 welcoming dining rooms."
    ],
    "it": [
      "Cucina tradizionale e inventiva. Prodotti freschi, piatti e dolci fatti in casa. Terrazza ombreggiata nel cuore del centro storico e due camere accoglienti."
    ],
    "fr": [
      "Cuisine traditionnelle et inventive. Produits frais, plats et desserts maison. Terrasse ombragée au cœur de la vielle ville et deux salles accueillantes."
    ],
    "nl": [
      "Traditionele en inventieve keuken. Verse producten, huisgemaakte gerechten en desserts. Schaduwrijk terras in het hart van de oude stad en twee gastvrije kamers."
    ],
    "es": [
      "Cocina tradicional e inventiva. Productos frescos, platos y postres caseros. Terraza sombreada en el corazón del casco antiguo y dos habitaciones acogedoras."
    ]
  },
