  "rdfs:comment": {
    "de": [
      "Konditorei, Teestube mit schattiger Terrasse für eine Kostprobe von Eis, Gebäck, Salaten und Sandwiches. Alles hausgemacht. Frühstück nach französischer Art wird von 8 bis 10 Uhr serviert Tarif: 6,50€."
    ],
    "en": [
      "Pastry shop, tea room with a shaded terrace for a tasting of ice cream, pastries, salads and sandwiches. All homemade. French breakfast served from 8am to 10am. Price: 6.50€."
    ],
    "it": [
      "Pasticceria, sala da tè con terrazza ombreggiata per gelati, pasticcini, insalate e panini. Tutti fatti in casa. La colazione alla francese viene servita dalle 8.00 alle 10.00."
    ],
    "fr": [
      "Pâtisserie, salon de thé avec terrasse ombragée pour une dégustation de glace, pâtisseries, salades et sandwichs. Le tout fabriqué maison. Petit déjeuner à la française servi de 8h à 10h tarif : 6.50€."
    ],
    "es": [
      "Pastelería, salón de té con terraza a la sombra para tomar helados, pasteles, ensaladas y sándwiches. Todo casero. El desayuno francés se sirve de 8 a 10 de la mañana."
    ],
    "nl": [
      "Banketbakkerij, theesalon met schaduwrijk terras voor ijs, gebak, salades en sandwiches. Allemaal zelfgemaakt. Frans ontbijt geserveerd van 8 tot 10 uur. Prijs: 6,50€."
    ]
  },
