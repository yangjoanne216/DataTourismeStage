  "rdfs:comment": {
    "de": [
      "T2. 35 m². Etage. 1 Schlafzimmer für 2 Personen. Wohnzimmer, TV, Schlafsofa für 2 Personen, Kochecke, Geschirrspüler, Waschmaschine, Mikrowelle, Gefrierkombination. Badezimmer mit Dusche. Möbliert und klimatisiert. Zugang zum Internet. Privater geschlossener Hof, Gartenmöbel. Bettwäsche wird gestellt."
    ],
    "en": [
      "T2. 35 m². Floor. 1 room 2 persons. Living room, TV, sofa bed 2 persons, kitchenette, dishwasher, washing machine, microwave, combined freezer. Shower room. Furnished air-conditioned. Internet access. Private enclosed courtyard, garden furniture. Linen provided."
    ],
    "it": [
      "T2. 35 m². Primo piano. 1 camera da letto per 2 persone. Soggiorno, TV, divano letto matrimoniale, angolo cottura, lavastoviglie, lavatrice, microonde, congelatore combinato. Bagno con doccia. Arredato con aria condizionata. Accesso a Internet. Cortile privato recintato, mobili da giardino. Biancheria fornita."
    ],
    "fr": [
      "T2. 35 m². Etage. 1 chambre 2 personnes. Séjour, TV, canapé-lit 2 personnes, coin cuisine, lave-vaisselle, lave-linge, micro-ondes, combiné congélateur. Salle d'eau. Meublé climatisé. Accès internet. Cour close privative, salon de jardin. Linge fourni."
    ],
    "es": [
      "T2. 35 m². Primera planta. 1 dormitorio para 2 personas. Salón, TV, sofá cama doble, cocina americana, lavavajillas, lavadora, microondas, congelador combinado. Ducha. Amueblado con aire acondicionado. Acceso a Internet. Patio privado cerrado, muebles de jardín. Se proporciona la ropa de cama."
    ],
    "nl": [
      "T2. 35 m². Eerste verdieping. 1 slaapkamer voor 2 personen. Woonkamer, TV, tweepersoons slaapbank, kitchenette, vaatwasser, wasmachine, magnetron, gecombineerde vriezer. Doucheruimte. Ingericht met airconditioning. Internettoegang. Eigen omheinde binnenplaats, tuinmeubilair. Linnen voorzien."
    ]
  },
