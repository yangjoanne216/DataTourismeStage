  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/2e1d873a-3bf9-361f-a330-317d9f019958",
      "dc:description": {
        "de": [
          "Kochecke, Backofen, Mikrowelle, Geschirrspüler, Waschmaschine, Internetzugang, WLAN, 2 Fernseher, Telefon, elektrische Heizung, Dusche, separates WC, 1 Schlafzimmer, 1 Doppelbett, 1 Etagenbett, 1 Klappbank, 1 Kinderbett, Kindersitz. Garten. Privater Parkplatz. Kurtaxe nicht inbegriffen. Mietnebenkosten inbegriffen."
        ],
        "en": [
          "Kitchenette, oven, microwave, dishwasher, washing machine, internet access, wifi access, 2 televisions, telephone, electric heating, shower, separate WC, 1 bedroom, 1 double bed, 1 bunk bed, 1 sofa bed, 1 child's bed, baby seat. Garden. Private parking. Tourist tax not included. Rental charges included."
        ],
        "it": [
          "Angolo cottura, forno, microonde, lavastoviglie, lavatrice, accesso internet, accesso wifi, 2 televisori, telefono, riscaldamento elettrico, doccia, WC separato, 1 camera da letto, 1 letto matrimoniale, 1 letto a castello, 1 divano letto, 1 letto per bambini, seggiolino. Giardino. Parcheggio privato. Tassa di soggiorno non inclusa. Spese di noleggio incluse."
        ],
        "fr": [
          "Coin cuisine, four, micro-ondes, lave-vaisselle, lave-linge, accès internet, accès wifi, 2 télévisions, téléphone, chauffage électrique, douche, WC séparés, 1 chambre, 1 lit/2pers., 1 lit superposé, 1 banquette clic-clac, 1 lit enfant, siège bébé. Jardin. Parking privé. Taxe de séjour non comprise. Charges locatives comprises."
        ],
        "es": [
          "Kitchenette, horno, microondas, lavavajillas, lavadora, acceso internet, acceso wifi, 2 televisores, teléfono, calefacción eléctrica, ducha, WC separado, 1 habitación, 1 cama doble, 1 litera, 1 sofá cama, 1 cama de niño, silla de bebé. Jardín. Aparcamiento privado. Tasa turística no incluida. Gastos de alquiler incluidos."
        ],
        "nl": [
          "Keukenhoek, oven, magnetron, vaatwasser, wasmachine, internettoegang, wifi, 2 televisies, telefoon, elektrische verwarming, douche, aparte WC, 1 slaapkamer, 1 tweepersoonsbed, 1 stapelbed, 1 slaapbank, 1 kinderbed, babyzitje. Tuin. Privé parkeerplaats. Toeristenbelasting niet inbegrepen. Huurkosten inbegrepen."
        ]
      },
