  "rdfs:comment": {
    "de": [
      "Paintball auf einem 8000 m² großen Waldgelände in Rivolet, im Herzen des Beaujolais, 30 km von Lyon entfernt. Mit einem Animateur, für alle ab 16 Jahren zugänglich. Jedes Spiel dauert 5 bis 10 Minuten."
    ],
    "en": [
      "Paintball in a 8000 m² wooded area in Rivolet, in the heart of the Beaujolais region, 30 km from Lyon. With an instructor, open to all from 16 years old. Each game lasts 5 to 10 minutes."
    ],
    "it": [
      "Paintball su un sito forestale di 8.000 m² a Rivolet, nel cuore della regione del Beaujolais, a 30 km da Lione. Con un istruttore, accessibile a tutti a partire dai 16 anni. Ogni partita dura dai 5 ai 10 minuti."
    ],
    "fr": [
      "Paintball sur un terrain forestier de 8 000 m² à Rivolet, au coeur du Beaujolais, à 30 km de Lyon. Avec un animateur, accessible à tous à partir de 16 ans. Chaque partie dure 5 à 10 minutes."
    ],
    "nl": [
      "Paintball op een bosterrein van 8.000 m² in Rivolet, in het hart van de Beaujolais, op 30 km van Lyon. Met een instructeur, toegankelijk voor iedereen vanaf 16 jaar. Elk spel duurt 5 tot 10 minuten."
    ],
    "es": [
      "Paintball en un terreno forestal de 8.000 m² en Rivolet, en el corazón de la región de Beaujolais, a 30 km de Lyon. Con un instructor, accesible para todos a partir de los 16 años. Cada juego dura entre 5 y 10 minutos."
    ]
  },
