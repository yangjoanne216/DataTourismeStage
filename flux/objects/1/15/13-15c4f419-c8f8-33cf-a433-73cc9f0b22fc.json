  "rdfs:comment": {
    "de": [
      "Restaurant in Mouans-Sartoux. Küche aus frischen Produkten, auf der Suche nach neuen Geschmacksrichtungen, verschiedenen Gewürzen... hausgemacht!\r\nMöglichkeit, Gerichte zum Mitnehmen zu bestellen..."
    ],
    "en": [
      "Restaurant located in Mouans-Sartoux. Cuisine made from fresh products, looking for new flavors, various spices... homemade!\r\nPossibility to order takeaway meals."
    ],
    "it": [
      "Ristorante situato a Mouans-Sartoux. Cucina fatta di prodotti freschi, ricerca di nuovi sapori, spezie varie... fatta in casa!\r\nPossibilità di ordinare pasti da asporto."
    ],
    "fr": [
      "Restaurant situé à Mouans-Sartoux. Cuisine élaborée à partir de produits frais, à la recherche de nouvelles saveurs, d'épices variées... faite maison !\r\nPossibilité de commander des plats à emporter.."
    ],
    "es": [
      "Restaurante situado en Mouans-Sartoux. Cocina elaborada con productos frescos, en busca de nuevos sabores, especias variadas... ¡casera!\r\nPosibilidad de pedir comida para llevar."
    ],
    "nl": [
      "Restaurant gelegen in Mouans-Sartoux. Keuken op basis van verse producten, op zoek naar nieuwe smaken, diverse kruiden... huisgemaakt!\r\nMogelijkheid om afhaalmaaltijden te bestellen."
    ]
  },
