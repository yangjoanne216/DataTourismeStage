  "rdfs:comment": {
    "de": [
      "Vorstellung der archäologischen Ausgrabungen unter der Leitung von Herrn Jean-Pierre Girault; Entdeckung von Überresten von Burgen, Verstecken usw. auf den Anhöhen über der Dordogne um Carennac."
    ],
    "en": [
      "Presentation of the archaeological excavations directed by Mr. Jean-Pierre Girault; discovery of the remains of castles, lairs, etc., on the heights overlooking the Dordogne around Carennac."
    ],
    "it": [
      "Presentazione degli scavi archeologici diretti da Jean-Pierre Girault; scoperta di resti di castelli, tane, ecc. sulle alture che dominano la Dordogna intorno a Carennac."
    ],
    "fr": [
      "Présentation des fouilles archéologiques dirigées par M. Jean-Pierre Girault ; découverte de vestiges de châteaux, repaires, etc, sur les hauteurs qui surplombent la Dordogne autour de Carennac."
    ],
    "nl": [
      "Voorstelling van de archeologische opgravingen onder leiding van de heer Jean-Pierre Girault; ontdekking van de overblijfselen van kastelen, burchten, enz. op de hoogten boven de Dordogne rond Carennac."
    ],
    "es": [
      "Presentación de las excavaciones arqueológicas dirigidas por el Sr. Jean-Pierre Girault; descubrimiento de restos de castillos, guaridas, etc., en las alturas que dominan la Dordoña en torno a Carennac."
    ]
  },
  "rdfs:label": {
