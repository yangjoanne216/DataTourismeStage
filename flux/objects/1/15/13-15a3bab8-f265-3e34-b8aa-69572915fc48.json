  "rdfs:comment": {
    "de": [
      "Die Brasserie du square liegt mitten im Stadtzentrum von Aurillac und bietet Ihnen qualitativ hochwertige, hausgemachte Gerichte mit Produkten aus kurzen Vertriebswegen und von lokalen Produzenten."
    ],
    "en": [
      "Located in the heart of downtown Aurillac, the Brasserie du square offers quality dishes, homemade, with products from short circuits and local producers."
    ],
    "it": [
      "Situata nel centro di Aurillac, la Brasserie du Square offre piatti casalinghi di qualità, con prodotti provenienti da produttori locali."
    ],
    "fr": [
      "Situé en plein centre-ville d'Aurillac, la Brasserie du square vous propose des plats de qualité, faits maisons, avec des produits de circuits courts et de producteurs locaux."
    ],
    "es": [
      "Situada en el centro de Aurillac, la Brasserie du Square ofrece platos caseros de calidad, con productos de productores locales."
    ],
    "nl": [
      "De Brasserie du Square, gelegen in het centrum van Aurillac, biedt huisgemaakte kwaliteitsgerechten, met producten van lokale producenten."
    ]
  },
