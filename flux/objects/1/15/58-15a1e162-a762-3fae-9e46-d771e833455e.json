  "rdfs:comment": {
    "de": [
      "La Placette ist eine Brasserie, die französische Küche mit mediterranem Einschlag zum Mittagessen, Tapas am Abend und einen Brunch am Sonntag anbietet."
    ],
    "en": [
      "La Placette is a brasserie that offers French cuisine with Mediterranean tendencies for lunch, tapas in the evening, and brunch on Sundays."
    ],
    "it": [
      "La Placette è una brasserie che offre cucina francese dal sapore mediterraneo a pranzo, tapas la sera e un brunch la domenica."
    ],
    "fr": [
      "La Placette est une brasserie qui propose une cuisine française à tendance méditerranéenne pour le déjeuner, des tapas le soir, un brunch le dimanche."
    ],
    "nl": [
      "La Placette is een brasserie met een Franse keuken met een mediterraan tintje voor de lunch, tapas in de avond en een brunch op zondag."
    ],
    "es": [
      "La Placette es una brasserie que sirve cocina francesa con un toque mediterráneo para el almuerzo, tapas por la noche y brunch los domingos."
    ]
  },
