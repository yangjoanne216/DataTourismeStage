  "rdfs:comment": {
    "de": [
      "\"Da ich internationale Sammler habe\", sagt Victor Orly, \"vertrete ich\r\ndie Provence"
    ],
    "en": [
      "\"Having international collectors,\" says Victor Orly, \"I represent\r\nprovence"
    ],
    "it": [
      "\"Avendo collezionisti internazionali\", dice Victor Orly, \"rappresento\r\nprovenza"
    ],
    "fr": [
      "\"Ayant des collectionneurs internationaux\", dit Victor Orly, \"je représente\r\nla Provence"
    ],
    "nl": [
      "\"Met internationale verzamelaars,\" zegt Victor Orly, \"vertegenwoordig ik\r\nprovence"
    ],
