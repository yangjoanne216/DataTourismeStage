  "rdfs:comment": {
    "de": [
      "Als Residenz der Bischöfe bewahrt dieses Dorf zahlreiche Überreste: den zentralen Platz, den Barbakanenturm, das Kuhtor und Häuser mit Erkern."
    ],
    "en": [
      "Residence of the bishops, this village preserves many vestiges: the central place, the barbican tower, the portal of the cow, the corbelled houses."
    ],
    "it": [
      "Residenza dei vescovi, questo villaggio ha conservato molte vestigia: la piazza centrale, la torre del barbacane, la porta delle vacche, le case a mensola."
    ],
    "fr": [
      "Résidence des évêques, ce village conserve de nombreux vestiges : la place centrale, le tour barbacane, le portail de la vache, des maisons à encorbellement."
    ],
    "es": [
      "Residencia de los obispos, este pueblo ha conservado muchos vestigios: la plaza central, la torre de la barbacana, la puerta de la vaca, las casas con ménsulas."
    ],
    "nl": [
      "Als residentie van de bisschoppen heeft dit dorp veel overblijfselen bewaard: het centrale plein, de barbikontoren, de koeienpoort, huizen met kraagstenen."
    ]
  },
