  "rdfs:comment": {
    "de": [
      "10 km von Figeac entfernt, auf dem Land, ebenerdige, sehr ruhige, unabhängige Unterkunft, 500 m vom Haus der Eigentümer entfernt. Geschlossener Hof, eingezäuntes Grundstück, Terrasse. Bach 100m von der Unterkunft entfernt unterhalb."
    ],
    "en": [
      "10km from Figeac, in the countryside, single storey gîte, very quiet, independent, 500m from the owners' house. Enclosed courtyard, enclosed land, terrace. Stream 100m from the gîte below."
    ],
    "it": [
      "A 10 km da Figeac, in campagna, gîte a un piano, molto tranquillo, indipendente, a 500 m dalla casa dei proprietari. Cortile chiuso, terreno recintato, terrazza. Ruscello a 100 m dal gîte sottostante."
    ],
    "fr": [
      "A 10km de Figeac, en pleine campagne, gîte de plain-pied, très calme, indépendant, à  500m de la maison des propriétaires. Cour fermée, terrain clos, terrasse. Ruisseau à 100m du gîte en contrebas."
    ],
    "es": [
      "A 10 km de Figeac, en el campo, casa rural de una sola planta, muy tranquila, independiente, a 500 m de la casa de los propietarios. Patio cerrado, terreno cerrado, terraza. Arroyo a 100 metros de la casa rural."
    ],
    "nl": [
      "10km van Figeac, op het platteland, gelijkvloerse gîte, zeer rustig, onafhankelijk, 500m van het huis van de eigenaar. Afgesloten binnenplaats, omheind terrein, terras. Beek 100m van de gîte beneden."
    ]
  },
