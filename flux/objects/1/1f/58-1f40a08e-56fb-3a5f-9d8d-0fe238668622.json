  "rdfs:comment": {
    "de": [
      "Restaurant unter freiem Himmel, italienisches Restaurant auf der Dachterrasse des Gebäudes Babel Community (ehemaliges Gebäude der Galeries Lafayette, erreichbar mit dem Aufzug in der Rue Haxo 17 bis zum 7. Stock)."
    ],
    "en": [
      "Open-air restaurant, Italian restaurant on the roof terrace of the Babel Community building (former Galeries Lafayette building, accessible by elevator from 17 rue Haxo to the 7th floor)."
    ],
    "it": [
      "Ristorante all'aperto, ristorante italiano sulla terrazza del tetto dell'edificio Babel Community (ex Galeries Lafayette, raggiungibile con l'ascensore da 17 rue Haxo al 7° piano)."
    ],
    "fr": [
      "Restaurant à ciel ouvert, restaurant italien sur le toit terrasse de l'immeuble Babel Community  (ancien immeuble des Galeries Lafayette, accessible par ascenseur au 17 rue Haxo jusqu'au 7e étage)."
    ],
    "es": [
      "Restaurante italiano al aire libre en la azotea del edificio de la Comunidad Babel (antiguo edificio de las Galerías Lafayette, accesible en ascensor desde el 17 rue Haxo hasta la 7ª planta)."
    ],
    "nl": [
      "Openluchtrestaurant, Italiaans restaurant op het dakterras van het gebouw van de Gemeenschap Babel (voormalig gebouw van de Galeries Lafayette, bereikbaar met de lift vanaf 17 rue Haxo tot de 7e verdieping)."
    ]
