  "rdfs:comment": {
    "de": [
      "Bildende Künstlerin, Bildhauerin auf Stein (Kalkstein, Marmor, Granit), Bronze.\nMalerei, Gravur.\nBesichtigung des Ateliers nach Vereinbarung.\nDas Atelier beherbergt temporäre Ausstellungen."
    ],
    "en": [
      "Sculptor"
    ],
    "it": [
      "Artista plastico scultore su pietra (calcare, marmo, granito), bronzo.\nPittura, incisione.\nVisita del laboratorio su appuntamento.\nIl laboratorio ospita mostre temporanee."
    ],
    "fr": [
      "Artiste plasticienne sculpteur sur pierre (calcaire, marbre,granit), bronze.\nPeinture, gravure.\nVisite de l’atelier sur rendez-vous.\nL’atelier accueille des expositions temporaires."
    ],
    "nl": [
      "Plastisch kunstenaar beeldhouwer op steen (kalksteen, marmer, graniet), brons.\nSchilderij, gravure.\nBezoek van de werkplaats op afspraak.\nIn het atelier worden tijdelijke tentoonstellingen gehouden."
    ],
    "es": [
      "Artista plástico escultor en piedra (caliza, mármol, granito), bronce.\nPintura, grabado.\nVisita del taller con cita previa.\nEl taller acoge exposiciones temporales."
    ]
  },
