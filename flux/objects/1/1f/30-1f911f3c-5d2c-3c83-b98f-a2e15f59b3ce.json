  "rdfs:comment": {
    "de": [
      "Freistehendes Haus, 850 m vom Strand entfernt, in einem Dorf zwischen Saint-Vaast-la-Hougue und Barfleur. Eingezäunter Garten von ca. 1200m². Große Terrasse mit Gartenmöbeln und Grill für Mahlzeiten im Freien. Garage. Parkplatz."
    ],
    "en": [
      "Detached house located 850 meters from the beach, in a village between Saint-Vaast-la-Hougue and Barfleur. Fenced garden of about 1200m². Large terrace with garden furniture and barbecue for outdoor meals. Garage. Parking..."
    ],
    "it": [
      "Casa indipendente situata a 850 metri dalla spiaggia, in un villaggio tra Saint-Vaast-la-Hougue e Barfleur. Giardino recintato di circa 1200m². Ampia terrazza con mobili da giardino e barbecue per mangiare all'aperto. Garage. Posto auto..."
    ],
    "fr": [
      "Maison indépendante située à 850 mètres de la plage, dans un village entre Saint-Vaast-la-Hougue et Barfleur. Jardin clôturé d'environ 1200m². Grande terrasse avec salon de jardin et barbecue pour prendre ses repas en plein air. Garage. Parking..."
    ],
    "nl": [
      "Vrijstaand huis op 850 meter van het strand, in een dorp tussen Saint-Vaast-la-Hougue en Barfleur. Omheinde tuin van ongeveer 1200m². Groot terras met tuinmeubelen en barbecue om buiten te eten. Garage. Parkeerplaats..."
    ],
    "es": [
      "Casa independiente situada a 850 metros de la playa, en un pueblo entre Saint-Vaast-la-Hougue y Barfleur. Jardín vallado de unos 1200m². Amplia terraza con muebles de jardín y barbacoa para comer al aire libre. Garaje. Plaza de aparcamiento..."
    ]
  },
