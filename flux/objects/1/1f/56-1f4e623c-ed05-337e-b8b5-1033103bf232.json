  "rdfs:comment": {
    "de": [
      "Wohnung in Villa, 1,2 km von den Thermen entfernt. Erdgeschoss- Unverbaubarer Blick auf den Canigou. Geschlossener Parkplatz. Sofa in 160 cm Breite. Ausstattung für Kinder. Gemeinsame Waschmaschine. 1 Bett 140 & Schlafcouch. Gartenmöbel, Wifi"
    ],
    "en": [
      "Apartment in a villa, 1.2 kms from the thermal baths. Ground floor - breathtaking view of the Canigou. Closed parking. Sofa in 160. Children equipment. Shared washing machine. 1 bed 140 & convertible. Garden furniture, wifi"
    ],
    "it": [
      "Appartamento in villa, a 1,2 km dalle terme. Piano terra - vista mozzafiato sul Canigou. Parcheggio recintato. Divano in 160. Attrezzature per bambini. Lavatrice in comune. 1 letto 140 & convertibile. Mobili da giardino, wifi"
    ],
    "fr": [
      "Appartement dans villa, à 1.2 kms des Thermes. RdC- Vue imprenable sur le Canigou. Parking clos. Canapé en 160. Equipement enfants. Lave-linge commun. 1 lit 140 & convertible. Salon de jardin, wifi"
    ],
    "es": [
      "Piso en una villa, a 1,2 kms de los baños termales. Planta baja - vista impresionante del Canigou. Aparcamiento cerrado. Sofá en 160. Equipamiento para niños. Lavadora compartida. 1 cama 140 y convertible. Muebles de jardín, wifi"
    ],
    "nl": [
      "Appartement in een villa, op 1,2 km van de thermale baden. Begane grond - adembenemend uitzicht op de Canigou. Afgesloten parkeerplaats. Bank in 160. Uitrusting voor kinderen. Gedeelde wasmachine. 1 bed van 140 en cabriolet. Tuinmeubilair, wifi"
    ]
  },
