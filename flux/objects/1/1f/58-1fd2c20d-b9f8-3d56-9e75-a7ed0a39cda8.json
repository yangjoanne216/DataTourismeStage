  "rdfs:comment": {
    "de": [
      "Ô Marché d'Asie freut sich, Sie begrüßen zu dürfen, damit Sie unsere köstlichen asiatischen Aromen genießen können."
    ],
    "en": [
      "Ô Marché d'Asie will be delighted to welcome you to savor our delicious Asian flavors."
    ],
    "it": [
      "Ô Marché d'Asie sarà lieto di accogliervi e di farvi assaporare i nostri deliziosi sapori asiatici."
    ],
    "fr": [
      "Ô Marché d'Asie sera ravi de vous accueillir pour vous faire savourer nos délicieuses saveurs d'Asie."
    ],
    "nl": [
      "Ô Marché d'Asie verwelkomt u graag om van onze heerlijke Aziatische smaken te genieten."
    ],
    "es": [
      "En Ô Marché d'Asie estaremos encantados de recibirle para que deguste nuestros deliciosos sabores asiáticos."
    ]
  },
