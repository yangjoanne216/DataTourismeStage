  "rdfs:comment": {
    "de": [
      "Near Sarlat (25 min).\nIn a small quiet country hamlet, a renovated stone barn cottage with heated pool set in grounds next to woodland. \nThe properties sleeps 11 combined with air conditionning, wifi and everything you need for a happy holiday."
    ],
    "en": [
      "Near Sarlat (25 min).\nIn a small quiet country hamlet, a renovated stone barn cottage with heated pool set in grounds next to woodland. \nThe properties sleeps 11 combined with air conditionning, wifi and everything you need for a happy holiday."
    ],
    "it": [
      "Vicino a Sarlat, proprietà in pietra con aria condizionata e piscina riscaldata.\nA 25 minuti da Sarlat-La-Canéda, la locazione è composta da un fienile in pietra interamente ristrutturato così come la sua dependance per offrirvi comfort, spazio e luminosità."
    ],
    "fr": [
      "Proche Sarlat, ensemble immobilier en pierre avec climatisation et une piscine chauffée.\nA 25 minutes de Sarlat-La-Canéda, la location est composée d'une grange en pierres entièrement restaurée ainsi que sa dépendance pour vous apporter confort, espace et luminosité."
    ],
    "es": [
      "Near Sarlat (25 min).\nIn a small quiet country hamlet, a renovated stone barn cottage with heated pool set in grounds next to woodland. \nThe properties sleeps 11 combined with air conditionning, wifi and everything you need for a happy holiday."
    ],
    "nl": [
      "Dichtbij Sarlat, stenen woning met airconditioning en een verwarmd zwembad.\nOp 25 minuten van Sarlat-La-Canéda, is de verhuur samengesteld uit een stenen schuur volledig gerestaureerd evenals zijn afhankelijkheid om u comfort, ruimte en helderheid te brengen."
    ]
  },
