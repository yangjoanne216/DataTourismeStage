  "rdfs:comment": {
    "de": [
      "Jahrmarkt in Saint-Bonnet-Tronçais, im Rahmen des Patronatsfestes.\r\nTrödelmarkt, Karussells, Animationen, Verpflegung...usw...\r\nAm Abend Feuerwehrball und Feuerwerk."
    ],
    "en": [
      "Fair in Saint-Bonnet-Tronçais, within the framework of the patronal festival.\r\nFlea market, carousels, animations, catering...etc...\r\nIn the evening, firemen's ball and fireworks."
    ],
    "it": [
      "Fiera a Saint-Bonnet-Tronçais, nell'ambito della festa patronale.\r\nMercatino delle pulci, giostre, animazione, ristorazione... ecc.\r\nIn serata, ballo dei pompieri e fuochi d'artificio."
    ],
    "fr": [
      "Fête foraine à Saint-Bonnet-Tronçais, dans le cadre de la fête patronale.\r\nBrocante, manèges, animations, restauration...etc...\r\nEn soirée bal des pompiers et feu d'artifice."
    ],
    "nl": [
      "Kermis in Saint-Bonnet-Tronçais, in het kader van het patroonsfeest.\r\nVlooienmarkt, attracties, animatie, catering... enz...\r\nS Avonds brandweerbal en vuurwerk."
    ],
    "es": [
      "Feria en Saint-Bonnet-Tronçais, en el marco de las fiestas patronales.\r\nMercadillo, atracciones, animaciones, restauración... etc.\r\nPor la noche, baile de los bomberos y fuegos artificiales."
    ]
