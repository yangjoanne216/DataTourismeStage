  "rdfs:comment": {
    "de": [
      "500 m vom Stadtzentrum, 10 km von der Front de Neige von La Bresse Hohneck, 8 km von den Pisten von La Bresse Lispach, 3 km von den Pisten von La Bresse Brabant, 9 km von der Domaine Nordique La Bresse - Lispach. 500 m von der Haltestelle Bus Hivernal, der während der französischen Schulferien und an Samstagen/Sonntagen im Januar verkehrt."
    ],
    "en": [
      "500 m from the town centre, 10 km from the La Bresse Hohneck Snow Front, 8 km from the La Bresse Lispach slopes, 3 km from the La Bresse Brabant slopes, 9 km from the Domaine Nordique La Bresse - Lispach. 500 m from the Bus Hivernal stop which runs during the French school holidays and on Saturdays/Sundays in January."
    ],
    "it": [
      "a 500 m dal centro del paese, a 10 km dal fronte di neve di La Bresse Hohneck, a 8 km dalle piste di La Bresse Lispach, a 3 km dalle piste di La Bresse Brabant, a 9 km dal Domaine Nordique La Bresse - Lispach. a 500 m dalla fermata dell'autobus Hivernal, che passa durante le vacanze scolastiche francesi e il sabato/domenica di gennaio."
    ],
    "fr": [
      "500 m du centre ville, 10 km du Front de Neige de La Bresse Hohneck, 8 km des pistes de La Bresse Lispach, 3 km des pistes de La Bresse Brabant, 9 km du Domaine Nordique La Bresse - Lispach. 500 m de l'arrêt Bus Hivernal qui circule pendant les vacances scolaires françaises et les samedi/dimanche de janvier."
    ],
    "es": [
      "a 500 m del centro de la ciudad, a 10 km del frente de nieve de La Bresse Hohneck, a 8 km de las pistas de La Bresse Lispach, a 3 km de las pistas de La Bresse Brabant, a 9 km del Domaine Nordique La Bresse - Lispach. a 500 m de la parada del Bus Hivernal que funciona durante las vacaciones escolares francesas y los sábados/domingos de enero."
    ],
    "nl": [
      "500 m van het stadscentrum, 10 km van het Sneeuwfront van La Bresse Hohneck, 8 km van de pistes van La Bresse Lispach, 3 km van de pistes van La Bresse Brabant, 9 km van het Domaine Nordique La Bresse - Lispach. 500 m van de bushalte Hivernal die rijdt tijdens de Franse schoolvakanties en op zaterdag/zondag in januari."
    ]
  },
