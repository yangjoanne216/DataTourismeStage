  "rdfs:comment": {
    "de": [
      "Städtische Bibliothek: Ausleihe von Büchern und Audio-CDs; Internet-Zugang."
    ],
    "en": [
      "Municipal Library: Loan of books and CDs, Internet access."
    ],
    "it": [
      "Benvenuti nella biblioteca comunale! La biblioteca Georges PEREC fa parte della rete bibliotecaria del Massiccio del Vercors. L'accesso alla biblioteca, alle attività, alla consultazione della stampa e di Internet è GRATUITO."
    ],
    "fr": [
      "Bienvenue à la bibliothèque municipale ! La bibliothèque Georges PEREC fait partie du réseau des bibliothèques du Massif du Vercors. L'accès aux bibliothèques, aux animations, la consultation sur place, de la presse et d’Internet, sont LIBRES ET GRATUITS."
    ],
    "nl": [
      "Welkom in de gemeentelijke bibliotheek! De Georges PEREC bibliotheek maakt deel uit van het bibliotheeknetwerk van het Massif du Vercors. De toegang tot de bibliotheek, de activiteiten, de raadpleging van de pers en het internet zijn GRATIS."
    ],
    "es": [
      "Bienvenido a la biblioteca municipal La biblioteca Georges PEREC forma parte de la red de bibliotecas del Macizo del Vercors. El acceso a la biblioteca, a las actividades, a la consulta de la prensa y a Internet, son GRATUITOS."
    ]
  },
