  "rdfs:comment": {
    "de": [
      "Einzigartiger Erlebniskurs im Herzen des Marsannenwaldes: 3 ha. arrangiert, 142 Spiele, 15 Kurse. Grün, Blau, Rot und Schwarz: 3 Stunden unvergessliche Aktivitäten! Ab 2 Jahren."
    ],
    "en": [
      "Unique adventure trail in the heart of the forest of Marsanne : 7½ acres set out, 142 games, 15 trails.  4 levels, green, blue, red and black : 3 hours of unforgettable activities ! From 2 years old."
    ],
    "it": [
      "Percorso avventura unico nel cuore della foresta di Marsanne: 3 ettari di aree paesaggistiche, 142 giochi, 15 percorsi. Livelli verde, blu, rosso e nero: 3 ore di attività indimenticabili! A partire da 2 anni."
    ],
    "fr": [
      "Parcours d'aventure unique au cœur de la forêt de Marsanne: 3 ha. aménagés, 142 jeux, 15 parcours. Niveaux vert, bleu, rouge et noir: 3 heures d'activités inoubliables! À partir de 2 ans."
    ],
    "es": [
      "Recorrido de aventura único en el corazón del bosque de Marsanne: 3 ha. de zonas ajardinadas, 142 juegos, 15 recorridos. Niveles verde, azul, rojo y negro: ¡3 horas de actividades inolvidables! A partir de 2 años."
    ],
    "nl": [
      "Een uniek en avontuurlijk parcours midden in de bossen van Marsanne: 3ha aanplant, 142 spellen, 15 routes met de niveaus groen, blauw, rood en zwart: 3 uurvan  onvergetelijke activiteiten. Vanaf 2 jaar."
    ]
  },
