  "rdfs:comment": {
    "de": [
      "Steakhaus. Französisches und ausländisches Qualitätsfleisch: Wagyu (Kobe-Rind aus Australien), Gallice-Fleisch (Spanien), argentinisches Fleisch...Salers, Blonde d'Aquitaine...\nSalate, Geflügel, Ente....\nWeinbar des Besitzers (im Glas oder an der Schnur)"
    ],
    "en": [
      "Steak house. French and foreign quality meats: Wagyu (Kobe beef of Australian origin), meat from Gallica (Spain), meat from Argentina...Salers, blonde from Aquitaine...\nSalads, poultry, duck....\nOwner's wine bar (by the glass or string)"
    ],
    "it": [
      "Bisteccheria. Carne francese e straniera di qualità: Wagyu (manzo di Kobe dall'Australia), carne di Gallice (Spagna), carne argentina... Salers, Blonde d'Aquitaine...\nInsalate, pollame, anatra....\nEnoteca del proprietario (al bicchiere o al filo)"
    ],
    "fr": [
      "Steak house. Viandes française et étrangère de qualité : Wagyu (boeuf de Kobé d'origine australienne), viande de Gallice (Espagne), viande Argentine...Salers, blonde d'Aquitaine...\nSalades, volailles, canard....\nBar à vin de propriétaire (au verre ou à la ficelle)"
    ],
    "es": [
      "Un asador. Carne de calidad francesa y extranjera: Wagyu (carne de Kobe de Australia), carne de Gallice (España), carne argentina... Salers, Blonde d'Aquitaine...\nEnsaladas, aves de corral, duck....\nBar de vinos del propietario (por copa o por cuerda)"
    ],
    "nl": [
      "Steakhouse. Frans en buitenlands kwaliteitsvlees: Wagyu (Kobe rundvlees uit Australië), Gallice vlees (Spanje), Argentijns vlees... Salers, Blonde d'Aquitaine...\nSalades, gevogelte, eend ....\nWijnbar van de eigenaar (per glas of per draad)"
    ]
  },
