  "rdfs:comment": {
    "de": [
      "Thai-Gourmetküche, zubereitet von einem echten thailändischen Küchenchef. 100 m2 großer Veranstaltungssaal, 80 m2 große schattige und blumengeschmückte Terrasse gegenüber dem Hafen von Pointe Rouge."
    ],
    "en": [
      "Gastronomic Thai cuisine prepared by a real Thai chef. Reception room of 100 m2, shaded and flowered terrace of 80 m2 in front of the Pointe Rouge harbour."
    ],
    "it": [
      "Cucina tailandese gourmet preparata da un vero chef tailandese. Sala di ricevimento di 100 m2, terrazza ombreggiata e fiorita di 80 m2 di fronte al porto di Pointe Rouge."
    ],
    "fr": [
      "Cuisine gastronomique thaï préparée par un vrai chef thaïlandais. Salle de réception de 100 m2., terrasse ombragée et fleurie de 80 m2 en face du port de la Pointe Rouge."
    ],
    "nl": [
      "Thaise keuken voor fijnproevers, bereid door een echte Thaise kok. Ontvangstruimte van 100 m2, schaduwrijk en bebloemd terras van 80 m2 voor de haven van Pointe Rouge."
    ],
    "es": [
      "Cocina tailandesa gourmet preparada por un auténtico chef tailandés. Sala de recepción de 100 m2, terraza sombreada y florida de 80 m2 frente al puerto de Pointe Rouge."
    ]
  },
