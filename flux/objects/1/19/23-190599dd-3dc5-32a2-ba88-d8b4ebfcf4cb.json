  "rdfs:comment": {
    "de": [
      "Boutique für Dessous - Miederwaren - Prêt à porter - Bademode. Männer, Frauen und Kinder. Spezialist für große Größen und tiefe Cups. Kostenlose Änderungen an den Kleidungsstücken. Anprobe und Lieferung nach Hause. Hinterlegung bei einer Reinigung."
    ],
    "en": [
      "Lingerie shop - corsetry - ready to wear - swimwear. Men, women, children. Specialised Free alterations. Fittings and home deliveries. Dry cleaner's depot."
    ],
    "it": [
      "Negozio di lingerie - corsetteria - pronto moda - costumi da bagno. Uomini, donne, bambini. Specializzato in taglie grandi e coppe profonde. Modifiche gratuite. Allestimenti e consegne a domicilio. Servizio di lavaggio a secco."
    ],
    "fr": [
      "Boutique de lingerie - corseterie - prêt à porter - maillots de bain. Hommes, femmes, enfants. Spécialiste des grandes tailles et bonnets profonds. Retouches gratuites. Essayage et livraisons à domicile. Dépôt de pressing."
    ],
    "es": [
      "Tienda de lencería - corsetería - prêt-à-porter - trajes de baño. Hombres, mujeres, niños. Especialista en tallas grandes y copas profundas. Alteraciones gratuitas. Equipamiento y entregas a domicilio. Depósito de la tintorería."
    ],
    "nl": [
      "Lingeriewinkel - corsetterie - confectiekleding - badmode. Mannen, vrouwen, kinderen. Specialist in grote maten en diepe cups. Gratis aanpassingen. Inrichtingen en leveringen aan huis. Stomerij service."
    ]
  },
