  "rdfs:comment": {
    "de": [
      "Locaboat ist seit 35 Jahren der Spezialist für Hausbootfahrten und führerscheinfreies Mieten von Booten und Hausbooten in Frankreich und Europa."
    ],
    "en": [
      "Locaboat is the specialist in river navigation and boat and barge rental without a licence in France and Europe for 35 years."
    ],
    "it": [
      "Locaboat è lo specialista della navigazione fluviale e del noleggio di barche e chiatte senza patente in Francia e in Europa da 35 anni."
    ],
    "fr": [
      "Locaboat est le spécialiste de la navigation fluviale et de la location de bateaux et pénichettes sans permis en France et en Europe depuis 35 ans."
    ],
    "nl": [
      "Locaboat is al 35 jaar de specialist in de riviervaart en de verhuur van boten en binnenschepen zonder vaarbewijs in Frankrijk en Europa."
    ],
    "es": [
      "Locaboat es el especialista en navegación fluvial y alquiler de barcos y barcazas sin licencia en Francia y Europa desde hace 35 años."
    ]
  },
