      "dc:description": {
        "de": [
          "Direktverkauf von Bauernhofkäse aus roher Kuhmilch. Unsere Produkte: -Milch -Faisselle -Frisch -Moellig -Halbtrocken -Mit Pfeffer -Mit Kräutern -Mit Nüssen -Façon chaource petit modèle -Façon chaource grand modèle -Affiniert mit Marc de Bourgogne"
        ],
        "en": [
          "Direct sale of farm cheeses made from raw cow's milk. Our products : -Milk -Dishes -Fresh -Soft -Dried -Pepper -Herbs -Walnuts -Small chaource -Large chaource -Marc de Bourgogne refined"
        ],
        "it": [
          "Vendita diretta di formaggi di fattoria prodotti con latte vaccino crudo. I nostri prodotti: -Latte -Piatti -Freschi -Morbidi -Essiccati -Pepe -Erbe -Noci -Caource piccolo -Caource grande -Marc de Bourgogne raffinato"
        ],
        "fr": [
          "Vente directe de fromages fermiers au lait cru de vache. Nos produits : -Lait -Faisselle - Frais -Moelleux -Demi-sec - Au poivre -Aux fines herbes -Aux noix -Façon chaource petit modèle -Façon chaource grand modèle -Affiné au marc de Bourgogne"
        ],
        "es": [
          "Venta directa de quesos de granja elaborados con leche cruda de vaca. Nuestros productos: -Leche -Plato -Fresco -Suave -Secado -Pimienta -Herbabuena -Nueces -Pequeña fuente -Grande fuente -Marc de Bourgogne refinado"
        ],
        "nl": [
          "Directe verkoop van boerenkazen gemaakt van rauwe koemelk. Onze producten: -Melk -Gerechten -Vers -Zacht -Droog -Peper -Kruiden -Walnoten -Kleine chaource -Grote chaource -Marc de Bourgogne geraffineerd"
        ]
      },
