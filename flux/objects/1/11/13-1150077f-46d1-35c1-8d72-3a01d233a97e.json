  "rdfs:comment": {
    "de": [
      "Seilbahn Le Rosay (vom Village aus), die für Fußgänger, Mountainbiker, Gleitschirmflieger und Kinderwagen zugänglich ist. Hochdruckreiniger am Fuße des Rosay verfügbar, um Mountainbikes zu reinigen."
    ],
    "en": [
      "The Rosay gondola (from the Village) accessible to pedestrians, mountain bikers, paragliders and pushchairs. High pressure cleaner available at the foot of the Rosay for cleaning mountain bikes."
    ],
    "it": [
      "Funivia Le Rosay (dal Villaggio) accessibile a pedoni, mountain bike, parapendio e passeggini. Ai piedi del Rosay è disponibile un'idropulitrice per la pulizia delle mountain bike."
    ],
    "fr": [
      "Télécabine Le Rosay (depuis le Village) accessible aux piétons, VTT, parapentistes et poussettes. Nettoyeur haute pression disponible au pied du Rosay pour le nettoyage des VTT."
    ],
    "es": [
      "Teleférico de Le Rosay (desde el pueblo) accesible a peatones, bicicletas de montaña, parapentes y cochecitos de niños. Limpiadora de alta presión disponible al pie del Rosay para limpiar las bicicletas de montaña."
    ],
    "nl": [
      "Le Rosay kabelbaan (vanuit het Dorp) toegankelijk voor voetgangers, mountainbikes, paragliders en kinderwagens. Hogedrukreiniger beschikbaar onderaan de Rosay voor het reinigen van mountainbikes."
    ]
  },
