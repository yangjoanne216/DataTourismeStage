  "rdfs:comment": {
    "de": [
      "\"Le Ciel est à vous\" bietet Ihnen touristische Ballonfahrten über unsere Berge an, bei denen Sie wunderschöne Landschaften bewundern können."
    ],
    "en": [
      "\"Le Ciel est à vous\" offers touristic hot-air balloon flights to fly over our mountains and admire wonderful landscapes."
    ],
    "it": [
      "\"Le Ciel est à vous\" vi offre voli turistici in mongolfiera per sorvolare le nostre montagne e ammirare gli splendidi paesaggi."
    ],
    "fr": [
      "\"Le Ciel est à vous\" vous propose des vols touristiques en montgolfière pour survoler nos montagnes et admirer de magnifiques paysages."
    ],
    "es": [
      "\"Le Ciel est à vous\" le ofrece vuelos turísticos en globo aerostático para sobrevolar nuestras montañas y admirar los hermosos paisajes."
    ],
    "nl": [
      "\"Le Ciel est à vous\" biedt u toeristische vluchten in een luchtballon om over onze bergen te vliegen en de prachtige landschappen te bewonderen."
    ]
  },
