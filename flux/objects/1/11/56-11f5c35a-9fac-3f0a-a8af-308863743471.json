  "rdfs:comment": {
    "de": [
      "Hühner, Perlhühner, Enten, Kaninchen, Eier. Verkaufsstelle auf dem Bauernhof und auf den Märkten von Amélie-les-Bains, Céret, Prats-de-Mollo, St Laurent-de-Cerdans und Port-Vendres."
    ],
    "en": [
      "Chickens, guinea fowl, ducks, rabbits, eggs. Sales point at the farm and on the markets of Amélie-les-Bains, Céret, Prats-de-Mollo, St Laurent-de-Cerdans and Port-Vendres."
    ],
    "it": [
      "Polli, faraone, anatre, conigli, uova. Punto vendita in azienda e nei mercati di Amélie-les-Bains, Céret, Prats-de-Mollo, St Laurent-de-Cerdans e Port-Vendres."
    ],
    "fr": [
      "Poulets, pintades, canards, lapins, oeufs. Point de vente à la ferme et sur les marchés d'Amélie-les-Bains, Céret, Prats-de-Mollo, St Laurent-de-Cerdans et Port-Vendres."
    ],
    "es": [
      "Pollos, pintadas, patos, conejos, huevos. Punto de venta en la granja y en los mercados de Amélie-les-Bains, Céret, Prats-de-Mollo, St Laurent-de-Cerdans y Port-Vendres."
    ],
    "nl": [
      "Kippen, parelhoenders, eenden, konijnen, eieren. Verkooppunt op de boerderij en op de markten van Amélie-les-Bains, Céret, Prats-de-Mollo, St Laurent-de-Cerdans en Port-Vendres."
    ]
  },
