      "dc:description": {
        "de": [
          "Le Batelier, 45 m² große Unterkunft im Erdgeschoss mit Wohnzimmer, Salon, Küchenzeile, TV, 1 Schlafzimmer mit Bett 160x200, 1 Badezimmer mit italienischer Dusche, WC, Wifi. Zugänglich für Personen mit eingeschränkter Mobilität."
        ],
        "en": [
          "Le Batelier, 45 m² accommodation on the ground floor with living room, kitchenette, TV, 1 bedroom with bed 160x200, 1 shower room with Italian shower, WC, Wifi. Accessible to PRM."
        ],
        "it": [
          "Le Batelier, alloggio di 45 m² al piano terra con soggiorno, angolo cottura, TV, 1 camera con letto 160x200, 1 bagno con doccia italiana, WC, Wifi. Accessibile alle PRM."
        ],
        "fr": [
          "Le Batelier, logement de 45 m² au rez-de-chaussée avec séjour, salon, kitchenette, TV, 1 chambre avec lit 160x200, 1 salle d'eau avec douche italienne, WC, Wifi. Accessible aux PMR."
        ],
        "es": [
          "Le Batelier, alojamiento de 45 m² en la planta baja con sala de estar, cocina americana, TV, 1 dormitorio con cama de 160x200, 1 cuarto de baño con ducha italiana, WC, Wifi. Accesible para PMR."
        ],
        "nl": [
          "Le Batelier, 45 m² accommodatie op de begane grond met woonkamer, kitchenette, TV, 1 slaapkamer met bed 160x200, 1 badkamer met Italiaanse douche, WC, Wifi. Toegankelijk voor PRM."
        ]
      },
