  "rdfs:comment": {
    "de": [
      "Verbringe einen coolen Nachmittag mit deinen Teenagern und surfe auf der Welle des Deiches, betreut von einem ausgebildeten Lehrer."
    ],
    "en": [
      "Teenage kayaking. For children older than 11 years old, even beginners, come spend and nice afternoon surfing on the wave of the dyke supervised by qualified monitors (or river descent depending of the water level). Session of about 2 hours."
    ],
    "it": [
      "Venite a trascorrere un bel pomeriggio con i vostri ragazzi facendo surf sull'onda della parete del mare con un istruttore qualificato."
    ],
    "fr": [
      "Venez passer un après-midi sympa entre ados en surfant sur la vague de la digue encadré par un moniteur diplômé."
    ],
    "es": [
      "Ven a pasar una agradable tarde con tus hijos adolescentes surfeando la ola en el dique con un instructor cualificado."
    ],
    "nl": [
      "Kom met uw tieners een leuke middag doorbrengen door met een gediplomeerde instructeur op de zeewering op de golf te surfen."
    ]
  },
