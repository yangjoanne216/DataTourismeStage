  "rdfs:comment": {
    "de": [
      "ÜBER DAS RESTAURANT ...\n\nHausgemachter Teig nach italienischer Methode mit hochwertigen italienischen Zutaten. Klassische und originelle Burger, die jede Woche anders sind."
    ],
    "en": [
      "ABOUT THE RESTAURANT...\n\nHomemade dough made the Italian way with quality Italian ingredients. Classic and original burgers different every week."
    ],
    "it": [
      "SUL RISTORANTE...\n\nImpasto fatto in casa alla maniera italiana con ingredienti italiani di qualità. Hamburger classici e originali diversi ogni settimana."
    ],
    "fr": [
      "A PROPOS DU RESTAURANT...\n\nUne pâte faite maison à la méthode italienne avec des ingédients italiens de qualité. Burgers classiques et originaux différents chaque semaine."
    ],
    "es": [
      "SOBRE EL RESTAURANTE...\n\nMasa casera hecha a la manera italiana con ingredientes italianos de calidad. Hamburguesas clásicas y originales diferentes cada semana."
    ],
    "nl": [
      "OVER HET RESTAURANT...\n\nZelfgemaakt deeg op Italiaanse wijze met Italiaanse kwaliteitsingrediënten. Klassieke en originele hamburgers, elke week anders."
    ]
  },
