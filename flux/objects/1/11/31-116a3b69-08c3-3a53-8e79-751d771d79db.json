      "dc:description": {
        "de": [
          "Menüs von 15? bis 35?, große Auswahl an Speisen, Zimmer ab 55? Spätdienst in der Saison. Spezialitäten: traditioneller Aligot, gegrilltes Fleisch, Pizza."
        ],
        "en": [
          "Fixed meals from 15€ to 35€, large choice from the menu, rooms from 55€. Service until late in the summer season. Specialities: traditional aligot, grilled meat, pizza."
        ],
        "it": [
          "Menu da 15? a 35?, ampia scelta di menu, sala da 55? Servizio in ritardo in stagione. Specialità: aligot tradizionale, carne alla griglia, pizza."
        ],
        "fr": [
          "Menus de 15€ à 35€, grand choix de carte, chambre à partir de 55 €. Service tardif en saison. Spécialités : aligot traditionnel, viande grillée, pizza."
        ],
        "nl": [
          "Menu's van 15? tot 35?, ruime keuze aan menu's, zaal vanaf 55? Late dienst in het seizoen. Specialiteiten: traditionele aligot, gegrild vlees, pizza."
        ],
        "es": [
          "Menús a 15€ y hasta 35€, carta muy variada, habitación a partir de 55€. Se extiende el horario de cocina en temporada alta. Especialidades: aligot tradicionàl, carne a la brasa, pizza."
        ]
      },
