      "dc:description": {
        "de": [
          "Von Oktober bis März, außerhalb der Schulferien, ist das Restaurant nur am Wochenende geöffnet (Freitag, Samstag und Sonntag). Im Februar geschlossen."
        ],
        "en": [
          "From October to March, outside school holidays, restaurant open at weekends only (Friday, Saturday and Sunday). Closed in February."
        ],
        "it": [
          "Da ottobre a marzo, tranne durante le vacanze scolastiche, il ristorante è aperto solo nei fine settimana (venerdì, sabato e domenica). Chiuso a febbraio."
        ],
        "fr": [
          "D'octobre à mars, hors vacances scolaires, restaurant ouvert le week-end uniquement (vendredi, samedi et dimanche). Fermé en Février."
        ],
        "nl": [
          "Van oktober tot maart, behalve tijdens de schoolvakanties, is het restaurant alleen in het weekend geopend (vrijdag, zaterdag en zondag). Gesloten in februari."
        ],
        "es": [
          "De octubre a marzo, excepto durante las vacaciones escolares, el restaurante sólo abre los fines de semana (viernes, sábado y domingo). Cerrado en febrero."
        ]
      },
