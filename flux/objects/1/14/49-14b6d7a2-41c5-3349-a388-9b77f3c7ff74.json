  "rdfs:comment": {
    "de": [
      "ÜBER DAS RESTAURANT ...\n\nSeit 1998 im La Gourmandine, lassen Fabien und Myriam Sie an ihrer Leidenschaft für Buchweizen teilhaben."
    ],
    "en": [
      "ABOUT THE RESTAURANT...\n\nSince 1998 at La Gourmandine, Fabien and Myriam have been sharing their passion for buckwheat with you."
    ],
    "it": [
      "SUL RISTORANTE...\n\nDal 1998, a La Gourmandine, Fabien e Myriam condividono con voi la loro passione per il grano saraceno."
    ],
    "fr": [
      "A PROPOS DU RESTAURANT...\n\nDepuis 1998 à la Gourmandine, Fabien et Myriam vous font partager leur passion du sarrasin."
    ],
    "es": [
      "SOBRE EL RESTAURANTE...\n\nDesde 1998, en La Gourmandine, Fabien y Myriam comparten con usted su pasión por el trigo sarraceno."
    ],
    "nl": [
      "OVER HET RESTAURANT...\n\nSinds 1998 delen Fabien en Myriam in La Gourmandine hun passie voor boekweit met u."
    ]
  },
