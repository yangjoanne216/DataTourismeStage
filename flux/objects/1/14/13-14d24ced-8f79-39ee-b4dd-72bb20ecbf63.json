  "rdfs:comment": {
    "de": [
      "In einem modernen Dekor genießen Sie eine traditionelle Küche, zubereitet mit Leidenschaft und Sorgfalt."
    ],
    "en": [
      "In front of the Belvedere gardens with a view of Lake Geneva, treat yourself to a break...you will taste the flavors of a tasty cuisine, worked with passion."
    ],
    "it": [
      "Di fronte ai giardini del Belvedere con vista sul lago di Ginevra, concedetevi una pausa... assaggerete i sapori di una cucina gustosa, lavorata con passione."
    ],
    "fr": [
      "En face des jardins du Belvédère avec une vue plongeante sur le lac Léman, offrez-vous une halte...vous goûterez aux saveurs d'une cuisine savoureuse, travaillée avec passion."
    ],
    "nl": [
      "Tegenover de Belvedere tuinen met uitzicht op het meer van Genève, trakteert u zichzelf op een pauze... u proeft de smaken van een smakelijke keuken, bewerkt met passie."
    ],
    "es": [
      "Frente a los jardines del Belvedere, con vistas al lago de Ginebra, regálese un descanso... degustará los sabores de una cocina sabrosa, trabajada con pasión."
    ]
  },
