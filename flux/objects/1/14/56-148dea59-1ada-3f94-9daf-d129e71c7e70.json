  "rdfs:comment": {
    "de": [
      "Bewachte Berghütte in der Nähe von La Llagonne, einem kleinen Dorf am Eingang zum Capcir, im Herzen des nordischen Gebiets von La Llose. Kapazität für 12 Personen im Schlafsaal."
    ],
    "en": [
      "Guarded refuge located near La Llagonne, a small village at the entrance of the Capcir, in the heart of the Nordic domain of La Llose. Capacity of 12 people in dormitory."
    ],
    "it": [
      "Rifugio custodito situato vicino a La Llagonne, un piccolo villaggio all'ingresso del Capcir, nel cuore dell'area nordica di La Llose. Capacità di 12 persone in un dormitorio."
    ],
    "fr": [
      "Refuge gardé situé près de La Llagonne, petit village à l'entrée du Capcir, au coeur du domaine nordique de La Llose. Capacité d'accueil de 12 personnes en dortoir."
    ],
    "es": [
      "Refugio vigilado situado cerca de La Llagonne, un pequeño pueblo a la entrada del Capcir, en el corazón de la zona nórdica de La Llose. Capacidad para 12 personas en un dormitorio."
    ],
    "nl": [
      "Bewaakt toevluchtsoord gelegen bij La Llagonne, een klein dorp aan de ingang van de Capcir, in het hart van het Noordse gebied van La Llose. Capaciteit van 12 personen in een slaapzaal."
    ]
  },
