  "rdfs:comment": {
    "de": [
      "Dominique empfängt Sie herzlich in ihren charmanten Gästezimmern für einen erholsamen Aufenthalt inmitten der Landschaft von Toulouse."
    ],
    "en": [
      "Dominique welcomes you warmly in her charming guest rooms for a relaxing stay in the heart of the Toulouse countryside."
    ],
    "it": [
      "Dominique vi dà un caloroso benvenuto nelle sue incantevoli camere per un soggiorno rilassante nel cuore della campagna di Tolosa."
    ],
    "fr": [
      "Dominique vous accueille chaleureusement dans ses chambres d'hôtes pleines de charmes pour un séjour reposant au cœur de la campagne toulousaine."
    ],
    "es": [
      "Dominique le da una calurosa bienvenida a sus encantadoras habitaciones para una relajante estancia en el corazón de la campiña de Toulouse."
    ],
    "nl": [
      "Dominique heet u van harte welkom in haar charmante gastenkamers voor een ontspannen verblijf in het hart van het platteland van Toulouse."
    ]
  },
