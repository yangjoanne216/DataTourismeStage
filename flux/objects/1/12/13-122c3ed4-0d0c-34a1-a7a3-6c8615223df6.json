  "rdfs:comment": {
    "de": [
      "Luxuswohnung mit Internet. \nSüdlage mit Meerblick auf den Strand und die Inseln Port-Cros und Porquerolles. \nSandstrand mit Grünflächen 50 Meter entfernt."
    ],
    "en": [
      "Luxury flat with internet access \nSouth facing with sea view on the beach and the islands of Port-Cros and Porquerolles \nSandy beach with green areas at 50 meters."
    ],
    "it": [
      "Appartamento di lusso con accesso a Internet \nEsposizione a sud con vista mare sulla spiaggia e sulle isole di Port-Cros e Porquerolles \nSpiaggia di sabbia con aree verdi a 50 metri."
    ],
    "fr": [
      "Appartement de standing avec Internet.\r\nPlein sud avec vue mer sur la plage et les îles de Port-Cros et de Porquerolles.\r\nPlage de sable avec espaces verts à 50 mètres."
    ],
    "es": [
      "Piso de lujo con Internet.\r\nOrientación sur con vistas a la playa y a las islas de Port-Cros y Porquerolles.\r\nPlaya de arena con zonas verdes a 50 metros."
    ],
    "nl": [
      "Luxe appartement met internet.\r\nOp het zuiden gelegen met uitzicht op het strand en de eilanden Port-Cros en Porquerolles.\r\nZandstrand met groene zones op 50 meter."
    ]
  },
