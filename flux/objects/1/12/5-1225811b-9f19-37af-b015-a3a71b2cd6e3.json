      "dc:description": {
        "de": [
          "Eisbecher \"Maitre Glacier Carte d'Or\", Eis zum Mitnehmen in Bechern & Tüten, Desserts (Muffins, Donuts, Crêpes), Getränke.\n\nSommerterrasse."
        ],
        "en": [
          "Maitre Glacier Carte d'Or\" ice cream cups, take-away ice cream in pots & cones, desserts (muffins, donuts, pancakes), drinks.\n\nSummer terrace."
        ],
        "it": [
          "Coppe gelato \"Maitre Glacier Carte d'Or\", gelato da asporto in vaschette e coni, dessert (muffin, ciambelle, frittelle), bevande.\n\nTerrazza estiva."
        ],
        "fr": [
          "Coupes glacées \"Maitre Glacier Carte d'Or\", glaces à emporter en pots & cornets, desserts (muffins, donuts, crêpes), boissons.\n\nTerrasse d'été."
        ],
        "nl": [
          "Maitre Glacier Carte d'Or\" ijsbekers, meeneemijs in potten en kegels, desserts (muffins, donuts, pannenkoeken), dranken.\n\nZomerterras."
        ],
        "es": [
          "Copas de helado \"Maitre Glacier Carte d'Or\", helados para llevar en tarros y conos, postres (magdalenas, donuts, tortitas), bebidas.\n\nTerraza de verano."
        ]
      },
