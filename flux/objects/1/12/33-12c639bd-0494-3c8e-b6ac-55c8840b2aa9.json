  "rdfs:comment": {
    "de": [
      "Im Wassersportzentrum von Boulogne faulenzt man am Rand des beheizten Schwimmbads."
    ],
    "en": [
      "At the nautical center of Boulogne, you can laze by the heated swimming pool."
    ],
    "it": [
      "Presso il centro di sport acquatici di Boulogne, potrete oziare nella piscina riscaldata."
    ],
    "fr": [
      "Au centre nautique de Boulogne, on lézarde au bord de la piscine chauffée."
    ],
    "nl": [
      "In het watersportcentrum van Boulogne kunt u heerlijk luieren in het verwarmde zwembad."
    ],
    "es": [
      "En el centro de deportes acuáticos de Boulogne, podrá holgazanear en la piscina climatizada."
    ]
  },
