  "rdfs:comment": {
    "de": [
      "Bar, Brasserie, PMU, Rapido, Rubbelspiele!!! Das Restaurant ist nur mittags geöffnet. Gruppenessen am Abend auf Vorbestellung möglich. Bar PMU ab 6 Uhr morgens bis Mitternacht geöffnet."
    ],
    "en": [
      "Bar, brasserie, PMU (betting), lottery and scratch cards!!  The restaurant is only open for lunch. However, dinners are available to book. PMU bar open from 6am to midnight."
    ],
    "it": [
      "Bar, brasserie, giochi PMU, Rapido, gratta e vinci!! Il ristorante è aperto solo a pranzo. Possibilità di pasti di gruppo la sera su prenotazione. Bar PMU aperto dalle 6 del mattino fino a mezzanotte."
    ],
    "fr": [
      "Bar, brasserie, PMU, Rapido, jeux à gratter !! Le restaurant est ouvert le midi uniquement. Possibilité de repas de groupe le soir sur réservation. Bar PMU ouvert dès 6h du matin jusqu'a minuit."
    ],
    "es": [
      "Bar, brasserie, PMU, Rapido, tarjetas de rascar El restaurante está abierto sólo a la hora del almuerzo. Posibilidad de comidas en grupo por la noche previa reserva. El bar de la PMU está abierto desde las 6 de la mañana hasta la medianoche."
    ],
    "nl": [
      "Bar, brasserie, PMU, Rapido, krasloten! Het restaurant is alleen 's middags geopend. Mogelijkheid tot groepsmaaltijden 's avonds op reservering. PMU bar open van 6 tot middernacht."
    ]
  },
