  "rdfs:comment": {
    "de": [
      "Jahrhundert aus mattem Holz, Teil eines anderen Altaraufsatzes, der mit sehr schönen Flachreliefs geschnitzt ist."
    ],
    "en": [
      "This altarpiece has a large 17th century panel in matt wood, part of another altarpiece, carved with beautifully crafted bas reliefs."
    ],
    "it": [
      "Questa pala d'altare presenta un grande pannello del XVII secolo in legno opaco, parte di un'altra pala d'altare, scolpito con bassorilievi di ottima fattura."
    ],
    "fr": [
      "Ce retable présente un large panneau du XVII°s, en bois mat, élément d'un autre retable, sculpté de bas reliefs de très belle facture."
    ],
    "es": [
      "Este retablo presenta un gran panel del siglo XVII en madera mate, parte de otro retablo, tallado con bajorrelieves de bella factura."
    ],
    "nl": [
      "Dit altaarstuk heeft een groot 17e-eeuws paneel in mat hout, dat deel uitmaakt van een ander altaarstuk, met prachtig bewerkte bas-reliëfs."
    ]
  },
