  "rdfs:comment": {
    "de": [
      "Provenzalische Spezialitäten: Kabeljau-Aioli, Auberginen-Tian mit Ziegenkäse, Füße und Pakete, Lammmaus mit Thymianblüte, ...\r\nSüße und herzhafte Crêpes, Waffeln, Eis ... Täglich geöffnet, durchgehender Service bis 17 Uhr (abends kein Service)"
    ],
    "en": [
      "Provençal specialities: cod aïoli, aubergine tian with goat cheese, feet and packets, lamb mouse with thyme flower, ...\r\nSweet and savoury pancakes, waffles, ice creams ... Open every day, continuous service until 5pm (no service in the evening)"
    ],
    "it": [
      "Specialità provenzali: aïoli di baccalà, tian di melanzane con formaggio di capra, piedini e paccheri, topo d'agnello con fiori di timo, ...\r\nCrepes dolci e salate, waffle, gelati... Aperto tutti i giorni, servizio continuo fino alle 17.00 (non servizio serale)"
    ],
    "fr": [
      "Spécialités provençales : aïoli de morue, tian d'aubergine au fromage de chèvre, pieds et paquets, souris d'agneau fleur de thym, ...\r\nCrêpes sucrées et salées, gaufres, glaces ... Ouvert tous les jours, service continu jusqu'à 17h (pas de service le soir)"
    ],
    "nl": [
      "Provençaalse specialiteiten: kabeljauw aïoli, tian van aubergine met geitenkaas, pootjes en pakketjes, lamsmuis met tijmbloemen, ...\r\nZoete en hartige pannenkoeken, wafels, ijsjes... Elke dag geopend, doorlopende dienst tot 17.00 uur (geen avonddienst)"
    ],
    "es": [
      "Especialidades provenzales: aïoli de bacalao, tian de berenjena con queso de cabra, pies y paquetes, ratón de cordero con flores de tomillo, ...\r\nCrepes dulces y salados, gofres, helados... Abierto todos los días, servicio continuo hasta las 17 horas (no hay servicio nocturno)"
    ]
  },
