  "rdfs:comment": {
    "de": [
      "Crêperie, Teesalon, Café. Buchweizenpfannkuchen, Salate, Crêpes, Desserts, Eis. Ein Gericht des Tages\nGeöffnet von 12:00 bis 15:30 Uhr und von 19:00 bis 21:30 Uhr (und täglich während der Pariser Schulferien)."
    ],
    "en": [
      "Creperie, tea room, coffee. Buckwheat pancakes, salads, crepes, desserts, ice cream. A dish of the day\nOpen from 12:00 to 15:30 and from 19:00 to 21:30 (and every day during the school vacations in Paris)."
    ],
    "it": [
      "Creperie, sala da tè, caffè. Frittelle di grano saraceno, insalate, crepes, dessert, gelati. Un piatto del giorno\nAperto dalle 12:00 alle 15:30 e dalle 19:00 alle 21:30 (e tutti i giorni durante le vacanze scolastiche di Parigi)."
    ],
    "fr": [
      "Crêperie, salon de thé, café. Galettes de blé noir, salades, crêpes, desserts, glaces. Un plat du jour. \nOuvert de 12h à 15h30 et de 19h à 21h30 (et tous les jours pendant les vacances scolaires de Paris)."
    ],
    "nl": [
      "Creperie, theesalon, café. Boekweitpannenkoeken, salades, crêpes, desserts, ijs. Een dagschotel\nOpen van 12.00 tot 15.30 uur en van 19.00 tot 21.30 uur (en elke dag tijdens de Parijse schoolvakanties)."
    ],
    "es": [
      "Crepería, salón de té, cafetería. Tortitas de trigo sarraceno, ensaladas, crepes, postres, helados. Un plato del día\nAbierto de 12:00 a 15:30 y de 19:00 a 21:30 (y todos los días durante las vacaciones escolares de París)."
    ]
  },
