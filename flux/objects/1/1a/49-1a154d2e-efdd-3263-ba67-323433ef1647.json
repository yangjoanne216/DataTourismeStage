  "rdfs:comment": {
    "de": [
      "Entdecken Sie den Beruf des Eisenschmieds in Form eines spielerischen und interaktiven Workshops im Musée des métiers de Saint-Laurent-de-la-Plaine in Mauges-sur-Loire."
    ],
    "en": [
      "Discover the craft of ironmongery in a fun, interactive workshop at the Musée des métiers de Saint-Laurent-de-la-Plaine in Mauges-sur-Loire."
    ],
    "it": [
      "Scoprite l'artigianato della ferramenta in un laboratorio divertente e interattivo presso il Musée des métiers de Saint-Laurent-de-la-Plaine a Mauges-sur-Loire."
    ],
    "fr": [
      "Découvrez le métier de ferronnier sous forme d'atelier ludique et interactif au Musée des métiers de Saint-Laurent-de-la-Plaine à Mauges-sur-Loire."
    ],
    "nl": [
      "Ontdek het ambacht van ijzerwaren in een leuke, interactieve workshop in het Musée des métiers de Saint-Laurent-de-la-Plaine in Mauges-sur-Loire."
    ],
    "es": [
      "Descubra el oficio de ferretero en un taller lúdico e interactivo en el Museo de los Oficios de Saint-Laurent-de-la-Plaine, en Mauges-sur-Loire."
    ]
  },
