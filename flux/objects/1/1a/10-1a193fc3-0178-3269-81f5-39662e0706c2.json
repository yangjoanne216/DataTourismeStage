  "rdfs:comment": {
    "de": [
      "Organisiert von der Association pour la Sauvegarde du Patrimoine de Momerstroff (Verein zur Erhaltung des Kulturerbes von Momerstroff). Empfang der Aussteller um 13 Uhr, ohne Reservierung, 2? pro laufendem Meter. Getränke und Snacks im Schulhof."
    ],
    "en": [
      "Organized by the Association pour la Sauvegarde du Patrimoine de Momerstroff. Exhibitors welcome at 1pm, no reservation required, 2? per linear metre. Refreshments and snacks in the schoolyard."
    ],
    "it": [
      "Organizzato dall'Association pour la Sauvegarde du Patrimoine de Momerstroff. Accoglienza degli espositori alle 13.00, senza prenotazione, 2? al metro lineare. Rinfresco e merenda nel cortile della scuola."
    ],
    "fr": [
      "Organisé par l'Association pour la Sauvegarde du Patrimoine de Momerstroff. Accueil des exposants à 13h, sans réservation, 2€ le mètre linéaire. Buvette et casse-croûtes dans la cour de l'école."
    ],
    "es": [
      "Organizado por la Association pour la Sauvegarde du Patrimoine de Momerstroff. Recepción de expositores a las 13.00 h, sin reserva previa, 2? por metro lineal. Refrescos y tentempiés en el patio de la escuela."
    ],
    "nl": [
      "Georganiseerd door de Association pour la Sauvegarde du Patrimoine de Momerstroff. Standhouders welkom om 13.00 uur, geen reservering nodig, 2? per strekkende meter. Versnaperingen en snacks op de binnenplaats van de school."
    ]
  },
