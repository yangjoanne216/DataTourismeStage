  "rdfs:comment": {
    "de": [
      "Kochen auf der Plancha. Meeresfrüchte und Muscheln, Gegrilltes, mediterrane Küche, Fisch, Salate. Musikalische Unterhaltung 4 Mal pro Woche auf der Terrasse. Animationen nur von Juni bis September."
    ],
    "en": [
      "Cooking à la plancha. Seafood and shellfish, grills, Mediterranean cuisine, fish, salads. Musical entertainment 4 times a week on the terrace. Animations only from June to September."
    ],
    "it": [
      "Cucinare à la plancha. Frutti di mare e crostacei, grigliate, cucina mediterranea, pesce, insalate. Intrattenimento musicale 4 volte a settimana sulla terrazza. Intrattenimento solo da giugno a settembre."
    ],
    "fr": [
      "Cuisine à la plancha. Fruits de mer et coquillages, grillades, cuisine méditerrannéenne, poissons, salades. Animations musicales 4 fois par semaine en terrasse. Animations uniquement de juin à septembre."
    ],
    "nl": [
      "Koken à la plancha. Zeevruchten en schaaldieren, grillen, mediterrane keuken, vis, salades. 4 keer per week muzikale animatie op het terras. Entertainment alleen van juni tot september."
    ],
    "es": [
      "Cocinar a la plancha. Mariscos y crustáceos, parrilladas, cocina mediterránea, pescados, ensaladas. Animación musical 4 veces por semana en la terraza. Entretenimiento sólo de junio a septiembre."
    ]
  },
