  "rdfs:comment": {
    "de": [
      "Pizza Saint Jerome ist eine Pizzeria, die Sie seit 1975 über dem Holzfeuer bedient, zum Mitnehmen oder als Lieferservice."
    ],
    "en": [
      "Pizza Saint Jérôme is a pizzeria that has been serving you since 1975 over a wood fire, to take away or to deliver."
    ],
    "it": [
      "La Pizza Saint Jérôme è una pizzeria che vi serve dal 1975 con forno a legna, da asporto o a domicilio."
    ],
    "fr": [
      "Pizza Saint Jérôme est une pizzeria qui vous sert depuis 1975 au feu de bois, à emporter ou en livraison."
    ],
    "nl": [
      "Pizza Saint Jérôme is een pizzeria die u sinds 1975 bedient in een houtoven, voor afhaal of bezorging."
    ],
    "es": [
      "Pizza Saint Jérôme es una pizzería que lleva sirviéndole desde 1975 en un horno de leña, para llevar o a domicilio."
    ]
