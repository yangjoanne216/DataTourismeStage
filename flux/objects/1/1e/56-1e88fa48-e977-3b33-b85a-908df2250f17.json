  "rdfs:comment": {
    "de": [
      "Gemütliches Restaurant in Strandnähe im historischen Collioure, in dem bevorzugt Fisch aus den kleinen Handwerksbetrieben von Port-Vendres verarbeitet wird."
    ],
    "en": [
      "Friendly restaurant, close to the beach, in the historical Collioure, where we privilege the fish of the small trades of Port-Vendres."
    ],
    "it": [
      "Ristorante accogliente, vicino alla spiaggia, nella zona storica di Collioure, dove l'accento è posto sul pesce proveniente dai piccoli commerci di Port-Vendres."
    ],
    "fr": [
      "Restaurant convivial, proche de la plage, dans le Collioure historique où l'on privilégie les poissons des petits métiers de Port-Vendres."
    ],
    "nl": [
      "Vriendelijk restaurant, dicht bij het strand, in de historische wijk Collioure, waar de nadruk ligt op vis uit de kleine ambachten van Port-Vendres."
    ],
    "es": [
      "Restaurante acogedor, cerca de la playa, en la zona histórica de Collioure, donde se hace hincapié en el pescado de los pequeños oficios de Port-Vendres."
    ]
  },
