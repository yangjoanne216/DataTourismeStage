  "rdfs:comment": {
    "de": [
      "Restaurant\r\nPoke Shop\r\nVor Ort oder zum Mitnehmen\r\nUber eats\r\nVon Montag bis Samstag\r\n11.30/14.00 Uhr und 18.30/21.30 Uhr\r\n\r\nFast die gesamte Speisekarte wird mit frischen Zutaten zubereitet."
    ],
    "en": [
      "Restaurant\r\nPoke Shop\r\nOn the spot or to go\r\nUber eats\r\nFrom Monday to Saturday\r\n11h30/14h and 18h30/21h30\r\n\r\nAlmost the entire menu is made with fresh products."
    ],
    "it": [
      "Il ristorante\r\nPoke Shop\r\nSul posto o da portare via\r\nMangiare con Uber\r\nDa lunedì a sabato\r\n11:30-14:00 e 18:30-21:30\r\n\r\nQuasi tutto il menu è realizzato con prodotti freschi."
    ],
    "fr": [
      "Restaurant\r\nPoke Shop\r\nSur place ou à emporter\r\nUber eats\r\nDu Lundi au Samedi\r\n11h30/14h et 18h30/21h30\r\n\r\nLa quasi totalité de la carte est élaborée avec des produits frais."
    ],
    "es": [
      "Restaurante\r\nTienda Poke\r\nEn el acto o para llevar\r\nUber come\r\nDe lunes a sábado\r\n11:30 - 14:00 y 18:30 - 21:30\r\n\r\nCasi todo el menú está elaborado con productos frescos."
    ],
    "nl": [
      "Restaurant\r\nPoke winkel\r\nTer plaatse of om mee te nemen\r\nUber eet\r\nMaandag tot zaterdag\r\n11u30 - 14u en 18u30 - 21u30\r\n\r\nBijna het hele menu is gemaakt met verse producten."
    ]
  },
