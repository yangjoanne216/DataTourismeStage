  "rdfs:comment": {
    "de": [
      "Wie Sherlock Holmes begeben sich die Kinder auf die Suche nach Spuren und Hinweisen, die von Tieren hinterlassen wurden. Animation für Familien."
    ],
    "en": [
      "Like Sherlock Holmes, the children will go in search of traces and clues of presence left by animals. Animation for families."
    ],
    "it": [
      "Come Sherlock Holmes, i bambini andranno alla ricerca di tracce e indizi lasciati dagli animali. Animazione per famiglie."
    ],
    "fr": [
      "Tels Sherlock Holmes, les enfants partiront à la recherche des traces et indices de présences laissés par des animaux. Animation pour les familles."
    ],
    "nl": [
      "Net als Sherlock Holmes gaan de kinderen op zoek naar sporen en aanwijzingen die dieren achterlaten. Animatie voor gezinnen."
    ],
    "es": [
      "Como Sherlock Holmes, los niños irán en busca de rastros y pistas dejados por los animales. Animación para familias."
    ]
  },
