  "rdfs:comment": {
    "de": [
      "Das Restaurant bietet alle Arten von Fleisch an: Stück vom Metzger mit Tagessoße, Kalbsschnitzel, Hähnchenschnitzel, Burger, Carpaccio, Entenbrust..."
    ],
    "en": [
      "The restaurant offers all kinds of meat: butcher's cuts with the sauce of the day, veal escalope, chicken escalope, burgers, carpaccio, duck breast..."
    ],
    "it": [
      "Il ristorante offre tutti i tipi di carne: tagli di macelleria con salsa del giorno, scaloppine di vitello, scaloppine di pollo, hamburger, carpaccio, petto d'anatra..."
    ],
    "fr": [
      "Le restaurant propose toutes sortes de viandes : pièce du boucher à la sauce du jour, escalope de veau, escalope de poulet, burgers, carpaccio, magret..."
    ],
    "nl": [
      "Het restaurant biedt alle soorten vlees: slagersstukken met de saus van de dag, escalope van kalfsvlees, escalope van kip, hamburgers, carpaccio, eendenborst..."
    ],
    "es": [
      "El restaurante ofrece todo tipo de carnes: cortes de carnicería con la salsa del día, escalope de ternera, escalope de pollo, hamburguesas, carpaccio, pechuga de pato..."
    ]
  },
