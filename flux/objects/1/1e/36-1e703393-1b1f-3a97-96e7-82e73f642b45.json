      "dc:description": {
        "de": [
          "Forellenfilet mit Jurançon,\nEntenconfit mit Früchten,\nLammkoteletts in Folie,\nGarbure (Spezialität des Küchenchefs)",
          "Jean-François bereitet für Sie regionale Gerichte zu."
        ],
        "en": [
          "Jean-François prepares regional dishes for you.",
          "Trout fillet with Jurançon,\nConfit of duck with fruit,\nLamb chops in foil,\nGarbure (chef's speciality)"
        ],
        "it": [
          "Filetto di trota con Jurançon,\nConfit d'anatra con frutta,\nCostolette di agnello al cartoccio,\nGarbure (specialità dello chef)",
          "Jean-François prepara per voi piatti regionali."
        ],
        "fr": [
          "Filet de Truite au Jurançon,\nConfit de canard aux fruits,\nCôtes d' agneau en papillote,\nGarbure (spécialité du chef)",
          "Jean-François vous prépare des plats régionaux."
        ],
        "nl": [
          "Forelfilet met Jurançon,\nGekonfijte eend met fruit,\nLamskoteletjes in folie,\nGarbure (specialiteit van de chef)",
          "Jean-François bereidt regionale gerechten voor u."
        ],
        "es": [
          "Filete de trucha con Jurançon,\nPato confitado con fruta,\nChuletas de cordero en papel de aluminio,\nGarbure (especialidad del chef)",
          "Jean-François le prepara platos regionales."
        ]
      },
