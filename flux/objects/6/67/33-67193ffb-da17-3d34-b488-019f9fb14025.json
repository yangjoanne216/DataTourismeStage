  "rdfs:comment": {
    "de": [
      "In den Hängen, abseits eines kleinen Dorfes, empfängt Sie ein typischer Commingeoise-Bauernhof."
    ],
    "en": [
      "In the hills, away from a small village, a typical Commingeoise farmhouse welcomes you."
    ],
    "it": [
      "In collina, lontano da un piccolo villaggio, vi accoglie una tipica fattoria della Comminge."
    ],
    "fr": [
      "Dans les coteaux, à l'écart d'un petit village, une ferme typique commingeoise vous accueille."
    ],
    "nl": [
      "In de heuvels, weg van een klein dorp, heet een typische Comminge boerderij u welkom."
    ],
    "es": [
      "En las colinas, lejos de un pequeño pueblo, una granja típica de Comminge le da la bienvenida."
    ]
