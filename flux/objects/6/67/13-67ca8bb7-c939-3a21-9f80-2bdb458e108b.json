  "rdfs:comment": {
    "de": [
      "1505 -1573. Kanzler von Frankreich.\r\nHumanist, Dichter, Jurist. Seine Familie stammte aus Allanche, aus dem Dorf L'Hôpital und besaß ein Haus an diesem Platz."
    ],
    "en": [
      "1505 -1573. Chancellor of France.\r\nHumanist, poet, jurist. His family was originally from Allanche, from the village of L'Hôpital and owned a house on this square."
    ],
    "it": [
      "1505 -1573. Cancelliere di Francia.\r\nUmanista, poeta, giurista. La sua famiglia era originaria di Allanche, del villaggio di l'Hôpital, e possedeva una casa situata in questa piazza."
    ],
    "fr": [
      "1505 -1573. Chancelier de France.\r\nHumaniste, poète, juriste. Sa famille était originaire d’Allanche, du village de l’Hôpital et propriétaire d’une maison située sur cette place."
    ],
    "nl": [
      "1505 -1573. Kanselier van Frankrijk.\r\nHumanist, dichter, jurist. Zijn familie kwam oorspronkelijk uit Allanche, uit het dorp l'Hôpital, en bezat een huis op dit plein."
    ],
    "es": [
      "1505 -1573. Canciller de Francia.\r\nHumanista, poeta, jurista. Su familia era originaria de Allanche, del pueblo de l'Hôpital, y poseía una casa situada en esta plaza."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Michel de l'Hospital"
    ]
  },
