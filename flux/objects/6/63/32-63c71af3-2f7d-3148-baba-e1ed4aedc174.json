  "rdfs:comment": {
    "de": [
      "Venez vous divertir autour des pièces de café-théâtre, théâtre burlesque, soirées musicales... Découvrez le domaine et dégustez des vins et tapas en famille ou entre amis."
    ],
    "en": [
      "Come and be entertained by café-theatre, burlesque theatre, musical evenings... Discover the estate and taste wines and tapas with your family or friends."
    ],
    "it": [
      "Venite a divertirvi con il teatro-caffè, il teatro burlesque, le serate musicali... Scoprite la tenuta e degustate vini e tapas con la famiglia o gli amici."
    ],
    "fr": [
      "Venez vous divertir autour des pièces de café-théâtre, théâtre burlesque, soirées musicales... Découvrez le domaine et dégustez des vins et tapas en famille ou entre amis."
    ],
    "es": [
      "Venga a entretenerse con el café-teatro, el teatro burlesco, las veladas musicales... Descubra la finca y deguste vinos y tapas con su familia o amigos."
    ],
    "nl": [
      "Kom en laat u vermaken door café-theater, burlesk theater, muzikale avonden... Ontdek het landgoed en proef wijnen en tapas met uw familie of vrienden."
    ]
  },
