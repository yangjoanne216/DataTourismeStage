  "rdfs:comment": {
    "de": [
      "Vergessen Sie nicht, wenn Sie im Fournil de la Barque vorbeikommen, Ihr Brot oder Gebäck zu kaufen."
    ],
    "en": [
      "Don't forget to buy your bread or pastry at the Fournil de la Barque."
    ],
    "it": [
      "Non dimenticate di acquistare il pane o i dolci al Fournil de la Barque."
    ],
    "fr": [
      "N'oubliez pas en passant votre pain ou votre pâtisserie au Fournil de la Barque."
    ],
    "nl": [
      "Vergeet niet uw brood of gebak te kopen bij de Fournil de la Barque."
    ],
    "es": [
      "No olvide comprar el pan o la bollería en el Fournil de la Barque."
    ]
  },
