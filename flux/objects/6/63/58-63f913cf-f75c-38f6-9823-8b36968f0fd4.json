  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/5465b233-b672-3639-acb5-2683d985f9c6",
      "dc:description": {
        "de": [
          "1 Raum - Terrasse - Privatparkplatz\r\nVon Frühling bis Herbst täglich einschließlich Feiertagen von 12:00 bis 15:30 Uhr und von 19:00 bis 23:00 Uhr geöffnet.\r\nküche:mediterran - Pizza\r\nmenü: min. 14,50 EUR - max. 24 EUR"
        ],
        "en": [
          "1 room - terrace - private parking\r\nOpen from spring to autumn every day, including public holidays, from 12h to 15h30 and from 19h to 23h.\r\ncuisine:mediterranean - pizzas\r\nmenu: mini 14,50 EUR - maxi 24 EUR"
        ],
        "it": [
          "1 camera - terrazza - parcheggio privato\r\nAperto dalla primavera all'autunno tutti i giorni, compresi i festivi, dalle 12 alle 15.30 e dalle 19 alle 23.\r\ncucina:Mediterraneo - pizze\r\nmenu: minimo 14,50 EUR - massimo 24 EUR"
        ],
        "fr": [
          "1 salle - terrasse - parking privé\r\nOuvert du printemps à l'automne tous les jours y compris les jours fériés, de 12h à 15h30 et de 19h à 23h.\r\ncuisine:méditerranéenne - pizzas\r\nmenu : mini 14,50 EUR - maxi 24 EUR"
        ],
        "nl": [
          "1 kamer - terras - privé parkeerplaats\r\nVan het voorjaar tot de herfst dagelijks geopend, ook op feestdagen, van 12 tot 15.30 uur en van 19 tot 23 uur.\r\nkeuken:Mediterrane - pizza's\r\nmenu: minimum 14,50 EUR - maximum 24 EUR"
        ],
        "es": [
          "1 habitación - terraza - aparcamiento privado\r\nAbierto de primavera a otoño todos los días, incluidos los festivos, de 12 a 15.30 y de 19 a 23 horas.\r\ncocina:mediterránea - pizzas\r\nmenú: mínimo 14,50 EUR - máximo 24 EUR"
        ]
      },
