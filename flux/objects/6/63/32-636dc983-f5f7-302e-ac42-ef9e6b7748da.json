  "rdfs:comment": {
    "de": [
      "Im Gourmetrestaurant von Vinipolis kommen die Produkte der Region und die Weine aus dem Weinkeller der Winzer von Florensac besonders gut zur Geltung."
    ],
    "en": [
      "The gastronomic restaurant at Vinipolis showcases local produce and wines from the Florensac wine growers' cellar."
    ],
    "it": [
      "Il ristorante gastronomico Vinipolis valorizza i prodotti locali e i vini della cantina Florensac."
    ],
    "fr": [
      "Le restaurant gastronomique de Vinipolis met en valeur les produits du terroir et les vins de la cave des vignerons de Florensac."
    ],
    "nl": [
      "Het gastronomische restaurant Vinipolis brengt streekproducten en wijnen uit de wijnkelder van Florensac onder de aandacht."
    ],
    "es": [
      "El restaurante gastronómico Vinipolis pone de relieve los productos locales y los vinos de la bodega de los viticultores de Florensac."
    ]
  },
