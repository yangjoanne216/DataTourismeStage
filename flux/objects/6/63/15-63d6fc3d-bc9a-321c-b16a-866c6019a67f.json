  "rdfs:comment": {
    "de": [
      "Ein Ausflug auf dem Fluss in einem traditionellen Holz- oder Segelboot mit einem professionellen Führer, um eine einzigartige Erfahrung auf der Loire zu erleben."
    ],
    "en": [
      "Excursion on the river in a traditional wooden boat and sailboat with a professional guide for a unique experience on the Loire river."
    ],
    "it": [
      "Escursione sul fiume in una tradizionale barca di legno e in una barca a vela con una guida professionale per un'esperienza unica sulla Loira."
    ],
    "fr": [
      "Excursion sur le fleuve en bateau traditionnel en bois et à voile avec un guide professionnel pour une vivre une expérience  sur la Loire unique ."
    ],
    "nl": [
      "Excursie op de rivier in een traditionele houten boot en zeilboot met een professionele gids voor een unieke ervaring op de Loire."
    ],
    "es": [
      "Excursión por el río en un barco tradicional de madera y en un velero con un guía profesional para vivir una experiencia única en el Loira."
    ]
  },
