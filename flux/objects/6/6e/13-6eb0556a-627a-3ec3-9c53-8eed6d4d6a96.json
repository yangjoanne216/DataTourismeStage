  "rdfs:comment": {
    "de": [
      "KREATIVES RESTAURANT - Im Herzen des Industriegebiets von Saint Quentin-Fallavier entdecken Sie die originelle, schnell servierte Küche des Restaurants Le 7ème Sens, das von Montag bis Freitag mittags und abends geöffnet ist."
    ],
    "en": [
      "CREATIVE RESTAURANT - In the heart of the industrial zone of Saint Quentin-Fallavier, discover the original cuisine, quickly served, of the restaurant Le 7ème Sens, open from Monday to Friday for lunch and dinner."
    ],
    "it": [
      "RISTORANTE CREATIVO - Nel cuore della zona industriale di Saint Quentin-Fallavier, scoprite la cucina originale, servita rapidamente, del ristorante Le 7ème Sens, aperto dal lunedì al venerdì per pranzo e cena."
    ],
    "fr": [
      "RESTAURANT CRÉATIF - Au cœur de la zone industrielle de Saint Quentin-Fallavier, découvrez la cuisine originale, rapidement servie, du restaurant Le 7ème Sens, ouvert du lundi au vendredi midi et soir."
    ],
    "es": [
      "RESTAURANTE CREATIVO - En el corazón de la zona industrial de Saint Quentin-Fallavier, descubra la cocina original, rápidamente servida, del restaurante Le 7ème Sens, abierto de lunes a viernes para el almuerzo y la cena."
    ],
    "nl": [
      "CREATIEF RESTAURANT - In het hart van de industriële zone van Saint Quentin-Fallavier, ontdek de originele keuken, snel geserveerd, van het restaurant Le 7ème Sens, geopend van maandag tot vrijdag voor lunch en diner."
    ]
  },
