  "rdfs:comment": {
    "de": [
      "Eingeschlossen mit deinem Team von 3 bis 5 Spielern, komm und stelle dein analytisches Denken und deine Beobachtungsgabe unter Beweis."
    ],
    "en": [
      "Locked in with your team of 3 to 5 players, come and use your analytical skills and your sense of observation."
    ],
    "it": [
      "In squadra da 3 a 5 giocatori, venite a sfruttare le vostre capacità analitiche e di osservazione."
    ],
    "fr": [
      "Enfermé avec votre équipe de 3 à 5 joueurs, venez mettre en oeuvre votre esprit d'analyse et votre sens de l'observation."
    ],
    "nl": [
      "Kom met je team van 3 tot 5 spelers je analytische en observationele vaardigheden gebruiken."
    ],
    "es": [
      "Encerrado con su equipo de 3 a 5 jugadores, venga a poner en práctica su capacidad de análisis y observación."
    ]
