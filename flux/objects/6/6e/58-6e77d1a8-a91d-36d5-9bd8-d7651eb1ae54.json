  "rdfs:comment": {
    "de": [
      "BioDici ist eine Boutique, die Lebensmittel, Bio-Brot, Weine aus Bouches du Rhône, Getränke, Reinigungs- und Hygieneprodukte anbietet."
    ],
    "en": [
      "BioDici is a shop which offers groceries, organic bread, wines from Bouches du Rhône, beverages, hygiene and maintenance products."
    ],
    "it": [
      "BioDici è un negozio che offre prodotti alimentari, pane biologico, vini delle Bouches du Rhône, bevande, prodotti per la pulizia e l'igiene."
    ],
    "fr": [
      "BioDici est une boutique qui propose des produits d'épicerie, du pain bio, des vins des Bouches du Rhône, des boissons, des produits d'entretien et d'hygiène."
    ],
    "nl": [
      "BioDici is een winkel met levensmiddelen, biologisch brood, wijnen uit de Bouches du Rhône, dranken, schoonmaak- en hygiëneproducten."
    ],
    "es": [
      "BioDici es una tienda que ofrece productos de alimentación, pan ecológico, vinos de las Bouches du Rhône, bebidas, productos de limpieza e higiene."
    ]
  },
