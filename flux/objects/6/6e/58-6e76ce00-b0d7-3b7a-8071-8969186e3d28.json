  "rdfs:comment": {
    "de": [
      "3 Gästezimmer von 16 bis 20 m² mit Terrasse, Balkon und Meerblick. Frühstück ist im Preis inbegriffen. Sie befinden sich 3 km vom Stadtzentrum und dem Hafen, 300 m vom Strand und 1 km vom Bahnhof entfernt."
    ],
    "en": [
      "3 guest rooms from 16 to 20 m² with terrace, balcony and sea view. Breakfast included. Located 3 km from the city center and the port, 300 m from the beach and 1 km from the train station."
    ],
    "it": [
      "3 camere da 16 a 20 m² con terrazza, balcone e vista mare. Colazione inclusa. Situato a 3 km dal centro città e dal porto, a 300 m dalla spiaggia e a 1 km dalla stazione ferroviaria."
    ],
    "fr": [
      "3 chambres d'hôtes de 16 à 20 m² avec terrasse, balcon et vue sur mer. Petit déjeuner inclus. Situées à 3 km du centre-ville et du port, 300 m de la plage et 1 km de la gare."
    ],
    "es": [
      "3 habitaciones de 16 a 20 m² con terraza, balcón y vistas al mar. Desayuno incluido. Situado a 3 km del centro de la ciudad y del puerto, a 300 m de la playa y a 1 km de la estación de tren."
    ],
    "nl": [
      "3 gastenkamers van 16 tot 20 m² met terras, balkon en zeezicht. Ontbijt inbegrepen. Gelegen op 3 km van het stadscentrum en de haven, 300 m van het strand en 1 km van het treinstation."
    ]
  },
