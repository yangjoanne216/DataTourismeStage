  "rdfs:comment": {
    "de": [
      "Das Bastideum lädt Sie zu einem unumgänglichen Spielefest ein! Mehr als fünfzig alte Spiele sind drinnen und draußen anwesend."
    ],
    "en": [
      "The Bastideum invites you to an unmissable game festival! More than fifty old games will be present inside and outside."
    ],
    "it": [
      "Il Bastideum vi invita a un imperdibile festival del gioco! Più di cinquanta giochi antichi saranno esposti all'interno e all'esterno."
    ],
    "fr": [
      "Le Bastideum vous convie à une incontournable fête du jeu ! Plus d'une cinquantaine de jeux anciens présents en intérieur et extérieur."
    ],
    "nl": [
      "Het Bastideum nodigt u uit voor een niet te missen spelletjesfestival! Meer dan vijftig oude spellen worden binnen en buiten tentoongesteld."
    ],
    "es": [
      "El Bastideum le invita a un festival de juegos ineludible Más de cincuenta juegos antiguos se expondrán en el interior y al aire libre."
    ]
