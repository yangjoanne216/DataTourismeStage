      "dc:description": {
        "de": [
          "Bar - Brasserie\nGastronomie nur mittags: kleine Gerichte, hausgemachte Pommes frites, Andouillettes, Entenbrust, Filet Mignon...\nBillard - Unterhaltung am Wochenende"
        ],
        "en": [
          "Bar - Brasserie\nCatering only at lunchtime: snacks, home-made chips, andouillettes, magret, filet mignon...\nBilliards - Animations on the weekend"
        ],
        "it": [
          "Bar - Brasserie\nRistorazione solo a pranzo: snack, patatine fritte fatte in casa, andouillettes, magret, filet mignon...\nBiliardo - Intrattenimento nel fine settimana"
        ],
        "fr": [
          "Bar - Brasserie\nRestauration uniquement le midi : petite restauration, frites maison, andouillettes, magret, filet mignon...\nBillard - Animations le weekend"
        ],
        "es": [
          "Bar - Brasserie\nCatering sólo a la hora de comer: bocadillos, patatas fritas caseras, andouillettes, magret, filet mignon...\nBillar - Entretenimiento en el fin de semana"
        ],
        "nl": [
          "Bar - Brasserie\nCatering alleen tijdens de lunch: snacks, huisgemaakte frieten, andouillettes, magret, filet mignon...\nBiljart - Vermaak in het weekend"
        ]
      },
