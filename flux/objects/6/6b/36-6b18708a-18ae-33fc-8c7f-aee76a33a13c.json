  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/53df2759-5eb6-320e-8b1b-7bb33bb6393a",
      "dc:description": {
        "de": [
          "Das Restaurant liegt nur 2 Schritte vom Rathaus und dem Jardin Massey entfernt.\nProbieren Sie die Spezialitäten des Küchenchefs!",
          "Eine regionale Familienküche mit frischen Produkten, die vor Ort zubereitet werden."
        ],
        "en": [
          "Hearty, local cuisine made using fresh products, prepared on-site.",
          "Just a stone’s throw away from the town hall and Massey gardens.\nCome and taste the chef’s specialities!"
        ],
        "it": [
          "Cucina familiare locale a base di prodotti freschi preparati in loco.",
          "Situato a 2 passi dal Municipio e dal Jardin Massey.\nVenite a gustare le specialità dello chef!"
        ],
        "fr": [
          "Situé à 2 pas de la Mairie et du Jardin Massey.\nVenez goûter aux spécialités du chef !",
          "Une cuisine familiale du terroir à base de produits frais préparés sur place."
        ],
        "es": [
          "Situado a 2 pasos del Ayuntamiento y del Jardín Massey.\n¡Ven a saborear las especialidades del chef!",
          "Una cocina familiar de la tierra elaborada con productos frescos preparados en el restaurante."
        ],
        "nl": [
          "Gelegen op 2 stappen van het stadhuis en de Jardin Massey.\nKom en proef de specialiteiten van de chef!",
          "Lokale familiekeuken op basis van verse producten ter plaatse bereid."
        ]
      },
