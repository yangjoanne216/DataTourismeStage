  "rdfs:comment": {
    "de": [
      "Ideales Hotel-Restaurant für einen angenehmen Zwischenstopp in einer herzlichen Atmosphäre im Herzen von Beaumont-sur-Sarthe. Auf halbem Weg zwischen Le Mans und Alençon, in der Nähe der Alpes Mancelles."
    ],
    "en": [
      "Ideal hotel-restaurant for a pleasant stopover in a warm atmosphere in the heart of Beaumont-sur-Sarthe. Halfway between Le Mans and Alençon, near the Alpes Mancelles."
    ],
    "it": [
      "Hotel-ristorante ideale per una sosta piacevole in un'atmosfera accogliente, nel cuore di Beaumont-sur-Sarthe. A metà strada tra Le Mans e Alençon, vicino alle Alpi Mancelles."
    ],
    "fr": [
      "Hôtel-restaurant idéal pour une étape agréable dans une ambiance chaleureuse, en plein coeur de Beaumont-sur-Sarthe. A mi chemin entre le Mans et Alençon, à proximité des Alpes Mancelles."
    ],
    "es": [
      "Hotel-restaurante ideal para una agradable parada en un ambiente cálido, en el corazón de Beaumont-sur-Sarthe. A medio camino entre Le Mans y Alençon, cerca de los Alpes Mancelles."
    ],
    "nl": [
      "Hotel-restaurant ideaal voor een aangename tussenstop in een warme sfeer, in het hart van Beaumont-sur-Sarthe. Halverwege Le Mans en Alençon, bij de Alpes Mancelles."
    ]
  },
