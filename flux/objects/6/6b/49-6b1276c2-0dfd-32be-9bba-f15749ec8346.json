  "rdfs:comment": {
    "de": [
      "Béatrice und Luc begrüßen Sie in diesem Landgasthof, der in einem kleinen Dorf am Rande des Waldes von Bercé liegt."
    ],
    "en": [
      "Béatrice and Luc welcome you to this country inn located in a small village on the edge of the Bercé forest."
    ],
    "it": [
      "Béatrice e Luc vi danno il benvenuto in questa locanda di campagna situata in un piccolo villaggio ai margini della foresta di Bercé."
    ],
    "fr": [
      "Béatrice et Luc vous accueillent dans cette auberge de campagne située dans un petit village en lisière de la forêt de Bercé."
    ],
    "es": [
      "Béatrice y Luc le dan la bienvenida a esta posada rural situada en un pequeño pueblo al borde del bosque de Bercé."
    ],
    "nl": [
      "Béatrice en Luc heten u welkom in deze landelijke herberg gelegen in een klein dorpje aan de rand van het bos van Bercé."
    ]
