  "rdfs:comment": {
    "de": [
      "LABEL Bienvenue à la Ferme (Willkommen auf dem Bauernhof). Camping auf dem Bauernhof, 10 Minuten von Nyons entfernt, inmitten von Weinbergen, im Schutz von Zypressen. Weinbauernhof im Herzen der Drôme Provençale."
    ],
    "en": [
      "\"Bienvenue à La Ferme\" approved campsite. Camping at the farm, 10 minutes from Nyons, surrounded by vineyards, sheltered by cypresses. Wine farm at the heart of the Drôme Provençale."
    ],
    "it": [
      "ETICHETTA Bienvenue à la Ferme. Campeggio in fattoria, a 10 minuti da Nyons, in mezzo ai vigneti, al riparo dei cipressi. Azienda vinicola nel cuore della Drôme Provençale."
    ],
    "fr": [
      "LABEL Bienvenue à la Ferme. Camping à la ferme, à 10 minutes de Nyons, au milieu des vignes, à l'abri des cyprès. Ferme viticole au cœur de la Drôme Provençale."
    ],
    "nl": [
      "Label ' Bienvenue a la ferme \r\nCamping à la ferme, 8 kilometer van Nyons, tussen de wijngaarden, beschut door de cypressen. Wijnverbouwer in het hart van de provençaalse Drôme."
    ],
    "es": [
      "ETIQUETA Bienvenue à la Ferme. Camping en la granja, a 10 minutos de Nyons, en medio de los viñedos, al abrigo de los cipreses. Granja vinícola en el corazón de la Drôme provenzal."
    ]
  },
