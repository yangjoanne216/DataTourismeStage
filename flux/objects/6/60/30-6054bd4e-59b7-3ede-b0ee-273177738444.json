  "rdfs:comment": {
    "de": [
      "Brasserie mit Tradition. Warme Garnelen, kleine Friture trouvillaise, Moules marinières oder à la crème, Meeresfrüchteplatte, Seezunge, Merlan, Tarte normande und Calvados-Sorbet."
    ],
    "en": [
      "Traditional brewery. Hot shrimps, small trouvillaise fry, mussels in marinara or cream sauce, seafood platter, sole, whiting, Normandy pie and Calvados sorbet."
    ],
    "it": [
      "Brasserie tradizionale. Gamberi caldi, piccola frittura di trouvillaise, cozze alla marinara o alla crema, piatto di frutti di mare, sogliola, merlano, crostata di Normandia e sorbetto al Calvados."
    ],
    "fr": [
      "Brasserie de tradition. Crevettes chaudes, petite friture trouvillaise, moules marinières ou à la crème, plateau de fruits de mer, sole, merlan, tarte normande et sorbet au Calvados."
    ],
    "nl": [
      "Traditionele brasserie. Hete garnalen, kleine trouvillaise, mosselen in marinara- of roomsaus, zeevruchtenschotel, tong, wijting, Normandische taart en Calvados-sorbet."
    ],
    "es": [
      "Brasserie tradicional. Gambas calientes, fritura trouvillaise, mejillones a la marinera o a la crema, plato de marisco, lenguado, pescadilla, tarta de Normandía y sorbete de Calvados."
    ]
  },
