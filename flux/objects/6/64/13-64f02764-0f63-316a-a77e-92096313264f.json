  "rdfs:comment": {
    "de": [
      "„Das sanfte Leben am Ufer der Ardèche“. Familiencampingplatz 300 m von den Stränden von Baume entfernt. Kanuvermietung. 2 km von Ruoms."
    ],
    "en": [
      "\"La dolce vita on the banks of the Ardèche\". Family-friendly campsite 300m from the Baume beaches. Canoe hire. 2km from Ruoms."
    ],
    "it": [
      "Approfittate delle rive dell'Ardèche e delle gole della Baume per trascorrere i fine settimana e le vacanze con la vostra famiglia."
    ],
    "fr": [
      "Profitez des berges de l'Ardèche et des gorges de Baume pour passer vos week end et vos vacances en famille."
    ],
    "nl": [
      "\"De pracht en de rust van het leven aan de oevers van de Ardèche\"."
    ],
    "es": [
      "Aproveche las orillas del Ardèche y las gargantas de la Baume para pasar sus fines de semana y vacaciones en familia."
    ]
  },
