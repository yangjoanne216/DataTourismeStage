  "rdfs:comment": {
    "de": [
      "Schlendern Sie anlässlich des 8. Mai durch die Blumen, entdecken Sie lokale Produkte und stöbern Sie auf dem Platz von Buzat!"
    ],
    "en": [
      "Come and stroll among the flowers, discover local products and go hunting on the Buzat square on the 8th of May!"
    ],
    "it": [
      "Venite a passeggiare tra i fiori, a scoprire i prodotti locali e a caccia di occasioni in piazza Buzat in occasione dell'8 maggio!"
    ],
    "fr": [
      "Venez flâner au milieu des fleurs, découvrir les produits locaux et chiner sur la place de Buzat à l'occasion du 8 mai!"
    ],
    "es": [
      "Venga a pasear entre las flores, a descubrir los productos locales y a buscar gangas en la plaza de Buzat con motivo del 8 de mayo"
    ],
    "nl": [
      "Kom ter gelegenheid van 8 mei wandelen tussen de bloemen, lokale producten ontdekken en koopjes jagen op het Buzatplein!"
    ]
  },
