  "rdfs:comment": {
    "de": [
      "Hotel, Restaurant und Schneeschuhverleih (15 Paare). Traditionelle Küche mit regionalen Gerichten (vogesische und elsässische Spezialitäten). Restaurant ganzjährig geöffnet; kontaktieren Sie 03 29 25 45 05 für weitere Informationen."
    ],
    "en": [
      "Hotel, restaurant and snowshoe rental (15 pairs). Traditional cuisine with regional dishes (Vosges and Alsatian specialities). Restaurant open all year round; contact 03 29 25 45 05 for further information."
    ],
    "it": [
      "Hotel, ristorante e noleggio di racchette da neve (15 paia). Cucina tradizionale con piatti regionali (specialità dei Vosgi e alsaziane). Ristorante aperto tutto l'anno; per ulteriori informazioni, contattare il numero 03 29 25 45 05."
    ],
    "fr": [
      "Hôtel, restaurant et location de raquettes (15 paires). Cuisine traditionnelle avec des plats régionaux (Spécialités Vosgiennes et Alsaciennes). Restaurant ouvert toute l'année ; contacter le 03 29 25 45 05 pour renseignements complémentaires."
    ],
    "es": [
      "Hotel, restaurante y alquiler de raquetas de nieve (15 pares). Cocina tradicional con platos regionales (especialidades de los Vosgos y de Alsacia). Restaurante abierto todo el año; contactar con el 03 29 25 45 05 para más información."
    ],
    "nl": [
      "Hotel, restaurant en verhuur van sneeuwschoenen (15 paar). Traditionele keuken met regionale gerechten (specialiteiten uit de Vogezen en de Elzas). Restaurant het hele jaar geopend; neem contact op met 03 29 25 45 05 voor meer informatie."
    ]
  },
