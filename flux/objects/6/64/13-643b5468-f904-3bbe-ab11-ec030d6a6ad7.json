  "rdfs:comment": {
    "de": [
      "Italienische und nizzaische Spezialitäten, handgemachte Pizzen aus dem Holzofen. Hausgemacht: handgemachtes Eis, Desserts, Salate, Aperitifs und Geselligkeit."
    ],
    "en": [
      "Italian and Niçoise specialities,  homemade,  wood-fired oven pizzas. Also homemade ice-cream, desserts, salads all in a friendly atmosphere."
    ],
    "it": [
      "Specialità italiane e nizzarde, pizze fatte in casa cotte a legna. Gelati artigianali, dessert, insalate, aperitivi e convivialità."
    ],
    "fr": [
      "Spécialités italiennes et niçoises, pizzas artisanales cuites au feu de bois. Fait maison: glaces artisanales, desserts, salades, apéros et convivialité."
    ],
    "nl": [
      "Italiaanse en Nicoise specialiteiten, huisgemaakte pizza's op houtvuur. Huisgemaakt ijs, desserts, salades, aperitieven en gezelligheid."
    ],
    "es": [
      "Especialidades italianas y nicoleñas, pizzas caseras cocinadas a la leña. Helados caseros, postres, ensaladas, aperitivos y convivencia."
    ]
  },
