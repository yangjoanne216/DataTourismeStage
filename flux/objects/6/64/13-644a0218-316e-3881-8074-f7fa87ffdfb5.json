  "rdfs:comment": {
    "de": [
      "Verkauf von Gemüse und Obst der Saison (Pfirsiche, Erdbeeren, Salat, Kartoffeln ...) auf den hof nach vereinbarung und in Geschäft von herstellern."
    ],
    "en": [
      "Farm shop sales of seasonal vegetables and fruits (peaches, strawberries, salads, potatoes…) by apointment and in producers shops (Prades, Aubenas)."
    ],
    "it": [
      "Vendita di verdura e frutta di stagione (pesche, fragole, insalate, patate, ecc.). Vendita in fattoria su appuntamento, prodotti disponibili nel negozio dei produttori (Prades, Aubenas)."
    ],
    "fr": [
      "Vente de légumes et fruits de saisons (pêches, fraises, salades, pommes de terre,...). Vente à la ferme sur rdv, produits disponibles en magasin de producteurs (Prades, Aubenas)."
    ],
    "nl": [
      "Verkoop van seizoensgroenten en -fruit (perziken, aardbeien, salades, aardappelen, enz.). Verkoop op de boerderij op afspraak, producten verkrijgbaar in de winkel van de producent (Prades, Aubenas)."
    ],
    "es": [
      "Venta de verduras y frutas de temporada (melocotones, fresas, ensaladas, patatas, etc.). Venta en la granja con cita previa, productos disponibles en la tienda de los productores (Prades, Aubenas)."
    ]
  },
