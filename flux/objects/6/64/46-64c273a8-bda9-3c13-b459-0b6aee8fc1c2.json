  "rdfs:comment": {
    "de": [
      "Italienische Spezialitäten. Empfang von Gruppen bis zu 50 Personen. Pizzas und Gerichte zum Mitnehmen, hausgemachtes Eis. Preise à la carte.12-14 Uhr und 19-22.30 Uhr geöffnet. Am Wochenende bis 23 Uhr. Montags geschlossen."
    ],
    "en": [
      "Italian specialties. Groups up to 50 people. Pizzas and take-away meals, home-made ice creams. Open from 12h to 14h and from 19h to 22h30. Open until 11pm at weekends. Closed on Mondays."
    ],
    "it": [
      "Specialità italiane. Gruppi fino a 50 persone sono benvenuti. Pizze e piatti da asporto, gelati artigianali. Aperto dalle 12:00 alle 14:00 e dalle 19:00 alle 22:30. Fino alle 23:00 nei fine settimana. Chiuso il lunedì."
    ],
    "fr": [
      "Spécialités italiennes. Accueil groupes jusqu'à 50 personnes. Pizzas et plats à emporter, glaces maison. Prix à la carte.Ouvert de 12h à 14h et de 19h à 22h30. Jusqu'à 23h le week-end. Fermé le lundi."
    ],
    "nl": [
      "Italiaanse specialiteiten. Groepen tot 50 personen zijn welkom. Pizza's en afhaalmaaltijden, zelfgemaakt ijs. Open van 12 tot 14 uur en van 19 tot 22.30 uur. Tot 23.00 uur in het weekend. Gesloten op maandag."
    ],
    "es": [
      "Especialidades italianas. Se admiten grupos de hasta 50 personas. Pizzas y comida para llevar, helados caseros. Abierto de 12 a 14 horas y de 19 a 22:30 horas. Hasta las 23 horas los fines de semana. Cerrado los lunes."
    ]
  },
