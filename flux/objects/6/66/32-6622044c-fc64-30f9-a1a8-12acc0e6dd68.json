  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/223e3f1b-a7dc-3659-a64a-ceffb5e571ba",
      "dc:description": {
        "de": [
          "Wohnung Studio, 16 m², 3. Stock Asc. 100m von einem Thermalbad, 600m vom Stadtzentrum, 7km vom Meer entfernt. Balkon, Garage. Heizung, Gefrierschrank, Mikrowelle, private Waschmaschine, Fernseher, DVD. Bett für 2 Personen. Vermietung von Wäsche, Reinigung. Klimaanlage."
        ],
        "en": [
          "Apartment, 16 m², third floor Lift. 100 meters to the thermal waters, 600 meters to the downtown area, 7km to seaside. Balcony, garage. Heating and air conditioning, freezer, microwave oven, private washing machine, TV, DVD. Sofa bed living room. Rent of linens, housework service."
        ],
        "it": [
          "Monolocale, 16 m², 3° piano Asc. A 100 m da un bagno termale, a 600 m dal centro città, a 7 km dal mare. Balcone, garage. Riscaldamento, congelatore, forno a microonde, lavatrice privata, televisione, DVD. Letto matrimoniale. Noleggio biancheria, servizio di pulizia. Aria condizionata."
        ],
        "fr": [
          "Appartement studio, 16 m², 3ème étage Asc. À 100m d'un établissement thermal, à 600m du centre-ville, à 7km du bord de mer. Balcon, garage. Chauffage, congélateur, four à micro-ondes, lave linge privatif, télévision, DVD. Lit 2 places. Location de linge, ménage. Climatisation."
        ],
        "nl": [
          "Studio, 16 m², 3e verdieping Asc. 100m van een thermaal bad, 600m van het stadscentrum, 7km van de zee. Balkon, garage. Verwarming, vriezer, magnetron, eigen wasmachine, televisie, DVD. Tweepersoonsbed. Linnenverhuur, schoonmaakservice. Airconditioning."
        ],
        "es": [
          "Estudio, 16 m², 3ª planta Asc. A 100 m de un baño termal, a 600 m del centro de la ciudad, a 7 km del mar. Balcón, garaje. Calefacción, congelador, microondas, lavadora privada, televisión, DVD. Cama doble. Alquiler de ropa de cama, servicio de limpieza. El aire acondicionado."
        ]
      },
