  "rdfs:comment": {
    "de": [
      "Jeden Tag ist in Eygalières ein wahres Spektakel zu sehen, der Sonnenuntergang in den Alpilles."
    ],
    "en": [
      "Every day in Eygalières, a real spectacle can be seen, the sunset in the Alpilles."
    ],
    "it": [
      "Ogni giorno a Eygalières si può assistere a un vero e proprio spettacolo, il tramonto sulle Alpilles."
    ],
    "fr": [
      "Chaque jour à Eygalières, un vrai spectacle peut être vu, le coucher de soleil dans les Alpilles."
    ],
    "es": [
      "Todos los días en Eygalières se puede ver un verdadero espectáculo, la puesta de sol en los Alpilles."
    ],
    "nl": [
      "In Eygalières is elke dag een waar spektakel te zien, de zonsondergang in de Alpilles."
    ]
  },
