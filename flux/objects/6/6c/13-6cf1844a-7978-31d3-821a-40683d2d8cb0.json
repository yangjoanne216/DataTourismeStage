  "rdfs:comment": {
    "de": [
      "Dieses Familienweingut befindet sich in Noailles, einem charmanten Dorf zwischen Albi und Cordes sur Ciel."
    ],
    "en": [
      "Situated 7km from Cordes sur Ciel, you will receive a friendly and warm welcome from the wine-grower before visiting the wine warehouse and vineyards. Wine-tasting included – tasting for groups available by appointment. Picnic area on-site."
    ],
    "it": [
      "Questa tenuta familiare si trova a Noailles, un incantevole villaggio tra Albi e Cordes sur Ciel."
    ],
    "fr": [
      "Ce domaine familial se situe à Noailles, charmant village entre Albi et Cordes sur Ciel."
    ],
    "es": [
      "Este dominio familiar se encuentra en Noailles, un encantador pueblo entre Albi y Cordes sur Ciel."
    ],
    "nl": [
      "Dit familiedomein ligt in Noailles, een charmant dorpje tussen Albi en Cordes sur Ciel."
    ]
  },
