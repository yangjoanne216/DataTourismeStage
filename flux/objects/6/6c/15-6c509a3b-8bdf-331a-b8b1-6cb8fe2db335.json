  "rdfs:comment": {
    "de": [
      "Tauchen Sie ein in die Welt der Bücher und Buchbinderei. Sie erhalten einen Einblick in die Geschichte der Buchbinderei,"
    ],
    "en": [
      "Dive into the world of books and bookbinding and get an overview of the history of bookbinding,"
    ],
    "it": [
      "Immergetevi nel mondo dei libri e della rilegatura e scoprite la storia della rilegatura,"
    ],
    "fr": [
      "Plongez dans l’univers du livre et de la reliure, vous aurez un aperçu de l'histoire de la reliure,"
    ],
    "es": [
      "Sumérjase en el mundo del libro y la encuadernación y conozca la historia de la encuadernación,"
    ],
    "nl": [
      "Duik in de wereld van boeken en boekbinden en krijg inzicht in de geschiedenis van het boekbinden,"
    ]
  },
