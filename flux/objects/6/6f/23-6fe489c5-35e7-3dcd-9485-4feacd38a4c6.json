  "rdfs:comment": {
    "de": [
      "Auf Sare, Haus aus dem 17. Jh. in einem denkmalgeschützten Viertel namens Ihalar, Rue Bastide. Ruhige Lage mit Blick auf den Berg La Rhune. Ideal, um das Baskenland zu besuchen."
    ],
    "en": [
      "Sur Sare, a 17th century house in a listed district called Ihalar, bastide street. Quiet with a view of the Rhune mountain. Ideal to visit the Basque Country."
    ],
    "it": [
      "Su Sare, casa del XVII secolo in un quartiere classificato come Ihalar, via della Bastide. Tranquillo con vista sul monte Rhune. Ideale per visitare i Paesi Baschi."
    ],
    "fr": [
      "Sur Sare, maison du XVIIe siècle dans un quartier classé nommé Ihalar, rue bastide. Au calme avec vue sur la montagne la Rhune. Idéal pour visiter le Pays Basque."
    ],
    "nl": [
      "Op Sare, huis van de XVIIe eeuw in een geclassificeerde wijk genaamd Ihalar, bastide straat. Rustig met uitzicht op de Rhune berg. Ideaal voor een bezoek aan Baskenland."
    ],
    "es": [
      "En Sare, casa del siglo XVII en un barrio clasificado llamado Ihalar, calle de la bastida. Tranquilo con vistas a la montaña de Rhune. Ideal para visitar el País Vasco."
    ]
  },
