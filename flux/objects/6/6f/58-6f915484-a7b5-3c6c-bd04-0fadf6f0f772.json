  "rdfs:comment": {
    "de": [
      "Israelische Spezialitäten (Hummus, Tehina, Falafel, Schawarma als Sandwich, Pitas oder Teller) und orientalische Spezialitäten (gegrillte Paprika, Keftas, Kebbab und Shish Kebab). Warme Gerichte und gemischte Salate."
    ],
    "en": [
      "Israeli specialties (hummus, tehina, falafel, schawarma in sandwiches, pita or plate, and oriental (grilled peppers, keftas, kebbab and shish kebab). Hot dishes, and mixed salads."
    ],
    "it": [
      "Specialità israeliane (hummus, tehina, falafel, schawarma in panini, pita o piatti) e orientali (peperoni alla griglia, kefta, kebbab e shish kebab). Piatti caldi e insalate miste."
    ],
    "fr": [
      "Spécialités israéliennes (houmous, tehina, falafel, schawarma en sandwichs, pitas ou assiette, et orientales (poivrons grillés, keftas, kebbab et chiche kebab). Plats chauds, et salades composées."
    ],
    "nl": [
      "Israëlische specialiteiten (hummus, tehina, falafel, schawarma in broodjes, pitas of borden) en oosterse (gegrilde paprika's, keftas, kebab en shish kebab). Warme gerechten en gemengde salades."
    ],
    "es": [
      "Especialidades israelíes (hummus, tehina, falafel, schawarma sándwiches, pitas o platos) y orientales (pimientos asados, keftas, kebbab y shish kebab). Platos calientes y ensaladas mixtas."
    ]
  },
