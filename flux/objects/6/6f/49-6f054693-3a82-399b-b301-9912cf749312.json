  "rdfs:comment": {
    "de": [
      "EIN HAUCH VON ITALIEN KOMMT AUF IHREN TISCH! Neapolitanische Pizzas aus dem Holzofen! Typische Produkte aus Italien. Frische und hausgemachte Produkte. Zusammenarbeit mit lokalen Lieferanten."
    ],
    "en": [
      "A LITTLE TASTE OF ITALY AT YOUR TABLE! Neapolitan wood-fired pizzas! Typical Italian products. Fresh, home-made products. Collaboration with local suppliers."
    ],
    "it": [
      "UN PICCOLO ASSAGGIO D'ITALIA ALLA VOSTRA TAVOLA! Pizze napoletane cotte a legna! Prodotti tipici italiani. Prodotti freschi e fatti in casa. Collaborazione con fornitori locali."
    ],
    "fr": [
      "UN PETIT AIR D'ITALIE S'INVITE A VOTRE TABLE ! Des Pizzas Napolitaines cuites au Feu de Bois ! Des produits typiques d'Italie. Des produits \"Frais et Faits Maison\". Une collaboration avec des fournisseurs locaux."
    ],
    "es": [
      "¡UN POCO DE ITALIA EN SU MESA! Pizzas napolitanas al horno de leña Productos típicos italianos. Productos frescos y caseros. Trabajo con proveedores locales."
    ],
    "nl": [
      "EEN VLEUGJE ITALIË AAN TAFEL! Napolitaanse houtgestookte pizza's! Typische Italiaanse producten. Verse, zelfgemaakte producten. Werken met lokale leveranciers."
    ]
  },
