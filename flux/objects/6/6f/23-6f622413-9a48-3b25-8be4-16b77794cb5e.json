  "rdfs:comment": {
    "de": [
      "Côte et Mer, eine Agentur am Port de Plaisance in Arcachon, bietet einen Boots- und Jetski-Verleih mit Lizenz zur Entdeckung der Arcachon Bay an."
    ],
    "en": [
      "Côte et Mer, agency located at the Port de Plaisance in Arcachon, offers boat and jet ski rental with license to discover the Arcachon Bay."
    ],
    "it": [
      "Côte et Mer, agenzia situata al Port de Plaisance d'Arcachon, offre il noleggio di barche e moto d'acqua con licenza per scoprire la baia di Arcachon."
    ],
    "fr": [
      "Côte et Mer, agence située au Port de Plaisance d'Arcachon, vous propose la location de bateaux et de jet-ski avec permis pour découvrir le Bassin d'Arcachon."
    ],
    "nl": [
      "Côte et Mer, een agentschap gevestigd in de Port de Plaisance d'Arcachon, verhuurt boten en jetski's met vergunning om de baai van Arcachon te ontdekken."
    ],
    "es": [
      "Côte et Mer, agencia ubicada en el Port de Plaisance en Arcachon, ofrece alquiler de botes y motos acuáticas con licencia para descubrir la Bahía de Arcachon."
    ]
  },
