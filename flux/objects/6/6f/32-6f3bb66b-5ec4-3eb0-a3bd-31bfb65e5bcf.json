      "dc:description": {
        "de": [
          "Eintritt 5 Euro, 1 graviertes Glas gratis + 1 Kostprobe.\nVerpflegung vor Ort. Musikalische Unterhaltung mit der Banda Bonhomme. Anzünden der Stadtmauern um 22.45 Uhr."
        ],
        "en": [
          "Admission 5 euros, 1 engraved glass free + 1 tasting.\nCatering on site. Musical entertainment with the Banda Bonhomme. Fireworks on the ramparts at 10:45pm."
        ],
        "it": [
          "Ingresso 5 euro, 1 bicchiere inciso in omaggio + 1 degustazione.\nRistorazione in loco. Intrattenimento musicale con la Banda Bonhomme. Fuochi d'artificio sui bastioni alle 22.45."
        ],
        "fr": [
          "Entrée à 5 euros, 1 verre gravé offert + 1 dégustation. \nRestauration sur place. Animation musicale avec la Banda Bonhomme. Embrasement des remparts à 22h45."
        ],
        "nl": [
          "Entree 5 euro, 1 gegraveerd glas gratis + 1 proeverij.\nCatering ter plaatse. Muzikale animatie met de Banda Bonhomme. Vuurwerk op de vestingmuren om 22.45 uur."
        ],
        "es": [
          "Entrada 5 euros, 1 copa grabada gratis + 1 degustación.\nCatering in situ. Animación musical con la Banda Bonhomme. Fuegos artificiales en las murallas a las 22.45 h."
        ]
      },
