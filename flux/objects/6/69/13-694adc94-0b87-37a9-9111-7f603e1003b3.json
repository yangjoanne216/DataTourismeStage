  "rdfs:comment": {
    "de": [
      "Pizza Roma empfängt Sie von Dienstag bis Samstag ab 16:30 Uhr.\r\nDie Pizza wird ausschließlich aus frischen Zutaten hergestellt, der Teig ist hausgemacht und von italienischer Qualität."
    ],
    "en": [
      "Pizza Roma welcomes you from Tuesday to Saturday from 4:30 pm.\r\nPizza made with fresh products only, home made dough of Italian quality."
    ],
    "it": [
      "Pizza Roma vi accoglie dal martedì al sabato dalle 16.30.\r\nPizza fatta solo con prodotti freschi, impasto casalingo di qualità italiana."
    ],
    "fr": [
      "Pizza Roma vous accueille du mardi au samedi à partir de 16 h30.\r\nPizza fabriqué avec des produits frais uniquement, pâte faite maison de qualité italienne."
    ],
    "nl": [
      "Pizza Roma verwelkomt u van dinsdag tot en met zaterdag vanaf 16.30 uur.\r\nPizza gemaakt met uitsluitend verse producten, huisgemaakt deeg van Italiaanse kwaliteit."
    ],
    "es": [
      "Pizza Roma le da la bienvenida de martes a sábado a partir de las 16.30 h.\r\nPizza elaborada únicamente con productos frescos, masa casera de calidad italiana."
    ]
  },
