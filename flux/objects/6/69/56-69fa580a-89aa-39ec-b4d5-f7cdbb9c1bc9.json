  "rdfs:comment": {
    "de": [
      "9 Zimmer, davon 2 mit Terrasse\nKleines Hotel mit allem Komfort in einem Dorf mit Parkplatz in der Nähe. 3 Min. vom Strand, allen Geschäften und verschiedenen Freizeitzentren entfernt."
    ],
    "en": [
      "9 rooms including 2 with terrace\nSmall hotel with all comforts in a village with parking nearby. At 3 mn from the beach, all shops and various leisure activities."
    ],
    "it": [
      "9 camere, 2 con terrazza\nPiccolo hotel con tutti i comfort in un villaggio con parcheggio nelle vicinanze. A 3 minuti dalla spiaggia, da tutti i negozi e da varie strutture ricreative."
    ],
    "fr": [
      "9 chambres dont 2 avec terrasse. \nPetit hôtel tout confort dans village avec parking à proximité. A 3 mn de la plage, de tous commerces et de diverses bases de loisirs."
    ],
    "nl": [
      "9 kamers, 2 met terras\nKlein hotel met alle comfort in een dorp met parkeerplaats in de buurt. 3 minuten van het strand, alle winkels en diverse recreatiemogelijkheden."
    ],
    "es": [
      "9 habitaciones, 2 con terraza\nPequeño hotel con todas las comodidades en un pueblo con aparcamiento cercano. A 3 minutos de la playa, de todos los comercios y de diversos lugares de ocio."
    ]
  },
