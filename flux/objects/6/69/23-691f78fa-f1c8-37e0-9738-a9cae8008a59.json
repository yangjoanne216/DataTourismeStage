  "rdfs:comment": {
    "de": [
      "Vermietung eines Studios in La Roche-Posay, 17 m² - Stadtzentrum - Erdgeschoss - 900 m von den Thermen des Connétable entfernt - Nähe zu allen Geschäften - Einzelbett mit Zugang zum Wohnbereich und Küchenecke, Sofa, TV -Bad (Dusche, Waschbecken, WC). Gemeinschaftsgarten - Kostenloses WLAN im Studio."
    ],
    "en": [
      "17 m² studio apartment for rent in La Roche-Posay - Town center - Ground floor - 900 m from the Thermes du Connétable - Close to all amenities - Single bed opening onto living area and kitchenette, sofa, TV - Bathroom (shower, sink, toilet). Shared garden - Free WiFi in studio."
    ],
    "it": [
      "Affitto di un monolocale di 17 m² a La Roche-Posay - Centro città - Piano terra - 900 m dalle Thermes du Connétable - Vicino a tutti i negozi - Letto singolo che si apre sulla zona giorno e sull'angolo cottura, divano, TV - Bagno (doccia, lavandino, wc). Giardino in comune - Wi-Fi gratuito nel monolocale."
    ],
    "fr": [
      "Location d'un studio à la Roche-Posay, de 17 m² - Centre-ville - Rez-de-chaussée  - 900 m des Thermes du Connétable - Proximité tous commerces -  Lit simple donnant sur espace séjour et coin cuisine, canapé, TV -salle d’eau (douche, lavabo, wc). Jardin commun - Wifi gratuit dans le studio."
    ],
    "nl": [
      "Verhuur van een studio van 17 m² in La Roche-Posay - Stadscentrum - Begane grond - 900 m van de Thermen van Connétable - Vlakbij alle winkels - Eenpersoonsbed dat uitgeeft op een woonkamer en kitchenette, bank, tv - Doucheruimte (douche, wastafel, wc). Gemeenschappelijke tuin - Gratis Wi-Fi in de studio."
    ],
    "es": [
      "Alquiler de un estudio de 17 m² en La Roche-Posay - Centro ciudad - Planta baja - A 900 m de las Termas del Connétable - Cerca de todos los comercios - Cama individual que da al salón y a la cocina americana, sofá, TV - Cuarto de baño (ducha, lavabo, wc). Jardín compartido - Wi-Fi gratuito en el estudio."
    ]
  },
