  "rdfs:comment": {
    "de": [
      "Das Tourismusbüro von Sainte Alvère öffnet seine Türen von Montag bis Freitag von 9:00 bis 12:30 Uhr sowie am Mittwochnachmittag von 14:00 bis 16:00 Uhr."
    ],
    "en": [
      "The Tourist Office of Sainte Alvère opens its doors from Monday to Friday from 9 am to 12.30 pm, as well as on Wednesday afternoon from 2 pm to 4 pm."
    ],
    "it": [
      "L'Ufficio del Turismo di Sainte Alvère è aperto dal lunedì al venerdì dalle 9.00 alle 12.30, e il mercoledì pomeriggio dalle 14.00 alle 16.00."
    ],
    "fr": [
      "L'Office de tourisme de Sainte Alvère vous ouvre ses portes du Lundi au Vendredi de 9 heures à 12 heures 30, ainsi que le Mercredi après-midi de 14 heures à 16 heures."
    ],
    "es": [
      "La Oficina de Turismo de Sainte Alvère está abierta de lunes a viernes de 9 a 12.30 horas, y los miércoles por la tarde de 14 a 16 horas."
    ],
    "nl": [
      "Het toeristenbureau van Sainte Alvère is geopend van maandag tot vrijdag van 9 tot 12.30 uur en op woensdagmiddag van 14 tot 16 uur."
    ]
  },
