  "rdfs:comment": {
    "de": [
      "Fleischspezialitäten: Fleischliebhaber kommen hier ganz auf ihre Kosten mit Tatar, Gegrilltem und Fleischfondue..."
    ],
    "en": [
      "Meat specialties: meat lovers will be delighted with our tartars, grills, pierrades, brasérades and fondues bourguignonnes...\r\nFor the happiness of families, children's menu at 1€ !"
    ],
    "it": [
      "Specialità di carne: gli appassionati vi troveranno la felicità con tartare, grigliate, carni cotte sulla pietra e fonduta alla bourguignonne."
    ],
    "fr": [
      "Spécialités de viandes: les amateurs y trouveront leur bonheur avec nos tartares, grillades, pierrades, brasérades et fondues bourguignonnes...\r\nPour le bonheur des familles, menu enfant à 1€ !"
    ],
    "nl": [
      "Vleesspecialiteiten: vleesliefhebbers zullen blij zijn met onze tartaars, grillades, pierrades, brasérades en fondues bourguignonnes...\r\nVoor het geluk van gezinnen, kindermenu aan 1€!"
    ],
    "es": [
      "Especialidades cárnicas: los amantes de la carne estarán encantados con nuestros tartares, parrilladas, pierrades, brasérades y fondues bourguignonnes...\r\nPara la felicidad de las familias, el menú infantil a 1?"
    ]
  },
