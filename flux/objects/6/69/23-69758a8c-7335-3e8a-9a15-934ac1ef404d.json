  "rdfs:comment": {
    "de": [
      "Bar, Tabakladen, Restaurant. Arbeitermenü, Vorspeisenbuffet, 2 bis 3 Gerichte zur Auswahl, Käse und Dessert. Getränke inbegriffen\n\nPartner des Fremdenverkehrsamts für KOSTENLOSES SICHERES WIFI."
    ],
    "en": [
      "Bar, tobacco, restaurant. Worker's menu, starter buffet, 2 to 3 courses of your choice, cheese and dessert. Drinks included\n\nPartner of the Tourist Office for FREE SECURE WIFI."
    ],
    "it": [
      "Bar, tabaccheria, ristorante. Menu del lavoratore, buffet di antipasti, scelta di 2 o 3 piatti, formaggio e dessert. Bevande incluse\n\nPartner dell'Ufficio del Turismo per il WIFI SICURO GRATUITO."
    ],
    "fr": [
      "Bar, tabac, restaurant. Menu ouvrier, buffet d'entrées, 2 à 3 plats au choix, fromage et dessert. Boisson comprise. \n\nPartenaire de l'Office de Tourisme pour le WIFI GRATUIT SECURISE."
    ],
    "nl": [
      "Bar, tabakswinkel, restaurant. Werknemersmenu, buffet van voorgerechten, keuze uit 2 of 3 gerechten, kaas en dessert. Drankjes inbegrepen\n\nPartner van de VVV voor GRATIS VEILIGE WIFI."
    ],
    "es": [
      "Bar, estanco, restaurante. Menú del trabajador, buffet de entrantes, elección de 2 ó 3 platos, queso y postre. Bebidas incluidas\n\nSocio de la Oficina de Turismo para el WIFI GRATUITO."
    ]
  },
