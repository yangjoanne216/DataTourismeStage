  "rdfs:comment": {
    "de": [
      "Praxis: Delta, Gleitschirm Orientierung: N Bemerkungen / Gefahren: Leicht für Gleitschirme. Ganz in der Nähe des Windsacks landen. Ausschließlich den Zugangsweg benutzen, um wieder zur Straße hinabzusteigen."
    ],
    "en": [
      "Sport: Handgliding, Paragliding\r\nDirection: North\r\nNotes/Hazards: Easy for Paragliding, landing close to windsock.  Stick to designated paths.  Flying height 300m from ground. Always respect the local regulations."
    ],
    "it": [
      "Pratica: deltaplano, parapendio Orientamento: N Osservazioni / Pericoli: facile per i parapendisti. Terreno vicino alla barriera corallina. Utilizzare solo la strada di accesso per tornare alla strada."
    ],
    "fr": [
      "Pratique : delta, parapente Orientation : N Observations / Dangers : facile pour les parapentes. Se poser tout près de la manche à air. Utiliser exclusivement le chemin d'accès pour redescendre à la route."
    ],
    "nl": [
      "Praktijk: deltavliegen, paragliding Oriëntatie: N Waarnemingen / Gevaren: gemakkelijk voor paragliders. Land dichtbij de windzak. Gebruik alleen de toegangsweg om terug te gaan naar de weg."
    ],
    "es": [
      "Práctica: ala delta, parapente Orientación: N Observaciones / Peligros: fácil para los parapentistas. Terreno cerca de la manga de viento. Utilice sólo el camino de acceso para bajar a la carretera."
    ]
  },
