  "rdfs:comment": {
    "de": [
      "Das Nähatelier La Petite Mirabelle bietet seine Umhängetaschen, Einkaufstaschen, Rucksäcke, PC-Taschen, Tablet-Taschen, Münzhalter, Geldbörsen, Clutches, Etuis, Schlüsselanhänger und Armbänder an."
    ],
    "en": [
      "The sewing workshop La Petite Mirabelle offers its shoulder bags, tote bags, backpacks, PC bags, tablet bags, wallets, purses, pouches, kits, key chains, bracelets."
    ],
    "it": [
      "Il laboratorio di cucito La Petite Mirabelle propone borse a tracolla, tote bag, zaini, borse per PC, borse per tablet, portafogli, borse, sacchetti, astucci, portachiavi, braccialetti."
    ],
    "fr": [
      "L'atelier de couture La Petite Mirabelle propose ses sacs à bandoulières, cabas, sacs à dos, sacs PC, sacs tablettes, portes monnaies, portefeuilles, pochettes, trousses, porte-clefs, bracelets."
    ],
    "nl": [
      "La Petite Mirabelle naaiatelier biedt haar schoudertassen, draagtassen, rugzakken, PC-tassen, tablet tassen, portefeuilles, portemonnees, zakjes, etuis, sleutelhangers, armbanden."
    ],
    "es": [
      "El taller de costura La Petite Mirabelle ofrece sus bandoleras, bolsos tote, mochilas, bolsos para PC, bolsos para tabletas, carteras, bolsos, estuches, llaveros, pulseras."
    ]
  },
