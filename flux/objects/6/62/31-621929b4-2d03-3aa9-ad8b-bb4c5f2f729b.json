  "rdfs:comment": {
    "de": [
      "Laguiole: ein Käse! Ein Besuch, um die Geheimnisse dieser Appellation zu erfahren. Ein Treffen mit unseren Produzenten, um unsere Bauernhöfe zu entdecken."
    ],
    "en": [
      "Discover the secrets of Laguiole cheese and Aubrac Aligot"
    ],
    "it": [
      "Laguiole: un formaggio! Una visita per conoscere i segreti di questa denominazione. Un incontro con i nostri produttori per scoprire le nostre aziende agricole."
    ],
    "fr": [
      "Le Laguiole : un fromage ! Une visite pour connaître les secrets de cette appellation. Une rencontre avec nos producteurs pour découvrir nos fermes."
    ],
    "nl": [
      "Laguiole: een kaas! Een bezoek om de geheimen van deze appellation te leren kennen. Een ontmoeting met onze producenten om onze boerderijen te ontdekken."
    ],
    "es": [
      "Laguiole: ¡un queso! Una visita para conocer los secretos de esta denominación. Un encuentro con nuestros productores para descubrir nuestras granjas."
    ]
