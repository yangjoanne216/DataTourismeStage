  "rdfs:comment": {
    "de": [
      "21 (rote Farbe) ? Kriegsberg/Typ: X-C\r\n\r\nEine Cross Country-Route durch die Almen von Champagny mit atemberaubender Aussicht auf die Gletscher der Vanoise."
    ],
    "en": [
      "21 (red) – Mont de la Guerre/Type: X-C\r\nDeparture point: Top of Champagny gondola\r\nA cross-country route through the mountain pastures of Champagny with breathtaking views of the Vanoise glaciers."
    ],
    "it": [
      "21 (colore rosso) ? Attacco/tipo di guerra: X-C\r\n\r\nUn itinerario di Cross Country attraverso gli alpeggi di Champagny con una vista mozzafiato sui ghiacciai della Vanoise."
    ],
    "fr": [
      "21 (couleur rouge) – Mont de la guerre/Type : X-C\r\n\r\nUn itinéraire Cross Country à travers les alpages de Champagny avec vue imprenable sur les glaciers de la Vanoise."
    ],
    "nl": [
      "21 (rode kleur) - War Mountain/Type: X-C\r\n\r\nEen Cross Country route door de bergweiden van Champagny met een adembenemend uitzicht op de gletsjers van de Vanoise."
    ],
    "es": [
      "21 (color rojo) ? Montura de guerra/Tipo: X-C\r\n\r\nUn itinerario de Cross Country por los pastos de montaña de Champagny con una vista impresionante de los glaciares de la Vanoise."
    ]
  },
