  "rdfs:comment": {
    "de": [
      "15m2 großes Studio, ausgestattet mit Kochplatte, Mikrowelle / Outdoor-Plancha / Nespresso-Maschine, Geschirr. Handtuch, Willkommensartikel. Kleiner sonniger Innenhof mit Blick auf den Whirlpool. Garantiert entspannende Atmosphäre."
    ],
    "en": [
      "Studio of 15m2, equipped with a cooking plate, microwave / outdoor plancha / Nespresso machine, crockery. Towel, welcome product. Small sunny courtyard overlooking the jacuzzi. Relaxing atmosphere guaranteed."
    ],
    "it": [
      "Monolocale di 15 m2, dotato di fornelli, microonde / grill all'aperto / macchina Nespresso, stoviglie. Asciugamano, prodotto di benvenuto. Piccolo cortile soleggiato con vista sulla vasca idromassaggio. Atmosfera rilassante garantita."
    ],
    "fr": [
      "Studio de 15m2, équipé d'une plaque de cuisson, micro-onde / plancha extérieur / machine Nespresso, vaisselleries. Serviette de bain, produit de bienvenu. Petite cour ensoleillée donnant sur le jacuzzi. Ambiance détente garantie."
    ],
    "nl": [
      "Studio van 15m2, uitgerust met fornuis, magnetron / buitengrill / Nespresso machine, serviesgoed. Handdoek, welkomstpakket. Kleine zonnige binnenplaats met uitzicht op de jacuzzi. Ontspannende sfeer gegarandeerd."
    ],
    "es": [
      "Estudio de 15m2, equipado con cocina, microondas / parrilla al aire libre / máquina Nespresso, vajilla. Toalla, paquete de bienvenida. Pequeño patio soleado con vistas al jacuzzi. Ambiente relajante garantizado."
    ]
  },
