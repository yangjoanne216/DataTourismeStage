  "rdfs:comment": {
    "de": [
      "Es liegt im Herzen eines kleinen Dorfes auf dem Land in der Gemeinde Vay, zwei Kilometer vom Staatswald von Gâvre entfernt."
    ],
    "en": [
      "Situated in the heart of a small country village in the commune of Vay, two kilometres from the Gâvre national forest."
    ],
    "it": [
      "Situato nel cuore di un piccolo villaggio di campagna nel comune di Vay, a due chilometri dalla foresta di Gâvre."
    ],
    "fr": [
      "Située au coeur d’un petit village de campagne de la commune de Vay à deux kilomètres de la forêt Domaniale du Gâvre."
    ],
    "es": [
      "Situado en el corazón de un pequeño pueblo de campo en la comuna de Vay, a dos kilómetros del bosque de Gâvre."
