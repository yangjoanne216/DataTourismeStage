  "rdfs:comment": {
    "de": [
      "Kommen Sie nach Saumur, um Spaß zu haben und an den 16 spielerischen Clip'n Climb-Kletterwänden mit einer Höhe von über acht Metern zu klettern!"
    ],
    "en": [
      "Come and have fun and climb on the 16 Clip'n Climb playful climbing walls of more than eight meters high in Saumur!"
    ],
    "it": [
      "Venite a divertirvi e ad arrampicarvi sulle 16 pareti di arrampicata ludica Clip'n Climb, alte più di otto metri, a Saumur!"
    ],
    "fr": [
      "Venez vous amuser et grimper sur les 16 murs d'escalade ludiques Clip'n Climb de plus de huit mètres de hauteur à Saumur !"
    ],
    "es": [
      "Venga a divertirse y a escalar en los 16 rocódromos lúdicos Clip'n Climb de más de ocho metros de altura en Saumur"
    ],
    "nl": [
      "Kom plezier maken en klimmen op de 16 Clip'n Climb speelse klimwanden van meer dan acht meter hoog in Saumur!"
    ]
  },
