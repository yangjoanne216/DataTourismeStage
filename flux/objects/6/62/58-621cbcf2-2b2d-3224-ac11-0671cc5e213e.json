  "rdfs:comment": {
    "de": [
      "Der Arc ist der Fluss, der das Departement vom Mont Aurelien bis zum Etang de Berre quer durchfließt."
    ],
    "en": [
      "The Arc is the river that crosses the department from the Mont Aurelien to the Etang de Berre."
    ],
    "it": [
      "L'Arc è il fiume che attraversa il dipartimento dal Mont Aurelien all'Etang de Berre."
    ],
    "fr": [
      "L'Arc est la rivière qui traverse transversalement le département depuis le Mont Aurelien jusqu'à l'Etang de Berre."
    ],
    "es": [
      "El Arc es el río que atraviesa el departamento desde el Monte Aurelien hasta el Etang de Berre."
    ],
    "nl": [
      "De Arc is de rivier die het departement doorkruist van de Mont Aurelien tot de Etang de Berre."
    ]
  },
