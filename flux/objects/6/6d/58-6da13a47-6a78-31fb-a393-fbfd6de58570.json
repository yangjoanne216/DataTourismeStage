  "rdfs:comment": {
    "de": [
      "Das B&B befindet sich in einer großen, charaktervollen Villa in ruhiger Lage mit Pool und bietet 5 Zimmer."
    ],
    "en": [
      "The guest house is located in a large villa with character, in a quiet area with a swimming pool, and offers 5 rooms."
    ],
    "it": [
      "Il bed and breakfast si trova in un'ampia e tranquilla villa di carattere con piscina e dispone di 5 camere."
    ],
    "fr": [
      "La chambre d'hôtes se trouve dans une grande villa de caractère, au calme avec piscine, et propose 5 chambres."
    ],
    "es": [
      "El bed and breakfast está situado en una villa grande, tranquila y con carácter, con piscina, y ofrece 5 habitaciones."
    ],
    "nl": [
      "De bed and breakfast is gevestigd in een grote, rustige, karaktervolle villa met een zwembad, en beschikt over 5 kamers."
    ]
