  "rdfs:comment": {
    "de": [
      "CHALET. 60 m². ERDGESCHOSS. 3 Schlafzimmer für 6 Personen. Wohnzimmer, TV, DVD-Player, Kochecke, Mikrowelle, Waschmaschine, Geschirrspüler. Badezimmer mit Dusche. WC. Geschlossener Garten, Gartenmöbel, Grill. Terrasse. Privater Parkplatz. 7,5 km vom Ozean, 2 km vom Bassin, 200 m von den Radwegen und 700 m vom Stadtzentrum entfernt. Kurtaxe im Mietpreis inbegriffen."
    ],
    "en": [
      "CHALET. 60 m². GROUND FLOOR. 3 bedrooms for 6 persons. Living room, TV, DVD player, kitchenette, microwave, washing machine, dishwasher. Shower room. WC. Enclosed garden, garden furniture, barbecue. Terrace. Private car park. 7.5 km from the ocean, 2 km from the basin, 200 m from the cycle tracks and 700 m from the town centre. Tourist tax included in the rental price."
    ],
    "it": [
      "CHALET. 60 m². PIANO TERRA. 3 camere da letto per 6 persone. Soggiorno, TV, lettore DVD, angolo cottura, microonde, lavatrice, lavastoviglie. Bagno con doccia. WC. Giardino recintato, mobili da giardino, barbecue. Terrazza. Parcheggio privato. A 7,5 km dall'oceano, a 2 km dal Bassin, a 200 m dalle piste ciclabili e a 700 m dal centro città. Tassa di soggiorno inclusa nel prezzo di affitto."
    ],
    "fr": [
      "CHALET. 60 m². RDC. 3 chambres pour 6 personnes. Séjour, TV, lecteur DVD, coin cuisine, micro-ondes, lave-linge, lave-vaisselle. Salle d'eau. WC. Jardin clos, salon de jardin, barbecue. Terrasse. Parking privé. A 7,5 Km de l'Océan, 2 km du Bassin, 200 m des pistes cyclables et 700 m du centre ville. Taxe de séjour incluse dans le tarif de la location."
    ],
    "es": [
      "CHALET. 60 m². PLANTA BAJA. 3 habitaciones para 6 personas. Salón, TV, reproductor de DVD, cocina americana, microondas, lavadora, lavavajillas. Ducha. WC. Jardín cerrado, muebles de jardín, barbacoa. Terraza. Aparcamiento privado. A 7,5 km del mar, a 2 km del Bassin, a 200 m de los carriles bici y a 700 m del centro de la ciudad. Tasa turística incluida en el precio del alquiler."
    ],
    "nl": [
      "CHALET. 60 m². BEGANE GROND. 3 slaapkamers voor 6 personen. Woonkamer, TV, DVD-speler, kitchenette, magnetron, wasmachine, vaatwasser. Doucheruimte. WC. Omheinde tuin, tuinmeubelen, barbecue. Terras. Privé parkeerplaats. 7,5 km van de oceaan, 2 km van het Bassin, 200 m van de fietspaden en 700 m van het stadscentrum. Toeristenbelasting inbegrepen in de huurprijs."
    ]
  },
