  "rdfs:comment": {
    "de": [
      "Abschnitt A12 des GR2013. Von Miramas aus in Richtung Grans. Eine 8,9 km lange Strecke, auf der Sie das Massiv von Pont de Rhaud entdecken können."
    ],
    "en": [
      "Section A12 of the GR2013. From Miramas to Grans. A 8,9km route to discover the Pont de Rhaud massif."
    ],
    "it": [
      "Sezione A12 del GR2013. Da Miramas a Grans. Un percorso di 8,9 km alla scoperta del massiccio del Pont de Rhaud."
    ],
    "fr": [
      "Tronçon A12 du GR2013. Au départ de Miramas vers Grans. Un trajet de 8,9km pour découvrir le massif du Pont de Rhaud."
    ],
    "es": [
      "Sección A12 del GR2013. De Miramas a Grans. Un recorrido de 8,9 km para descubrir el macizo del Pont de Rhaud."
    ],
    "nl": [
      "Afdeling A12 van de GR2013. Van Miramas tot Grans. Een route van 8,9 km om het massief van de Pont de Rhaud te ontdekken."
    ]
  },
