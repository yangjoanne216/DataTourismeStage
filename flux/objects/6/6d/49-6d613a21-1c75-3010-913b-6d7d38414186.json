  "rdfs:comment": {
    "de": [
      "Für einen erholsamen Aufenthalt am Ufer der Sarthe in einer grünen Umgebung."
    ],
    "en": [
      "For a restful stay on the banks of the Sarthe in a green setting."
    ],
    "it": [
      "Per un soggiorno rilassante sulle rive della Sarthe in un ambiente verde."
    ],
    "fr": [
      "Pour un séjour reposant au bord de Sarthe dans un cadre verdoyant."
    ],
    "nl": [
      "Voor een ontspannen verblijf aan de oevers van de Sarthe in een groene omgeving."
    ],
    "es": [
      "Para una estancia relajante a orillas del Sarthe en un entorno verde."
    ]
