  "rdfs:comment": {
    "de": [
      "Leiter für den Zugang zum Plateau von Céüze. Höhe des Weges; 40 m. Der Abstieg ist verboten. Unerlässliche Ausrüstung: zugelassener Helm, Klettergurt und Leinen. Dauer des Parcours; 20 bis 30 Minuten."
    ],
    "en": [
      "Access ladder to the Céüze plateau. Height of the track; 40 m. Track not allowed to descend. Essential equipment: helmet, harness and approved lanyards. Duration of the route: 20 to 30 minutes."
    ],
    "it": [
      "Scala di accesso all'altopiano di Céüze. Altezza della pista: 40 m. Discesa vietata. Attrezzatura essenziale: casco, imbracatura e cordini omologati. Durata del corso: 20-30 minuti."
    ],
    "fr": [
      "Echelle d'accès au plateau de Céüze. Hauteur de la voie ; 40 m. Voie interdite à la descente. Matériel indispensable : casque, harnais et longes homologués. Durée du parcours ; de 20 à 30 mn."
    ],
    "nl": [
      "Toegangsladder naar het Céüze plateau. Hoogte van de baan; 40 m. Afdaling verboden. Essentiële uitrusting: helm, harnas en goedgekeurde koorden. Duur van de cursus: 20 tot 30 minuten."
    ],
    "es": [
      "Escalera de acceso a la meseta de Céüze. Altura de la pista; 40 m. Descenso prohibido. Equipo imprescindible: casco, arnés y cuerdas de seguridad homologadas. Duración del curso: de 20 a 30 minutos."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Via Ferrata de Céüse"
    ]
  },
