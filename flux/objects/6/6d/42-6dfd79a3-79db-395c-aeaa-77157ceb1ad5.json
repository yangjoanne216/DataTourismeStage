  "rdfs:comment": {
    "de": [
      "Im Geschäft Crazy Games - ZAC de Villers-Semeuse (gegenüber von Norauto). Der Spielplatz befindet sich in Aiglemont. Zahlreiche Szenarien: Indianer vs. Cowboys, Grabenkrieg, Angriff auf Burgen, Angriff auf Dörfer, Speed-Palette..."
    ],
    "en": [
      "At the Crazy Games - ZAC store in Villers-Semeuse (opposite Norauto). Playground located in Aiglemont. Many settings: Indian vs Cowboys, trench warfare, attack of castles, attack of villages; speed-pallet..."
    ],
    "it": [
      "Presso il negozio Crazy Games - ZAC de Villers-Semeuse (di fronte a Norauto). Parco giochi situato ad Aiglemont. Numerose ambientazioni: indiani contro cowboy, guerra di trincea, attacco a castelli, attacco a villaggi; velocità..."
    ],
    "fr": [
      "Au magasin Crazy Games - ZAC de Villers-Semeuse (face à Norauto). Terrain de jeux situé à Aiglemont. Nombreux décors: Indien vs Cowboys, guerre des tranchées, attaque de châteaux-forts, attaque de villages; speed-palette..."
    ],
    "es": [
      "En la tienda Crazy Games - ZAC de Villers-Semeuse (frente a Norauto). Parque infantil situado en Aiglemont. Numerosos escenarios: indios contra vaqueros, guerra de trincheras, ataque a castillos, ataque a pueblos..."
    ],
    "nl": [
      "Bij de Crazy Games winkel - ZAC de Villers-Semeuse (tegenover Norauto). Speeltuin in Aiglemont. Talloze settings: Indianen vs Cowboys, loopgravenoorlog, kastelen aanvallen, dorpen aanvallen; speed-pallet..."
    ]
