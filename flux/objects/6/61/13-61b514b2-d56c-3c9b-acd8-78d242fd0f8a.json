  "rdfs:comment": {
    "de": [
      "In Servon - Indoor-Spielpark - einzigartig in der Île-de-France - auf 2200 m2: 6m-Riesenrutsche, 9m-Klettervulkan, Motorrad-Kartbahn für Kinder, Spiel-Labyrinth, Trampolinbereich und Sprung-Airbag."
    ],
    "en": [
      "In Servon - Indoor playground - unique in Ile-de-France - on 2200 m2: giant 6m slide, 9m climbing volcano, go-kart motorbike track for children, play maze, trampoline and jumping airbag area."
    ],
    "it": [
      "A Servon - Parco giochi al coperto - unico nell'Ile-de-France - su 2200 m2: scivolo gigante di 6 m, vulcano di arrampicata di 9 m, pista di karting per bambini, labirinto di giochi, trampolini e area di salto con airbag."
    ],
    "fr": [
      "A Servon - Parc de jeux indoor - unique en Île-de-France - sur 2200 m2 : toboggan géant 6m, volcan à escalader de 9m, piste de moto karting pour enfants, labyrinthe de jeux, espace de trampolines et d'airbag de saut."
    ],
    "nl": [
      "In Servon - Indoor speeltuin - uniek in Ile-de-France - op 2200 m2: reuze glijbaan van 6m, klimvulkaan van 9m, kartbaan voor kinderen, speeldoolhof, trampolines en airbag springzone."
    ],
    "es": [
      "En Servon - Parque infantil cubierto - único en Ile-de-France - en 2200 m2: tobogán gigante de 6 m, volcán de escalada de 9 m, pista de karting para niños, laberinto de juegos, camas elásticas y zona de salto con airbag."
    ]
  },
