  "rdfs:comment": {
    "de": [
      "Gehen Sie mit Michel auf Entdeckungsreise auf der Loire. An Bord seines Bootes entdecken Sie den Fluss. Für die Sportlicheren unter Ihnen: Versuchen Sie das Abenteuer Paddle!"
    ],
    "en": [
      "Discover the Loire in the company of Michel. On board his boat, you will discover the river. For the more sporty, try the Paddle adventure!"
    ],
    "it": [
      "Scoprite la Loira in compagnia di Michel. A bordo della sua barca, scoprirete il fiume. Per i più sportivi, provate l'avventura del paddle boarding!"
    ],
    "fr": [
      "Partez à la découverte de la Loire en compagnie de Michel. A bord de son bateau, vous découvrirez le fleuve. Pour les plus sportifs, tentez l'aventure du Paddle !"
    ],
    "nl": [
      "Ontdek de Loire in gezelschap van Michel. Aan boord van zijn boot ontdekt u de rivier. Voor de sportievelingen: probeer het avontuur van paddle boarding!"
    ],
    "es": [
      "Descubra el Loira en compañía de Michel. A bordo de su barco, descubrirás el río. Para los más deportistas, pruebe la aventura del paddle boarding"
    ]
  },
