  "rdfs:comment": {
    "de": [
      "Teesalon & Second-Hand-Buchhandlung. Auswahl an Tees & Kräutertees, Kaffee, Bio-Getränken und Kuchen im englischen Stil in einem warmen & gemütlichen Rahmen."
    ],
    "en": [
      "Tearoom & second hand bookshop. Selection of teas & herbal teas, coffees, organic drinks and English style cakes in a warm & welcoming setting."
    ],
    "it": [
      "Sala da tè e libreria di seconda mano. Selezione di tè e tisane, caffè, bevande biologiche e torte all'inglese in un ambiente caldo e accogliente."
    ],
    "fr": [
      "Salon de thé & librairie d'occasion. Sélection de thés & tisanes, de cafés, de boissons bio et de gâteaux façon anglais dans un cadre chaleureux & accueillant."
    ],
    "nl": [
      "Tearoom en tweedehands boekenwinkel. Selectie van thee en kruidenthee, koffie, biologische dranken en Engels gebak in een warme en gastvrije omgeving."
    ],
    "es": [
      "Salón de té y librería de segunda mano. Selección de tés e infusiones, cafés, bebidas ecológicas y pasteles ingleses en un ambiente cálido y acogedor."
    ]
  },
