  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/455bf489-691b-3aa0-a8c9-ccc4fac41fce",
      "dc:description": {
        "de": [
          "Rührkuchen, Fleur de Sel, Macarons d'Amiens, Pavé Saint Vulfran, Picardische Dachziegel, Blaue Disteln, Glocken Saint Vulfran, Bisteu.\nTäglich von 7 Uhr bis 19.30 Uhr geöffnet."
        ],
        "en": [
          "Beaten cake, fleur de sel, macaroons d'Amiens, Saint Vulfran cobblestones, Picardes tiles, blue thistles, Saint Vulfran bells, bisteu.\nOpen every day from 7am to 7.30pm."
        ],
        "it": [
          "Torta pastellata, fleur de sel, amaretti di Amiens, sampietrini di Saint Vulfran, piastrelle della Piccardia, cardi blu, campane di Saint Vulfran, bisteu.\nAperto tutti i giorni dalle 7.00 alle 19.30."
        ],
        "fr": [
          "Gâteau battu, fleur de sel, macarons d'Amiens, pavés Saint Vulfran, tuiles picardes, chardons bleus, cloches Saint Vulfran, bisteu.\nOuvert tous les jours de 7h à 19h30."
        ],
        "es": [
          "Tarta rebozada, flor de sal, macarrones de Amiens, adoquines de Saint Vulfran, tejas de Picardía, cardos azules, campanas de Saint Vulfran, bisteu.\nAbierto todos los días de 7:00 a 19:30."
        ],
        "nl": [
          "Gebakken cake, fleur de sel, bitterkoekjes uit Amiens, kasseien uit Saint Vulfran, Picardische tegels, blauwe distels, klokken uit Saint Vulfran, bisteu.\nDagelijks geopend van 7.00 tot 19.30 uur."
        ]
      },
