  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/0442b70b-e5eb-3018-849e-e14db5c1da48",
      "dc:description": {
        "de": [
          "Lokale Produkte und Küche aus aller Welt\nWein und handwerklich gebrautes Bier\nGourmet-Desserts\n\nKonzerte: La Talvera, Djé Balèti, Trobar Dub Sound.\n\nGetränke und aufblasbare Spiele"
        ],
        "en": [
          "Local products and world cuisine\nWine and craft beer\nGourmet desserts\n\nConcerts: La Talvera, Djé Balèti,Trobar Dub Sound.\n\nRefreshment bar and inflatable games"
        ],
        "it": [
          "Prodotti locali e cucina mondiale\nVino e birra artigianale\nDolci gourmet\n\nConcerti: La Talvera, Djé Balèti, Trobar Dub Sound.\n\nBar e giochi gonfiabili"
        ],
        "fr": [
          "Produits locaux et cuisine du monde\nVin et bière artisanale \nDesserts gourmands\n\nConcerts: La Talvera, Djé Balèti,Trobar Dub Sound.\n\nBuvette et Jeux gonflables"
        ],
        "es": [
          "Productos locales y cocina internacional\nVinos y cervezas artesanales\nPostres gastronómicos\n\nConciertos: La Talvera, Djé Balèti,Trobar Dub Sound.\n\nBar de refrescos y juegos hinchables"
        ],
        "nl": [
          "Lokale producten en wereldkeuken\nWijn en ambachtelijk bier\nDesserts voor fijnproevers\n\nConcerten: La Talvera, Djé Balèti,Trobar Dub Sound.\n\nVerfrissingsbar en opblaasbare spelletjes"
        ]
      },
