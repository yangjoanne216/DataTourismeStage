  "rdfs:comment": {
    "de": [
      "Flambeau war der Vaguemestre-Hund, der den in der Festung Sollières stationierten Soldaten zwischen 1928 und 1937 die Post brachte. Er wurde für seine Verdienste ausgezeichnet."
    ],
    "en": [
      "Flambeau was the postdog that brought mail to soldiers stationed at the forts of Sollières between 1928 adn 1937. It was rewarded for its good services."
    ],
    "it": [
      "Flambeau era il cane vaguemestre che portava la posta ai soldati di stanza al forte di Sollières tra il 1928 e il 1937. È stato decorato per i suoi meriti."
    ],
    "fr": [
      "Flambeau était le chien vaguemestre qui apportait le courrier aux soldats stationnés au fortin de Sollières entre 1928 et 1937. Il a été décoré pour ses mérites."
    ],
    "nl": [
      "Flambeau was de vaguemestre hond die tussen 1928 en 1937 post bracht naar soldaten die gestationeerd waren in het fort van Sollières. Hij werd onderscheiden voor zijn verdiensten."
    ],
    "es": [
      "Flambeau era el perro vaguemestre que llevaba el correo a los soldados destinados en el fuerte de Sollières entre 1928 y 1937. Fue condecorado por sus méritos."
    ]
  },
  "rdfs:label": {
    "en": [
      "Memorial of Flambeau the postdog"
    ],
    "fr": [
      "Mémorial du chien Flambeau"
    ],
    "nl": [
      "Monument van de hond Flambeau"
    ]
  },
