      "dc:description": {
        "de": [
          "Programm :\r\n\r\n18.00 Uhr: Treffpunkt am Kulturrelais \"Vortrag von Luc Long\".\r\n19:30 Uhr: Besuch des Museums von Saintes Maries de la Mer (in Begleitung des Museumsdirektors Giorgio Spada).\r\n20:30 Uhr: Autogrammstunde und Umtrunk"
        ],
        "en": [
          "Program :\r\n\r\n6:00 pm: meeting at the cultural relay \"Conference of Luc Long\".\r\n7:30 pm: Visit of the Museum of Saintes Maries de la Mer (accompanied by the Director of the Museum Giorgio Spada).\r\n8:30 pm: book signing and a drink of friendship"
        ],
        "it": [
          "Programma :\r\n\r\n18:00: Incontro presso il relais culturale \"Conferenza di Luc Long\".\r\nore 19.30: Visita del Museo di Saintes Maries de la Mer (accompagnati dal direttore del Museo Giorgio Spada).\r\nore 20.30: sessione di firme e aperitivo per gli amici"
        ],
        "fr": [
          "Programme :\r\n\r\n18h00 : rendez-vous au relais culturel « Conférence de Luc Long ».\r\n19h30 : Visite du Musée des Saintes Maries de la Mer (accompagné par le Directeur du Musée Giorgio Spada).\r\n20h30 : séance dédicace et verre de l’amitié"
        ],
        "nl": [
          "Programma :\r\n\r\n18:00: Bijeenkomst in het culturele relais \"Conferentie van Luc Long\".\r\n19.30 uur: Bezoek aan het museum van Saintes Maries de la Mer (onder begeleiding van de directeur van het museum Giorgio Spada).\r\n20.30 uur: signeren van het boek en een drankje voor vrienden"
        ],
        "es": [
          "Programa :\r\n\r\n18:00: Reunión en el relevo cultural \"Conferencia de Luc Long\".\r\n19.30 h: Visita del Museo de Saintes Maries de la Mer (acompañado por el Director del Museo Giorgio Spada).\r\n20.30 horas: sesión de firmas y copa para los amigos"
        ]
      },
