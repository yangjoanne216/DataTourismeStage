      "dc:description": {
        "de": [
          "Handwerkliches Räuchern von Fisch über Buchenholz und Salzen mit Salz aus Guérande.\nLachs, Makrelen, Heringe, Fischrillettes .\nLabel Biologische Landwirtschaft"
        ],
        "en": [
          "Artisanal fish smoking with beech wood and salting with Guérande salt.\nSalmon, mackerel, herring, fish rillettes.\nLabel Agriculture Biologique"
        ],
        "it": [
          "Affumicatura artigianale del pesce su legno di faggio e salatura con sale di Guérande.\nSalmone, sgombro, aringa, rillettes di pesce.\nMarchio di agricoltura biologica"
        ],
        "fr": [
          "Fumage artisanal de poissons au bois de Hêtre et salage au sel de Guérande. \nSaumon, maqueraux, harengs, rillettes de poissons .\nLabel Agriculture Biologique"
        ],
        "nl": [
          "Artisanaal roken van vis op beukenhout en zouten met Guérande zout.\nZalm, makreel, haring, vis rillettes.\nLabel biologische landbouw"
        ],
        "es": [
          "Ahumado artesanal del pescado sobre madera de haya y salazón con sal de Guérande.\nSalmón, caballa, arenque, rillettes de pescado.\nEtiqueta de agricultura ecológica"
        ]
      },
