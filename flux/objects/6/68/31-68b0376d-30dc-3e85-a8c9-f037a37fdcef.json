  "rdfs:comment": {
    "de": [
      "Kornspeicher des Herrn Granièr de Monsenhor (ca. 1460): Der Kornspeicher des Bischofs, in dem die Steuereinnahmen in Form von Getreide gelagert wurden. Von außen sichtbar."
    ],
    "en": [
      "Grenier de Monsieur Granièr de Monsenhor (around 1460) Attic of the bishop.."
    ],
    "it": [
      "Granaio di Monsieur Granièr de Monsenhor (1460 ca.): granaio vescovile utilizzato per conservare le riscossioni fiscali sotto forma di grano. Visibile dall'esterno."
    ],
    "fr": [
      "Grenier de Monsieur  Granièr de Monsenhor (vers 1460): Grenier de l'évêque servant à stocker les collectes d'impôts sous forme de grains. Visible de l'extérieur."
    ],
    "es": [
      "Granero de Monsieur Granièr de Monsenhor (hacia 1460) Granero del obispo."
    ],
    "nl": [
      "Graanschuur van Monsieur Granièr de Monsenhor (ca. 1460): graanschuur van de bisschop die werd gebruikt voor de opslag van belastinginkomsten in de vorm van graan. Van buitenaf zichtbaar."
    ]
  },
