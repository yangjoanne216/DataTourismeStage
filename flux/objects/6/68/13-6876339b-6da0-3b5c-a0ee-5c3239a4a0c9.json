  "rdfs:comment": {
    "de": [
      "Für alle Outdoor-Aktivitäten: Klettersteig, Klettern, Wandern, Canyoning, Bergsteigen, Skitouren, Eisfallklettern... das Bureau des Guides de Thônes begleitet Sie."
    ],
    "en": [
      "For all outdoor activities: via ferrata, climbing, hiking, canyoning, mountaineering, ski touring, ice climbing... the Bureau des Guides de Thônes will accompany you."
    ],
    "it": [
      "Per tutte le attività all'aperto: via ferrata, arrampicata, trekking, canyoning, alpinismo, sci alpinismo, arrampicata su ghiaccio... il Bureau des Guides de Thônes vi accompagnerà."
    ],
    "fr": [
      "Pour toutes activités outdoor : via ferrata, escalade, randonnée, canyoning, alpinisme, ski de randonnée, escalade de cascade de glace... le Bureau des Guides de Thônes vous accompagne."
    ],
    "nl": [
      "Voor alle buitenactiviteiten: via ferrata, klimmen, wandelen, canyoning, bergbeklimmen, skitochten, ijsklimmen... het Bureau des Guides de Thônes begeleidt u."
    ],
    "es": [
      "Para todas las actividades al aire libre: vía ferrata, escalada, senderismo, barranquismo, alpinismo, esquí de travesía, escalada en hielo... el Bureau des Guides de Thônes le acompañará."
    ]
  },
