  "rdfs:comment": {
    "de": [
      "Winter- und Sommerrodelbahnen. Pisten mit Steilkurven oder Sprüngen. Wintersportvergnügen für die ganze Familie! Aktivität zu jeder Jahreszeit. Verpflegung vor Ort."
    ],
    "en": [
      "Winter and summer sleds. Tracks with raised turns or jumps. The joys of sliding in family! All season activity. Catering on site."
    ],
    "it": [
      "Slitte invernali ed estive. Piste con curve a gomito o salti. Il piacere di scivolare con la famiglia! Attività per tutte le stagioni. Ristorazione in loco."
    ],
    "fr": [
      "Luges d'hiver et d'été. Pistes avec virages relevés ou sauts. Les joies de la glisse en famille ! Activité toutes saison. Restauration sur place."
    ],
    "nl": [
      "Winter- en zomersleeën. Trajecten met scherpe bochten of sprongen. Het plezier van glijden met het gezin! Activiteit voor het hele seizoen. Catering ter plaatse."
    ],
    "es": [
      "Trineos de invierno y de verano. Pistas con curvas peraltadas o saltos. El placer de deslizarse en familia Actividad durante toda la temporada. Restauración in situ."
    ]
  },
