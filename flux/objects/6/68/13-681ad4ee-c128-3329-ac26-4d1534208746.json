  "rdfs:comment": {
    "de": [
      "Einige Kilometer nördlich von Montluçon, in der Nähe des Canal de Berry und des Waldes von Tronçais, empfängt Sie das Table de Reugny in einem modernen und gemütlichen Rahmen."
    ],
    "en": [
      "Located a few kilometres north of Montluçon, near the Berry canal and the Tronçais forest, La Table de Reugny welcomes you in a modern and warm setting."
    ],
    "it": [
      "Situato a pochi chilometri a nord di Montluçon, vicino al canale di Berry e alla foresta di Tronçais, la Table de Reugny vi accoglie in un ambiente moderno e caloroso."
    ],
    "fr": [
      "Situé à quelques kilomètre au nord de Montluçon, à proximité du canal de Berry et de la forêt de Tronçais, la Table de Reugny vous accueille dans un cadre moderne et chaleureux."
    ],
    "es": [
      "Situada a unos kilómetros al norte de Montluçon, cerca del canal de Berry y del bosque de Tronçais, la Table de Reugny le acoge en un entorno moderno y cálido."
    ],
    "nl": [
      "De Table de Reugny ligt enkele kilometers ten noorden van Montluçon, vlakbij het kanaal van Berry en het bos van Tronçais, en ontvangt u in een modern en warm kader."
    ]
  },
