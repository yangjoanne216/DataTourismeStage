  "rdfs:comment": {
    "de": [
      "Eiscreme, Speiseeis, Sorbets, Eisdesserts, Dessertcremes, Milch und Joghurt aus Vercors-Milch.\nProdukte vom Bauernhof, die mit der guten Milch unserer Kühe hergestellt werden."
    ],
    "en": [
      "Ice creams, sorbets, frozen desserts, cream desserts, milk and yoghurts made from Vercors milk. Farm products made with the good milk of our cows."
    ],
    "it": [
      "Gelati, sorbetti, dessert congelati, creme da dessert, latte e yogurt prodotti con il latte del Vercors.\nProdotti agricoli realizzati con il buon latte delle nostre mucche."
    ],
    "fr": [
      "Crèmes glacées, glaces, sorbets, dessert glacés, crèmes dessert, lait et yaourts au lait du Vercors.\nProduits de la ferme réalisés avec le bon lait de nos vaches."
    ],
    "nl": [
      "IJs, sorbets, diepvriesdesserts, dessertroom, melk en yoghurt gemaakt van Vercors melk.\nBoerderijproducten gemaakt van de goede melk van onze koeien."
    ],
    "es": [
      "Helados, sorbetes, postres congelados, cremas de postre, leche y yogures elaborados con leche de Vercors.\nProductos de granja elaborados con la buena leche de nuestras vacas."
    ]
  },
