  "rdfs:comment": {
    "de": [
      "24 (blaue Farbe) ? Les Genévriers / Typ: Enduro\r\nEnduro mit einem schönen Höhenunterschied, aber viel weniger technisch als sein Vorgänger (Nr. 23). Ideal, um mit dem Endurofahren zu beginnen oder sich zu Beginn des Tages zu orientieren."
    ],
    "en": [
      "24 (red) – Les Genevriers/Type: Enduro\r\nStarting point: Top of Champagny gondola \r\nAn enduro route with a great descent. Ideal for newcomers to get their bearings early in the day."
    ],
    "it": [
      "24 (colore blu) ? Les Genévriers / Tipo: Enduro\r\nUn enduro con una buona pendenza ma molto meno tecnico del suo predecessore (n°23). Ideale per introdurre i principianti all'enduro o per orientarsi all'inizio della giornata."
    ],
    "fr": [
      "24 (couleur bleue) – Les Genévriers / Type : Enduro\r\nEnduro offrant un beau dénivelé mais beaucoup moins technique que son prédécesseur (n°23). Idéal pour s’initier à l’enduro ou prendre ses marques en début de journée."
    ],
    "es": [
      "24 (color azul) ? Les Genévriers / Tipo: Enduro\r\nUn enduro con un buen desnivel pero mucho menos técnico que su predecesor (n°23). Ideal para iniciarse en el enduro o para orientarse al principio de la jornada."
    ],
    "nl": [
      "24 (blauw) - Les Genévriers / Type: Enduro\r\nEnduro met een mooi hoogteverschil maar veel minder technisch dan zijn voorganger (n°23). Ideaal om met enduro te beginnen of om aan het begin van de dag te wennen."
    ]
  },
  "rdfs:label": {
    "en": [
      "Mountain biking route – Les Genévriers"
    ],
    "fr": [
      "Itinéraire VTT Les Genévriers"
    ],
    "nl": [
      "Moutainbike route - Les Genévriers"
    ]
  },
