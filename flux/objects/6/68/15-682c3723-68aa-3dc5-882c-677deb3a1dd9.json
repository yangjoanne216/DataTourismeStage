  "rdfs:comment": {
    "de": [
      "Am Ufer der Loire befindet sich der Picknickbereich von Maimbray, der mit Tischen ausgestattet ist."
    ],
    "en": [
      "On the banks of the Loire, the picnic area of Maimbray is equipped with tables."
    ],
    "it": [
      "Sulle rive della Loira, l'area picnic di Maimbray è attrezzata con tavoli."
    ],
    "fr": [
      "En bordure de Loire, la zone de pique-nique de Maimbray est aménagée de tables."
    ],
    "nl": [
      "De picknickplaats van Maimbray ligt aan de oevers van de Loire en is uitgerust met tafels."
    ],
    "es": [
      "A orillas del Loira, la zona de picnic de Maimbray está equipada con mesas."
    ]
  },
