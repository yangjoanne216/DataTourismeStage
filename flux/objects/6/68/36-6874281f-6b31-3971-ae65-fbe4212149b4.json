  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/b89d1181-a84c-30d0-8d4e-165226153849",
      "dc:description": {
        "de": [
          "1 Schlafzimmer im Erdgeschoss, 3 Schlafzimmer im Obergeschoss. Wohnzimmer mit Küche, Wohnzimmer mit Holzofen. Eingezäunter Garten.",
          "Traditionelles Bigourdan-Haus im Herzen des Dorfes."
        ],
        "en": [
          "1 bedroom on the ground floor, 3 bedrooms upstairs. Living room with kitchen, sitting room with wood stove. Enclosed garden.",
          "Traditional Bigourdane house in the heart of the village."
        ],
        "it": [
          "1 camera da letto al piano terra, 3 camere da letto al piano superiore. Soggiorno con cucina, salotto con bruciatore a legna. Giardino recintato.",
          "Casa tradizionale di Bigourdane nel cuore del villaggio."
        ],
        "fr": [
          "1 chambre au RdC, 3 chambres à l'étage. Séjour avec cuisine, salon avec poêle à bois. Jardin clos.",
          "Maison traditionnelle bigourdane au cœur du village."
        ],
        "nl": [
          "1 slaapkamer op de begane grond, 3 slaapkamers boven. Woonkamer met keuken, salon met houtkachel. Omheinde tuin.",
          "Traditioneel Bigourdane huis in het hart van het dorp."
        ],
        "es": [
          "1 dormitorio en la planta baja, 3 dormitorios en la planta superior. Sala de estar con cocina, salón con estufa de leña. Jardín cerrado.",
          "Casa tradicional de Bigourdane en el corazón del pueblo."
        ]
      },
