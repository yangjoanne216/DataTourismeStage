  "rdfs:comment": {
    "de": [
      "Myriam empfängt Sie das ganze Jahr über in ihrer charmanten kleinen Crêperie auf dem Kirchplatz von Saint Michel."
    ],
    "en": [
      "Myriam welcomes you all year round to her charming little crêperie on the church square in Saint Michel."
    ],
    "it": [
      "Myriam vi accoglie tutto l'anno nella sua piccola e affascinante crêperie sulla piazza della chiesa di Saint Michel."
    ],
    "fr": [
      "Myriam vous accueille toute l'année dans sa charmante petite crêperie sur la place de l'Eglise de Saint Michel."
    ],
    "nl": [
      "Myriam verwelkomt je het hele jaar door in haar charmante kleine crêperie op het kerkplein in Saint Michel."
    ],
    "es": [
      "Myriam le recibe todo el año en su encantadora crepería de la plaza de la iglesia de Saint Michel."
    ]
  },
