  "rdfs:comment": {
    "de": [
      "Panoramablick auf den Grand-Ballon und den Ballon d'Alsace.\nVon St. Etienne bis Ostern ist der Bauernhof an Wochenenden und in den Schulferien auf Reservierung geöffnet."
    ],
    "en": [
      "Panoramic view of the Grand-Ballon and the Ballon d'Alsace.\nFrom St Etienne to Easter, the farm is open on weekends and school holidays by reservation."
    ],
    "it": [
      "Vista panoramica sul Grand-Ballon e sul Ballon d'Alsace.\nDa Saint Etienne a Pasqua, la fattoria è aperta nei fine settimana e nelle vacanze scolastiche su prenotazione."
    ],
    "fr": [
      "Vue panoramique sur le Grand-Ballon et le Ballon d'Alsace.\nDe la St Etienne à Pâques, la ferme est ouverte les Week-end et vacances scolaires sur réservation."
    ],
    "es": [
      "Vista panorámica del Grand-Ballon y del Ballon d'Alsace.\nDesde St Etienne hasta Semana Santa, la granja está abierta los fines de semana y las vacaciones escolares previa reserva."
    ],
    "nl": [
      "Panoramisch uitzicht op de Grand-Ballon en de Ballon d'Alsace.\nVan St Etienne tot Pasen is de boerderij in het weekend en tijdens schoolvakanties op reservering geopend."
    ]
  },
