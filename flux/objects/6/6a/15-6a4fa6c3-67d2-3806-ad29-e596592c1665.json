  "rdfs:comment": {
    "de": [
      "Sie werden Gerichte genießen, die sich je nach Jahreszeit ändern und vollständig hausgemacht sind, in einer gemütlichen Umgebung."
    ],
    "en": [
      "You will taste dishes that change with the seasons, entirely homemade, in a warm setting."
    ],
    "it": [
      "Potrete gustare piatti che cambiano con le stagioni, interamente fatti in casa, in un ambiente accogliente."
    ],
    "fr": [
      "Vous dégusterez des plats qui changent au gré des saisons, entièrement fait maison, dans un cadre chaleureux."
    ],
    "nl": [
      "U geniet van gerechten die veranderen met de seizoenen, volledig huisgemaakt, in een warm kader."
    ],
    "es": [
      "Disfrutará de platos que cambian con las estaciones, totalmente caseros, en un ambiente cálido."
