  "rdfs:comment": {
    "de": [
      "\"La Pinstrie\", nicht weit von der Mayenne und ihrem Treidelpfad entfernt, empfängt Sie in Gästezimmern mit allem Komfort zu Ihrer Entspannung."
    ],
    "en": [
      "\"La Pinstrie\", not far from the Mayenne river and its towpath, welcomes you to its comfortable guest rooms for your relaxation."
    ],
    "it": [
      "\"La Pinstrie\", non lontano dalla Mayenne e dalla sua alzaia, vi accoglie nelle sue confortevoli camere per il vostro relax."
    ],
    "fr": [
      "\"La Pinstrie\", non loin de la Mayenne et de son chemin de halage, vous accueille en chambres d'hôtes tout confort pour votre détente."
    ],
    "es": [
      "\"La Pinstrie\", no lejos del Mayenne y su camino de sirga, le da la bienvenida a sus cómodas habitaciones de huéspedes para su descanso."
    ],
    "nl": [
      "\"La Pinstrie\", niet ver van de Mayenne en zijn jaagpad, verwelkomt u in zijn comfortabele gastenkamers voor uw ontspanning."
    ]
  },
