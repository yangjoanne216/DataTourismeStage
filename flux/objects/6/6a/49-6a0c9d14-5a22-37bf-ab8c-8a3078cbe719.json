  "rdfs:comment": {
    "de": [
      "Führungen durch die Obstplantagen, die Kelter und den Keller. Beenden Sie Ihren Besuch mit einer Verkostung der Apfelweinprodukte des Bauernhofs."
    ],
    "en": [
      "Guided tours of the orchards, the wine press, the cellar... Finish your visit with a tasting of the cider products of the farm."
    ],
    "it": [
      "Visite guidate ai frutteti, al torchio, alla cantina... Concludete la visita con una degustazione dei prodotti a base di sidro della fattoria."
    ],
    "fr": [
      "Visites guidées des vergers, du pressoir, de la cave... Terminez votre visite par la dégustation des produits cidricoles de la ferme."
    ],
    "es": [
      "Visitas guiadas a los huertos, el lagar, la bodega... Termine su visita con una degustación de los productos de sidra de la granja."
    ],
    "nl": [
      "Rondleidingen door de boomgaarden, de pers, de kelder... Sluit uw bezoek af met een proeverij van de ciderproducten van de boerderij."
    ]
