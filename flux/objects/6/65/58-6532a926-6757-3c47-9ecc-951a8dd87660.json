  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/1140cb73-640f-3ecf-a612-c35eeb66f322",
      "dc:description": {
        "de": [
          "Backofen, Mikrowelle, Waschmaschine, Internetzugang, Wi-Fi, Fernseher, Zentralheizung, Dusche, separates WC, 1 Schlafzimmer, 1 Doppelbett, Klappsofa, Handtücher und Haushaltswäsche (kostenpflichtiger Service), Garten. Privater Parkplatz. Kurtaxe nicht inbegriffen. Mietnebenkosten inbegriffen."
        ],
        "en": [
          "Oven, microwave, washing machine, internet access, wifi access, television, central heating, shower, separate WC, 1 bedroom, 1 double bed, sofa bed, towels and household linen (service charge), garden. Private parking. Tourist tax not included. Rental charges included."
        ],
        "it": [
          "Forno, microonde, lavatrice, accesso internet, accesso wifi, televisione, riscaldamento centrale, doccia, WC separato, 1 camera da letto, 1 letto matrimoniale, divano letto, asciugamani e biancheria per la casa (a pagamento), giardino. Parcheggio privato. Tassa di soggiorno non inclusa. Spese di noleggio incluse."
        ],
        "fr": [
          "Four, micro-ondes, lave-linge, accès internet, accès wifi, télévision, chauffage central, douche, WC séparés, 1 chambre, 1 lit/2 pers., banquette clic-clac, linge de toilette et de maison (service payant), Jardin. Parking privé. Taxe de séjour non comprise. Charges locatives comprises."
        ],
        "nl": [
          "Oven, magnetron, wasmachine, internettoegang, wifi, televisie, centrale verwarming, douche, aparte WC, 1 slaapkamer, 1 tweepersoonsbed, slaapbank, handdoeken en huishoudlinnen (servicekosten), tuin. Privé parkeerplaats. Toeristenbelasting niet inbegrepen. Huurkosten inbegrepen."
        ],
        "es": [
          "Horno, microondas, lavadora, acceso a internet, acceso wifi, televisión, calefacción central, ducha, WC separado, 1 dormitorio, 1 cama doble, sofá cama, toallas y ropa de casa (servicio de pago), jardín. Aparcamiento privado. Tasa turística no incluida. Gastos de alquiler incluidos."
        ]
      },
