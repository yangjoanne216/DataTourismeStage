  "rdfs:comment": {
    "de": [
      "In der Nähe der Vendée können Sie hoch hinaus und den Manis-Felsen in Le Longeron zwischen Nantes und Cholet erklimmen."
    ],
    "en": [
      "Just a stone's throw from the Vendée, climb the Manis rock in Longeron between Nantes and Cholet."
    ],
    "it": [
      "A due passi dalla Vandea, scalate la roccia Manis a Le Longeron, tra Nantes e Cholet."
    ],
    "fr": [
      "A deux pas de la Vendée, prenez de la hauteur et escaladez le rocher du Manis au Longeron entre Nantes et Cholet."
    ],
    "nl": [
      "Op een steenworp afstand van de Vendée beklimt u de Manis-rots in Le Longeron tussen Nantes en Cholet."
    ],
    "es": [
      "A un paso de la Vendée, suba a la roca Manis en Le Longeron, entre Nantes y Cholet."
    ]
