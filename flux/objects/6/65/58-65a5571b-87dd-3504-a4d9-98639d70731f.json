  "rdfs:comment": {
    "de": [
      "Green'Scoot, das sich auf dem Campingplatz Marius in La Couronne befindet, vermietet 100% elektrische Motorroller, mit denen Sie Martigues, Carro, Saint-Julien les Martigues, Sausset-les-Pins, Carry-le-Rouet... besichtigen können."
    ],
    "en": [
      "Green'Scoot, based at the Marius campsite in La Couronne, offers 100% electric scooters for hire to visit Martigues, Carro, Saint-Julien les Martigues, Sausset-les-Pins, Carry-le-Rouet..."
    ],
    "it": [
      "Green'Scoot, situato nel campeggio Marius a La Couronne, offre scooter 100% elettrici a noleggio per visitare Martigues, Carro, Saint-Julien les Martigues, Sausset-les-Pins, Carry-le-Rouet..."
    ],
    "fr": [
      "Green'Scoot, installé sur le camping Marius à la Couronne, propose à la location, des scooters 100% électriques pour visiter Martigues, Carro, Saint-Julien les Martigues, Sausset-les-Pins, Carry-le-Rouet..."
    ],
    "nl": [
      "Green'Scoot, gelegen op camping Marius in La Couronne, verhuurt 100% elektrische scooters voor een bezoek aan Martigues, Carro, Saint-Julien les Martigues, Sausset-les-Pins, Carry-le-Rouet..."
    ],
    "es": [
      "Green'Scoot, con sede en el camping Marius de La Couronne, ofrece alquiler de scooters 100% eléctricos para visitar Martigues, Carro, Saint-Julien les Martigues, Sausset-les-Pins, Carry-le-Rouet..."
    ]
