  "rdfs:comment": {
    "de": [
      "Dieser Campingplatz liegt in der Gemeinde Breuillet vor den Toren von Royan. Sie werden den Charme und die Ruhe der Landschaft schätzen, nur 10 Minuten mit dem Auto von den Stränden entfernt."
    ],
    "en": [
      "Campsite located on the commune of Breuillet at the doors of Royan. You will appreciate the charm and the calm of the countryside at 10 minutes from the beaches by car."
    ],
    "it": [
      "Campeggio situato nel comune di Breuillet, alle porte di Royan. Apprezzerete il fascino e la calma della campagna a 10 minuti di auto dalle spiagge."
    ],
    "fr": [
      "Camping situé sur la commune de Breuillet aux portes de Royan. Vous apprécierez le charme et le calme de la campagne à 10 minutes des plages en voiture."
    ],
    "es": [
      "Camping situado en el municipio de Breuillet, a las puertas de Royan. Apreciará el encanto y la calma del campo a 10 minutos de las playas en coche."
    ],
    "nl": [
      "Camping gelegen in de gemeente Breuillet aan de poorten van Royan. U zult de charme en de rust van het platteland waarderen op 10 minuten rijden van de stranden."
    ]
  },
