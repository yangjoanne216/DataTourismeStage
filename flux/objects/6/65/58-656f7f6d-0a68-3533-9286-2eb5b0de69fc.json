  "rdfs:comment": {
    "de": [
      "Eingebettet im Herzen des mittelalterlichen Dorfes Miramas le Vieux, Restaurant mit hausgemachter bistronomischer Küche, die die Jahreszeiten respektiert und lokavore ist."
    ],
    "en": [
      "This restaurant is at the heart of the medieval village of Miramas-le-Vieux, serving gourmet bistro-style, homemade cuisine, that respects the seasons and gives priority to local products."
    ],
    "it": [
      "Immerso nel cuore del borgo medievale di Miramas le Vieux, questo ristorante offre una cucina bistronomica fatta in casa, nel rispetto delle stagioni e del territorio."
    ],
    "fr": [
      "Niché au cœur du village médiéval de Miramas le Vieux, restaurant proposant une cuisine bistronomique fait maison, respectueux des saisons et locavore."
    ],
    "nl": [
      "Gelegen in het hart van het middeleeuwse dorp Miramas le Vieux, biedt dit restaurant huisgemaakte bistronomische gerechten, met respect voor de seizoenen en locavore."
    ],
    "es": [
      "Enclavado en el corazón del pueblo medieval de Miramas le Vieux, este restaurante ofrece una cocina bistronómica casera, respetuosa con las estaciones y locavorecida."
    ]
  },
