  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/72bc6e3d-d5ae-32c4-88a4-9b46977f3faa",
      "dc:description": {
        "de": [
          "Terrasse mit Panoramablick.\r\nEnglisches Wohnzimmer mit Kamin.\r\n\r\nFormule 15 Euro mittags unter der Woche\r\nMenüs 25 Euro\r\nMenükarte 30 Euro\r\n\r\nReservierungen werden dringend empfohlen."
        ],
        "en": [
          "Panoramic terrace.\r\nEnglish-style lounge with fireplace.\r\n\r\nSet menu for 15 euros on weekday lunchtimes\r\nSet menu for 25 euros\r\nChoose from the menu for 30 euros\r\n\r\nBookings are strongly recommended."
        ],
        "it": [
          "Terrazza panoramica.\r\nSalone all'inglese con camino.\r\n\r\n15 euro a pranzo durante la settimana\r\nMenu 25 euro\r\nMenu carta 30 euro\r\n\r\nSi consiglia vivamente la prenotazione."
        ],
        "fr": [
          "Terrasse panoramique.\r\nSalon anglais avec cheminée.\r\n\r\nFormule 15 euros le midi en semaine\r\nMenus 25 euros \r\nMenu-carte 30 euros\r\n\r\nRéservation fortement conseillé."
        ],
        "es": [
          "Terraza panorámica.\r\nSalón inglés con chimenea.\r\n\r\nMenú de mediodía de 15 euros entre semana\r\nMenús 25 euros\r\nMenú de tarjetas 30 euros\r\n\r\nSe recomienda reservar."
        ],
        "nl": [
          "Panoramisch terras.\r\nEngelse salon met open haard.\r\n\r\n15 euro lunchmenu tijdens de week\r\nMenu's 25 euro\r\nKaartmenu 30 euro\r\n\r\nReserveren wordt sterk aangeraden."
        ]
      },
