  "rdfs:comment": {
    "de": [
      "Organisiert von den Freunden der Schule.\nZahlreiche Aussteller von Kunsthandwerkern. Weihnachtsmann von 10:30 bis 11:30 Uhr und von 14:00 bis 16:30 Uhr. Schminken und Spiele für Kinder\nSanitärpass erforderlich."
    ],
    "en": [
      "Organized by the Friends of the School.\nMany creative exhibitors. Santa Claus from 10:30 to 11:30 and from 14:00 to 16:30. Make-up and games for children\nHealth pass required."
    ],
    "it": [
      "Organizzato dagli Amici della Scuola.\nMolti espositori creativi. Babbo Natale dalle 10:30 alle 11:30 e dalle 14:00 alle 16:30. Trucco e giochi per bambini\nÈ richiesta la tessera sanitaria."
    ],
    "fr": [
      "Organisé par les Amis de l'école.\nNombreux exposants créateurs. Père Noël de 10h30 à 11h30 et de 14h à 16h30. Maquillage et jeux pour enfants. \nPass sanitaire obligatoire."
    ],
    "es": [
      "Organizado por los Amigos de la Escuela.\nMuchos expositores creativos. Papá Noel de 10:30 a 11:30 y de 14:00 a 16:30. Maquillaje y juegos para niños\nSe requiere el pase sanitario."
    ],
    "nl": [
      "Georganiseerd door de Vrienden van de School.\nVeel creatieve exposanten. De kerstman van 10:30 tot 11:30 uur en van 14:00 tot 16:30 uur. Schminken en spelletjes voor kinderen\nGezondheidspas vereist."
    ]
  },
