  "rdfs:comment": {
    "de": [
      "Abschnitt A18 des GR2013. Von der Corniche de Rognac in Richtung Gare Aix TGV. Eine 13,4 km lange Strecke, auf der Sie das Massif de l'Arbois entdecken können."
    ],
    "en": [
      "Section A18 of the GR2013. From the Corniche de Rognac to Aix TGV station. A 13,4km route to discover the Arbois massif."
    ],
    "it": [
      "Sezione A18 del GR2013. Dalla Corniche de Rognac alla stazione TGV di Aix. Un percorso di 13,4 km alla scoperta del massiccio dell'Arbois."
    ],
    "fr": [
      "Tronçon A18 du GR2013. Au départ de la Corniche de Rognac vers Gare Aix TGV. Un trajet de 13,4km pour découvrir le massif de l'Arbois."
    ],
    "es": [
      "Sección A18 del GR2013. De la Corniche de Rognac a la estación de TGV de Aix. Un recorrido de 13,4 km para descubrir el macizo de Arbois."
    ],
    "nl": [
      "Afdeling A18 van de GR2013. Van de Corniche de Rognac naar het TGV-station van Aix. Een route van 13,4 km om het massief van Arbois te ontdekken."
    ]
