      "dc:description": {
        "de": [
          "Bress'fest\nRock, Hard Rock, Heavy Metal Festival.\n4 Konzerte: 16:00 Amonyt, 18:00 Undercover New skin, 20:00 Flem's, 22:00 MRV.\n2 Außenbühnen, 1 Innenbühne.\nBar und Verpflegung vor Ort.\nAussteller, Erste-Hilfe-Station, Parkplatz für Motorräder/Autos"
        ],
        "en": [
          "Bress'fest\nRock, Hard Rock, Heavy Metal festival.\n4 concerts: 4pm Amonyt, 6pm Undercover New skin, 8pm Flem's, 10pm MRV\n2 outdoor stages, 1 indoor stage\nBar and catering on site\nExhibitors, first aid post, parking for motorcycles/caravans"
        ],
        "it": [
          "Bress'fest\nFestival di rock, hard rock e heavy metal.\n4 concerti: ore 16:00 Amonyt, ore 18:00 Undercover New skin, ore 20:00 Flem's, ore 22:00 MRV\n2 palchi all'aperto, 1 palco al coperto\nBar e ristorazione in loco\nEspositori, postazione di pronto soccorso, parcheggio per moto/caravan"
        ],
        "fr": [
          "Bress'fest\nFestival Rock, Hard Rock, Heavy Metal.\n4 concerts : 16h00 Amonyt, 18h00 Undercover New skin, 20h00 Flem's,  22h00 MRV\n2 scènes extérieures, 1 scène intérieure\nBar et restauration sur place\nExposants, poste de secours, parking pour motos/autos"
        ],
        "nl": [
          "Bress'fest\nRock, Hard Rock, Heavy Metal festival.\n4 concerten: 16u Amonyt, 18u Undercover New skin, 20u Flem's, 22u MRV\n2 buitenpodia, 1 binnenpodium\nBar en catering ter plaatse\nStandhouders, EHBO-post, parking voor motoren/caravans"
        ],
        "es": [
          "Bress'fest\nFestival de Rock, Hard Rock, Heavy Metal.\n4 conciertos: 16h Amonyt, 18h Undercover New skin, 20h Flem's, 22h MRV\n2 escenarios exteriores, 1 escenario interior\nBar y catering in situ\nExpositores, puesto de primeros auxilios, aparcamiento para motos/caravanas"
        ]
      },
