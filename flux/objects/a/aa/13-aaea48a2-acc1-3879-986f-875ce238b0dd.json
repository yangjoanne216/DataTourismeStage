  "rdfs:comment": {
    "de": [
      "Das auf den Hügeln in nur 2.5 km Entfernung vom mittelalterlichen Dorf Saint-Paul-de-Vence eingebettete Pop-Art-Hotel verspricht einen ruhigen Aufenthalt."
    ],
    "en": [
      "Nestling at the heart of the hills around Vence, only 2.5 km from the mediaeval village of Saint-Paul-de-Vence, the pop-art hotel offers a moment of peace."
    ],
    "it": [
      "Situato nel cuore delle colline attorno a Vence, a soli 2.5 km dal borgo medievale di Saint-Paul-de-Vence, l'hotel in stile pop art promette momenti di quiete."
    ],
    "fr": [
      "Niché au coeur des collines vençoises, à 2.5km seulement du village médiéval de Saint-Paul-de-Vence, l'hôtel pop art promet un moment de quiétude."
    ],
    "nl": [
      "Genesteld in het hart van de Venetiaanse heuvels, op slechts 2,5 km van het middeleeuwse dorp Saint-Paul-de-Vence, belooft het pop-art hotel een moment van rust."
    ],
    "es": [
      "Enclavado en el corazón de las colinas venecianas, a sólo 2,5 km del pueblo medieval de Saint-Paul-de-Vence, este hotel pop art promete un momento de tranquilidad."
    ]
  },
