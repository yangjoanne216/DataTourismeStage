  "rdfs:comment": {
    "de": [
      "Der Point Accueil Jeunes (PAJ) ist ein geselliger Ort am Eingang des Dorfes Caniac-du-Causse. Sie haben Zugang zu einem Parkplatz, Toiletten und einem Picknickbereich mit Tischen und einem gestellten Grill."
    ],
    "en": [
      "The Point Accueil Jeunes (PAJ) is a friendly place located at the entrance of the village of Caniac-du-Causse. You have access to a parking lot, toilets and a picnic area with tables and barbecue."
    ],
    "it": [
      "Il Point Accueil Jeunes (PAJ) è un luogo accogliente situato all'ingresso del villaggio di Caniac-du-Causse. È possibile accedere a un parcheggio, ai servizi igienici e a un'area picnic con tavoli e barbecue."
    ],
    "fr": [
      "Le Point Accueil Jeunes (PAJ) est un lieu convivial situé à l'entrée du village de Caniac-du-Causse. Vous avez accès à un stationnement, des toilettes et un espace pique-nique avec tables et barbecue bâti."
    ],
    "nl": [
      "Het Jongeren Welkom Centrum (PAJ) is een gezellige plek bij de ingang van het dorp Caniac-du-Causse. U heeft toegang tot een parkeerplaats, toiletten en een picknickplaats met tafels en barbecue."
    ],
    "es": [
      "El Centro de Acogida para Jóvenes (PAJ) es un lugar acogedor situado a la entrada del pueblo de Caniac-du-Causse. Tiene acceso a un aparcamiento, aseos y una zona de picnic con mesas y barbacoa."
    ]
  },
