  "rdfs:comment": {
    "de": [
      "Ein Ort der Entspannung und Erholung, umgeben von einer reinen und zarten Atmosphäre, wo man sich entspannen und die Natur bewundern kann."
    ],
    "en": [
      "A space of relaxation surrounded by a pure and delicate atmosphere where you can relax and admire the nature."
    ],
    "it": [
      "Un luogo per rilassarsi e distendersi, circondato da un'atmosfera pura e delicata dove rilassarsi e godersi la natura."
    ],
    "fr": [
      "Un espace de détente et de relaxation entouré d'une atmosphère pure et délicat ou on peu se détendre en admirant la nature."
    ],
    "es": [
      "Un lugar para relajarse y desconectar rodeado de un ambiente puro y delicado en el que relajarse y disfrutar de la naturaleza."
    ],
    "nl": [
      "Een plek om te ontspannen en tot rust te komen, omgeven door een zuivere en delicate sfeer waar u kunt ontspannen en genieten van de natuur."
    ]
  },
