  "rdfs:comment": {
    "de": [
      "Electric bicycle rental in the municipality of Moustiers-Sainte-Marie. Open every day in season, located at the exit of the village direction Castellane. Rent high-end VAE on site or at your place of accommodation."
    ],
    "en": [
      "Electric bicycle rental in the municipality of Moustiers-Sainte-Marie. Open every day in season, located at the exit of the village direction Castellane. Rent high-end VAE on site or at your place of accommodation."
