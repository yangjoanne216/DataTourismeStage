  "rdfs:comment": {
    "de": [
      "ALLES IST GUT, ALLES IST HAUSGEMACHT!\nSandwiches, warme Gerichte, Salate, Pokes, Desserts, süße und salzige Kuchen, Leckereien: Muffins, Cakes, Cookies, Brownies und Müsliriegel."
    ],
    "en": [
      "EVERYTHING IS GOOD, EVERYTHING IS HOMEMADE!\nSandwiches, hot dishes, salads, pokes, desserts, sweet and savory pies, sweets: muffins, cakes, cookies, brownies and cereal bars."
    ],
    "it": [
      "TUTTO È BUONO, TUTTO È FATTO IN CASA!\nPanini, piatti caldi, insalate, dolci, torte dolci e salate, dolci: muffin, torte, biscotti, brownies e barrette ai cereali."
    ],
    "fr": [
      "TOUT EST BON, TOUT EST FAIT MAISON !\nSandwichs, plats chauds, salades, pokes, desserts, tartes salées et sucrées, gourmandises : muffins, cakes, cookies, brownies et barres de céréales."
    ],
    "es": [
      "TODO ES BUENO, ¡TODO ES CASERO!\nBocadillos, platos calientes, ensaladas, pokes, postres, tartas dulces y saladas, dulces: magdalenas, pasteles, galletas, brownies y barritas de cereales."
    ],
    "nl": [
      "ALLES IS GOED, ALLES IS HUISGEMAAKT!\nBoterhammen, warme gerechten, salades, poffertjes, desserts, zoete en hartige taarten, zoetigheden: muffins, cakes, koekjes, brownies en mueslirepen."
    ]
  },
  "rdfs:label": {
    "fr": [
      "RESTAURANT PICNICS WAVES ACTISUD"
    ]
  },
