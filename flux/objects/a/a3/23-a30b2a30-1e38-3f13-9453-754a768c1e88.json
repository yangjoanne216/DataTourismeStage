  "rdfs:comment": {
    "de": [
      "18:30 Uhr: Konzert von Tidy Mess. Getränke und Essen vom Comité des Fêtes. 22.30 Uhr: Vorführung des Films \"Maison de Retraite\". Bei Regen Ausweichmöglichkeit in der Mehrzweckhalle."
    ],
    "en": [
      "6.30pm: Concert by Tidy Mess. Refreshments and food provided by the Comité des Fêtes. 10:30pm: Screening of the film \"Maison de Retraite\". In case of rain, retreat to the Salle polyvalente."
    ],
    "it": [
      "18.30: Concerto dei Tidy Mess. Rinfresco e catering a cura del Comité des Fêtes. 22.30: Proiezione del film \"Maison de Retraite\". In caso di pioggia, ritirarsi nella sala polivalente."
    ],
    "fr": [
      "18h30 : Concert de Tidy Mess. Buvette et restauration du Comité des Fêtes. 22h30 : Projection du film \"Maison de Retraite\". En cas de pluie, repli à la salle polyvalente."
    ],
    "nl": [
      "18.30 uur: Concert door Tidy Mess. Versnaperingen en eten verzorgd door het Comité des Fêtes. 22.30 uur: Vertoning van de film \"Maison de Retraite\". Bij regen kun je je terugtrekken in de polyvalente zaal."
    ],
    "es": [
      "18.30 h: Concierto de Tidy Mess. Refrescos y comida a cargo del Comité des Fêtes. 22.30 h: Proyección de la película \"Maison de Retraite\". En caso de lluvia, retirada a la sala polivalente."
    ]
