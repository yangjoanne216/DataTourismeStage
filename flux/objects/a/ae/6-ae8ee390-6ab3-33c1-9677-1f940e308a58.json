      "dc:description": {
        "de": [
          "Kleine Bäckerei, die in den frühen Morgenstunden geöffnet ist: Bäckerei, Konditorei, Pressestelle.\nVerpassen Sie nicht den köstlichen altmodischen Haushaltskuchen!"
        ],
        "en": [
          "Small bakery open at dawn: bakery, pastry shop, press shop.\nDon't miss the delicious old-fashioned household cake!"
        ],
        "it": [
          "Piccola pasticceria aperta all'alba: panetteria, pasticceria, stireria.\nNon perdetevi la deliziosa torta casalinga all'antica!"
        ],
        "fr": [
          "Petite boulangerie ouverte aux aurores : boulangerie, pâtisserie, point presse.\nNe ratez pas le délicieux gâteau de ménage à l'ancienne !"
        ],
        "es": [
          "Pequeña panadería abierta al amanecer: panadería, pastelería, tienda de prensa.\nNo te pierdas el delicioso pastel casero de toda la vida"
        ],
        "nl": [
          "Kleine bakkerij geopend bij dageraad: bakkerij, banketbakkerij, perserij.\nMis de heerlijke ouderwetse huishoudelijke taart niet!"
        ]
      },
