  "rdfs:comment": {
    "de": [
      "Nur 20 Minuten von Toulouse und Carcassonne entfernt, in einer grünen, ruhigen Umgebung, können Sie sich in aller Ruhe am Ufer des Canal du Midi erholen."
    ],
    "en": [
      "Just 20 minutes from Toulouse and Carcassonne, in a green and quiet setting, come and rest in complete peace and quiet on the banks of the Canal du Midi."
    ],
    "it": [
      "A soli 20 minuti da Tolosa e Carcassonne, in un ambiente verde e tranquillo, venite a riposare in pace sulle rive del Canal du Midi."
    ],
    "fr": [
      "A 20 mn à peine de Toulouse et de Carcassonne, dans un cadre verdoyant, au calme, venez vous reposer en toute quiétude sur le bord du Canal du Midi."
    ],
    "nl": [
      "Op 20 minuten van Toulouse en Carcassonne, in een groene en rustige omgeving, kom tot rust aan de oevers van het Canal du Midi."
    ],
    "es": [
      "A 20 minutos de Toulouse y Carcassonne, en un entorno verde y tranquilo, venga a relajarse a orillas del Canal du Midi."
    ]
  },
