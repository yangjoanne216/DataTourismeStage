  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/aede1414-806b-39a0-a765-4d7ab08b3da4",
      "dc:description": {
        "de": [
          "Restaurant in der Nähe der Abtei und des Gestüts. \nHausgemachte Küche und familiäre Atmosphäre (Terrasse im Sommer verfügbar).\nHausgemachte italienische Küche (Lasagne, Pizza, Tiramisu).\nGroße Auswahl an Pizzen vor Ort und zum Mitnehmen."
        ],
        "en": [
          "Restaurant located near the Abbey and the Stud Farm. \nHome-made cuisine and family atmosphere (terrace available in summer).\nHome-made Italian cuisine (lasagne, pizza, tiramisu).\nLarge choice of pizzas on the spot and to take away."
        ],
        "it": [
          "Ristorante situato vicino all'Abbazia e alla Scuderia.\nCucina casalinga e atmosfera familiare (terrazza disponibile in estate).\nCucina italiana fatta in casa (lasagne, pizza, tiramisù).\nAmpia scelta di pizze sul posto e da asporto."
        ],
        "fr": [
          "Restaurant situé à proximité de l'Abbaye et du Haras. \nCuisine faite maison et ambiance familiale (terrasse disponible l'été).\nCuisine italienne maison (lasagne, pizza, tiramisu).\nGrand choix de pizzas sur place et à emporter."
        ],
        "es": [
          "Restaurante situado cerca de la Abadía y de la Yeguada.\nCocina casera y ambiente familiar (terraza disponible en verano).\nCocina italiana casera (lasaña, pizza, tiramisú).\nGran variedad de pizzas para llevar y para comer in situ."
        ],
        "nl": [
          "Restaurant in de buurt van de abdij en de stoeterij.\nHuisgemaakte keuken en familiale sfeer (terras beschikbaar in de zomer).\nHuisgemaakte Italiaanse keuken (lasagne, pizza, tiramisu).\nGrote keuze aan pizza's ter plaatse en om mee te nemen."
        ]
      },
