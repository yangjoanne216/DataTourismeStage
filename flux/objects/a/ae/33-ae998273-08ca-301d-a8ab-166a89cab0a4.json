  "rdfs:comment": {
    "de": [
      "Machen Sie eine Pause vor den Toren der Gascogne: Gastronomie, Entspannung, Ruhe und Gelassenheit - es gibt nichts Besseres, um neu zu starten."
    ],
    "en": [
      "Take a break at the gates of Gascony: gastronomy, relaxation, calm and serenity, nothing better to start afresh."
    ],
    "it": [
      "Concedetevi una pausa alle porte della Guascogna: gastronomia, relax, calma e serenità, non c'è niente di meglio per ripartire."
    ],
    "fr": [
      "Faites une pause aux portes de la Gascogne : gastronomie, détente, calme et sérénité, rien de tel pour repartir à neuf."
    ],
    "es": [
      "Tómese un descanso en la puerta de Gascuña: gastronomía, relajación, calma y serenidad, no hay nada como esto para empezar de nuevo."
    ],
    "nl": [
      "Neem een pauze aan de poort van de Gascogne: gastronomie, ontspanning, rust en sereniteit, er gaat niets boven een nieuwe start."
    ]
  },
