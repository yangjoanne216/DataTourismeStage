  "rdfs:comment": {
    "de": [
      "Auf dem Sportplatz unter dem Thinat-Deck können Sie Ihre sportlichen Fähigkeiten trainieren."
    ],
    "en": [
      "The sports area under the Thinat deck is where you can practice your sports skills."
    ],
    "it": [
      "Nell'area sportiva sotto il ponte Thinat è possibile esercitare le proprie abilità sportive."
    ],
    "fr": [
      "C'est sur l'aire de sport aménagée sous le pont Thinat que vous exercerez vos aptitudes sportives."
    ],
    "nl": [
      "Op het sportterrein onder de Thinat brug kunt u uw sportieve vaardigheden oefenen."
    ],
    "es": [
      "En la zona deportiva situada bajo el puente Thinat podrá ejercitar sus habilidades deportivas."
    ]
  },
