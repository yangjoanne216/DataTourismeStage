  "rdfs:comment": {
    "de": [
      "Locavore\"-Restaurant, das mit lokalen Rohstoffen aus einem Umkreis von maximal 200 km arbeitet. Monatlich wechselnde Speisekarte und wöchentlich wechselndes Marktmenü am Mittag."
    ],
    "en": [
      "Locavore\" restaurant, which works with local raw materials, within a radius of 200 km. The menu changes every month and the lunchtime market menu changes every week."
    ],
    "it": [
      "Ristorante \"Locavore\", che lavora con materie prime locali, in un raggio di 200 km. Il menu cambia ogni mese e il menu del mercato a pranzo cambia ogni settimana."
    ],
    "fr": [
      "Restaurant \"locavore\", qui travaille à partir de matières premières locales, dans un rayon de 200 km maximum. Carte changeant tous les mois et menu du marché le midi différent chaque semaine."
    ],
    "es": [
      "Restaurante \"locavore\", que trabaja con materias primas locales, en un radio de 200 km. El menú cambia cada mes y el del mercado de mediodía, cada semana."
    ],
    "nl": [
      "Locavore\" restaurant, dat werkt met lokale grondstoffen, binnen een straal van 200 km. Het menu verandert elke maand en het menu van de lunchmarkt verandert elke week."
    ]
  },
