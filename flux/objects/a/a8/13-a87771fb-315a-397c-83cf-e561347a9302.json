  "rdfs:comment": {
    "de": [
      "Das Relais des Adrets empfängt Sie zu Ihren Mahlzeiten mit einer Speisekarte voller frischer und hausgemachter Gerichte. Eine traditionelle Küche mit mediterranen Akzenten und viel Finesse."
    ],
    "en": [
      "Le Relais des Adrets welcomes you for your meals, with a menu of fresh and homemade dishes. A traditional cuisine with Mediterranean accents, all with finesse."
    ],
    "it": [
      "Il Relais des Adrets vi accoglie per i vostri pasti, con un menu di piatti freschi e fatti in casa. Una cucina tradizionale con accenti mediterranei, il tutto con finezza."
    ],
    "fr": [
      "Le Relais des Adrets vous accueille pour vos repas, avec une carte de plats frais et faits maison. Une cuisine traditionnelle aux accents méditerranéens, tout en finesse."
    ],
    "nl": [
      "Het Relais des Adrets ontvangt u voor uw maaltijden, met een menu van verse en huisgemaakte gerechten. Een traditionele keuken met mediterrane accenten, alles met finesse."
    ],
    "es": [
      "El Relais des Adrets le acoge para sus comidas, con una carta de platos frescos y caseros. Una cocina tradicional con acentos mediterráneos, todo con delicadeza."
    ]
  },
  "rdfs:label": {
