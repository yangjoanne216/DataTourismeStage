  "rdfs:comment": {
    "de": [
      "Die Snackbar l'Impérial befindet sich am Ufer des Gemeindegewässers und eines Fischteichs in Roumazières-Loubert. Die Snackbar ist durchgehend von 11:00 bis 22:00 Uhr geöffnet."
    ],
    "en": [
      "The snack bar l'Impérial is located at the edge of the communal lake and fishing pond of Roumazières-Loubert. The snack bar is open non-stop from 11am to 10pm."
    ],
    "it": [
      "Lo snack bar l'Impérial si trova ai margini del lago comunale e del laghetto di pesca di Roumazières-Loubert. Lo snack bar è aperto ininterrottamente dalle 11:00 alle 22:00."
    ],
    "fr": [
      "Le snack l'Impérial se trouve au bord du plan d'eau communal et d'un étang de pêche de Roumazières-Loubert. Le snack est ouvert sans interruption de 11h à 22h."
    ],
    "es": [
      "El snack bar l'Impérial está situado al borde del lago y estanque de pesca comunitario de Roumazières-Loubert. El bar de aperitivos está abierto ininterrumpidamente de 11 a 22 horas."
    ],
    "nl": [
      "De snackbar l'Impérial ligt aan de rand van het gemeenschappelijke meer en de visvijver van Roumazières-Loubert. De snackbar is non-stop geopend van 11.00 tot 22.00 uur."
    ]
  },
  "rdfs:label": {
