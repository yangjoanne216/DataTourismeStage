  "rdfs:comment": {
    "de": [
      "Espace sport de la Turmelière in Liré, 40 km von Nantes entfernt auf dem Weg nach Angers. Turnhalle, Kletterwand, Swin Golf, Orientierungslauf, Badminton, Tennis, Fahrrad."
    ],
    "en": [
      "Espace sport de la Turmelière in Liré, 40 km from Nantes on the road to Angers. Gymnasium, climbing wall, swin golf, orientation course, badminton, tennis, cycling."
    ],
    "it": [
      "Area sportiva La Turmelière a Liré, a 40 km da Nantes sulla strada per Angers. Palestra, parete di arrampicata, swin golf, campo di orienteering, badminton, tennis, ciclismo."
    ],
    "fr": [
      "Espace sport de la Turmelière à Liré, à 40 km de Nantes sur la route d'Angers. Gymnase, mur d'escalade, swin golf, parcours d'orientation, badminton, tennis, vélo."
    ],
    "es": [
      "Zona deportiva de La Turmelière en Liré, a 40 km de Nantes en la carretera de Angers. Gimnasio, muro de escalada, swin golf, curso de orientación, bádminton, tenis, ciclismo."
    ],
    "nl": [
      "La Turmelière sportterrein in Liré, 40 km van Nantes op de weg naar Angers. Gymnasium, klimmuur, swin golf, oriëntatiebaan, badminton, tennis, fietsen."
    ]
  },
