  "rdfs:comment": {
    "de": [
      "Marina Comparato, Mezzosopran, lädt uns zu einer europäischen Reise mit Mozart, Schubert, Gounod... ein. Klavierbegleitung durch Gaël Tardivel.\nFür alle Zuschauer zugänglich. Getränkestand."
    ],
    "en": [
      "Marina Comparato, mezzo-soprano, offers us a European journey with Mozart, Schubert, Gounod... Piano accompaniment by Gaël Tardivel.\nAll public. Refreshment bar."
    ],
    "it": [
      "Marina Comparato, mezzosoprano, ci propone un viaggio europeo con Mozart, Schubert, Gounod... Accompagnamento al pianoforte di Gaël Tardivel.\nTutti i pubblici. Bar per il ristoro."
    ],
    "fr": [
      "Marina Comparato, mezzo-soprano, nous propose un voyage européen avec Mozart, Schubert, Gounod... Accompagnement piano par Gaël Tardivel.\nTout public. Buvette."
    ],
    "es": [
      "Marina Comparato, mezzosoprano, nos ofrece un viaje europeo con Mozart, Schubert, Gounod... Acompañamiento de piano por Gaël Tardivel.\nPara el público en general. Bar de refrescos."
    ],
    "nl": [
      "Marina Comparato, mezzosopraan, biedt ons een Europese reis met Mozart, Schubert, Gounod... Pianobegeleiding door Gaël Tardivel.\nAlle doelgroepen. Versnaperingsbar."
    ]
  },
