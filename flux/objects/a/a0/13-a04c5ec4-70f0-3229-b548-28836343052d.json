  "rdfs:comment": {
    "de": [
      "Von Dienstag bis Freitag mittags und abends geöffnet. Wir bieten alle Arten von hausgemachten traditionellen Gerichten an."
    ],
    "en": [
      "Open for lunchs and diners, from tuesday to friday. All kinds of typical meals.\r\nduring the Covid 19 pandemic,  take-out meals only, from monday to friday from 11:00 am to 14:00 pm."
    ],
    "it": [
      "Aperti a pranzo e a cena dal martedì al venerdì, offriamo tutti i tipi di piatti tradizionali fatti in casa."
    ],
    "fr": [
      "Ouvert midis et soirs du mardi au vendredi, nous proposons toutes sortes de plats traditionnels faits maison."
    ],
    "nl": [
      "Geopend voor lunch en diner van dinsdag tot vrijdag, bieden wij allerlei traditionele huisgemaakte gerechten."
    ],
    "es": [
      "Abierto para el almuerzo y la cena de martes a viernes, ofrecemos todo tipo de platos caseros tradicionales."
    ]
  },
