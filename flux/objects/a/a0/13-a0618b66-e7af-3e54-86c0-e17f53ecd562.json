  "rdfs:comment": {
    "de": [
      "Entdecken Sie die Etangs du Bord de Loire, die sich in der Ebene von Emblavez im Département Haute-Loire befinden. Ein außergewöhnliches Gebiet von 18 Hektar, in dem Ihnen zahlreiche Aktivitäten im Freien angeboten werden."
    ],
    "en": [
      "Located on the plain of Emblavez, in Haute-Loire, discover the Etangs du Bord de Loire. An exceptional domain of 18 hectares where many outdoor activities are proposed to you."
    ],
    "it": [
      "Situato nella piana di Emblavez, nell'Alta Loira, scoprite gli Etangs du Bord de Loire. Un'eccezionale tenuta di 18 ettari che offre numerose attività all'aperto."
    ],
    "fr": [
      "Situés sur la plaine de l'Emblavez, en Haute-Loire, découvrez les Etangs du Bord de Loire. Un domaine exceptionnel de 18 hectares où de nombreuses activités plein air vous sont proposées."
    ],
    "es": [
      "Situado en la llanura de Emblavez, en el Alto Loira, descubra los Etangs du Bord de Loire. Una finca excepcional de 18 hectáreas donde se ofrecen numerosas actividades al aire libre."
    ],
    "nl": [
      "Op de vlakte van Emblavez, in de Haute-Loire, ontdekt u de Etangs du Bord de Loire. Een uitzonderlijk landgoed van 18 hectare waar vele buitenactiviteiten worden aangeboden."
    ]
  },
  "rdfs:label": {
