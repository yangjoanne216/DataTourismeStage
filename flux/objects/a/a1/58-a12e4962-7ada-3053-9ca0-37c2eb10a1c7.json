  "rdfs:comment": {
    "de": [
      "Der Huveaune fließt durch den östlichen Teil des Départements Bouches-du-Rhône und mündet in Marseille ins Meer."
    ],
    "en": [
      "The Huveaune river crosses the eastern part of the Bouches-du-Rhône, and flows into the sea in Marseille."
    ],
    "it": [
      "L'Huveaune attraversa la parte orientale delle Bouches-du-Rhône e sfocia in mare a Marsiglia."
    ],
    "fr": [
      "L'Huveaune traverse la partie Est des Bouches-du-Rhône, et se jette dans la mer à Marseille."
    ],
    "es": [
      "El Huveaune atraviesa la parte oriental de Bouches-du-Rhône y desemboca en el mar en Marsella."
    ],
    "nl": [
      "De Huveaune stroomt door het oostelijke deel van de Bouches-du-Rhône, en mondt bij Marseille uit in zee."
    ]
  },
