  "rdfs:comment": {
    "de": [
      "Ein Familienstall in der Nähe von Fontainebleau, der alle Arten von Reitern empfängt, von Anfängerkursen über wöchentliche Kurse bis hin zu Lehrgängen und Wettbewerben!"
    ],
    "en": [
      "A family stable near Fontainebleau, which welcomes all types of riders, from initiation, to weekly lessons, through training courses and competitions!"
    ],
    "it": [
      "Una scuderia familiare vicino a Fontainebleau, che accoglie tutti i tipi di cavalieri, dai principianti alle lezioni settimanali, passando per i corsi e le gare!"
    ],
    "fr": [
      "Une écurie familiale proche de Fontainebleau, qui accueille tous types de cavaliers, de l'initiation, aux cours hebdomadaires, en passant par les stages et les concours !"
    ],
    "es": [
      "Una cuadra familiar cerca de Fontainebleau, que acoge a todo tipo de jinetes, desde principiantes hasta clases semanales, así como cursos de formación y competiciones"
    ],
    "nl": [
      "Een familiestal in de buurt van Fontainebleau, waar alle soorten ruiters welkom zijn, van beginners tot wekelijkse lessen, inclusief cursussen en wedstrijden!"
    ]
  },
