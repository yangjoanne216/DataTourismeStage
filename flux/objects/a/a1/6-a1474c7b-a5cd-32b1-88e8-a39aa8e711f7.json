  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/ede6da6e-7ef3-39ed-a0bf-375ea82559c1",
      "dc:description": {
        "de": [
          "Traditionelle und regionale Küche. \nSommerspezialitäten: Carpaccio vom Charolais-Rind, Barschfilet, Rinderfilet mit Morcheln, Melone mit Macvin.\nWinterspezialitäten: Fondue, Raclette, warmer Mont d'Or."
        ],
        "en": [
          "Traditional and regional cuisine. \nSummer specialities: Charolais beef carpaccio, perch fillet, beef fillet with morels, melon with macvin.\nWinter specialities: fondue, raclette, hot Mont d'Or."
        ],
        "it": [
          "Cucina tradizionale e regionale.\nSpecialità estive: carpaccio di manzo Charolais, filetto di pesce persico, filetto di manzo con spugnole, melone con macvin.\nSpecialità invernali: fonduta, raclette, Mont d'Or caldo."
        ],
        "fr": [
          "Cuisine traditionnelle et régionale. \nSpécialités été : carpaccio de bœuf charolais, filet de perche, filet de bœuf aux morilles, melon au macvin.\nSpécialités hiver : fondue, raclette, Mont d'Or chaud."
        ],
        "nl": [
          "Traditionele en regionale keuken.\nZomerspecialiteiten: carpaccio van Charolais rundvlees, baarsfilet, runderfilet met morilles, meloen met macvin.\nWinterspecialiteiten: fondue, raclette, warme Mont d'Or."
        ],
        "es": [
          "Cocina tradicional y regional.\nEspecialidades de verano: carpaccio de buey Charolais, filete de perca, filete de buey con colmenillas, melón con macvin.\nEspecialidades de invierno: fondue, raclette, Mont d'Or caliente."
        ]
      },
