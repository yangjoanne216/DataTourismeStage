  "rdfs:comment": {
    "de": [
      "Bergrestaurant mit Self-Bar, Verkauf zum Mitnehmen, Savoyer Spezialitäten und traditioneller Küche. \"La Roche de Mio\" befindet sich auf den Pisten, im Herzen des Paradiski®-Gebiets und an der Ankunft der Seilbahn."
    ],
    "en": [
      "High altitude restaurant with Self-Bar, takeaway, Savoyard specialities and traditional cuisine, \"La Roche de Mio\" is located on the slopes, at the heart of the Paradiski® ski area at the cable car station."
    ],
    "it": [
      "Ristorante d'alta quota con self-bar, take-away, specialità savoiarde e cucina tradizionale, \"La Roche de Mio\" si trova sulle piste, proprio nel cuore del dominio Paradiski® e all'arrivo della funivia."
    ],
    "fr": [
      "Restaurant d'altitude avec self-bar, vente à emporter, spécialités Savoyardes et cuisine traditionnelle, \"La Roche de Mio\" est située sur les pistes, en plein cœur du domaine Paradiski® et à l'arrivée de la télécabine."
    ],
    "es": [
      "Restaurante de altura con auto-bar, comida para llevar, especialidades saboyanas y cocina tradicional, \"La Roche de Mio\" se encuentra en las pistas, en pleno corazón del dominio Paradiski® y a la llegada del teleférico."
    ],
    "nl": [
      "Bergrestaurant met bar self-service, take-away, specialiteiten van Savoie en tranditionele keuken.  \"La Roche de Mio\" is gelegen op de pistes, in het hart van het skidomein Paradiski® en aan de aankomst van de gondelbaan."
    ]
  },
