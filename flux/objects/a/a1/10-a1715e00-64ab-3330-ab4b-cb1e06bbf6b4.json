  "rdfs:comment": {
    "de": [
      "Konditor, Chocolatier, Eisdiele, Teesalon. Spezialitäten: Eisgebäck, Chibouste mirabelle, Palets messins, Visitandines, Belles de Lorraine (Schokolade, Branntwein) und Macarons mirabelles."
    ],
    "en": [
      "Pastry chef, chocolatier, ice-cream parlour, tea room. Specialities: iced desserts, chibouste mirabelle plum, palets messins, visitandines, Belles de Lorraine (chocolate, brandy) and mirabelle plum macaroons."
    ],
    "it": [
      "Pasticcere, cioccolatiere, gelataio, sala da tè. Specialità: dessert congelati, chibouste di prugne mirabelle, palets messins, visitandines, Belles de Lorraine (cioccolato, brandy) e macaron mirabelle."
    ],
    "fr": [
      "Pâtissier, chocolatier, glacier, salon de thé. Spécialités: entremets glacé, chibouste mirabelle, palets messins, visitandines, Belles de Lorraine (chocolat, eau de vie) et macarons mirabelles."
    ],
    "es": [
      "Pastelero, chocolatero, heladero, salón de té. Especialidades: postres congelados, chibouste de ciruela mirabelle, palets messins, visitandines, Belles de Lorraine (chocolate, brandy) y macarrones mirabelle."
    ],
    "nl": [
      "Banketbakker, chocolademaker, ijsmaker, theesalon. Specialiteiten: diepvriesdesserts, mirabelle pruimen chibouste, palets messins, visitandines, Belles de Lorraine (chocolade, brandewijn) en mirabelle bitterkoekjes."
    ]
  },
