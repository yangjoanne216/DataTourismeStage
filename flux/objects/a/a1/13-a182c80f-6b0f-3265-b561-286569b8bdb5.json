  "rdfs:comment": {
    "de": [
      "Handwerkliche Bäckerei und Konditorei.\r\nHausspezialität (einzigartiges Rezept mit der Marke Goûtez l'Ardèche): Délice Ardéchois ®, ein köstliches Makronengebäck (glutenfrei), das mit Kastaniencreme AOP Châtaigne d'Ardèche und Buttercreme garniert wird."
    ],
    "en": [
      "Artisan bakery and pastry shop.\r\nHouse speciality (unique recipe accredited by Goûtez l'Ardèche brand) : the Délice ardéchois ®, a succulent almond biscuit-cake (gluten free) garnished with chestnut cream and butter cream."
    ],
    "it": [
      "Panificio-pasticceria artigianale.\r\nSpecialità della casa (ricetta unica con il marchio Goûtez l'Ardèche): Délice Ardéchois ®, un delizioso biscotto all'amaretto (senza glutine) ripieno di crema di castagne e crema al burro DOP Châtaigne d'Ardèche."
    ],
    "fr": [
      "Boulangerie-pâtisserie artisanale.\r\nSpécialité maison (recette unique bénéficiant de la marque Goûtez l'Ardèche) : le Délice Ardéchois ®, un succulent biscuit macaron (sans gluten) garni de crème de marron AOP Châtaigne d'Ardèche et de crème au beurre."
    ],
    "es": [
      "Panadería-pastelería artesanal.\r\nEspecialidad de la casa (receta única con el sello Goûtez l'Ardèche): Délice Ardéchois ®, una deliciosa galleta de macarrones (sin gluten) rellena de crema de castañas DOP Châtaigne d'Ardèche y crema de mantequilla."
    ],
    "nl": [
      "Ambachtelijke bakkerij-banketbakkerij.\r\nSpecialiteit van het huis (uniek recept met het label Goûtez l'Ardèche): Délice Ardéchois ®, een heerlijk bitterkoekje (glutenvrij) gevuld met kastanjecrème en botercrème van de BOB Châtaigne d'Ardèche."
    ]
  },
