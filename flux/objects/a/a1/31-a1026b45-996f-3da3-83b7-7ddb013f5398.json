  "rdfs:comment": {
    "de": [
      "Canyoning ist die spielerische Aktivität par excellence. Sie werden Sprünge, Rutschen, Rutschen und Abseilen aneinanderreihen."
    ],
    "en": [
      "Funny and technical, canyoning is a combination of jumps, waterslides, abseiling, zipline … all the sensation will be here ! Accessible for kids over 12 if they know how to swim !"
    ],
    "it": [
      "Il canyoning è l'attività più divertente in assoluto. Si salta, si scivola, si scende e si scende in corda doppia."
    ],
    "fr": [
      "Le canyoning est l'activité ludique par excellence. Vous enchaînerez sauts, toboggans, glissades, descentes en rappel."
    ],
    "es": [
      "El barranquismo es la actividad más divertida. Saltarás, te deslizarás, descenderás y harás rappel."
    ],
    "nl": [
      "Canyoning is de ultieme leuke activiteit. Je zult springen, glijden, naar beneden glijden en abseilen."
    ]
