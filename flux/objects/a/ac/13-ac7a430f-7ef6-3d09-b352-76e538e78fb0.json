  "rdfs:comment": {
    "de": [
      "Orientierung: NNE, ENE, SE\r\nBemerkungen / Gefahren: Vorsicht beim Start bei starkem Ostwind; nicht starten. Entlang des Gemeindewegs parken. Kurzer Start."
    ],
    "en": [
      "Orientation: NNE, ENE, SE\r\nObservations / Hazards: Be careful during take-off in strong easterly winds; do not take off. Park along the communal path. Short take-off."
    ],
    "it": [
      "Direzione: NNE, ENE, SE\r\nOsservazioni / Pericoli: attenzione al decollo in caso di forte vento da est; non decollare. Parcheggiare lungo la strada comunale. Decollo breve."
    ],
    "fr": [
      "Orientation : NNE, ENE, SE\r\nObservations / Dangers : attention au décollage par fort vent d'est ; ne pas décoller. Se garer le long du chemin communal. Décollage court."
    ],
    "nl": [
      "Richting: NNE, ENE, SE\r\nObservaties / Gevaren: pas op met opstijgen bij sterke oostenwind; niet opstijgen. Parkeer langs de gemeenschappelijke weg. Kort opstijgen."
    ],
    "es": [
      "Dirección: NNE, ENE, SE\r\nObservaciones / Peligros: cuidado con el despegue en caso de vientos fuertes del este; no despegue. Aparcar a lo largo de la carretera comunal. Despegue corto."
    ]
  },
