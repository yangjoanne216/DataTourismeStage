  "rdfs:comment": {
    "de": [
      "Gemütlicher Raum für Erwachsene, Jugendliche und Kinder. Hier finden Sie Romane, Comics, Dokumentarfilme, Bücher, Audios und Zeitschriften. Neuheiten sind über den Online-Katalog erhältlich. Kostenloses Wi-Fi."
    ],
    "en": [
      "Comfortable space for adults, teens and children. You will find novels, comics, documentaries, books, audios and magazines. New books available on the online catalog. Free Wi-Fi."
    ],
    "it": [
      "Spazio confortevole per adulti, adolescenti e bambini. Troverete romanzi, fumetti, documentari, libri, audio e riviste. Nuovi libri disponibili sul catalogo online. Wi-Fi gratuito."
    ],
    "fr": [
      "Espace confortable pour les adultes, les ados et les enfants. Vous y trouverez des romans, BDs, documentaires, livres, audios et revues. Nouveautés disponibles sur le catalogue en ligne. Wi-Fi gratuit."
    ],
    "es": [
      "Espacio confortable para adultos, adolescentes y niños. Encontrará novelas, cómics, documentales, libros, audios y revistas. Nuevos libros disponibles en el catálogo en línea. Wi-Fi gratuito."
    ],
    "nl": [
      "Comfortabele ruimte voor volwassenen, tieners en kinderen. U vindt er romans, strips, documentaires, boeken, audio en tijdschriften. Nieuwe boeken beschikbaar in de online catalogus. Gratis Wi-Fi."
    ]
  },
