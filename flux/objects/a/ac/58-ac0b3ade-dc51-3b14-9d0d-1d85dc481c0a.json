  "rdfs:comment": {
    "de": [
      "Jérôme und Kathleen Imbert, Galeristen seit 30 Jahren, Fotografen seit 45 Jahren und Kunstliebhaber seit jeher, heißen Sie in ihrer Kunstgalerie im Stadtzentrum von Aix-en-Provence herzlich willkommen."
    ],
    "en": [
      "Gallery owners for 30 years, photographers for 45 years and art lovers since forever, Jérôme and Kathleen Imbert, welcome you in their art gallery in downtown Aix-en-Provence."
    ],
    "it": [
      "Galleristi da 30 anni, fotografi da 45 anni e amanti dell'arte da sempre, Jérôme e Kathleen Imbert vi danno il benvenuto nella loro galleria d'arte nel centro di Aix-en-Provence."
    ],
    "fr": [
      "Galeristes depuis 30 ans, photographes depuis 45 et amateurs d'art depuis toujours, Jérôme et Kathleen Imbert, vous accueillent dans leur galerie d'art au centre-ville d'Aix-en-Provence."
    ],
    "nl": [
      "Jérôme en Kathleen Imbert, al 30 jaar galeriehouders, al 45 jaar fotografen en kunstliefhebbers zolang zij zich kunnen herinneren, heten u welkom in hun galerie in het centrum van Aix-en-Provence."
    ],
    "es": [
      "Galeristas desde hace 30 años, fotógrafos desde hace 45 años y amantes del arte desde siempre, Jérôme y Kathleen Imbert le dan la bienvenida a su galería de arte en el centro de Aix-en-Provence."
    ]
  },
