  "rdfs:comment": {
    "de": [
      "Lernen Sie die Fledermäuse kennen, erstaunliche kleine, fliegende Säugetiere, die in der Abtei von Noirlac und in der umliegenden Bocage leben."
    ],
    "en": [
      "Come and discover the bats, amazing little mammals, flying, living within the abbey of Noirlac and in the surrounding bocage."
    ],
    "it": [
      "Venite a scoprire i pipistrelli, piccoli e sorprendenti mammiferi volanti che vivono nell'abbazia di Noirlac e nel bocage circostante."
    ],
    "fr": [
      "Venez découvrir les chauve-souris, étonnants petits mammifères, volant, vivant au sein de l'abbaye de Noirlac et dans le bocage environnant."
    ],
    "es": [
      "Venga a descubrir los murciélagos, sorprendentes pequeños mamíferos voladores que viven en la abadía de Noirlac y en el bocage circundante."
    ],
    "nl": [
      "Kom en ontdek de vleermuizen, verbazingwekkende kleine vliegende zoogdieren die in de abdij van Noirlac en in de omliggende bocage leven."
    ]
  },
