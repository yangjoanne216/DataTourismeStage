  "rdfs:comment": {
    "de": [
      "TAXI, KRANKENWAGEN, VSL\r\nShuttle: Transportdienst auf Abruf (Verbindung mit öffentlichen Verkehrsmitteln nach Nyons, Vaison und Valréas)\r\nTransport Minibus: Wanderer, Abendveranstaltungen...\r\n7/7 alle Entfernungen"
    ],
    "en": [
      "TAXI, AMBULANCE, VSL \r\nShuttle: transport service on demand (public transport link to Nyons, Vaison and Valréas)\r\nMini-bus transport: hikers, parties...\r\n7/7 all distances"
    ],
    "it": [
      "TAXI, AMBULANZA, VSL\r\nNavetta: servizio di trasporto su richiesta (collegamento con i mezzi pubblici a Nyons, Vaison e Valréas)\r\nTrasporto in minibus: escursionisti, feste...\r\n7/7 tutte le distanze"
    ],
    "fr": [
      "TAXI, AMBULANCE, VSL \r\nNavette : service de transport à la demande (liaison transport public à destination de Nyons, Vaison et Valréas)\r\nTransport Mini-bus : randonneurs, soirées...\r\n7/7 toutes distances"
    ],
    "es": [
      "TAXI, AMBULANCIA, VSL\r\nLanzadera: servicio de transporte a la carta (enlace de transporte público con Nyons, Vaison y Valréas)\r\nTransporte en minibús: excursionistas, fiestas...\r\n7/7 todas las distancias"
    ],
    "nl": [
      "TAXI, AMBULANCE, VSL \r\nShuttle: vervoersdienst op aanvraag (in verbinding met openbaar vervoer van Nyons, Vaison en Valréas)\r\nMinibusvervoer: wandelaars, feesten...\r\n7/7 alle afstanden"
    ]
  },
