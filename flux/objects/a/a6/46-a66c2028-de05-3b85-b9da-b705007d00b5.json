  "rdfs:comment": {
    "de": [
      "Traditionell am Mittag. Restaurant für Arbeiter. Pizzéria am Abend von Freitag bis Sonntag. Raffiniertes Menü von 20 bis 42 €. Abends von Montag bis Donnerstag geschlossen. Im August 3 Wochen geschlossen. Terrasse."
    ],
    "en": [
      "Traditional at lunchtime. Worker's restaurant. Pizzeria in the evening from Friday to Sunday. Refined menu from 20 to 42 €. Closed evenings from Monday to Thursday. Closed 3 weeks in August. Terrace."
    ],
    "it": [
      "Tradizionale all'ora di pranzo. Ristorante di classe operaia. Pizzeria la sera dal venerdì alla domenica. Menu raffinato da 20 a 42 ? Chiuso dal lunedì al giovedì sera. Chiuso 3 settimane in agosto. Terrazza."
    ],
    "fr": [
      "Traditionnel le midi. Restaurant ouvrier. Pizzéria le soir du vendredi au dimanche. Menu raffiné de 20 à 42 €. Fermé les soirs du lundi au jeudi. Fermé 3 semaines en août. Terrasse."
    ],
    "nl": [
      "Traditioneel tijdens de lunch. Arbeidersklasse restaurant. Pizzeria 's avonds van vrijdag tot zondag. Verfijnd menu van 20 tot 42 €. S Avonds gesloten van maandag tot donderdag. Drie weken gesloten in augustus. Terras."
    ],
    "es": [
      "Tradicional a la hora de comer. Restaurante de clase trabajadora. Pizzería por la noche de viernes a domingo. Menú refinado de 20 a 42 €. Cerrado de lunes a jueves por la noche. Cerrado 3 semanas en agosto. Terraza."
    ]
  },
