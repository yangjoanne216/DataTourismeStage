  "rdfs:comment": {
    "de": [
      "Entdecken Sie das Hauptprodukt von Mouriès, das Olivenöl, im Rahmen eines Kochworkshops am Sonntag, den 25. Juni um 10 Uhr!"
    ],
    "en": [
      "Come and discover Mouriès' flagship product, olive oil, in a cookery workshop on Sunday 25 June at 10am!"
    ],
    "it": [
      "Venite a scoprire il prodotto di punta di Mouriès, l'olio d'oliva, durante un laboratorio di cucina domenica 25 giugno alle 10.00!"
    ],
    "fr": [
      "Venez découvrir le produit phare de Mouriès, l'huile d'olive, à l'occasion d'un atelier cuisine, le dimanche 25 juin à 10h !"
    ],
    "es": [
      "Venga a descubrir el producto estrella de Mouriès, el aceite de oliva, en un taller de cocina el domingo 25 de junio a las 10h"
    ],
    "nl": [
      "Kom het vlaggenschip van Mouriès, olijfolie, ontdekken tijdens een kookworkshop op zondag 25 juni om 10 uur!"
    ]
