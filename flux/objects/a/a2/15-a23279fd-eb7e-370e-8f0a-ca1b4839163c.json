  "rdfs:comment": {
    "de": [
      "Am Ufer der Loire, unter Bäumen oder in der Sonne - wählen Sie den Tisch, der am besten zu Ihnen passt!"
    ],
    "en": [
      "On the banks of the Loire, under the trees or in the sun, choose the table that suits you best!"
    ],
    "it": [
      "In riva alla Loira, sotto gli alberi o al sole, scegliete il tavolo che fa per voi!"
    ],
    "fr": [
      "Au bord de la Loire, sous les arbres ou au soleil, choisissez la table qui vous convient le mieux !"
    ],
    "es": [
      "Junto al Loira, bajo los árboles o al sol, ¡elija la mesa que más le convenga!"
    ],
    "nl": [
      "Aan de Loire, onder de bomen of in de zon, kies de tafel die het beste bij u past!"
    ]
  },
