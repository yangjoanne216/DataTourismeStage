  "rdfs:comment": {
    "de": [
      "Bar, Restaurant, Eisdiele in Le Frioul: Fleisch, Fisch, armenische Spezialitäten. Außergewöhnlicher Rahmen, weit weg vom Lärm der Stadt, gegenüber dem Hafen. Große Sonnenterrasse und überdachte Veranda."
    ],
    "en": [
      "Bar, restaurant, ice cream parlour in Friuli: meat, fish, Armenian specialties. Exceptional setting, far from the noise of the city, facing the port. Large sunny terrace and covered veranda."
    ],
    "it": [
      "Bar, ristorante, gelateria in Friuli: carne, pesce, specialità armene. Ambiente eccezionale, lontano dal rumore della città, di fronte al porto. Ampia terrazza soleggiata e veranda coperta."
    ],
    "fr": [
      "Bar, restaurant, glacier au Frioul : viandes, poissons, spécialités arméniennes. Cadre exceptionnel, loin des bruits de la ville, face au port. Grande terrasse ensoleillée, et véranda couverte."
    ],
    "es": [
      "Bar, restaurante, heladería en Frioul: carne, pescado, especialidades armenias. Entorno excepcional, lejos del ruido de la ciudad, frente al puerto. Gran terraza soleada y porche cubierto."
    ],
    "nl": [
      "Bar, restaurant, ijssalon in Friuli: vlees, vis, Armeense specialiteiten. Uitzonderlijke ligging, ver van het lawaai van de stad, tegenover de haven. Groot zonnig terras en overdekte veranda."
    ]
  },
