  "rdfs:comment": {
    "de": [
      "Bar Restaurant La Passerelle im Freizeitbad-Komplex in La Bresse. Von Montag bis Freitag Mittag, Moules Frites, Pizza (vor Ort oder zum Mitnehmen). Kleiner Imbiss (nachmittags). Crêpes."
    ],
    "en": [
      "Bar Restaurant La Passerelle in the leisure pool complex in La Bresse. From Monday to Friday noon, mussels and chips, pizza (on site or to take away). Small catering (afternoon). Pancakes."
    ],
    "it": [
      "Bar Ristorante La Passerelle nel complesso di piscine di La Bresse. Dal lunedì al venerdì a pranzo, cozze e patatine, pizza (sul posto o da asporto). Spuntini (pomeriggio). Frittelle."
    ],
    "fr": [
      "Bar Restaurant La Passerelle dans le complexe piscine loisirs à La Bresse. Du lundi au vendredi midi, moules frites, pizza (sur place ou à emporter). Petite restauration (après-midi). Crêpes."
    ],
    "es": [
      "Bar Restaurante La Passerelle en el complejo de piscinas de La Bresse. De lunes a viernes al mediodía, mejillones y patatas fritas, pizza (en el lugar o para llevar). Merienda (por la tarde). Panqueques."
    ],
    "nl": [
      "Bar Restaurant La Passerelle in het zwembadcomplex in La Bresse. Maandag tot vrijdag tijdens de lunch, mosselen en friet, pizza (ter plaatse of om mee te nemen). Snacks (middag). Pannenkoeken."
    ]
  },
