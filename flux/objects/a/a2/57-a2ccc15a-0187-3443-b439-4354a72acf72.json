  "rdfs:comment": {
    "de": [
      "Lassen Sie sich von der Stimmung im Sumpf am Morgen, wenn alles ruhig ist, verzaubern!"
    ],
    "en": [
      "Let yourself be enchanted by the atmosphere of the marsh in the morning, when everything is quiet!"
    ],
    "it": [
      "Lasciatevi incantare dall'atmosfera della palude al mattino, quando tutto è tranquillo!"
    ],
    "fr": [
      "Laissez-vous envoûter par l'ambiance du marais le matin, quand tout est calme !"
    ],
    "nl": [
      "Laat u betoveren door de sfeer van het moeras in de ochtend, wanneer alles rustig is!"
    ],
    "es": [
      "Déjese hechizar por el ambiente del pantano por la mañana, cuando todo está en calma"
    ]
  },
