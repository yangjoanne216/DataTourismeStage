  "rdfs:comment": {
    "de": [
      "La Recette ist Freunde, guter Wein und ein gutes Essen..."
    ],
    "en": [
      "The Recipe is friends, good wine and a good meal..."
    ],
    "it": [
      "Venite a scoprire il nostro menu in un'atmosfera moderna e contemporanea e scoprite la nostra cucina dal sapore meridionale"
    ],
    "fr": [
      "Venez découvrir notre carte dans une ambiance actuelle et conptemporaine afin de découvrir notre cuisine parfumée aux accents du Sud"
    ],
    "es": [
      "Venga a descubrir nuestra carta en un ambiente moderno y contemporáneo y descubra nuestra cocina con sabor sureño"
    ],
    "nl": [
      "Kom onze kaart ontdekken in een moderne en hedendaagse sfeer en ontdek onze keuken met een zuidelijk tintje"
    ]
  },
