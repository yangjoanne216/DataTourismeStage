  "rdfs:comment": {
    "de": [
      "Schöne Villa, 11km vom Meer entfernt, 5min. zu Fuß zum Dorfzentrum, beheizter Privatpool, Südlage, SH 150m2, 8 Schlafplätze, 4 Schlafzimmer, 2 Badezimmer, umkehrbare Klimaanlage,\nInternet, Innenhof, Grill, Terrasse, eingezäunter Garten mit Sitzecke, Garage."
    ],
    "en": [
      "Beautiful villa at 11km from the sea, 5min. walk from the center of the village, private heated pool, south facing, SH 150m2, sleeps 8, 4 bedrooms, 2 bathrooms, reversible air conditioning,\nInternet, patio, barbecue, terrace, fenced garden with sitting area, garage."
    ],
    "it": [
      "Bella villa a 11 km dal mare, a 5 minuti a piedi dal centro del paese, piscina privata riscaldata, esposizione a sud, SH 150m2, 8 posti letto, 4 camere da letto, 2 bagni, aria condizionata reversibile,\nInternet, patio, barbecue, terrazza, giardino recintato con area salotto, garage."
    ],
    "fr": [
      "Belle villa à 11km de la mer, à 5min. à pied du centre du village, piscine privée chauffée, orientée plein sud, SH 150m2, 8 couchages, 4 chambres,  2 SDB, climatisation réversible, \nInternet, patio, barbecue, terrasse, jardin clôturé avec coin salon, garage."
    ],
    "nl": [
      "Mooie villa op 11km van de zee, 5min. lopen van het centrum van het dorp, verwarmd privé zwembad, op het zuiden gelegen, SH 150m2, 8 slaapplaatsen, 4 slaapkamers, 2 badkamers, omkeerbare airconditioning,\nInternet, patio, barbecue, terras, omheinde tuin met zithoek, garage."
    ],
    "es": [
      "Hermoso chalet a 11km del mar, 5min. a pie del centro del pueblo, piscina privada climatizada, orientación sur, SH 150m2, capacidad para 8 personas, 4 dormitorios, 2 baños, aire acondicionado reversible,\nInternet, patio, barbacoa, terraza, jardín vallado con zona de estar, garaje."
    ]
