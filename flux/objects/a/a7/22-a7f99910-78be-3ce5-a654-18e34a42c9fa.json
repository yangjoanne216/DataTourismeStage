  "rdfs:comment": {
    "de": [
      "Handwerkliche Herstellung von Gebrauchs- und Dekorationskeramik. Atelier expo am Ortseingang von Carennac. Schalen, Teekannen, Lampions, entdecken Sie die Neuheiten."
    ],
    "en": [
      "Handcrafted utilitarian and decorative ceramics. Exhibition workshop located at the entrance of Carennac. Bowls, teapot, lanterns, come and discover the new products."
    ],
    "it": [
      "Produzione artigianale di ceramiche utilitarie e decorative. Laboratorio espositivo situato all'ingresso di Carennac. Ciotole, teiere, lanterne, venite a scoprire i nuovi prodotti."
    ],
    "fr": [
      "Fabrique artisanale de céramique utilitaire et décorative. Atelier expo situé à l'entrée de Carennac. Bols, théière, lampions, venez découvrir les nouveautés."
    ],
    "es": [
      "Fabricación artesanal de cerámica utilitaria y decorativa. Taller de exposiciones situado en la entrada de Carennac. Cuencos, teteras, linternas, venga a descubrir los nuevos productos."
    ],
    "nl": [
      "Ambachtelijke vervaardiging van gebruiks- en sierkeramiek. Tentoonstellingsatelier bij de ingang van Carennac. Schalen, theepotten, lantaarns, kom en ontdek onze nieuwe producten."
    ]
  },
