  "rdfs:comment": {
    "de": [
      "Hugo Espinasse, ein staatlich geprüfter Gleitschirmflieger, bietet Ihnen im Winter Gleitschirmflüge über dem Skigebiet und im Sommer vom Startplatz Pertuiset aus an."
    ],
    "en": [
      "Hugo Espinasse, paragliding instructor, offers you paragliding flights in winter, above the resort and in summer from the take-off site of Pertuiset."
    ],
    "it": [
      "Hugo Espinasse, istruttore di parapendio, vi offre voli in parapendio in inverno, sopra la località e in estate dal sito di decollo di Pertuiset."
    ],
    "fr": [
      "Hugo Espinasse, parapentiste diplômé d'état vous propose des vols en parapente l'hiver, au dessus de la station et l'été depuis le site de décollage de Pertuiset."
    ],
    "nl": [
      "Hugo Espinasse, paragliding instructeur, biedt u paragliding vluchten aan in de winter, boven het resort en in de zomer vanaf de opstijgplaats Pertuiset."
    ],
    "es": [
      "Hugo Espinasse, instructor de parapente, le ofrece vuelos en parapente en invierno, por encima de la estación y en verano desde el lugar de despegue de Pertuiset."
    ]
  },
