  "rdfs:comment": {
    "de": [
      "Ponyclub - Bogenschießen vom Pferd aus\r\nEmpfang von Ferienkolonien, Schulkindern\r\nGîte relais équestre (Unterkunft mit Reiterhof)\r\nOrganisiert auch Aufführungen.\r\nVermietung eines ausgestatteten Saals, ganz aus Holz. Idealer Ort für Themenabende."
    ],
    "en": [
      "Pony club - archery on horseback\r\nSummer and school camps\r\nGîte equestrian relay\r\nOrganizes shows.\r\nRental of equipped room, all in wood. Ideal place for theme evenings."
    ],
    "it": [
      "Pony club - tiro con l'arco a cavallo\r\nAccoglienza di campi di vacanza e scolaresche\r\nLoggia per la staffetta equestre\r\nOrganizza spettacoli.\r\nAffitto di una sala completamente attrezzata e in legno. Luogo ideale per serate a tema."
    ],
    "fr": [
      "Poney club - tir à l'arc à cheval\r\nAccueil colonies de vacances, de scolaires\r\nGîte relais équestre\r\nOrganise des spectacles.\r\nLocation de salle équipée, tout en bois. Lieu idéal pour soirée à thème."
    ],
    "es": [
      "Pony club - tiro con arco a caballo\r\nRecepción de colonias de vacaciones y escolares\r\nAlbergue de relevos ecuestres\r\nOrganiza espectáculos.\r\nAlquiler de una sala totalmente equipada y de madera. Lugar ideal para veladas temáticas."
    ],
    "nl": [
      "Pony club - boogschieten te paard\r\nOpvang van vakantiekampen en schoolkinderen\r\nPaarden estafette loge\r\nOrganiseert shows.\r\nVerhuur van een volledig uitgeruste, houten zaal. Ideale plek voor thema-avonden."
    ]
  },
