  "rdfs:comment": {
    "de": [
      "Biès - Ein Ort für die Utopie.\nDieser kleine Campingplatz empfängt Sie in einer intimen und erholsamen Umgebung inmitten der Täler des Lauragais."
    ],
    "en": [
      "Biès - A place for Utopia.\nThis small campsite welcomes you in an intimate and relaxing setting, in the heart of the Lauragais valleys."
    ],
    "it": [
      "Biès - Un luogo di utopia.\nQuesto piccolo campeggio vi accoglie in un ambiente intimo e rilassante, nel cuore delle valli del Lauragais."
    ],
    "fr": [
      "Biès - Un lieu pour l'Utopie.\nCe petit camping vous accueille dans un cadre intime et reposant, en plein cœur des vallons du Lauragais."
    ],
    "es": [
      "Biès - Un lugar para la utopía.\nEste pequeño camping le acoge en un entorno íntimo y relajante, en el corazón de los valles del Lauragais."
    ],
    "nl": [
      "Biès - Een plaats voor Utopia.\nDeze kleine camping ontvangt u in een intieme en ontspannende omgeving, in het hart van de Lauragais-valleien."
    ]
  },
