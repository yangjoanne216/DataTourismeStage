  "rdfs:comment": {
    "de": [
      "Was für eine Seltsamkeit! Eine unvollendete Kapelle inmitten des Kurparks von Barbazan."
    ],
    "en": [
      "What a weirdness! An unfinished chapel in the heart of the Barbazan thermal park."
    ],
    "it": [
      "Che stranezza! Una cappella incompiuta nel cuore del parco termale di Barbazan."
    ],
    "fr": [
      "Quelle bizarrerie ! Une chapelle inachevée en plein cœur du parc thermal de Barbazan."
    ],
    "es": [
      "¡Qué rareza! Una capilla inacabada en el corazón del parque termal de Barbazán."
    ],
    "nl": [
      "Wat een rariteit! Een onvoltooide kapel in het hart van het thermale park van Barbazan."
    ]
