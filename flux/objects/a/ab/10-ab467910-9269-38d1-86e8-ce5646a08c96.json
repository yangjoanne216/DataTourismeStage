  "rdfs:comment": {
    "de": [
      "Getränkestand, Schnellimbiss, Sandwiches vom Grill, Pommes frites.\nMenü mit Reservierung (vor dem 24. Juni; 12?): Rohkost, Beinschinken, Kartoffelspalten, Käse, grüner Salat, Apfelkuchen."
    ],
    "en": [
      "Refreshment bar, fast food, grilled sandwiches, French fries.\nMenu on reservation (before June 24; 12?): Crudités, ham cooked on the bone, grated potatoes, cheese, green salad, apple pie."
    ],
    "it": [
      "Bar, fast food, panini alla griglia e patatine.\nMenu su prenotazione (entro il 24 giugno; 12?): Crudité, prosciutto cotto con osso, patate grattugiate, formaggio, insalata verde, torta di mele."
    ],
    "fr": [
      "Buvette, restauration rapide, sandwich grillades, frites.\nMenu sur réservation (avant le 24 juin ; 12€) : Crudités, jambon cuit à l'os, râpés de pommes de terre, fromage salade verte, tarte au pommes."
    ],
    "nl": [
      "Snackbar, fastfood, tosti's en friet.\nMenu op reservering (voor 24 juni; 12?): Crudités, beenham, geraspte aardappelen, kaas, groene salade, appeltaart."
    ],
    "es": [
      "Bar de refrescos, comida rápida, sándwiches a la plancha y patatas fritas.\nMenú previa reserva (antes del 24 de junio; 12?): Crudités, jamón cocido con hueso, patatas ralladas, queso, ensalada verde, tarta de manzana."
    ]
  },
