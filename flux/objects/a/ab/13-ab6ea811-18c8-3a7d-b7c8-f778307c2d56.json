  "rdfs:comment": {
    "de": [
      "La Passion selon saint Jean (Die Johannespassion). Das Festival vereint nun jedes Jahr 5000 Festivalbesucher bei 25 Veranstaltungen pro Ausgabe und ist heute das Referenzfestival in Frankreich."
    ],
    "en": [
      "The Passion according to Saint John. The Festival now gathers every year 5000 festival-goers for 25 events per edition, becoming today the reference festival in France."
    ],
    "it": [
      "La Passione di San Giovanni. Il Festival riunisce ogni anno 5000 spettatori per 25 eventi a edizione, diventando oggi il festival di riferimento in Francia."
    ],
    "fr": [
      "La Passion selon saint Jean. Le Festival réunit désormais chaque année 5000 festivaliers pour 25 événements par édition, devenant aujourd’hui le festival de référence en France."
    ],
    "es": [
      "La Pasión de San Juan. En la actualidad, el Festival reúne cada año a 5.000 festivaleros en 25 actos por edición, convirtiéndose hoy en el festival de referencia en Francia."
    ],
    "nl": [
      "De Johannes Passion. Het festival brengt nu elk jaar 5000 festivalgangers samen voor 25 evenementen per editie en is vandaag de dag het referentiefestival in Frankrijk."
    ]
  },
