  "rdfs:comment": {
    "de": [
      "Charakteristisches Gebäude mit Blick auf das \"Loch von Bozouls\". Zimmer mit Aussicht. Raffinierte und authentische Landküche. 1 * im Michelin"
    ],
    "en": [
      "Old building full of character overlooks the \"trou de Bozouls\". Rooms with view. Refined cooking using local products. 1* Michelin"
    ],
    "it": [
      "Edificio caratteristico con vista sul \"trou de Bozouls\". Camere con vista. Una cucina contadina raffinata e autentica. 1 * nella Guida Michelin"
    ],
    "fr": [
      "Bâtisse de caractère surplombant le \"trou de Bozouls\". Chambres avec vue. Cuisine paysanne raffinée et authentique. 1 * au Michelin"
    ],
    "es": [
      "Vieja casona con carácter, que domina el \"Trou de Bozouls\". Habitaciones con vista. y cocina de mercado refinada. 1* Michelin"
    ],
    "nl": [
      "Karakteristiek gebouw met uitzicht op de \"trou de Bozouls\". Kamers met uitzicht. Verfijnde en authentieke landelijke keuken. 1 * in de Michelin gids"
    ]
  },
