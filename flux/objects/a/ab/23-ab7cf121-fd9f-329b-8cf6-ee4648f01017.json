  "rdfs:comment": {
    "de": [
      "1 Gemeindesaal, der bis zu 150 Personen aufnehmen kann\nBeschreibung: Parkettboden, blassgrüne Wände, Bühne, Sanitäranlagen, Garderoben, Küche (Kühlschrank, Gefrierschrank, Wärmeschrank), Parkplatz, Grünfläche."
    ],
    "en": [
      "1 communal room that can accommodate up to 150 people\nDescription : parquet floor, pale green walls, stage, sanitary facilities, checkrooms, kitchen (refrigerator, freezer, heating cabinet), parking, green space."
    ],
    "it": [
      "1 sala comunitaria per un massimo di 150 persone\nDescrizione: pavimento in parquet, pareti verde chiaro, palco, servizi igienici, guardaroba, cucina (frigorifero, congelatore, armadio caldo), parcheggio, spazio verde."
    ],
    "fr": [
      "1 salle communale pouvant accueillir jusqu'à 150 personnes. \nDescriptif : parquet, murs couleur vert pâle, scène, sanitaires, vestiaires, cuisine (réfrigérateur, congélateur, armoire chauffante), parking, espace vert."
    ],
    "es": [
      "1 sala comunitaria con capacidad para 150 personas\nDescripción: suelo de madera, paredes de color verde pálido, escenario, instalaciones sanitarias, vestuarios, cocina (nevera, congelador, armario caliente), aparcamiento, espacio verde."
    ],
    "nl": [
      "1 gemeenschapsruimte voor maximaal 150 personen\nBeschrijving: parketvloer, lichtgroene muren, podium, sanitair, vestiaires, keuken (koelkast, vriezer, warmhoudkast), parkeerplaats, groene ruimte."
    ]
  },
