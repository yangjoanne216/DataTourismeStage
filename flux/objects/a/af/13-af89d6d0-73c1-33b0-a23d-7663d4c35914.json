  "rdfs:comment": {
    "de": [
      "Familienhotel in einer Umgebung mit trockenen Steinen und Olivenbäumen, 40 km von La Caverne du Pont d'Arc entfernt. Schattige Terrasse mit Pool. Ort der Entspannung für Groß und Klein."
    ],
    "en": [
      "Family-run hotel set against a backdrop of old stones and olive trees 40km from Pont d'Arc Cave. Shaded terrace with pool. Relaxing spot for young and old alike."
    ],
    "it": [
      "Hotel a conduzione familiare in un ambiente di pietre secche e ulivi, situato a 40 km da La Caverne du Pont d'Arc. Terrazza ombreggiata con piscina. Un luogo di relax per grandi e piccini."
    ],
    "fr": [
      "Hôtel familial dans un décor de pierres sèches et oliviers, situé à 40 km de La Caverne du Pont d’Arc. Terrasse ombragée avec piscine. Lieu de détente pour les petits et les grands."
    ],
    "es": [
      "Hotel familiar en un entorno de piedras secas y olivos, situado a 40 km de La Caverne du Pont d'Arc. Terraza sombreada con piscina. Un lugar de relax para jóvenes y mayores."
    ],
    "nl": [
      "Familiehotel in een omgeving van droge stenen en olijfbomen, gelegen op 40 km van Chauvet 2. Schaduwrijk terras met zwembad. Ontspanningsplek voor jong en oud."
    ]
  },
