  "rdfs:comment": {
    "de": [
      "Zögern Sie nicht, die Tür zu öffnen. Die Designerin wird Sie gerne in ihrer farbenfrohen Welt willkommen heißen."
    ],
    "en": [
      "Do not hesitate to push the door, the creator will welcome you with pleasure in her colorful universe."
    ],
    "it": [
      "Non esitate a spingere la porta, la designer vi accoglierà con piacere nel suo universo colorato."
    ],
    "fr": [
      "N’hésitez pas à pousser la porte, la créatrice vous accueillera avec plaisir dans son univers coloré."
    ],
    "es": [
      "No dude en empujar la puerta, la diseñadora le acogerá con gusto en su colorido universo."
    ],
    "nl": [
      "Aarzel niet om de deur in te trappen, de ontwerpster zal u met plezier verwelkomen in haar kleurrijke universum."
    ]
  },
