  "rdfs:comment": {
    "de": [
      "Bei Arénal Pizza, benannt nach einem Vulkan in Costa Rica, bieten Ihnen Christelle und Didier Pizzen zum Mitnehmen (oder zum Verzehr vor Ort) an"
    ],
    "en": [
      "At Arénal Pizza, named after a volcano in Costa Rica, Christelle and Didier offer you pizzas to take away (or to taste on the spot)"
    ],
    "it": [
      "All'Arénal Pizza, che prende il nome da un vulcano della Costa Rica, Christelle e Didier vi offrono pizze da asporto (o da gustare sul posto)"
    ],
    "fr": [
      "Chez Arénal Pizza, du nom d'un volcan du Costa Rica, Christelle et Didier vous proposent des pizzas à emporter (ou à déguster sur place)"
    ],
    "es": [
      "En Arénal Pizza, que lleva el nombre de un volcán de Costa Rica, Christelle y Didier te ofrecen pizzas para llevar (o para degustar en el momento)"
    ],
    "nl": [
      "Bij Arénal Pizza, genoemd naar een vulkaan in Costa Rica, bieden Christelle en Didier u pizza's om mee te nemen (of om ter plekke te proeven)"
    ]
