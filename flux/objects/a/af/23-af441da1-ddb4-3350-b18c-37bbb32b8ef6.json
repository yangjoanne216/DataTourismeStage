  "rdfs:comment": {
    "de": [
      "Schnelle Rennen im Becken: Dune du Pilat, Banc d'Arguin, Restaurants, Spaziergänge rund um die Île aux Oiseaux, Besuch von Austernbänken und Austernhäfen, Aufstieg auf Leyre, Bootsverleih mit Skipper."
    ],
    "en": [
      "Rapid races on the basin: Dune du Pilat, Banc d'Arguin, restaurants, walks around Île aux Oiseaux, visit to oyster beds and oyster ports, ascent of Leyre, boat rental with skipper."
    ],
    "it": [
      "Gite veloci intorno al bacino: Dune du Pilat, Banc d'Arguin, ristoranti, passeggiate intorno all'Ile aux Oiseaux, visite ai banchi di ostriche e ai porti di ostriche, risalita della Leyre, noleggio di barche con skipper."
    ],
    "fr": [
      "Courses rapides sur le bassin : Dune du Pilat, Banc d'Arguin, restaurants, balades autour de l'Île aux Oiseaux, visite des parcs à huîtres et des ports ostréicoles, remontée de la Leyre, location du bateau avec skipper."
    ],
    "nl": [
      "Snelle uitstapjes rond het bekken: Dune du Pilat, Banc d'Arguin, restaurants, wandelingen rond het Ile aux Oiseaux, bezoek aan oesterbanken en oesterhavens, tocht op de Leyre, bootverhuur met schipper."
    ],
    "es": [
      "Carreras rápidas en la cuenca: Dune du Pilat, Banc d'Arguin, restaurantes, paseos por Île aux Oiseaux, visita a bancos de ostras y puertos de ostras, ascenso de Leyre, alquiler de barcos con patrón."
    ]
  },
