  "rdfs:comment": {
    "de": [
      "Auf dieser Strecke können Sie schöne Aussichtspunkte auf die Combraille und die Bocage Bourbonnais bewundern."
    ],
    "en": [
      "This circuit offers beautiful views over the Combraille and the Bourbonnais bocage."
    ],
    "it": [
      "Questo percorso offre una splendida vista sulla Combraille e sul bocage del Bourbonnais."
    ],
    "fr": [
      "Ce circuit permet  d'admirer de beaux points de vue sur la Combraille et le bocage Bourbonnais."
    ],
    "es": [
      "Esta ruta ofrece magníficas vistas de la Combraille y del bocage de Bourbonnais."
    ],
    "nl": [
      "Deze route biedt prachtige uitzichten op de Combraille en de Bourbonnais bocage."
    ]
