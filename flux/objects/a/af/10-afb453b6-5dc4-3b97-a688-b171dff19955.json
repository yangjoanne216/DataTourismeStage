  "rdfs:comment": {
    "de": [
      "SCHNEESCHUHWANDERN IM WINTER, SCHNEESCHUHRALLYE, NATURSPIELE, ORIENTIERUNG, NAVIGATION, ENTDECKUNG DES KULTURERBES, ENTDECKUNG DER NATUR. KURSE ÜBER DIE KUNST DES WANDERNS UND WELLNESS-KURSE IM WALD.\nMITGLIED DES BERGBÜROS VON GÉRARDMER."
    ],
    "en": [
      "WINTER WITH SNOWSHOES, SNOWSHOE RALLY, NATURE GAMES, ORIENTATION, NAVIGATION, HERITAGE DISCOVERY. TRAINING COURSES ART OF WALKING AND TRAINING COURSES WELL BEING IN THE FOREST.\nMEMBER OF THE MOUNTAIN OFFICE OF GÉRARDMER."
    ],
    "it": [
      "CIASPOLATE INVERNALI, RADUNO DI CIASPOLE, GIOCHI NATURALISTICI, ORIENTAMENTO, NAVIGAZIONE, SCOPERTA DEL PATRIMONIO, SCOPERTA DELLA NATURA. CORSI SULL'ARTE DEL CAMMINARE E DEL BENESSERE NELLA FORESTA.\nMEMBRO DELL'UFFICIO DI MONTAGNA DI GERARDMER."
    ],
    "fr": [
      "L'HIVER A RAQUETTE, RALLYE RAQUETTE JEUX NATURE, ORIENTATION, NAVIGATION, DÉCOUVERTE PATRIMOINE, DÉCOUVERTE NATURE. STAGES ART DE MARCHER ET STAGES BIEN ETRE EN FORET.\nMEMBRE DU BUREAU MONTAGNE DE GÉRARDMER."
    ],
    "nl": [
      "WINTER SNEEUWSCHOENWANDELEN, SNEEUWSCHOENRALLY, NATUURSPELEN, ORIËNTATIE, NAVIGATIE, ERFGOEDONTDEKKING, NATUURONTDEKKING. WANDEL- EN WELZIJNSCURSUSSEN IN HET BOS.\nLID VAN HET BERGBUREAU VAN GERARDMER."
    ],
    "es": [
      "RAQUETAS DE INVIERNO, RALLY CON RAQUETAS DE NIEVE, JUEGOS DE NATURALEZA, ORIENTACIÓN, NAVEGACIÓN, DESCUBRIMIENTO DEL PATRIMONIO, DESCUBRIMIENTO DE LA NATURALEZA. CURSOS SOBRE EL ARTE DE CAMINAR Y EL BIENESTAR EN EL BOSQUE.\nMIEMBRO DE LA OFICINA DE MONTAÑA DE GERARDMER."
    ]
