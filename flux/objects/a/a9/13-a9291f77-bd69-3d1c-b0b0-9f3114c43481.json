  "rdfs:comment": {
    "de": [
      "Gîte en RDC plein sud, proche du centre d'Autrans, avec séjour et cuisine, salle de bains avec wc avec baignoire, chauffage central, Wifi. Ferienhaus im Erdgeschoss, Südlage, in der Nähe des Zentrums von Autrans, mit Wohnzimmer und Küche, Badezimmer mit WC und Badewanne, Zentralheizung, WLAN."
    ],
    "en": [
      "Gite on the ground floor, south facing, close to the centre of Autrans, with living room and kitchen, bathroom with toilet and bath, central heating, Wi-Fi."
    ],
    "it": [
      "Gite al piano terra, esposto a sud, vicino al centro di Autrans, con soggiorno e cucina, bagno con wc e vasca, riscaldamento centrale, Wifi."
    ],
    "fr": [
      "Gîte en RDC plein sud, proche du centre d'Autrans, avec séjour et cuisine, salle de bains avec wc avec baignoire, chauffage central, Wifi."
