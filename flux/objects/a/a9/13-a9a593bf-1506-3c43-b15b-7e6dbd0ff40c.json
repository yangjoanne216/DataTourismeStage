  "rdfs:comment": {
    "de": [
      "Der Sessellift Le Torraz transportiert Wanderer, Mountainbiker, Gleitschirmflieger und Landschaftsschwärmer in 1930 m Höhe, um den schönsten Panoramablick des Val d´Arly zu genießen."
    ],
    "en": [
      "Walkers, mountain bikers, paragliders or people seeking peace and quiet will all appreciate the Torraz chair lift taking them up to an altitude of 1930 m to enjoy the most beautiful panoramic view in the Val d’Arly."
    ],
    "it": [
      "Escursionisti, mountain biker, parapendisti o contemplatori, la seggiovia Torraz vi porta a 1930 metri di altitudine per ammirare il più bel panorama della Val d'Arly."
    ],
    "fr": [
      "Randonneurs, vététistes, parapentistes ou contemplatifs, le télésiège du Torraz vous transporte à 1930 mètres d'altitude pour admirer le plus beau panorama du Val d'Arly."
    ],
    "nl": [
      "Wandelaars, mountainbikers, paragliders of beschouwers, de Torraz stoeltjeslift brengt u naar een hoogte van 1930 meter om het mooiste panorama van het Val d'Arly te bewonderen."
    ],
    "es": [
      "Senderistas, ciclistas de montaña, parapentistas o contempladores, el telesilla de Torraz le lleva a 1930 metros de altitud para admirar el panorama más bello del Val d'Arly."
    ]
  },
  "rdfs:label": {
    "de": [
      "Sessellift La Tête du Torraz"
    ],
    "en": [
      "Le Torraz chair lift"
    ],
    "fr": [
      "Télésiège de la Tête du Torraz"
    ]
  },
