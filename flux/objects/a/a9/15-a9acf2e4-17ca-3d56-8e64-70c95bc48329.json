  "rdfs:comment": {
    "de": [
      "Climb'Up: Kletterhalle. Accro'Climb: Erobern Sie die Hochseilgärten im Herzen des Sarner Waldes bis zu 8 m über dem Boden."
    ],
    "en": [
      "Set yourself a challenge with tree-top routes in the heart of the Saran forest up to 8 metres above the ground."
    ],
    "it": [
      "Climb'Up : Sala di arrampicata. Accro'Climb : Partite alla conquista dei percorsi arroccati nel cuore della foresta di Saran fino a 8 metri di altezza."
    ],
    "fr": [
      "Climb'Up : Salle d'escalade. Accro'Climb : Partez à la conquête des parcours perchés au cœur de la forêt saranaise jusqu'à 8 mètres au-dessus du sol."
    ],
    "nl": [
      "Ontdek de boomparcoursen in het hart van het bos van Saran, tot wel 8 meter boven de grond."
    ],
    "es": [
      "Climb'Up : Sala de escalada. Accro'Climb : Lánzate a la conquista de los recorridos encaramados en el corazón del bosque de Saran hasta 8 metros de altura."
    ]
  },
