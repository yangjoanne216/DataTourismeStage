  "rdfs:comment": {
    "de": [
      "Reitzentrum mit Gütesiegel, das der Fédération Française d'Équitation angehört. Diplomierte Lehrkräfte.\r\nZwei Reitplätze, überdachte Reithalle, Clubhaus..\r\nReiten das ganze Jahr über ab 3 Jahren."
    ],
    "en": [
      "Horse riding centre with a label, affiliated to the French Federation of horse riding.\r\nGraduate teachers.\r\nTwo arena, riding hall, club house...\r\nHorse riding all year round from the age of 3."
    ],
    "it": [
      "Centro ippico, affiliato alla Federazione Francese di Equitazione. Insegnanti qualificati.\r\nDue arene, maneggio coperto, club house?\r\nEquitazione tutto l'anno a partire dai 3 anni."
    ],
    "fr": [
      "Centre Équestre labellisé, affilié la Fédération Française d’Équitation. Enseignants diplômés.\r\nDeux carrières, manège couvert, club house… \r\nÉquitation toute l’année à partir de 3 ans."
    ],
    "nl": [
      "Hippisch Centrum, aangesloten bij de Franse Ruiterbond. Gekwalificeerde leraren.\r\nTwee arena's, overdekte manege, clubhuis..\r\nPaardrijden het hele jaar door vanaf 3 jaar."
    ],
    "es": [
      "Centro Ecuestre, afiliado a la Federación Ecuestre Francesa. Profesores cualificados.\r\nDos pistas, picadero cubierto, casa club..\r\nMontar todo el año a partir de los 3 años."
    ]
  },
