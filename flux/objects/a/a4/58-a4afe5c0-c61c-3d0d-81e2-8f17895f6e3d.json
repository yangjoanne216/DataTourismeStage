      "dc:description": {
        "de": [
          "Desserts: Café gourmand, Mousse au chocolat, Tiramisu, Crème brulée, Fiadone, Erdbeer-Gazpacho ( im Sommer ), Eiskugeln...\r\nEmpfang von Gruppen."
        ],
        "en": [
          "Desserts: gourmet coffee, chocolate mousse, tiramisu, crème brulée, fiadone, strawberry gazpacho (in summer), ice cream scoops...\r\nGroups welcome."
        ],
        "it": [
          "Dessert: caffè gourmet, mousse al cioccolato, tiramisù, crème brulée, fiadone, gazpacho alla fragola (in estate), palline di gelato...\r\nI gruppi sono benvenuti."
        ],
        "fr": [
          "Les desserts :café gourmand, mousse au chocolat, tiramisu, crème brulée, fiadone, gaspacho de fraises ( l'été ), boules de glaces...\r\nAccueil de groupes."
        ],
        "es": [
          "Postres: café gourmet, mousse de chocolate, tiramisú, crème brulée, fiadone, gazpacho de fresa (en verano), bolas de helado...\r\nLos grupos son bienvenidos."
        ],
        "nl": [
          "Desserts: gourmetkoffie, chocolademousse, tiramisu, crème brulée, fiadone, aardbeiengazpacho (in de zomer), bolletjes ijs...\r\nGroepen zijn welkom."
        ]
      },
