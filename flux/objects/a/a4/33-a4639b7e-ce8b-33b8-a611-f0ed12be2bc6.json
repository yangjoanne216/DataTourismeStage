  "rdfs:comment": {
    "de": [
      "Gönnen Sie sich einen Moment der Entspannung mit Blick auf die Pyrenäen im Wellnessbereich La Pause en Soi."
    ],
    "en": [
      "Treat yourself to a moment of relaxation in front of the Pyrenees at La Pause en Soi."
    ],
    "it": [
      "Concedetevi un momento di relax di fronte ai Pirenei presso il centro benessere La Pause en Soi."
    ],
    "fr": [
      "Offrez-vous un moment de détente face aux Pyrénées à l'espace bien-être La Pause en Soi."
    ],
