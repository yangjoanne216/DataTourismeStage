  "rdfs:comment": {
    "de": [
      "Das Solhotel befindet sich in Banyuls-sur-Mer und bietet einen Blick auf das Mittelmeer. Seine Zimmer verfügen über einen Balkon mit Panoramablick auf das Meer. Kostenloser Privatparkplatz."
    ],
    "en": [
      "Located in Banyuls-sur-Mer, the Solhotel overlooks the Mediterranean Sea. Its rooms feature a balcony with a panoramic view of the sea. Free private parking is available."
    ],
    "it": [
      "Situato a Banyuls-sur-Mer, il Solhotel si affaccia sul Mar Mediterraneo. Le sue camere dispongono di un balcone con vista panoramica sul mare. È disponibile un parcheggio privato gratuito."
    ],
    "fr": [
      "Situé à Banyuls-sur-Mer, le Solhotel donne sur la mer Méditerranée. Ses chambres disposent d'un balcon avec une vue panoramique sur la mer. Parking privé gratuit."
    ],
    "es": [
      "Situado en Banyuls-sur-Mer, el Solhotel tiene vistas al mar Mediterráneo. Sus habitaciones cuentan con un balcón con vistas panorámicas al mar. Hay aparcamiento privado gratuito."
    ],
    "nl": [
      "Het Solhotel ligt in Banyuls-sur-Mer en kijkt uit over de Middellandse Zee. De kamers hebben een balkon met een panoramisch uitzicht op de zee. Er is gratis privéparkeergelegenheid beschikbaar."
    ]
  },
