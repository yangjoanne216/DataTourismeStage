  "rdfs:comment": {
    "de": [
      "Töpferei-Workshops für Erwachsene und Kinder. Formen, dekorieren, frei erkunden. Nach telefonischer Anmeldung oder zögern Sie nicht, mich im Atelier zu besuchen, dort können Sie meine Töpfereien sehen."
    ],
    "en": [
      "Pottery workshop for adults and children. Come to shape, decorate and explore freely. Please phone for bookings or meet me in the workshop and take the opportunity to discover my pottery pieces."
    ],
    "it": [
      "Laboratori di ceramica per adulti e bambini. Modellare, decorare, esplorare liberamente. Iscrivetevi telefonicamente o venite a trovarmi in laboratorio, dove potrete vedere le mie ceramiche."
    ],
    "fr": [
      "Ateliers de poterie pour adultes et enfants. Façonnage, décoration, exploration libre. Sur inscription par téléphone ou n'hésitez pas à venir me rencontrer à l'atelier, vous pourrez y voir mes poteries."
    ],
    "es": [
      "Talleres de cerámica para adultos y niños. Modelado, decoración, exploración libre. Inscríbete por teléfono o no dudes en venir a conocerme al taller, donde podrás ver mi cerámica."
    ],
    "nl": [
      "Cursussen en lessen pottenbakken voor volwassenen en kinderen. Creëren, versieren, vrije expressie. Op inschrijving per telefoon of stap gewoon binnen in mijn atelier, waar je mijn werk kunt bekijken."
    ]
  },
  "rdfs:label": {
    "en": [
      "Pottery Workshop with Sylvia Palma"
    ],
    "fr": [
      "Ateliers de poterie avec Sylvie Palma"
    ]
  },
