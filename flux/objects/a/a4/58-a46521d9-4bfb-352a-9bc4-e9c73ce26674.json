  "rdfs:comment": {
    "de": [
      "La Station ist ein Coffeeshop im französischen Stil, der zwischen Fastfood und traditionellen Restaurants angesiedelt ist."
    ],
    "en": [
      "La Station is a French-style coffee shop, the fruit of the imagination and desire of Romain and Olivier, father and son. Wanting to combine their love of American Coffee Shops with French cuisine, they decided to create La Station."
    ],
    "it": [
      "La Station è un caffè in stile francese situato tra fast food e ristoranti tradizionali."
    ],
    "fr": [
      "La Station est un coffeeshop à la française situé entre restauration rapide et restauration traditionnelle."
    ],
    "nl": [
      "La Station is een coffeeshop in Franse stijl, gelegen tussen fastfood en traditionele restaurants."
    ],
    "es": [
