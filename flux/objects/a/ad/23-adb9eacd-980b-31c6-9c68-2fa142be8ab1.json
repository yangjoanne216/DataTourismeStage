  "rdfs:comment": {
    "de": [
      "Ebenerdiges Gästezimmer mit allem Komfort in dem charmanten kleinen Dorf Pimbo. Gastronomie in der Nähe im Dorfzentrum."
    ],
    "en": [
      "Bed and breakfast on one level, with all comforts, in the charming little village of Pimbo. Restaurant nearby in the centre of the village."
    ],
    "it": [
      "Confortevole pensione a un piano nell'incantevole villaggio di Pimbo. Ristorante nelle vicinanze, nel centro del paese."
    ],
    "fr": [
      "Chambre d'hôtes de plain pied, tout confort, dans le charmant petit village de Pimbo. Restauration à proximité au centre du village."
    ],
    "es": [
      "Confortable casa de huéspedes de una sola planta en el encantador pueblo de Pimbo. Restaurante cercano en el centro del pueblo."
    ],
