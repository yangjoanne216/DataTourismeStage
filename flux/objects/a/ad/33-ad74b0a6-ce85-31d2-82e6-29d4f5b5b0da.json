  "rdfs:comment": {
    "de": [
      "Die PIZZ' ONIA ist eine Pizzeria in einer kleinen, ruhigen und gemütlichen Einkaufszone in La Magdelaine sur Tarn, einem Dorf im Norden von Toulouse."
    ],
    "en": [
      "The PIZZ' ONIA is a pizzeria located in a small, quiet and friendly shopping area in La Magdelaine sur Tarn, a village in the north of Toulouse."
    ],
    "it": [
      "La PIZZ' ONIA è una pizzeria situata in una piccola zona commerciale tranquilla e accogliente a La Magdelaine sur Tarn, un villaggio nel nord di Tolosa."
    ],
    "fr": [
      "La PIZZ' ONIA  est une pizzeria située dans une petite zone commerciale calme et chaleureuse à La Magdelaine sur Tarn, village du nord toulousain."
    ],
    "es": [
      "La PIZZ' ONIA es una pizzería situada en una pequeña, tranquila y acogedora zona comercial de La Magdelaine sur Tarn, un pueblo del norte de Toulouse."
    ],
    "nl": [
      "De PIZZ' ONIA is een pizzeria in een klein, rustig en vriendelijk winkelgebied in La Magdelaine sur Tarn, een dorp in het noorden van Toulouse."
    ]
  },
