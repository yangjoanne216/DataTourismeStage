  "rdfs:comment": {
    "de": [
      "Teilweise zerstört heute die romanische Kapelle Häusern Wandmalereien aus dem vierzehnten Jahrhundert. Unter Denkmalschutz."
    ],
    "en": [
      "Partly destroyed today, this Romanesque chapel houses murals of the fourteenth century. Listed building."
    ],
    "it": [
      "Oggi in parte distrutta, questa cappella romanica ospita dipinti murali del XIV secolo. È stato inserito nell'elenco dei monumenti storici. Interno non accessibile"
    ],
    "fr": [
      "En partie détruite aujourd'hui, cette chapelle romane abrite des peintures murales du XIVe siècle. Classée aux Monuments Historiques. Intérieur non accessible"
    ],
    "es": [
      "Parcialmente destruido hoy, las casas románicas murales de la capilla del siglo XIV. Clasificado monumento histórico."
    ],
    "nl": [
      "Gedeeltelijk verwoest vandaag, de romaanse kapel huizen muurschilderingen van de veertiende eeuw. Onder monumentenzorg."
    ]
  },
