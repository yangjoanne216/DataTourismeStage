      "dc:description": {
        "de": [
          "Doppelhaushälfte - 2 Schlafzimmer (2 Einzelbetten - 1 Doppelbett) - Küche - Mikrowelle - Esszimmer - Wohnzimmer - TV-Anschluss - Badezimmer mit Badewanne und WC - Kamin - Waschmaschine - Gartenmöbel - Veranda - Parkplatz - Obstgarten und Gartenhaus."
        ],
        "en": [
          "Semi-detached house - 2 bedrooms (2 single beds - 1 double bed) - kitchen - microwave - dining room - living room - TV socket - shower room with bathtub and WC - fireplace - washing machine - garden furniture - veranda - car park - orchard and garden shed."
        ],
        "it": [
          "Casa bifamiliare - 2 camere da letto (2 letti singoli - 1 letto matrimoniale) - cucina - microonde - sala da pranzo - soggiorno - presa TV - bagno con vasca e WC - camino - lavatrice - mobili da giardino - veranda - parcheggio - frutteto e casetta da giardino."
        ],
        "fr": [
          "Maison mitoyenne - 2 chambres (2 lits 1 pers - 1 lit 2 pers) - cuisine - micro-ondes - salle à manger - salon - prise TV - salle d'eau avec baignoire et WC - cheminée - lave-linge - salon de jardin - véranda - parking - verger et abri de jardin."
        ],
        "nl": [
          "Halfvrijstaand huis - 2 slaapkamers (2 eenpersoonsbedden - 1 tweepersoonsbed) - keuken - magnetron - eetkamer - woonkamer - TV-aansluiting - doucheruimte met bad en WC - open haard - wasmachine - tuinmeubelen - veranda - parkeerplaats - boomgaard en tuinhuisje."
        ],
        "es": [
          "Casa adosada - 2 habitaciones (2 camas individuales - 1 cama doble) - cocina - microondas - comedor - sala de estar - toma de TV - cuarto de ducha con baño y WC - chimenea - lavadora - muebles de jardín - veranda - aparcamiento - huerto y caseta de jardín."
        ]
      },
