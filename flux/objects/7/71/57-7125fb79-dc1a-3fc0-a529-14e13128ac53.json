  "rdfs:comment": {
    "de": [
      "Ein Spaziergang im Herzen des Grünen Venedigs mit einem passionierten Animationsführer, um die Geheimnisse des Marais Poitevin zu entdecken!"
    ],
    "en": [
      "Walk in the heart of the Green Venice, with a passionate guide, to discover the secrets of the Marais Poitevin!"
    ],
    "it": [
      "Passeggiata nel cuore della Venezia Verde, con una guida appassionata, per scoprire i segreti del Marais Poitevin!"
    ],
    "fr": [
      "Balade à pied au coeur de la Venise Verte, avec un guide animateur passionné, pour découvrir les secrets du Marais Poitevin !"
    ],
    "es": [
      "¡Pasee por el corazón de la Venecia Verde, con un guía apasionado, para descubrir los secretos del Marais Poitevin!"
    ],
    "nl": [
      "Wandeling in het hart van het Groene Venetië, met een gepassioneerde gids, om de geheimen van het Marais Poitevin te ontdekken!"
    ]
  },
