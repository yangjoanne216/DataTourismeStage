  "rdfs:comment": {
    "de": [
      "Weinkeller in Saint Paul lez Durance. Hervé und Sophie sind leidenschaftliche Weinkenner und bieten jeden Tag Weinproben auf der Grundlage erstaunlicher Kombinationen von Speisen und Weinen an."
    ],
    "en": [
      "Wine cellar in Saint Paul lez Durance. Passionate about wine, Hervé and Sophie offer daily tastings based on amazing food and wine pairings."
    ],
    "it": [
      "Cantina a Saint Paul lez Durance. Appassionati di vino, Hervé e Sophie propongono ogni giorno degustazioni basate su sorprendenti abbinamenti cibo-vino."
    ],
    "fr": [
      "Cave à vins à Saint Paul lez Durance. Passionnés de vins, Hervé et Sophie proposent chaque jour des dégustations sur la base d’accords mets et vins étonnants."
    ],
    "nl": [
      "Wijnkelder in Saint Paul lez Durance. Hervé en Sophie zijn gepassioneerd door wijn en bieden dagelijks proeverijen aan op basis van verrassende wijn-spijs combinaties."
    ],
    "es": [
      "Bodega en Saint Paul lez Durance. Apasionados por el vino, Hervé y Sophie ofrecen catas diarias basadas en sorprendentes maridajes."
    ]
  },
