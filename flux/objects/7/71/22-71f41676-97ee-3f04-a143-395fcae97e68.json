  "rdfs:comment": {
    "de": [
      "Die Winzerin Caroline CASSOT öffnet Ihnen die Türen ihres Weinbergs, des Château La Coustarelle\nEntdecken Sie ihre Rotweine AOC CAHORS, die auf Malbec basieren."
    ],
    "en": [
      "Caroline CASSOT, winemaker, opens the doors of her vineyard, Château La Coustarelle\nCome and discover her red AOC CAHORS wines, made from Malbec."
    ],
    "it": [
      "Caroline CASSOT, enologa, apre le porte del suo vigneto, Château La Coustarelle\nVenite a scoprire i suoi vini rossi AOC CAHORS, a base di Malbec."
    ],
    "fr": [
      "Caroline CASSOT, vigneronne, vous ouvre les portes de son vignoble, le Château La Coustarelle. \nVenez découvrir ses vin rouges AOC CAHORS, à base de Malbec."
    ],
    "nl": [
      "Caroline CASSOT, wijnmaakster, opent de deuren van haar wijngaard, Château La Coustarelle\nKom en ontdek haar rode AOC CAHORS wijnen, gemaakt van Malbec."
    ],
    "es": [
      "Caroline CASSOT, viticultora, abre las puertas de su viñedo, Château La Coustarelle. Ven a descubrir sus vinos tintos AOC CAHORS, elaborados con Malbec."
    ]
  },
