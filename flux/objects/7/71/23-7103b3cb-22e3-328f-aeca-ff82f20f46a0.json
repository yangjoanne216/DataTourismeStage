  "rdfs:comment": {
    "de": [
      "Restaurant im Herzen von Boussac mit Blick auf den Platz, Terrasse und Veranda. Familienküche mit Produkten aus der Region."
    ],
    "en": [
      "Restaurant located in the heart of Boussac, overlooking the square, terrace and veranda. Family cuisine with local products. Low season: Daily menu every day from Monday to Friday.\nHigh season: open from Monday to Saturday for lunch and dinner (a la carte evening).\nGroups on reservation."
    ],
    "it": [
      "Ristorante situato nel cuore di Boussac, con vista sulla piazza, terrazza e veranda. Cucina familiare con prodotti locali."
    ],
    "fr": [
      "Restaurant situé au cœur de Boussac, avec vue sur la place, terrasse et véranda. Cuisine familiale avec produits du terroir."
    ],
    "es": [
      "Restaurante situado en el corazón de Boussac, con vistas a la plaza, terraza y veranda. Cocina familiar con productos locales."
    ],
    "nl": [
