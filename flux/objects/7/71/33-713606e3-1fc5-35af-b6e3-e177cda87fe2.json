  "rdfs:comment": {
    "de": [
      "Im Herzen der Domaine du Peyrolade, im ehemaligen Schloss von Daux, gönnen Sie sich einen Moment der Gaumenfreuden."
    ],
    "en": [
      "In the heart of the Domaine du Peyrolade, in the old castle of Daux, treat yourself to a moment of gourmet pleasure."
    ],
    "it": [
      "Nel cuore del Domaine du Peyrolade, nell'antico castello di Daux, concedetevi un momento di piacere gastronomico."
    ],
    "fr": [
      "Au cœur du Domaine du Peyrolade, dans l'ancien château de Daux, offrez-vous un moment de gourmandise."
    ],
    "nl": [
      "In het hart van het Domaine du Peyrolade, in het voormalige kasteel van Daux, kunt u uzelf trakteren op een moment van gastronomisch genot."
    ],
    "es": [
      "En el corazón del Domaine du Peyrolade, en el antiguo castillo de Daux, regálese un momento de placer gastronómico."
    ]
  },
