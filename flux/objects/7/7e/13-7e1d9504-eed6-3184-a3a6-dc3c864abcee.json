  "rdfs:comment": {
    "de": [
      "Mineralogisches Museum und Museum der Bergbaugeschichte. Animationsprogramm während der gesamten Saison (Einführung in das Schlagwerk, Mineraliensuche im Steinbruch, Workshops...). Aktive und spielerische Entdeckung des Mineralreichs."
    ],
    "en": [
      "Mineralogical and mining history museum. Activities programme all along the season (laden initiation, research of quarry minerals, workshops…). Active and fun discovery of the plant kingdom."
    ],
    "it": [
      "Museo mineralogico e di storia mineraria. Un programma di attività per tutta la stagione (introduzione alla batée, ricerca di minerali nella cava, laboratori, ecc.) Una scoperta attiva e ludica del regno minerale."
    ],
    "fr": [
      "Musée minéralogique et de l'histoire minière. Programme d'animations tout au long de la saison (initiation à la batée, recherche de minéraux en carrière, ateliers...). Découverte active et ludique du règne minéral."
    ],
    "es": [
      "Museo de historia mineralógica y minera. Un programa de actividades a lo largo de la temporada (iniciación a la batida, búsqueda de minerales en la cantera, talleres, etc.). Descubrimiento activo y lúdico del reino mineral."
    ],
    "nl": [
      "Mineralogisch museum en mijngeschiedenis. Programma van animaties gedurende het seizoen (initiatie, geo-wandelen, workshops ...). Actieve en leuke ontdekking van het mineralenrijk."
    ]
  },
