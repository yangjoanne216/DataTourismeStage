  "rdfs:comment": {
    "de": [
      "Eine zweisprachige okzitanisch-französische Ausstellung, die von den kleinen Berufen von früher erzählt. Einen Monat lang in den Räumlichkeiten der interkommunalen Mediathek Larzac et Vallées zu entdecken."
    ],
    "en": [
      "A bilingual Occitan-French exhibition recounting the trades of yesteryear. On display for a month at the Larzac et Vallées intercommunal media library."
    ],
    "it": [
      "Una mostra bilingue occitano-francese che racconta la storia dei piccoli mestieri di un tempo. Esposta per un mese presso la mediateca intercomunale Larzac et Vallées."
    ],
    "fr": [
      "Une exposition bilingue occitan-français qui raconte les petits métiers d'autrefois. A découvrir pendant un mois dans les locaux de la médiathèque intercommunale Larzac et Vallées."
    ],
    "es": [
      "Una exposición bilingüe occitano-francés que cuenta la historia de los pequeños oficios de antaño. Expuesta durante un mes en la mediateca intercomunal de Larzac et Vallées."
    ],
    "nl": [
      "Een tweetalige Occitaans-Franse tentoonstelling over de kleine ambachten van weleer. Een maand lang te zien in de mediatheek van de intercommunale Larzac et Vallées."
    ]
  },
