  "rdfs:comment": {
    "de": [
      "Kochen am Kamin . Dekoration, Poster und Bilder, Gemütlichkeit, man fühlt sich wie zu Hause. Mitten in der Camargue gelegen! Das Ende der Welt."
    ],
    "en": [
      "Kitchen with fireplace. Decoration, posters and paintings, conviviality, you feel like at home. Located in the middle of the Camargue! The end of the world."
    ],
    "it": [
      "Cucina con camino. Decorazione, poster e dipinti, convivialità, ci si sente come a casa. Situato nel mezzo della Camargue! La fine del mondo."
    ],
    "fr": [
      "Cuisine à la cheminée . Décoration, affiches et tableaux, convivialité, on se sent comme à la maison. Situé en pleine Camargue! Le bout du monde."
    ],
    "nl": [
      "Keuken met open haard. Decoratie, posters en schilderijen, gezelligheid, je voelt je er thuis. Gelegen in het midden van de Camargue! Het einde van de wereld."
    ],
    "es": [
      "Cocina con chimenea. Decoración, carteles y cuadros, convivialidad, te sientes como en casa. ¡Situado en medio de la Camarga! El fin del mundo."
    ]
  },
