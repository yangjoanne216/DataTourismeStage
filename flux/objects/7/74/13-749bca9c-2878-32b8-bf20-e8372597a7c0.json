  "rdfs:comment": {
    "de": [
      "Olivier Bonelli, ein Bergbegleiter, nimmt Sie mit auf die Wege und Wanderpfade, halbtags, ganztags oder mehrtägig, um die Täler von Champsaur Valgaudemar zu entdecken."
    ],
    "en": [
      "Olivier Bonelli, mountain leader, will take you on hiking trails and paths, for half a day, a day or more, to discover the valleys of Champsaur Valgaudemar."
    ],
    "it": [
      "Olivier Bonelli, guida alpina, vi accompagnerà in escursioni di mezza giornata, giornata intera o più lunghe alla scoperta delle valli di Champsaur Valgaudemar."
    ],
    "fr": [
      "Olivier Bonelli, accompagnateur en montagne, vous emmène sur les chemins et sentiers de randonnée, à la demi-journée, journée ou plus, pour découvrir les vallées du Champsaur Valgaudemar."
    ],
    "es": [
      "Olivier Bonelli, guía de montaña, le llevará a realizar excursiones de media jornada, de una jornada completa o más largas para descubrir los valles de Champsaur Valgaudemar."
    ],
    "nl": [
      "Olivier Bonelli, berggids, neemt u mee op halve, hele of langere wandelingen om de valleien van Champsaur Valgaudemar te ontdekken."
    ]
  },
