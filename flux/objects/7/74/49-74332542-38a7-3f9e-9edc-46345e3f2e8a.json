  "rdfs:comment": {
    "de": [
      "Städtischer Campingplatz, von April bis Oktober geöffnet, mit 10 schattigen Stellplätzen mit Hecken, 10 Minuten von Segré und Lion d'Angers entfernt, im blauen Anjou."
    ],
    "en": [
      "Municipal campsite open from April to October offering 10 shaded pitches with separating hedges, 10 minutes from Segré and Le Lion d'Angers, in the Anjou Bleu region."
    ],
    "it": [
      "Campeggio comunale aperto da aprile a ottobre che offre 10 piazzole ombreggiate con siepi separate, a 10 minuti da Segré e Le Lion d'Angers, nella regione dell'Anjou Bleu."
    ],
    "fr": [
      "Camping municipal ouvert d'avril à octobre proposant 10 emplacements ombragés avec haies séparatives, à 10 minutes de Segré et du Lion d'Angers, en Anjou bleu."
    ],
    "nl": [
      "Gemeentelijke camping geopend van april tot oktober met 10 schaduwrijke plaatsen met gescheiden heggen, op 10 minuten van Segré en Le Lion d'Angers, in de regio Anjou Bleu."
    ],
    "es": [
      "Camping municipal abierto de abril a octubre que ofrece 10 parcelas sombreadas con setos separados, a 10 minutos de Segré y Le Lion d'Angers, en la región de Anjou Bleu."
    ]
  },
