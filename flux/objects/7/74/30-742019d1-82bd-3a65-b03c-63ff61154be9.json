  "rdfs:comment": {
    "de": [
      "Restaurant mit Pizza, Meeresfrüchten, hausgemachten Muscheln mit Pommes frites, Fisch und Fleisch. Aperitifformel mit Bulots. Bar. Gemütliches Restaurant mit Konzert am Dienstagabend im Sommer."
    ],
    "en": [
      "Restaurant offering pizzas, seafood, home-made mussels and French fries, fish and meat. Formula aperitif bulots. Bar. Friendly restaurant with concert on Tuesday evening in summer."
    ],
    "it": [
      "Ristorante che offre pizze, frutti di mare, cozze e patatine fatte in casa, pesce e carne. Formula aperitivo bulots. Bar. Ristorante accogliente con concerto il martedì sera in estate."
    ],
    "fr": [
      "Restaurant proposant des pizzas, fruits de mer, moules-frites maison, poissons et viandes. Formule apéro bulots. Bar. Restaurant convivial avec concert le mardi soir en été."
    ],
    "es": [
      "Restaurante que ofrece pizzas, marisco, mejillones y patatas fritas caseras, pescado y carne. Fórmula aperitivo bulots. Bar. Restaurante acogedor con concierto los martes por la noche en verano."
    ],
    "nl": [
      "Restaurant met pizza's, zeevruchten, huisgemaakte mosselen en friet, vis en vlees. Formule aperitief bulten. Bar. Vriendelijk restaurant met concert op dinsdagavond in de zomer."
    ]
  },
