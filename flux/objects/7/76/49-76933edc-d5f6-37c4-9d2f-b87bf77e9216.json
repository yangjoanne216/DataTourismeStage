  "rdfs:comment": {
    "de": [
      "Der Belvédère de Perseigne bietet Ihnen ein 360°-Panorama über die Baumwipfel und das Kronendach"
    ],
    "en": [
      "Rise to the top of the peaks to admire the canopy thanks to a 360° panorama is what the Belvedere of Perseigne offers you!"
    ],
    "it": [
      "Il Belvédère de Perseigne vi offre una vista panoramica a 360° dalla cima delle vette!"
    ],
    "fr": [
      "S'élever en haut des cimes pour admirer la canopée grâce à un panorama à 360° voilà ce que vous offre le Belvédère de Perseigne !"
    ],
    "nl": [
      "Het Belvédère de Perseigne biedt u een panoramisch uitzicht van 360° vanaf de top van de bergtoppen!"
    ],
    "es": [
      "El Belvédère de Perseigne le ofrece una vista panorámica de 360° desde la cima de los picos"
    ]
