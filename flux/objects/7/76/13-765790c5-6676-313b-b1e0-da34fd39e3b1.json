  "rdfs:comment": {
    "de": [
      "Café ? Vereinsbibliothek und Spielothek in Saint Michel les Portes.\r\nFreitagabends und Samstagvormittags geöffnet (außer in den Schulferien)\r\nKonzert an einem Freitag im Monat (Infos auf facebook oder über Mailingliste)"
    ],
    "en": [
      "Café ? Library and game library of Saint Michel les Portes.\r\nOpen on Friday evenings and Saturday mornings (except school vacations)\r\nConcert one friday per month (info on facebook or via mailing list)"
    ],
    "it": [
      "Caffè? Biblioteca e ludoteca a Saint Michel les Portes.\r\nAperto il venerdì sera e il sabato mattina (escluse le vacanze scolastiche)\r\nConcerto un venerdì al mese (info su facebook o tramite mailing list)"
    ],
    "fr": [
      "Café – Bibliothèque et ludothèque associatif de Saint Michel les Portes.\r\nOuvert les vendredis soirs et samedis matins (sauf vacances scolaires)\r\nConcert un vendredi par mois (infos sur facebook ou via mailing list)"
    ],
    "es": [
      "¿Café? Biblioteca y ludoteca en Saint Michel les Portes.\r\nAbierto los viernes por la noche y los sábados por la mañana (excepto las vacaciones escolares)\r\nConcierto un viernes al mes (información en facebook o a través de la lista de correo)"
    ],
    "nl": [
      "Café? Bibliotheek en speelotheek in Saint Michel les Portes.\r\nGeopend op vrijdagavond en zaterdagochtend (behalve schoolvakanties)\r\nConcert één vrijdag per maand (info op facebook of via mailinglijst)"
    ]
  },
  "rdfs:label": {
