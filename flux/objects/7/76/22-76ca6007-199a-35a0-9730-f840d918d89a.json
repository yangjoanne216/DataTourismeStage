  "rdfs:comment": {
    "de": [
      "Padiparc bietet Animationen mit Verteilung von Schokolade :\n11.00 Uhr Fütterung der Schildkröten\n14:30 Uhr Fütterung der Chamäleons\n16 Uhr Fütterung einer Schlange\nErmäßigter Tarif für alle Kinder."
    ],
    "en": [
      "Padiparc proposes animations with distribution of chocolates:\n11h feeding of the turtles\n2:30 pm feeding of chameleons\n16h : feeding of a snake\nReduced rate for all children."
    ],
    "it": [
      "Il Padiparc propone animazioni con distribuzione di cioccolatini:\nore 11.00 alimentazione delle tartarughe\nore 14.30 alimentazione dei camaleonti\nore 16: alimentazione di un serpente\nTariffa ridotta per tutti i bambini."
    ],
    "fr": [
      "Padiparc propose des animations avec distribution de chocolats :\n11h nourrissage des tortues\n14h30 nourrissage des caméléons\n16h : nourrissage d'un serpent\nTarif réduit pour tous les enfants."
    ],
    "nl": [
      "Padiparc stelt animaties voor met uitdeling van pralines:\n11 uur voederen van de schildpadden\n14.30u voeding van kameleons\n16.00 uur: voeding van een slang\nGereduceerd tarief voor alle kinderen."
    ],
    "es": [
      "Padiparc propone animaciones con reparto de bombones:\n11h alimentación de las tortugas\n14.30 h: alimentación de camaleones\n16 h: alimentación de una serpiente\nTarifa reducida para todos los niños."
    ]
  },
