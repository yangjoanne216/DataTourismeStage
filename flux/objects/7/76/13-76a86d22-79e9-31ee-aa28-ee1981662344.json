  "rdfs:comment": {
    "de": [
      "In L'écume de mer können Sie Meeresfrüchte probieren: Austern, Miesmuscheln, Venusmuscheln, Garnelen, Wellhornschnecken und andere Muscheln."
    ],
    "en": [
      "L'écume de mer invites you to come and taste seafood: oysters, mussels, clams, shrimps, whelks and other shellfish."
    ],
    "it": [
      "L'écume de mer vi invita a venire a degustare i prodotti del mare: ostriche, cozze, vongole, gamberi, buccini e altri molluschi."
    ],
    "fr": [
      "L'écume de mer vous propose de venir déguster les produits de la mer : huîtres, moules, palourdes, crevettes, bulots et autres coquillages."
    ],
    "nl": [
      "L'écume de mer nodigt u uit om de producten van de zee te komen proeven: oesters, mosselen, kokkels, garnalen, wulken en andere schelpdieren."
    ],
    "es": [
      "L'écume de mer le invita a venir a degustar los productos del mar: ostras, mejillones, almejas, gambas, buccinos y otros mariscos."
    ]
  },
