  "rdfs:comment": {
    "de": [
      "Städtische Etappenunterkunft in freier Verwaltung in Vernet Les Bains. Es werden verschiedene Unterbringungsmöglichkeiten angeboten, vom Schlafsaal bis zur Box. Kapazität für 32 Personen. 3 Schlafsäle für 8 Personen und 4 Boxen für 2 Personen."
    ],
    "en": [
      "Municipal lodging stage in free management located at Vernet Les Bains. Different types of accommodation are available from dormitory to box. Capacity of 32 people. 3 dormitories of 8 persons and 4 boxes of 2 persons."
    ],
    "it": [
      "Gite comunale di sosta con angolo cottura situata a Vernet Les Bains. Sono disponibili diversi tipi di alloggio, dai dormitori ai box. Capacità di 32 persone. 3 dormitori da 8 persone e 4 box da 2 persone."
    ],
    "fr": [
      "Gîte étape municipal en gestion libre situé à Vernet Les Bains. Différentes formules d'hébergements sont proposées du dortoir au box. Capacité d'accueil de 32 personnes. 3 dortoirs de 8 personnes et 4 box de 2 personnes."
    ],
    "nl": [
      "Vakantieverblijf in Vernet Les Bains. Er zijn verschillende soorten accommodatie beschikbaar, van slaapzaal tot box. Capaciteit van 32 personen. 3 slaapzalen van 8 personen en 4 boxen van 2 personen."
    ],
    "es": [
      "Casa rural municipal con autoservicio situada en Vernet Les Bains. Hay diferentes tipos de alojamiento, desde dormitorios hasta cajas. Capacidad para 32 personas. 3 dormitorios de 8 personas y 4 boxes de 2 personas."
    ]
  },
