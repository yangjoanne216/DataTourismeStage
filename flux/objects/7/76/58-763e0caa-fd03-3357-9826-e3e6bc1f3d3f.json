  "rdfs:comment": {
    "de": [
      "Ein Entdeckungspfad auf dem Weingut Château Gassier, nördlich von Aix-en-Provence. Genießen Sie einen lehrreichen Spaziergang im Herzen des Weinbergs!"
    ],
    "en": [
      "A discovery trail at the Château Gassier estate, north of Aix-en-Provence. Enjoy an instructive walk in the heart of the vineyard!"
    ],
    "it": [
      "Un percorso di scoperta nella tenuta di Château Gassier, a nord di Aix-en-Provence. Godetevi una passeggiata istruttiva nel cuore del vigneto!"
    ],
    "fr": [
      "Un sentier découverte au domaine de Château Gassier, au nord d'Aix-en-Provence. Profitez d’une promenade instructive au cœur du vignoble !"
    ],
    "es": [
      "Un recorrido de descubrimiento en la finca del Château Gassier, al norte de Aix-en-Provence. Disfrute de un paseo instructivo en el corazón del viñedo"
    ],
    "nl": [
      "Een ontdekkingsparcours op het landgoed van Château Gassier, ten noorden van Aix-en-Provence. Geniet van een leerzame wandeling in het hart van de wijngaard!"
    ]
  },
