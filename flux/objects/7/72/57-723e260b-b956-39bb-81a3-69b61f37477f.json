  "rdfs:comment": {
    "de": [
      "Besuch der Orte mit allen Sinnen, wo sich Klänge und Farben mit der Geschichte der Steine und des Dorfes vermischen."
    ],
    "en": [
      "Sensory visit of the place, where sounds and colors are mixed with the history of the stones and the village."
    ],
    "it": [
      "Una visita sensoriale del sito, dove suoni e colori si mescolano alla storia delle pietre e del villaggio."
    ],
    "fr": [
      "Visite sensorielle des lieux, où sons et couleurs se mêlent à l’histoire des pierres et du bourg."
    ],
    "es": [
      "Una visita sensorial del yacimiento, donde los sonidos y los colores se mezclan con la historia de las piedras y del pueblo."
    ],
    "nl": [
      "Een zintuiglijk bezoek aan de site, waar geluiden en kleuren zich vermengen met de geschiedenis van de stenen en het dorp."
    ]
  },
