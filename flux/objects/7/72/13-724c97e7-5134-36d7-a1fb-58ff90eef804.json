  "rdfs:comment": {
    "de": [
      "GAEC in der Milchproduktion. Besuche das ganze Jahr über möglich. Verkaufsladen montags bis freitags von 17:30 bis 19:30 Uhr und samstags morgens von 10 bis 12 Uhr geöffnet."
    ],
    "en": [
      "GAEC in dairy production. Possibility of visits all year round. Store open from Monday to Friday from 5:30 pm to 7:30 pm and Saturday morning from 10 am to 12 am."
    ],
    "it": [
      "BCAA in produzione lattiero-casearia. Visite possibili tutto l'anno. Negozio di vendita aperto dal lunedì al venerdì dalle 17.30 alle 19.30 e il sabato mattina dalle 10.00 alle 12.00."
    ],
    "fr": [
      "GAEC en production laitière. Possibilité de visites toute l'année. Magasin de vente ouvert du lundi au vendredi de 17h30 à 19h30 et le samedi matin de 10h à 12h."
    ],
    "nl": [
      "GAEC in zuivelproductie. Bezoeken zijn het hele jaar mogelijk. Verkoopwinkel geopend van maandag t/m vrijdag van 17.30 tot 19.30 uur en zaterdagochtend van 10.00 tot 12.00 uur."
    ],
    "es": [
      "BCAM en producción lechera. Posibilidad de visitas durante todo el año. Tienda de venta abierta de lunes a viernes de 17.30 a 19.30 horas y los sábados por la mañana de 10.00 a 12.00 horas."
    ]
  },
