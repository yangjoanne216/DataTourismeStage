  "rdfs:comment": {
    "de": [
      "Der Jardin Langert empfängt Sie im Herzen des Dorfes Muttersholtz zu einer spielerischen und sensorischen Entdeckung des grundwasserführenden Bachs."
    ],
    "en": [
      "The Jardin Langert welcomes you in the heart of the village of Muttersholtz for a playful and sensory discovery of the phreatic stream."
    ],
    "it": [
      "Il Giardino di Langert vi accoglie nel cuore del villaggio di Muttersholtz per una scoperta ludica e sensoriale del corso d'acqua."
    ],
    "fr": [
      "Le Jardin Langert vous accueille en plein coeur du village de Muttersholtz pour une découverte ludique et sensorielle du ruisseau phréatique."
    ],
    "es": [
      "El Jardín de Langert le da la bienvenida en el corazón del pueblo de Muttersholtz para un descubrimiento lúdico y sensorial del curso de agua."
    ],
    "nl": [
      "De Tuin van Langert verwelkomt u in het hart van het dorp Muttersholtz voor een speelse en zintuiglijke ontdekking van de waterloop."
    ]
  },
