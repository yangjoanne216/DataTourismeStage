  "rdfs:comment": {
    "de": [
      "My Beers Arles, Ihr Bierkeller und Ihre Bierbar in Bouches-du-Rhône. Entdecken Sie mehr als 400 Referenzen aus aller Welt im Kellerbereich, 90 Flaschen im Kühlschrank und 8 Zapfanlagen im Barbereich."
    ],
    "en": [
      "My Beers Arles, your cellar and beer bar in the Bouches-du-Rhône. Discover more than 400 references from around the world in the cellar, 90 bottles in the fridge and 8 draught bottles in the bar."
    ],
    "it": [
      "My Beers Arles, la vostra cantina e beer bar nelle Bouches-du-Rhône. Scoprite più di 400 referenze da tutto il mondo in cantina, 90 bottiglie in frigo e 8 birre alla spina al bar."
    ],
    "fr": [
      "My Beers Arles, votre cave et bar à bière des Bouches-du-Rhône. Découvrez plus de 400 références du monde partie cave, 90 bouteilles frigo et 8 tirages pression partie bar."
    ],
    "nl": [
      "My Beers Arles, uw kelder en bierbar in de Bouches-du-Rhône. Ontdek meer dan 400 referenties uit de hele wereld in de kelder, 90 flessen in de koelkast en 8 tapinstallaties in de bar."
    ],
    "es": [
      "My Beers Arles, su bodega y bar de cerveza en Bouches-du-Rhône. Descubra más de 400 referencias de todo el mundo en la bodega, 90 botellas en la nevera y 8 cervezas de bar."
    ]
  },
