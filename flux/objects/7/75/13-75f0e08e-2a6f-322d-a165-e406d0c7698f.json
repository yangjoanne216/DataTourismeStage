  "rdfs:comment": {
    "de": [
      "Traditionelles Restaurant am Ufer der Wasserfläche von Pas des Ondes.\r\nSpeisekarte, Mechoui, Plancha, Rotisserie, Fisch, Pizzas aus dem Holzofen.\r\nEmpfang von Gruppen möglich.\r\nMusikalische Veranstaltungen während der Sommermonate."
    ],
    "en": [
      "Traditional restaurant at the edge of the water level of the Pas des Ondes.\r\nMenu, mechoui, plancha, rotisserie, fish, wood-fired pizzas.\r\nReception of group possible.\r\nMusical events during summer."
    ],
    "it": [
      "Ristorante tradizionale ai margini del lago Pas des Ondes.\r\nMenu, mechoui, plancha, rosticceria, pesce, pizze a legna.\r\nI gruppi sono benvenuti.\r\nEventi musicali durante l'estate."
    ],
    "fr": [
      "Restaurant traditionnel au bord du plan d'eau du Pas des Ondes.\r\nCarte, méchoui, plancha, rôtisserie, poissons, pizzas au feu de bois.\r\nAccueil de groupe possible.\r\nEvènements  musicaux pendant période estivale."
    ],
    "nl": [
      "Traditioneel restaurant aan de rand van het meer Pas des Ondes.\r\nMenu, mechoui, plancha, rotisserie, vis, houtgestookte pizza's.\r\nGroepen zijn welkom.\r\nMuzikale evenementen tijdens de zomer."
    ],
    "es": [
      "Restaurante tradicional a orillas del lago Pas des Ondes.\r\nMenú, mechoui, plancha, asador, pescado, pizzas a la leña.\r\nLos grupos son bienvenidos.\r\nEventos musicales durante el verano."
    ]
  },
