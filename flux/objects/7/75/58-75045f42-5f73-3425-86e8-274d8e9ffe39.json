  "rdfs:comment": {
    "de": [
      "Auf dem Hauptplatz des Dorfes pierre et Vacances, gegenüber dem See entdecken Sie eine provenzalische Küche."
    ],
    "en": [
      "On the main square of the village of pierre et Vacances, facing the lake, come and discover a Provencal cuisine."
    ],
    "it": [
      "Sulla piazza principale del villaggio, di fronte al lago, venite a scoprire la cucina provenzale."
    ],
    "fr": [
      "Sur la place principale du village de pierre et Vacances, face au lac venez découvrir une cuisine Provençale."
    ],
    "nl": [
      "Op het hoofdplein van het dorp Pierre et Vacances, tegenover het meer, kunt u kennismaken met de Provençaalse keuken."
    ],
    "es": [
      "En la plaza principal del pueblo, frente al lago, venga a descubrir una cocina provenzal."
    ]
  },
