  "rdfs:comment": {
    "de": [
      "Der Menhir von Kermarquer weist Spuren von alten Gravuren aus der Jungsteinzeit auf. Er steht unter Denkmalschutz.",
      "Der höchste Menhir in Morbihan."
    ],
    "en": [
      "The highest menhir in Morbihan.",
      "The menhir of Kermarquer shows traces of ancient engravings from the Neolithic period. Listed as a historical monument."
    ],
    "it": [
      "Il menhir di Kermarquer mostra tracce di antiche incisioni risalenti al Neolitico. Classificato come monumento storico.",
      "Il menhir più alto del Morbihan."
    ],
    "fr": [
      "Le menhir de Kermarquer présente des traces de gravures anciennes du néolithique. Classé monument historique.",
      "Le menhir le plus haut du Morbihan."
    ],
    "es": [
      "El menhir de Kermarquer muestra restos de antiguos grabados del neolítico. Clasificado como monumento histórico.",
      "El menhir más alto de Morbihan."
