  "rdfs:comment": {
    "de": [
      "Produzenten von Ziegenkäse, Joghurt, Faseln, Holzscheiten, Tommes (gepresste ungekochte Teigwaren). Von März bis Dezember nur auf Märkten und lokalen Verkaufstouren."
    ],
    "en": [
      "Producers of goat cheese, yoghurts, faisselles, logs, tommes (uncooked pressed paste). From March to December only on markets and local sales rounds."
    ],
    "it": [
      "Produttori di formaggio di capra, yogurt, faisselles, tronchetti, tommes (formaggio pressato non cotto). Da marzo a dicembre solo nei mercati e nei turni di vendita locali."
    ],
    "fr": [
      "Producteurs de fromages de chèvre, yaourts , faisselles, bûches, tommes (pâte pressée non cuites). De mars à décembre uniquement sur les marchés et les tournées de ventes locales."
    ],
    "es": [
      "Productores de queso de cabra, yogur, faisselles, troncos, tommes (queso prensado sin cocer). De marzo a diciembre sólo en los mercados y en las rondas de ventas locales."
    ],
    "nl": [
      "Producenten van geitenkaas, yoghurt, faisselles, houtblokken, tommes (ongekookte geperste kaas). Van maart tot december alleen op markten en lokale verkooprondes."
    ]
  },
