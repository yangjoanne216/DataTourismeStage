  "rdfs:comment": {
    "de": [
      "\"James Cameron: Die Odyssee eines Filmemachers\", von D. Fakrikian, Drehbuchautor und Regisseur.\nUm 20:30 Uhr: Vorpremiere von \"Avatar 2: Der Weg des Wassers\" von James Cameron.\nFür alle Zuschauer."
    ],
    "en": [
      "\"James Cameron: A Filmmaker's Odyssey,\" by D. Fakrikian, writer and director.\nAt 8:30 p.m., preview screening of James Cameron's \"Avatar 2: The Way of the Water\".\nAll audiences."
    ],
    "it": [
      "\"James Cameron: l'odissea di un regista\", di D. Fakrikian, scrittore e regista.\nAlle 20.30, anteprima di \"Avatar 2: La via dell'acqua\" di James Cameron.\nTutti i pubblici."
    ],
    "fr": [
      "\"James Cameron : l'odyssée d'un cinéaste\", par D. Fakrikian, scénariste et réalisateur.\nA 20 h 30, projection en avant-première de \"Avatar 2 : la voie de l'eau\" de James Cameron.\nTout public."
    ],
    "es": [
      "\"James Cameron: la odisea de un cineasta\", por D. Fakrikian, escritor y director.\nA las 20:30 horas, preestreno de \"Avatar 2: La forma del agua\" de James Cameron.\nTodos los públicos."
    ],
    "nl": [
      "\"James Cameron: A Filmmaker's Odyssey\", door D. Fakrikian, schrijver en regisseur.\nOm 20.30 uur, voorvertoning van \"Avatar 2: The Way of the Water\" door James Cameron.\nAlle doelgroepen."
    ]
  },
