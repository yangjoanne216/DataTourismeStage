  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/635cc090-0997-358f-b4a9-ec8e9e71629d",
      "dc:description": {
        "de": [
          "Tierpark (Bauernhoftiere), Lehrgarten, Weinberg, Teiche, Duftpfad (aromatische Pflanzen), kleine Wasserflächen, Spurensuche (Spuren der lokalen Fauna), Picknickbereich."
        ],
        "en": [
          "Animal park (farm animals), educational garden, vineyard, ponds, scent trail (aromatic plants), small bodies of water, trail game (footprints of the local fauna), picnic area."
        ],
        "it": [
          "Parco degli animali (animali della fattoria), orto didattico, vigneto, stagni, sentiero dei profumi (piante aromatiche), piccoli specchi d'acqua, sentiero-gioco (impronte della fauna locale), area picnic."
        ],
        "fr": [
          "Parc animalier (animaux de la ferme), jardin éducatif, vigne, mares, sentier ''senteurs'' (plantes aromatiques), petits plans d'eau, jeu des traces (empreintes de la faune locale), aire de pique-nique."
        ],
        "es": [
          "Parque de animales (animales de granja), jardín educativo, viñedo, estanques, sendero de aromas (plantas aromáticas), pequeñas masas de agua, juego de pistas (huellas de la fauna local), zona de picnic."
        ],
        "nl": [
          "Dierenpark (boerderijdieren), educatieve tuin, wijngaard, vijvers, geurspoor (aromatische planten), kleine waterpartijen, spoorspel (voetsporen van de plaatselijke fauna), picknickplaats."
        ]
      },
