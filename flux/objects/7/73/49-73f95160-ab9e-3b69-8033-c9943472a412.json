  "rdfs:comment": {
    "de": [
      "ÜBER DIE BIBLIOTHEK :\n\nDie Bibliothek von Fresnay bietet einen Bücherverleih für alle Altersgruppen; Comics, Romane, Kinderbücher...\n\nAbonnements sind für ein Jahr oder einen Monat möglich."
    ],
    "en": [
      "ABOUT THE LIBRARY:\n\nThe Fresnay library offers the loan of books for the general public; comics, novels, children's books...\n\nYearly or monthly subscriptions are possible."
    ],
    "it": [
      "SULLA BIBLIOTECA :\n\nLa biblioteca Fresnay offre libri per tutte le età: fumetti, romanzi, libri per bambini...\n\nÈ possibile abbonarsi per un anno o per un mese."
    ],
    "fr": [
      "A PROPOS DE LA BIBLIOTHEQUE :\n\nLa bibliothèque de Fresnay propose le prêt de livres pour tout public; Bd, romans, livres enfants...\n\nAbonnements possibles à l'année ou au mois."
    ],
    "es": [
      "SOBRE LA BIBLIOTECA :\n\nLa biblioteca de Fresnay ofrece libros para todas las edades; cómics, novelas, libros infantiles...\n\nEs posible suscribirse por un año o un mes."
    ],
    "nl": [
      "OVER DE BIBLIOTHEEK :\n\nDe bibliotheek van Fresnay biedt boeken voor alle leeftijden; strips, romans, kinderboeken...\n\nAbonnementen zijn mogelijk voor een jaar of een maand."
    ]
  },
