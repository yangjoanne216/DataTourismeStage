  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/113fbe6b-c38f-3d54-8fd8-51648d17c3eb",
      "dc:description": {
        "de": [
          "2 Min. von Capvern-Les-Bains, einem Kur- und Wellnessort, entfernt.",
          "Auf 2 Ebenen: 2 Schlafzimmer (2 Betten 2 pl) Küche, Duschbad, Terrasse, Gartenmöbel, Grill, Garage, nicht eingezäuntes Grundstück. Elektrische Heizung. Nebenkosten extra."
        ],
        "en": [
          "At 2 min from Capvern-Les-Bains, spa and fitness center.",
          "On 2 levels : 2 bedrooms (2 beds 2 pl) kitchen, shower room, terrace, garden furniture, barbecue, garage, not closed ground. Electric heating. Charges in addition."
        ],
        "it": [
          "A 2 minuti da Capvern-Les-Bains, località termale e di cura.",
          "Su 2 livelli: 2 camere (2 letti matrimoniali), cucina, bagno con doccia, terrazza, mobili da giardino, barbecue, garage, terreno non recintato. Riscaldamento elettrico. Spese non incluse."
        ],
        "fr": [
          "A 2 min de Capvern-Les-Bains, station thermale et de remise en forme.",
          "Sur 2 niveaux : 2 Chambres (2 lits 2 pl) cuisine, salle d'eau, terrasse, salon de jardin, barbecue, garage, terrain non clos. Chauffage électrique. Charges en sus."
        ],
        "es": [
          "A 2 minutos de Capvern-Les-Bains, balneario y centro de salud.",
          "En 2 niveles: 2 dormitorios (2 camas dobles), cocina, cuarto de ducha, terraza, muebles de jardín, barbacoa, garaje, terreno sin vallar. Calefacción eléctrica. Gastos no incluidos."
        ],
        "nl": [
          "2 minuten van Capvern-Les-Bains, een kuuroord.",
          "Op 2 niveaus: 2 slaapkamers (2 tweepersoonsbedden), keuken, doucheruimte, terras, tuinmeubelen, barbecue, garage, niet omheind terrein. Elektrische verwarming. Kosten niet inbegrepen."
        ]
      },
