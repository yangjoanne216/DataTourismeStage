  "rdfs:comment": {
    "de": [
      "Gourmet-Restaurant im Herzen von Marseille im Stadtteil Castellane. Einfache, kreative Küche, die lokale und saisonale Produkte verwendet."
    ],
    "en": [
      "Gastronomic restaurant located in the heart of Marseille in the Castellane district. Simple, creative cuisine using local and seasonal products."
    ],
    "it": [
      "Ristorante gastronomico situato nel cuore di Marsiglia, nel quartiere Castellane. Cucina semplice e creativa con prodotti locali e di stagione."
    ],
    "fr": [
      "Restaurant gastronomique situé au coeur de Marseille dans le quartier de Castellane. Cuisine simple, créative utilisant des produits locaux et de saison."
    ],
    "es": [
      "Restaurante gastronómico situado en el corazón de Marsella, en el barrio de Castellane. Cocina sencilla y creativa con productos locales de temporada."
    ],
    "nl": [
      "Gastronomisch restaurant in het hart van Marseille in de wijk Castellane. Eenvoudige, creatieve keuken met lokale en seizoensgebonden producten."
    ]
