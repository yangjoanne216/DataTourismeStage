  "rdfs:comment": {
    "de": [
      "Le Bayle-Vert (Baile-Vert) ist ein provenzalisches Bauernhaus aus dem 18. Jahrhundert, das in die Liste der Nationalen Monumente aufgenommen wurde und am Rande der Crau inmitten von landwirtschaftlichen Flächen liegt."
    ],
    "en": [
      "Le Bayle-Vert (Baile-Vert) is an 18th century Provencal farmhouse, listed as a National Monument, situated on the banks of the Crau river, in the middle of farmland."
    ],
    "it": [
      "La Bayle-Vert (Baile-Vert) è una casa colonica provenzale del XVIII secolo, classificata come monumento nazionale, situata ai margini della Crau, in mezzo a terreni agricoli."
    ],
    "fr": [
      "Le Bayle-Vert (Baile-Vert) est un mas provençal du 18ème siècle, inscrit aux Monuments Nationaux, situé au abords de la Crau, au milieu de terres agricoles."
    ],
    "es": [
      "La Bayle-Vert es una granja provenzal del siglo XVIII, catalogada como Monumento Nacional, situada al borde de la Crau, en medio de las tierras de cultivo."
    ],
    "nl": [
      "De Bayle-Vert is een Provençaalse boerderij uit de 18e eeuw, geklasseerd als Nationaal Monument, gelegen aan de rand van de Crau, te midden van landbouwgrond."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Le Bayle-Vert (Centre Mas-Felipe Delavouët)"
    ]
  },
