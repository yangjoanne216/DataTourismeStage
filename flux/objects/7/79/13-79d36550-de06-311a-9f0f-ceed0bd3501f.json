  "rdfs:comment": {
    "de": [
      "Sie beziehen ein altes, aus Basalt gebautes Bauernhaus, dessen Ursprung auf das 17."
    ],
    "en": [
      "Enjoy a stay in this 17th centuary former farmhouse built from basalt stone."
    ],
    "it": [
      "Prenderete possesso di un'antica casa colonica in basalto risalente al XVII secolo."
    ],
    "fr": [
      "Vous prendrez possession d'une ancienne ferme bâtie en basalte dont l'origine remonte au 17e siècle."
    ],
    "nl": [
      "U neemt bezit van een oude basaltboerderij uit de 17e eeuw."
    ],
    "es": [
      "Tomará posesión de una antigua granja de basalto que data del siglo XVII."
    ]
  },
