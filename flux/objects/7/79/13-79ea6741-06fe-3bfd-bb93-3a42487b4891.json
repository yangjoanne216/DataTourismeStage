  "rdfs:comment": {
    "de": [
      "Bar-Restaurant Le Nant Cortet im Dorfzentrum von Cohennoz. Küche mit lokalen Produkten aus Savoyen. Reservierung empfohlen. Kam am 8.1.2014 in der Sendung »Villages de Montagne« des französischen Fernsehsenders TF1. Speisekarte (Spezialitäten) online unter Pages Jaunes.fr und bei Facebook."
    ],
    "en": [
      "Bar-Restaurant \"Le Nant Cortet\" located in Cohennoz village. Gastronomic menus based on local Savoyard produce. Booking recommended. As seen on French TV channel TF1 \"Villages de Montagne\" on 8/01/2014. Menu (specialities) on Pages Jaunes.fr and Facebook."
    ],
    "it": [
      "Bar-ristorante \"Le Nant Cortet\", nel capoluogo di Cohennoz. Gastronomia savoiarda. È necessaria la prenotazione. Visto su TF1 Villages de Montagne l'08/01/2014. Menu (specialità) su Pages Jaunes.fr e Facebook."
    ],
    "fr": [
      "Bar-Restaurant \"Le Nant Cortet\", au chef lieu de Cohennoz. Gastronomie du terroir savoyard. Réservation souhaitée. Vu à TF1 Villages de Montagne le 08/01/2014. Menus (spécialités) sur Pages Jaunes.fr et Facebook."
    ],
    "es": [
      "Bar-Restaurante \"Le Nant Cortet\", en la ciudad principal de Cohennoz. Gastronomía saboyana. Es necesario reservar. Visto en TF1 Villages de Montagne el 08/01/2014. Menús (especialidades) en Pages Jaunes.fr y Facebook."
    ],
    "nl": [
      "Bar-Restaurant \"Le Nant Cortet\", in de hoofdplaats Cohennoz. Savoyaardse gastronomie. Reserveren verplicht. Gezien op TF1 Villages de Montagne op 08/01/2014. Menu's (specialiteiten) op Pages Jaunes.fr en Facebook."
    ]
