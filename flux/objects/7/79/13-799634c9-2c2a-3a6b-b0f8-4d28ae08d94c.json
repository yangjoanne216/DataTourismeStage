  "rdfs:comment": {
    "de": [
      "Sunsea plage in Saint-Clair empfängt Sie für einen entspannten Moment am Strand mit Matratzen & Sonnenschirm. Sunsea plage bietet Ihnen eine mediterrane Küche direkt am Wasser."
    ],
    "en": [
      "Sunsea plage in Saint-Clair welcomes you for a relaxing moment on the beach with mattress & umbrella. Sunsea plage offers you a Mediterranean cuisine at the water's edge."
    ],
    "it": [
      "Sunsea plage a Saint-Clair vi accoglie per un momento di relax sulla spiaggia con materassino e ombrellone. Sunsea plage vi offre una cucina mediterranea in riva al mare."
    ],
    "fr": [
      "Sunsea plage à Saint-Clair vous accueille pour un moment détente sur la plage avec matelas & parasol. Sunsea plage vous propose une cuisine méditerranéenne au bord de l'eau."
    ],
    "nl": [
      "Sunsea plage in Saint-Clair verwelkomt u voor een ontspannen moment op het strand met matras & parasol. Sunsea plage biedt u een mediterrane keuken aan de waterkant."
    ],
    "es": [
      "Sunsea plage en Saint-Clair le da la bienvenida para un momento de relax en la playa con colchón y sombrilla. Sunsea plage le ofrece una cocina mediterránea al borde del agua."
    ]
  },
