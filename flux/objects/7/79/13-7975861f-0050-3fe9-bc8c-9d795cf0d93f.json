  "rdfs:comment": {
    "de": [
      "Port Cros, Perle der goldenen Inseln, mit Vincent Blondel, einem Naturführer. Er nimmt Sie auf den Wegen von Port Cros mit auf eine begleitete Wanderung im Herzen des Nationalparks und der grünen Oase vor der Küste von Hyères."
    ],
    "en": [
      "Port Cros, pearl of the golden islands, with Vincent Blondel, naturalist guide. He will take you on a guided walk in the heart of the national park and the green setting off Hyères."
    ],
    "it": [
      "Port Cros, perla delle isole d'oro, con Vincent Blondel, guida naturalistica. Vi accompagnerà in una visita guidata di Port-Cros, nel cuore del parco nazionale e nella verde cornice di Hyères."
    ],
    "fr": [
      "Port Cros, perle des îles d'or, avec Vincent Blondel, guide naturaliste. Il vous emmène sur les chemins de Port-Cros pour une balade accompagnée au coeur du parc national et de l'écrin de verdure au large de Hyères."
    ],
    "es": [
      "Port Cros, perla de las islas doradas, con Vincent Blondel, guía naturalista. Le llevará a realizar una visita guiada por Port-Cros, en el corazón del parque nacional y el entorno verde de Hyères."
    ],
    "nl": [
      "Port Cros, parel van de gouden eilanden, met Vincent Blondel, natuurgids. Hij neemt u mee op een rondleiding door Port-Cros in het hart van het nationale park en de groene omgeving van Hyères."
    ]
  },
  "rdfs:label": {
    "en": [
      "Guided Tour in Port Cros National Park"
    ],
    "fr": [
      "Balade nature accompagnée à Port-Cros avec Vincent Blondel"
    ]
  },
