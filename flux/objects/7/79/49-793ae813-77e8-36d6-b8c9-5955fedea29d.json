  "rdfs:comment": {
    "de": [
      "Die Auster als Leidenschaft! Besuchen Sie uns direkt auf der Austernfarm und entdecken Sie die hochwertigen Austern, die in der Bucht von Bourgneuf produziert werden."
    ],
    "en": [
      "The oyster for passion ! Go directly to the oyster farm to discover high quality oysters, produced in the Bay of Bourgneuf."
    ],
    "it": [
      "Ostriche per passione! Visitate l'allevamento di ostriche per scoprire le ostriche di alta qualità prodotte nella baia di Bourgneuf."
    ],
    "fr": [
      "L'huitre pour passion ! Rendez-vous sur directement sur l'exploitation ostréicole pour découvrir des huîtres de grande qualité, produites dans la Baie de Bourgneuf."
    ],
    "es": [
      "¡Ostras por pasión! Visite la ostrería para descubrir las ostras de gran calidad que se producen en la bahía de Bourgneuf."
    ],
    "nl": [
      "Oesters voor de passie! Bezoek de oesterkwekerij en ontdek de hoge kwaliteit oesters die in de baai van Bourgneuf worden geproduceerd."
    ]
  },
