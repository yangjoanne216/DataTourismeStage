  "rdfs:comment": {
    "de": [
      "Lindenkapelle, die 1613 erbaut wurde und eine Kombination aus Blaustein (Sockel, Harfe und Türrahmen) und traditionellem Backstein (Mauerwerksfüllung) aufweist."
    ],
    "en": [
      "Lime tree chapel built in 1613, combining blue stone (base, harpoon and door frame) and traditional brick (masonry filling)."
    ],
    "it": [
      "Cappella in tiglio costruita nel 1613, che combina pietra blu (base, arpione e cornice della porta) e mattoni tradizionali (tamponamento in muratura)."
    ],
    "fr": [
      "Chapelle aux tilleuls construire en 1613, associant pierre bleue (soubassement, harpage et encadrement de porte) et brique traditionnelle (remplissage de maçonnerie)."
    ],
    "nl": [
      "Lindeboomkapel gebouwd in 1613, met een combinatie van arduin (basis, harpoen en deurkozijn) en traditionele baksteen (gemetselde invulling)."
    ],
    "es": [
      "Capilla de tilo construida en 1613, combinando piedra azul (base, arpón y marco de la puerta) y ladrillo tradicional (relleno de mampostería)."
    ]
  },
