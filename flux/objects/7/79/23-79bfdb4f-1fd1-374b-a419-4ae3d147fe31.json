  "rdfs:comment": {
    "de": [
      "Restaurant, Bar, Café. Konvivial. Pizzen... Gutes Preis-Leistungs-Verhältnis. Kostenloses Konzert im Sommer und Winter. Seit 2008 jedes Jahr im Guide du Routard gelistet. Kostenloser Wifi-Zugang. Reservierung empfohlen."
    ],
    "en": [
      "Restaurant - Bar - Charming cafe with live music."
    ],
    "it": [
      "Ristorante, bar, caffè. Conviviale. Pizze... Buon rapporto qualità-prezzo. Concerto gratuito in estate e in inverno. Presente nella guida dei viaggiatori zaino in spalla ogni anno dal 2008. Accesso wifi gratuito. Prenotazione consigliata."
    ],
    "fr": [
      "Restaurant, bar, café. Convivial. Pizzas... Bon rapport qualité/prix. Concert gratuit été et hiver. Référencé dans le guide du routard chaque année depuis 2008. Accès wifi gratuit. Réservation conseillée."
    ],
    "es": [
      "Restaurante, bar, cafetería. Convivial. Pizzas... Buena relación calidad-precio. Concierto gratuito en verano e invierno. Incluido en la guía de mochileros todos los años desde 2008. Acceso wifi gratuito. Se recomienda reservar."
    ],
    "nl": [
      "Restaurant, bar, café. Gezellig. Pizza's... Goede prijs-kwaliteitverhouding. Gratis concert in zomer en winter. Staat sinds 2008 elk jaar in de gids voor rugzaktoeristen. Gratis wifi. Reservering aanbevolen."
    ]
  },
