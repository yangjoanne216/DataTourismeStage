      "dc:description": {
        "de": [
          "Traditionelles Restaurant, hausgemachte Küche. Großer offener Kamin zum Grillen. Terrasse. Jeden Mittag von Montag bis Freitag geöffnet, außer am Wochenende."
        ],
        "en": [
          "Traditional restaurant, home cooking. Large open fireplace for grilling. Terrace. Open for lunch every day from Monday to Friday, except weekends."
        ],
        "it": [
          "Ristorante tradizionale, cucina casalinga. Grande camino aperto per le grigliate. Terrazza. Aperto tutti i giorni dal lunedì al venerdì, tranne il fine settimana."
        ],
        "fr": [
          "Restaurant traditionnel, cuisine maison. Grande cheminée ouverte pour grillades. Terrasse. Ouvert tous les midis, du lundi au vendredi, sauf le week-end."
        ],
        "nl": [
          "Traditioneel restaurant, huiselijke keuken. Grote open haard om te grillen. Terras. Dagelijks geopend van maandag tot vrijdag, behalve in het weekend."
        ],
        "es": [
          "Restaurante tradicional, cocina casera. Gran chimenea abierta para asar. Terraza. Abierto todos los días de lunes a viernes, excepto los fines de semana."
        ]
      },
