  "rdfs:comment": {
    "de": [
      "Unabhängige Unterkunft auf 2 Ebenen, eingezäunter Garten (1200m2) und überdachte Terrasse. Esszimmer (Holzofen), Küchenbereich (Geschirrspüler), Wohnzimmer (2 Sofas, davon 1 ausziehbar, TV), 3 Schlafzimmer (2x140 + 2x90), Badezimmer (Waschmaschine), Gartenmöbel."
    ],
    "en": [
      "Independent gite on 2 levels in a village. 3 Rooms (2 x 140, 2 x 90), bathroom, cellar (washing machine), kitchenette (dishwasher) / dining room, lounge (2 sofas one of which is convertible, television), complete electric heating, private enclosed garden (1,000 m²) and covered terrace (18 m²), garden lounge, barbecue."
    ],
    "it": [
      "Alloggio indipendente su 2 livelli, giardino recintato (1200 m2) e terrazza coperta. Sala da pranzo (stufa a legna), angolo cottura (lavastoviglie), soggiorno (2 divani, 1 convertibile, TV), 3 camere da letto (2x140 + 2x90), bagno con doccia (lavatrice), mobili da giardino."
    ],
    "fr": [
      "Gîte indépendant sur 2 niveaux, jardin clos (1200m2) et terrasse couverte. Salle à manger (poêle à bois), espace cuisine (lav- vaisselle), salon (2 canapés dont 1 convertible, TV), 3 chambres (2x140 + 2x90), salle d'eau (lave-linge), salon de jardin."
    ],
    "nl": [
      "Onafhankelijke gîte op 2 niveaus, omheinde tuin (1200m2) en overdekt terras. Eetkamer (houtkachel), keukenhoek (vaatwasser), woonkamer (2 banken, 1 slaapbank, TV), 3 slaapkamers (2x140 + 2x90), badkamer met douche (wasmachine), tuinmeubels."
    ],
    "es": [
      "Casa rural independiente en 2 niveles, jardín cerrado (1200m2) y terraza cubierta. Comedor (estufa de leña), zona de cocina (lavavajillas), sala de estar (2 sofás, 1 convertible, TV), 3 dormitorios (2x140 + 2x90), cuarto de ducha (lavadora), muebles de jardín."
    ]
  },
