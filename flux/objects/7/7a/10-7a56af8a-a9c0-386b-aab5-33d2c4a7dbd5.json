  "rdfs:comment": {
    "de": [
      "Kleiner Familienbetrieb, der die Umwelt respektiert und einen Vertrag für nachhaltige Landwirtschaft hat. Spezialitäten: Honig, Lebkuchen und Apfelsaft. Besichtigung mit Verkostung nach Vereinbarung."
    ],
    "en": [
      "Small family farm, respecting the environment, under a sustainable agriculture contract. Specialities: honey, gingerbread and apple juice. Visit with tasting by appointment."
    ],
    "it": [
      "Piccola azienda agricola a conduzione familiare, rispettosa dell'ambiente, con un contratto di agricoltura sostenibile. Specialità: miele, pan di zenzero e succo di mela. Visita con degustazione su appuntamento."
    ],
    "fr": [
      "Petite exploitation familiale, respectant l'environnement, sous contrat d'agriculture durable. Spécialités: miel, pain d'épices et jus de pomme. Visite avec dégustation sur rendez-vous."
    ],
    "es": [
      "Pequeña explotación familiar, respetuosa con el medio ambiente, con un contrato de agricultura sostenible. Especialidades: miel, pan de jengibre y zumo de manzana. Visita con degustación con cita previa."
    ],
    "nl": [
      "Klein familiebedrijf, met respect voor het milieu, onder een duurzaam landbouwcontract. Specialiteiten: honing, peperkoek en appelsap. Bezoek met proeverij op afspraak."
    ]
  },
