  "rdfs:comment": {
    "de": [
      "Christian GIRAUDON, handwerklicher Destillateur, bietet Ihnen ätherische Öle und Hydrolate an, die aus Pflanzen aus biologischem Anbau, lokalen, regionalen und von Hand gepflückten Pflanzen gewonnen werden."
    ],
    "en": [
      "Christian GIRAUDON, Artisan Distiller, offers you essential oils and hydrolats extracted from organically grown, local, regional, hand-picked plants."
    ],
    "it": [
      "Christian GIRAUDON, distillatore artigianale, vi offre oli essenziali e idrolati estratti da piante biologiche, locali, regionali e raccolte a mano."
    ],
    "fr": [
      "Christian GIRAUDON, Artisan Distillateur, vous propose des huiles essentielles et hydrolats extraites de plantes en culture biologique, locales, régionales, de cueillettes manuelles."
    ],
    "es": [
      "Christian GIRAUDON, destilador artesanal, le ofrece aceites esenciales e hidrolatos extraídos de plantas de cultivo ecológico, locales, regionales y recogidas a mano."
    ],
    "nl": [
      "Christian GIRAUDON, ambachtelijk distilleerder, biedt u essentiële oliën en hydrolaten die worden gewonnen uit biologisch geteelde, lokale, regionale, met de hand geplukte planten."
    ]
  },
