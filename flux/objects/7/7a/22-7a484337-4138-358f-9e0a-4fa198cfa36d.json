      "dc:description": {
        "de": [
          "19.00 Uhr: Gitarrenensemble (aktuelle Musik)\n19.30 Uhr: Gesang\n20 Uhr: Konzert des Workshops \"Jazz Improvisation\"\n21 Uhr: Afrikanische Perkussion und Musik\nGetränke und traditionelle senegalesische Snacks vor Ort"
        ],
        "en": [
          "7 p.m.: guitar ensemble (contemporary music)\n7:30 pm: singing\n8 p.m.: Jazz Improvisation workshop concert\n9 p.m.: African music and percussion\nDrinks and traditional Senegalese snacks on site"
        ],
        "it": [
          "ore 19.00: ensemble di chitarre (musica contemporanea)\n19.30: canto\n20.00: Concerto del laboratorio di improvvisazione jazz\nore 21.00: percussioni e musica africana\nBevande e spuntini tradizionali senegalesi in loco"
        ],
        "fr": [
          "19 h : ensemble de guitares (musiques actuelles)\n19 h 30 : chant\n20 h : concert de l'atelier Jazz Improvisation\n21 h : percussions et musiques africaine\nBoissons et en-cas traditionnels Sénégalais sur place"
        ],
        "nl": [
          "19.00 uur: gitaarensemble (hedendaagse muziek)\n19.30 uur: zang\n20.00 uur: Jazz Improvisatie workshop concert\n21.00 uur: Afrikaanse percussie en muziek\nTraditionele Senegalese dranken en snacks ter plaatse"
        ],
        "es": [
          "19.00 horas: conjunto de guitarras (música contemporánea)\n19.30 h.: canto\n20:00 horas: Concierto del taller de improvisación de jazz\n21:00 horas: percusión y música africana\nBebidas y aperitivos tradicionales senegaleses in situ"
        ]
      },
