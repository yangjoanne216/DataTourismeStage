  "rdfs:comment": {
    "de": [
      "Gedenkstätten in Castellar.\r\nErster Weltkrieg: Gedenktafeln für die für Frankreich Gefallenen\r\nZweiter Weltkrieg: Gedenktafeln für die Befreiung durch die 1st Special Service Force der Amerikaner und Kanadier."
    ],
    "en": [
      "Memorials of Castellar.\r\nFirst World War: commemorative plaques to the dead for France\r\nSecond World War: Commemorative Plaques of the Liberation 1st Special Service Force, Canadian American"
    ],
    "it": [
      "Memorials of Castellar.\r\nFirst World War: commemorative plaques to the dead for France\r\nSecond World War: Commemorative Plaques of the Liberation della la 1st Special Service Force Americano-Canadese"
    ],
    "fr": [
      "Monuments commémoratifs de Castellar.\r\nPremière Guerre Mondiale : plaques commémoratives aux morts pour la France\r\nSeconde Guerre Mondiale : plaques commémoratives de la Libération par la 1st Special Service Force Américano-Canadienne."
    ],
    "es": [
      "Monumentos conmemorativos de Castellar.\r\nPrimera Guerra Mundial: placas conmemorativas de los muertos en Francia\r\nSegunda Guerra Mundial: placas conmemorativas de la Liberación por parte de la 1ª Fuerza de Servicios Especiales estadounidense-canadiense."
    ],
    "nl": [
      "Herdenkingsmonumenten van Castellar.\r\nEerste Wereldoorlog: gedenkplaten voor de doden in Frankrijk\r\nTweede Wereldoorlog: gedenkplaten van de bevrijding door de Amerikaans-Canadese 1e Special Service Force."
    ]
  },
