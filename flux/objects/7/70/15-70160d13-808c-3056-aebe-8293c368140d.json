  "rdfs:comment": {
    "de": [
      "ECO CAMPING reserviert für Zeltplanen (max. 10 Zelte), am Ufer der Creuse in einer ruhigen Umgebung gelegen, Anlegestelle für Kanu und Kajak und Angeln."
    ],
    "en": [
      "On the banks of the Creuse. Peace and shade."
    ],
    "it": [
      "ECO CAMPING riservato alle tende (max 10 tende), situato sulle rive della Creuse in un ambiente tranquillo, con un molo per la canoa e la pesca."
    ],
    "fr": [
      "ECO CAMPING réservé aux toiles de tentes (max 10 tentes), situé en bordure de la Creuse dans un cadre calme, embarcadère pour canoë-kayak et pêche."
    ],
    "es": [
      "ECO CAMPING reservado para tiendas (máx. 10 tiendas), situado a orillas del Creuse en un entorno tranquilo, con un embarcadero para la práctica del piragüismo y la pesca."
    ],
    "nl": [
      "ECO CAMPING gereserveerd voor tenten (max 10 tenten), gelegen aan de oevers van de Creuse in een rustige omgeving, met een pier om te kanoën en te vissen."
    ]
  },
