  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/bbfeb30e-858e-3f66-99f4-b39a60863856",
      "dc:description": {
        "de": [
          "Im Bistro am Place Paul Doumer können Sie bei einer Mahlzeit oder nur für einen Drink den wunderschönen Art-déco-Rahmen entdecken, der in die Liste der historischen Monumente aufgenommen wurde."
        ],
        "en": [
          "At the Bistro, place Paul Doumer, come and discover the magnificent Art Deco setting, listed as a Historic Monument, for a meal or just for a drink."
        ],
        "it": [
          "Al Bistro, place Paul Doumer, venite a scoprire il magnifico ambiente Art Déco, classificato come Monumento Storico, per un pasto o un semplice drink."
        ],
        "fr": [
          "Au Bistro, place Paul Doumer, venez découvrir le magnifique cadre Art déco, inscrit aux Monuments Historiques, à l'occasion d'un repas ou juste le temps d'un verre."
        ],
        "nl": [
          "Kom in de Bistro, place Paul Doumer, het prachtige Art Deco decor, geklasseerd als Historisch Monument, ontdekken voor een maaltijd of gewoon een drankje."
        ],
        "es": [
          "En el Bistro, plaza Paul Doumer, venga a descubrir el magnífico marco Art Decó, catalogado como Monumento Histórico, para comer o simplemente tomar una copa."
        ]
      },
