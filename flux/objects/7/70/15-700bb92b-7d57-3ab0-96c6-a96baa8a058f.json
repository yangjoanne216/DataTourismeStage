  "rdfs:comment": {
    "de": [
      "Lust, ohne Kraftaufwand in die Pedale zu treten? Fragen Sie nach unseren Fahrrädern mit elektrischer Unterstützung."
    ],
    "en": [
      "Want to pedal lightly? Ask for our electrically assisted bikes."
    ],
    "it": [
      "Volete pedalare senza affaticarvi? Chiedete delle nostre biciclette a pedalata assistita."
    ],
    "fr": [
      "Envie de pédaler sans forcer ? Demandez nos vélos à assistance électrique."
    ],
    "es": [
      "¿Quieres pedalear sin esforzarte? Pregunte por nuestras bicicletas con asistencia eléctrica."
    ],
    "nl": [
      "Wil je trappen zonder jezelf te belasten? Vraag naar onze elektrisch ondersteunde fietsen."
    ]
  },
