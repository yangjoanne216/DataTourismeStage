  "rdfs:comment": {
    "de": [
      "Restaurant im Herzen des Dorfes; familiäre und großzügige Küche. All-you-can-eat-Buffet als Vorspeise. Freitagabends Themenessen, vor Ort oder zum Mitnehmen."
    ],
    "en": [
      "Restaurant located in the heart of the village; generous family cuisine. All-you-can-eat buffet as a starter. Theme meals on Friday evenings, on the spot or to take away."
    ],
    "it": [
      "Ristorante situato nel cuore del villaggio; generosa cucina familiare. Buffet di antipasti a volontà. Il venerdì sera pasti a tema, sul posto o da asporto."
    ],
    "fr": [
      "Restaurant situé au coeur du village; cuisine familiale et généreuse. Buffet à volonté en entrée. Repas à thème les vendredis soirs, sur place ou à emporter."
    ],
    "es": [
      "Restaurante situado en el corazón del pueblo; generosa cocina familiar. Un buffet libre para empezar. Comidas temáticas los viernes por la noche, in situ o para llevar."
    ],
    "nl": [
      "Restaurant gelegen in het hart van het dorp; royale familiekeuken. All-you-can-eat buffet voor starters. Themamaaltijden op vrijdagavond, ter plaatse of om mee te nemen."
    ]
