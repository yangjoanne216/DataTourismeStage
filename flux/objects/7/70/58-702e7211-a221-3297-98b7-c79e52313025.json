  "rdfs:comment": {
    "de": [
      "Ferienhaus von 80 m² . In einem familienfreundlichen Weiler in der Mitte des Hügels, am Fuße der Wanderwege gelegen. Absolute Ruhe inmitten der Natur."
    ],
    "en": [
      "Gite of 80 m² . Located in a family hamlet in the middle of the hill, at the foot of the hiking trails. Absolute quietness in the middle of the nature."
    ],
    "it": [
      "Gîte di 80 m². Situato in un borgo familiare in piena collina, ai piedi dei sentieri escursionistici. Pace e tranquillità assolute in mezzo alla natura."
    ],
    "fr": [
      "Gîte de 80 m² . Situé dans un hameau familial au milieu de la colline, au pied des pistes de randonnée. Calme absolu au milieu de la nature."
    ],
    "nl": [
      "Gîte van 80 m² . Gelegen in een familiegehucht midden op de heuvel, aan de voet van de wandelpaden. Absolute rust midden in de natuur."
    ],
    "es": [
      "Casa rural de 80 m² . Situado en una aldea familiar en medio de la colina, al pie de las rutas de senderismo. Paz y tranquilidad absolutas en plena naturaleza."
    ]
  },
