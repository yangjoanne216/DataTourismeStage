  "rdfs:comment": {
    "de": [
      "Brigitte Vidieu, Fremdenführerin und Animatorin für das Zentrum der Nationaldenkmäler, zeigt Ihnen, wie Sie Ihren Vornamen kalligraphieren und illuminieren können.\n Ab 8 Jahren. Mit Reservierung."
    ],
    "en": [
      "Brigitte Vidieu, lecturer and animator for the National Monuments Center, shows you how to calligraphy and illuminate your name.\n From 8 years old. On reservation."
    ],
    "it": [
      "Brigitte Vidieu, guida e animatrice del Centre des Monuments Nationaux, vi mostra come calligrafare e illuminare il vostro nome di battesimo.\n A partire da 8 anni. Solo su prenotazione."
    ],
    "fr": [
      "Brigitte Vidieu guide conférencière et animatrice pour le centre des Monuments Nationaux, vous montre comment calligraphier et enluminer votre prénom.\n A partir de 8 ans. Sur réservation."
    ],
    "es": [
      "Brigitte Vidieu, guía y animadora del Centro de Monumentos Nacionales, le enseña a caligrafiar e iluminar su nombre.\n A partir de los 8 años. Sólo con reserva."
    ],
    "nl": [
      "Brigitte Vidieu, gids en animator van het Centre des Monuments Nationaux, laat u zien hoe u uw voornaam kunt kalligraferen en verlichten.\n Vanaf 8 jaar. Alleen op reservering."
    ]
  },
