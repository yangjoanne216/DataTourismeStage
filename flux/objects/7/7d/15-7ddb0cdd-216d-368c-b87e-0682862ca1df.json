  "rdfs:comment": {
    "de": [
      "Entdeckung des Angelns durch eine Einführung in das Angeln mit einem Float Tube auf dem großen See von Belle-Isle."
    ],
    "en": [
      "Discover fishing through an introduction to float tube fishing on the large Belle-Isle lake."
    ],
    "it": [
      "Scoprite la pesca con un'introduzione alla pesca con il tubo galleggiante sul grande lago di Belle Isle."
    ],
    "fr": [
      "Découverte de la pêche par une initiation à la pêche avec un float tube sur le grand lac de Belle-Isle."
    ],
    "nl": [
      "Ontdek het vissen met een introductie tot het vissen met een drijfbuis op het grote Belle Isle meer."
    ],
    "es": [
      "Descubra la pesca con una introducción a la pesca con tubo flotador en el gran lago de Belle Isle."
    ]
  },
