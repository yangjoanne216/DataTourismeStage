  "rdfs:comment": {
    "de": [
      "4-Sterne-Hotel Obernai, Elsass. 62 individuell dekorierte Zimmer und Suiten, 2 Restaurants: La Table und La Stub, 3 Bars, 2 Swimmingpools. Asiane Spa mit Massagen, Hammams, Sauna, Jacuzzis, Fitness. Garten."
    ],
    "en": [
      "4 star hotel Obernai, Alsace. 62 individually decorated rooms and suites, 2 restaurants: la Table and la Stub, 3 bars, 2 swimming pools. Asiane Spa with massages, hammams, sauna, jacuzzis, fitness. Garden."
    ],
    "it": [
      "Hotel 4 stelle Obernai, Alsazia. 62 camere e suite decorate individualmente, 2 ristoranti: la Table e la Stub, 3 bar, 2 piscine. Asiane Spa con massaggi, hammam, sauna, jacuzzi, fitness. Giardino."
    ],
    "fr": [
      "Hotel 4 étoiles Obernai, Alsace. 62 chambres et suites décorées individuellement, 2 restaurants : la Table et la Stub, 3 bars, 2 piscines. Asiane Spa avec massages, hammams, sauna, jacuzzis, fitness. Jardin."
    ],
    "es": [
      "Hotel de 4 estrellas en Obernai, Alsacia. 62 habitaciones y suites decoradas individualmente, 2 restaurantes: la Table y la Stub, 3 bares, 2 piscinas. Spa asiático con masajes, hammams, sauna, jacuzzis, gimnasio. Jardín."
    ],
    "nl": [
      "4 sterren hotel Obernai, Elzas. 62 individueel ingerichte kamers en suites, 2 restaurants: la Table en la Stub, 3 bars, 2 zwembaden. Asiane Spa met massages, hammams, sauna, jacuzzi's, fitness. Tuin."
