  "rdfs:comment": {
    "de": [
      "Teestube, Tagesgericht, Küche des Périgord, ganztägig Snacks. Fusionsküche Frankreich Indonesien. Backwaren und Rezepte zu 100 % hausgemacht. Terrasse auf dem Marktplatz."
    ],
    "en": [
      "Tea room, daily special, Périgord cuisine, snacks are available all day long. Fusion French Indonesian cuisine. Pastry and recipes are 100% homemade."
    ],
    "it": [
      "Sala da tè, piatto del giorno, cucina del Perigord, snack tutto il giorno. Cucina fusion franco-indonesiana. Pasticceria e ricette 100% fatte in casa. Terrazza sul mercato."
    ],
    "fr": [
      "Salon de thé, plat du jour, cuisine du Périgord, snack toute la journée. Cuisine fusion France Indonésienne. Pâtisseries et recettes 100% fait maison. Terrasse sur la place du marché."
    ],
    "nl": [
      "Theesalon, dagschotel, Perigord keuken, snack de hele dag. Frans Indonesische fusion keuken. Gebak en recepten 100% huisgemaakt. Terras op de markt."
    ],
    "es": [
      "Plats del día salón de té cafètería. Gastronomía del Périgord, Franco/Indonesia todo cosero. Terrasa en la plaza del mercado."
    ]
  },
