  "rdfs:comment": {
    "de": [
      "Neu in der Ardèche ist ein Escape Game, das zu 100% in der Natur stattfindet, mitten im Wald. Ausgestattet mit einem digitalen Tablet machen Sie sich auf die Suche nach versteckten Hinweisen und lösen Rätsel."
    ],
    "en": [
      "New in Ardèche, a 100% nature escape game, in the heart of the forest. Equipped with a digital tablet, go in search of hidden clues and solve the riddles."
    ],
    "it": [
      "Novità nell'Ardèche, un gioco di fuga 100% naturale, nel cuore della foresta. Muniti di una tavoletta digitale, andate alla ricerca di indizi nascosti e risolvete gli enigmi."
    ],
    "fr": [
      "Nouveau en Ardèche, un escape game 100% nature, au cœur même de la forêt. Equipés d'une tablette numérique, partez à la recherche des indices cachés, et résolvez les énigmes."
    ],
    "es": [
      "Nuevo en el Ardèche, un juego de escape 100% natural, en el corazón del bosque. Equipado con una tableta digital, vaya en busca de pistas ocultas y resuelva los enigmas."
    ],
    "nl": [
      "Nieuw in Ardèche, een 100% natuurlijke escape game, midden in het bos. Uitgerust met een digitale tablet, ga je op zoek naar verborgen aanwijzingen en los de puzzels op."
    ]
  },
