  "rdfs:comment": {
    "de": [
      "Parade um 10 Uhr: Revue des 14/07. Ehrenwein im Hof der Henri-Breton-Schule gegen 11:30 Uhr, anschließend republikanisches Essen im Rathaus gegen 13 Uhr. Reservierung beim Rathaus."
    ],
    "en": [
      "Parade at 10am: review of the 14/07. Vin d'honneur in the courtyard of the Ecole Henri Breton at 11:30am, followed by a Republican meal at the Town Hall at 1pm. Reservations at the Town Hall."
    ],
    "it": [
      "Sfilata alle 10: rassegna del 14/07. Vin d'honneur nel cortile dell'Ecole Henri Breton alle 11.30 circa, seguito da un pranzo repubblicano presso il Municipio alle 13.00 circa. Le prenotazioni possono essere effettuate presso il Municipio."
    ],
    "fr": [
      "Défilé à 10h : revue du 14/07. Vin d'honneur dans la cour de l'Ecole Henri Breton vers 11h30 puis repas républicain à l'hôtel de ville vers 13h. Réservation auprès de la Mairie."
    ],
    "nl": [
      "Parade om 10.00 uur: 14/07 overzicht. Vin d'honneur op de binnenplaats van de Ecole Henri Breton rond 11.30 uur, gevolgd door een Republikeinse maaltijd op het stadhuis rond 13.00 uur. Reserveren kan op het stadhuis."
    ],
    "es": [
      "Desfile a las 10h: reseña del 14/07. Vin d'honneur en el patio de la Escuela Henri Breton hacia las 11h30, seguido de una comida republicana en el Ayuntamiento hacia las 13h. Las reservas pueden hacerse en el Ayuntamiento."
    ]
  },
  "rdfs:label": {
    "fr": [
