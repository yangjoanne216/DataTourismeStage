  "rdfs:comment": {
    "de": [
      "Das Hippodrom von Toulouse liegt im Herzen der rosafarbenen Stadt, im Viertel La Cépière, und erstreckt sich über 34 Hektar Grünfläche."
    ],
    "en": [
      "Located in the heart of the pink city, in the Cépière district, the Hippodrome de Toulouse extends over 34 hectares of greenery."
    ],
    "it": [
      "Situato nel cuore della città rosa, nel quartiere Cépière, l'Ippodromo di Tolosa si estende su 34 ettari di verde."
    ],
    "fr": [
      "Situé au cœur de la ville rose, dans le quartier de la Cépière, l’Hippodrome de Toulouse s’étend sur 34 hectares de verdure."
    ],
    "es": [
      "Situado en el corazón de la ciudad rosa, en el barrio de la Cépière, el Hipódromo de Toulouse se extiende sobre 34 hectáreas de vegetación."
    ],
    "nl": [
      "Gelegen in het hart van de roze stad, in de wijk Cépière, strekt het Hippodrome de Toulouse zich uit over 34 hectare groen."
    ]
  },
