  "rdfs:comment": {
    "de": [
      "Besuchen Sie das Museum mit freiem Eintritt. Das Museum zeigt Werke der Malerin Josette Bournet (Vichy 1905 - Nizza 1962): Porträts, Meeresbilder, Stillleben, religiöse Werke und Keramiken."
    ],
    "en": [
      "Free visit. The museum presents, in a space of 125 m² of the works of the painter Josette Bournet (Vichy 1905 - Nice1962): portraits, marines, still lifes, religious works and ceramics."
    ],
    "it": [
      "Visita gratuita. Il museo presenta opere della pittrice Josette Bournet (Vichy 1905 - Nizza 1962): ritratti, paesaggi marini, nature morte, opere religiose e ceramiche."
    ],
    "fr": [
      "Visite libre. Le musée présente des oeuvres de la peintre Josette Bournet (Vichy 1905 - Nice 1962) : portraits, marines, natures mortes, oeuvres religieuses et céramiques."
    ],
    "es": [
      "Visita gratuita. El museo presenta obras de la pintora Josette Bournet (Vichy 1905 - Niza 1962): retratos, marinas, bodegones, obras religiosas y cerámicas."
    ],
    "nl": [
      "Gratis bezoek. Het museum presenteert werken van de schilderes Josette Bournet (Vichy 1905 - Nice 1962): portretten, zeegezichten, stillevens, religieuze werken en keramiek."
    ]
  },
