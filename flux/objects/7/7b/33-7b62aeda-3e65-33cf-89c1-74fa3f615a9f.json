  "rdfs:comment": {
    "de": [
      "Verbringen Sie einen Moment mit der Familie oder zu zweit mitten im Wald in einer hochgelegenen Hütte."
    ],
    "en": [
      "Come and spend some time with your family or your loved ones in the middle of the forest in a perched cabin."
    ],
    "it": [
      "Venite a trascorrere un po' di tempo con la vostra famiglia o i vostri cari in mezzo alla foresta in una capanna arroccata."
    ],
    "fr": [
      "Venez passer un moment en famille ou en amoureux au milieu de la forêt dans une cabane perchée."
    ],
    "es": [
      "Venga a pasar un rato con su familia o sus seres queridos en medio del bosque, en una cabaña encaramada."
    ],
    "nl": [
      "Kom en breng wat tijd door met uw familie of uw geliefden midden in het bos in een hut op een heuvel."
    ]
  },
