  "rdfs:comment": {
    "de": [
      "Crêperie, Café Restaurant Eisdiele. Tirolerisch-österreichisches Dekor. Bio-Kuchen und -Crêpes, Waffeln, hausgemachtes Eis, Savoyer Spezialitäten,\r\nsavoyisches Fondue, Savoyer Raclette, AOC-Weine. Brasserie, Nachmittagssnacks in der Saison."
    ],
    "en": [
      "Restaurant, crêperie. Tyrolean decor and atmosphere. Savoury pancakes, Savoyard fondue, raclette, reblochonnade, AOC wines. Homemade crêpes and ice creams. Brasserie, afternoon snacks. Open every day. Free Wi-Fi."
    ],
    "it": [
      ": ristorante, creperia. Arredamento e atmosfera tirolese. Galettes, fonduta savoiarda, raclette, reblochonnade, vini DOC. Crêpe e gelati artigianali. Brasserie, merende pomeridiane. Aperto tutti i giorni. Wi-fi gratuito."
    ],
    "fr": [
      "Crêperie, café restaurant glacier. Décor tyrolien autrichien. Galettes et crêpes bio, gaufres, glaces artisanales, spécialités savoyardes,\r\nfondue savoyarde, raclette de Savoie, vins AOC. Brasserie, goûters l’après-midi en saison."
    ],
    "nl": [
      "Creperie, café restaurant en ijssalon. Oostenrijks Tirools decor. Biologische pannenkoeken en crêpes, wafels, huisgemaakt ijs, Savoyaardse specialiteiten,\r\nsavoie fondue, Savoie raclette, AOC wijnen. Brasserie, 's middags snacks in het seizoen."
    ],
    "es": [
      "Crepería, café restaurante y heladería. Decoración tirolesa austriaca. Tortitas y crepes ecológicas, gofres, helados artesanales, especialidades de Saboya,\r\nfondue savoyarde, raclette de Savoie, vinos AOC. Brasserie, merienda en temporada."
    ]
  },
