  "rdfs:comment": {
    "de": [
      "Mit der Seilbahn von Montalbert können Sie den Gipfel Fornelet und das 2000 m hoch gelegene Gebiet Prajourdan erreichen."
    ],
    "en": [
      "The gondola lift of Montalbert allow you to join the summit of the Fornelet and the Prajourdan area at an altitude of 2000 metres."
    ],
    "it": [
      "La funivia di Montalbert conduce alla cima del Fornelet e al comprensorio sciistico di Prajourdan, a 2000 m di altitudine."
    ],
    "fr": [
      "La télécabine de Montalbert vous permet de rejoindre le sommet de Fornelet et le domaine de Prajourdan situé à 2000m d'altitude."
    ],
    "es": [
      "El teleférico de Montalbert le lleva a la cumbre de Fornelet y al dominio esquiable de Prajourdan, a 2.000 m de altitud."
    ],
    "nl": [
      "De Montalbert kabelbaan brengt u naar de Fornelet top en het Prajourdan skigebied op 2000m hoogte."
    ]
  },
