  "rdfs:comment": {
    "de": [
      "Dieser Bereich liegt im Herzen des Dorfes Castanet auf den Höhen des Ségala-Plateaus und ist geschmackvoll eingerichtet."
    ],
    "en": [
      "Located in the heart of the village of Castanet, on the hills of the Ségala plateau, this area is tastefully fit out."
    ],
    "it": [
      "Situato nel cuore del villaggio di Castanet, sulle alture dell'altopiano di Ségala, questo spazio è arredato con gusto."
    ],
    "fr": [
      "Située au coeur du village de Castanet, sur les hauteurs du plateau du Ségala, cette aire est aménagée avec goût."
    ],
    "es": [
      "Situado en el corazón del pueblo de Castanet, en las alturas de la meseta del Ségala, este espacio está amueblado con gusto."
    ],
    "nl": [
      "Gelegen in het hart van het dorp Castanet, op de hoogten van het plateau van Ségala, is deze ruimte smaakvol ingericht."
    ]
  },
