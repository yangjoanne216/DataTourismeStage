  "rdfs:comment": {
    "de": [
      "Wohnung F1, 26 m², Erdgeschoss. Im Dorfzentrum, 80m vom Flussufer, 1,2km von einem Thermalbad entfernt. Heizung, Gefrierschrank, Mikrowelle, private Waschmaschine, Geschirrspüler, Fernseher, Internetzugang, DVD, WIFI. Schlafsofa im Wohnzimmer. Hifi."
    ],
    "en": [
      "Apartment F1, 26 m², ground floor. In the centre of the village, 80m from the riverside, 1.2km from a thermal spa. Heating, freezer, microwave oven, private washing machine, dishwasher, television, internet access, DVD, WIFI. Convertible sofa bed in the living room. Hifi."
    ],
    "it": [
      "Appartamento F1, 26 m², piano terra. Nel centro del paese, a 80 m dal fiume, a 1,2 km da un centro termale. Riscaldamento, congelatore, forno a microonde, lavatrice privata, lavastoviglie, televisione, accesso a Internet, DVD, WIFI. Divano letto in soggiorno. Hifi."
    ],
    "fr": [
      "Appartement F1, 26 m², RDC. Au centre-village, à 80m du bord de la rivière, à 1,2km d'un établissement thermal. Chauffage, congélateur, four à micro-ondes, lave linge privatif, lave vaisselle, télévision, accès Internet, DVD, WIFI. Canapé convertible séjour. Hifi."
    ],
    "es": [
      "Piso F1, 26 m², planta baja. En el centro del pueblo, a 80 m del río, a 1,2 km de un balneario. Calefacción, congelador, microondas, lavadora privada, lavavajillas, televisión, acceso a Internet, DVD, WIFI. Sofá cama en el salón. Hifi."
    ],
    "nl": [
      "Flat F1, 26 m², begane grond. In het dorpscentrum, 80m van de rivier, 1,2km van een kuuroord. Verwarming, vriezer, magnetron, eigen wasmachine, vaatwasser, televisie, internettoegang, DVD, WIFI. Slaapbank in de woonkamer. Hifi."
    ]
  },
