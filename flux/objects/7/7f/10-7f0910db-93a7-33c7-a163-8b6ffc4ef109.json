  "rdfs:comment": {
    "de": [
      "Supermarkt, vor Ort gebackenes Brot (außer Sonntagmorgen), Metzgerei, Frischprodukte, lose Bioprodukte.\nDrive: vival.fr - Lieferung nach Hause -Regionale Produkte, Zeitungen.\ntreuekarte: \"Cmacarte\"."
    ],
    "en": [
      "Supermarket, bread baked on site (except Sunday morning), butcher's shop, fresh products, organic products in bulk.\nDrive : vival.fr - home delivery - Regional products, press.\nloyalty card : \"Cmacarte\"."
    ],
    "it": [
      "Negozio di alimentari, pane cotto in loco (tranne la domenica mattina), macelleria, prodotti freschi, prodotti biologici sfusi.\nDrive : vival.fr - consegna a domicilio - prodotti regionali, stampa.\ncarta fedeltà: \"Cmacarte\"."
    ],
    "fr": [
      "Supérette, pain cuit sur place (sauf dimanche matin), boucherie, produits frais, produits bio vrac.\nDrive : vival.fr - livraison à domicile -Produits régionaux, presse.\ncarte de Fidélité : \"Cmacarte\"."
    ],
    "es": [
      "Tienda de comestibles, pan horneado en el establecimiento (excepto los domingos por la mañana), carnicería, productos frescos, productos ecológicos a granel.\nDrive : vival.fr - entrega a domicilio - productos regionales, prensa.\ntarjeta de fidelidad: \"Cmacarte\"."
    ],
    "nl": [
      "Kruidenierszaak, ter plaatse gebakken brood (behalve op zondagmorgen), slagerij, verse producten, biologische bulkproducten.\nDrive : vival.fr - levering aan huis - regionale producten, pers.\nklantenkaart: \"Cmacarte\"."
    ]
  },
