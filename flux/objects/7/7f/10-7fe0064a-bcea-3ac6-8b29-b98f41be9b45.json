  "rdfs:comment": {
    "de": [
      "2 km vom Dorfzentrum entfernt, Richtung Col de Grosse Pierre - Le Droit. 11 km von der Front de Neige von La Bresse Hohneck, 9 km von den Pisten von La Bresse Lispach, 5.5 km von den Pisten von La Bresse Brabant."
    ],
    "en": [
      "2 km from the centre of the village, towards the Col de Grosse Pierre - Le Droit. 11 km from the La Bresse Hohneck Snow Front, 9 km from the La Bresse Lispach slopes, 5.5 km from the La Bresse Brabant slopes."
    ],
    "it": [
      "a 2 km dal centro del paese, sul versante del Col de Grosse Pierre - Le Droit. 11 km dal fronte neve di La Bresse Hohneck, 9 km dalle piste di La Bresse Lispach, 5,5 km dalle piste di La Bresse Brabant."
    ],
    "fr": [
      "2 km du centre du village, versant Col de Grosse Pierre - Le Droit. 11 km du Front de Neige de La Bresse Hohneck, 9 km des pistes de La Bresse Lispach, 5.5 km des pistes de La Bresse Brabant."
    ],
    "nl": [
      "op 2 km van het dorpscentrum, aan de kant van de Col de Grosse Pierre - Le Droit. 11 km van het Sneeuwfront van La Bresse Hohneck, 9 km van de pistes van La Bresse Lispach, 5,5 km van de pistes van La Bresse Brabant."
    ],
    "es": [
      "a 2 km del centro del pueblo, en el lado del Col de Grosse Pierre - Le Droit. a 11 km del frente de nieve de La Bresse Hohneck, a 9 km de las pistas de La Bresse Lispach y a 5,5 km de las pistas de La Bresse Brabant."
    ]
  },
