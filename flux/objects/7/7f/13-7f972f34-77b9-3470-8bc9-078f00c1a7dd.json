  "rdfs:comment": {
    "de": [
      "Cascade Aventure ist ein Park mit sportlichen Aktivitäten im Freien in den Bergen für alle, unabhängig von Alter und körperlicher Verfassung, am Wasserfall von Nyon in Morzine, Hochsavoyen."
    ],
    "en": [
      "Cascade Aventure is an alpine outdoor adventure park geared to everyone regardless of their age or fitness level. It is located on the site of the Nyon waterfall, in Morzine in the Haute-Savoie region of the French Alps."
    ],
    "it": [
      "Cascade Aventure è un parco di sport di montagna all'aperto per tutte le età e i livelli di fitness presso la cascata di Nyon a Morzine, in Alta Savoia."
    ],
    "fr": [
      "Cascade Aventure est un parc d'activités sportives de plein air en montagne,  destiné à tous, quel que soit l'âge et la forme physique,  sur le site de la cascade de Nyon, à Morzine, en Haute-Savoie."
    ],
    "nl": [
      "Cascade Aventure is een outdoor bergsportpark voor alle leeftijden en fitnessniveaus bij de Nyon waterval in Morzine, Haute-Savoie."
    ],
    "es": [
      "Cascade Aventure es un parque de deportes de montaña al aire libre para todas las edades y niveles de fitness en la cascada de Nyon, en Morzine, Alta Saboya."
    ]
  },
