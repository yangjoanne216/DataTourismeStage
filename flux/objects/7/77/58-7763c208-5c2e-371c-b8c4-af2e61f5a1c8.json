  "rdfs:comment": {
    "de": [
      "Ambition Mer ist ein Schulschiff in Marseille, das im Zeichen der Geselligkeit steht. Es ist auf Bootsführerscheine spezialisiert und hat eine Erfolgsquote von über 90% bei den Prüfungen."
    ],
    "en": [
      "Ambition Mer is a boat school in Marseilles placed under the sign of conviviality, specializing in boat licenses, with a success rate of over 90% in the exams."
    ],
    "it": [
      "Ambition Mer è un'accogliente scuola nautica di Marsiglia, specializzata in patenti nautiche, con un tasso di superamento superiore al 90%."
    ],
    "fr": [
      "Ambition Mer est un bateau école de Marseille placé sous le signe de la convivialité, spécialiste des permis bateau, avec un taux de réussite aux examens supérieur à 90%."
    ],
    "nl": [
      "Ambition Mer is een gezellige bootschool in Marseille, gespecialiseerd in vaarbewijzen, met een slagingspercentage van meer dan 90%."
    ],
    "es": [
      "Ambition Mer es una acogedora escuela náutica de Marsella, especializada en la obtención de licencias para embarcaciones, con un índice de aprobados superior al 90%."
    ]
  },
