  "rdfs:comment": {
    "de": [
      "Gourmetwanderung in Fontanes du Causse. Wanderung am frühen Abend, um den Sonnenuntergang über dem Causse zu genießen und mit einem Essen im Dorf mit lokalen Produkten abzuschließen."
    ],
    "en": [
      "Gourmet walk in Fontanes du Causse. Walk in the early evening to enjoy a sunset on the Causse and finish with a meal in the village, with local products."
    ],
    "it": [
      "Passeggiata gastronomica a Fontanes du Causse. Passeggiata in prima serata per ammirare il tramonto sulla Causse e conclusione con un pasto in paese, con prodotti locali."
    ],
    "fr": [
      "Marche gourmande à Fontanes du Causse. Randonnée en début de soirée pour profiter d'un coucher de soleil sur le causse et terminer avec un repas dans le village, avec des produits locaux."
    ],
    "es": [
      "Paseo gastronómico en Fontanes du Causse. Camine a primera hora de la tarde para disfrutar de una puesta de sol en el Causse y termine con una comida en el pueblo, con productos locales."
    ],
    "nl": [
      "Gastronomische wandeling in Fontanes du Causse. Wandel in de vroege avond om te genieten van een zonsondergang op de Causse en eindig met een maaltijd in het dorp, met lokale producten."
    ]
  },
