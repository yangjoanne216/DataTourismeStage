  "rdfs:comment": {
    "de": [
      "Restaurant mit Terrasse neben dem Park Allées du Rhône, Terrasse mit Blick auf den Jachthafen. Fisch- und Fleischspezialitäten à la plancha."
    ],
    "en": [
      "Restaurant with terrace next to the Allées du Rhône park, terrace overlooking the marina. Fish and meat specialities a la plancha."
    ],
    "it": [
      "Ristorante con terrazza vicino al parco Allées du Rhône, terrazza con vista sul porto turistico. Specialità: pesce e carne à la plancha."
    ],
    "fr": [
      "Restaurant avec terrasse à coté le parc des Allées du Rhône, terrasse avec vue sur le port de plaisance. Spécialités poissons et viandes à la plancha."
    ],
    "es": [
      "Restaurante con terraza junto al parque Allées du Rhône, terraza con vistas al puerto deportivo. Especialidades: pescado y carne a la plancha."
    ],
    "nl": [
      "Restaurant met terras naast het park Allées du Rhône, terras met uitzicht op de jachthaven. Specialiteiten: vis en vlees à la plancha."
    ]
  },
