  "rdfs:comment": {
    "de": [
      "Jh. Scheunenkirche, 1860 erweitert, mit einem 1842 wieder aufgebauten Chor. Jahrhundert. Wunderschöner Hauptaltar aus geschnitztem und vergoldetem Holz."
    ],
    "en": [
      "18th century barn church, enlarged in 1860, with a choir rebuilt in 1842. Superb carved and gilded wood high altar from the 19th century."
    ],
    "it": [
      "Chiesa vescovile del XVIII secolo, ampliata nel 1860, con coro ricostruito nel 1842. Superbo altare maggiore in legno intagliato e dorato del XIX secolo."
    ],
    "fr": [
      "Eglise-grange du XVIIIe siècle, agrandie en 1860, avec un choeur reconstruit en 1842. Superbe maître-autel en bois sculpté et doré du XIXe siècle."
    ],
    "es": [
      "Iglesia episcopal del siglo XVIII, ampliada en 1860, con un coro reconstruido en 1842. Magnífico altar mayor de madera tallada y dorada del siglo XIX."
    ],
    "nl": [
      "18e eeuwse bisschopskerk, vergroot in 1860, met een in 1842 herbouwd koor. Schitterend gesneden en verguld houten hoogaltaar uit de 19e eeuw."
    ]
  },
