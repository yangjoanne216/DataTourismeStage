  "rdfs:comment": {
    "de": [
      "Eine Lage im Herzen von Marseille, das Hôtel Raphaël Prado ist nur 5 Gehminuten vom Parc des Expositions, dem Stade Orange Vélodrome und den Stränden entfernt."
    ],
    "en": [
      "A location in the heart of Marseille, the Hotel Raphaël Prado is only 5 minutes walk from the Exhibition Center, the Orange Velodrome stadium, and the beaches."
    ],
    "it": [
      "Situato nel cuore di Marsiglia, l'Hotel Raphaël Prado dista solo 5 minuti a piedi dal Parc des Expositions, dallo stadio Orange Velodrome e dalle spiagge."
    ],
    "fr": [
      "Un emplacement au coeur de Marseille, l'Hôtel Raphaël Prado se trouve à seulement 5 minutes à pied du Parc des Expositions, du stade Orange Vélodrome, et des plages."
    ],
    "es": [
      "Situado en el corazón de Marsella, el Hotel Raphaël Prado está a sólo 5 minutos a pie del Parque de Exposiciones, del estadio Orange Velodrome y de las playas."
    ],
    "nl": [
      "Hotel Raphaël Prado ligt in het hart van Marseille, op slechts 5 minuten lopen van het Parc des Expositions, het stadion Orange Velodrome en de stranden."
    ]
  },
