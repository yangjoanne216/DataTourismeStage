  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/a7c996cf-29be-3b5b-b7a6-50da30c63129",
      "dc:description": {
        "de": [
          "Brasserie mit schnellem Service. Vorspeise, Tagesgericht, Käse und Dessert als Buffet.\nVon Montag bis Freitag geöffnet. Am Wochenende nur auf Reservierung für Bankette."
        ],
        "en": [
          "Fast service brewery. Starter, dish of the day, cheese and dessert buffet.\nOpen from Monday to Friday. Weekends only on reservation for banquets."
        ],
        "it": [
          "Brasserie con servizio veloce. Antipasto, piatto del giorno, buffet di formaggi e dessert.\nAperto dal lunedì al venerdì. Solo nei fine settimana su prenotazione per i banchetti."
        ],
        "fr": [
          "Brasserie service rapide. Entrée, plat du jour, fromage et dessert en buffet.\nOuvert du lundi au vendredi. Le week-end uniquement sur réservation pour les banquets."
        ],
        "nl": [
          "Brasserie met snelle service. Voorgerecht, dagschotel, kaas- en dessertbuffet.\nOpen van maandag tot vrijdag. Weekends alleen op reservering voor banketten."
        ],
        "es": [
          "Brasserie con servicio rápido. Entrante, plato del día, buffet de quesos y postres.\nAbierto de lunes a viernes. Fines de semana sólo con reserva para banquetes."
        ]
      },
      "@type": [
        "Description"
      ],
