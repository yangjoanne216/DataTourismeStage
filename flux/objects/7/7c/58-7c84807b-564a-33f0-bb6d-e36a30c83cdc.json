  "rdfs:comment": {
    "de": [
      "In einem Bauernhaus entlang der Petit Rhône untergebracht, befinden sich die Zimmer im regionalen Naturpark Camargue."
    ],
    "en": [
      "Located in a farmhouse along the Petit Rhône, the rooms are in the Camargue Regional Nature Park."
    ],
    "it": [
      "Situate in una casa colonica lungo il Petit Rhône, le camere si trovano nel Parco Naturale Regionale della Camargue."
    ],
    "fr": [
      "Installé dans une ferme le long du Petit Rhône, les chambres se trouvent dans le parc naturel régional de Camargue."
    ],
    "es": [
      "Ubicadas en una granja a lo largo del Petit Rhône, las habitaciones están situadas en el Parque Natural Regional de la Camarga."
    ],
    "nl": [
      "De kamers zijn gevestigd in een boerderij langs de Petit Rhône en liggen in het regionale natuurpark Camargue."
    ]
  },
