      "dc:description": {
        "de": [
          "Am Ufer der Creuse, ländliches Fest ab 20 Uhr, Feuerwerk über dem Fluss gegen 23 Uhr, kostenloser Tanz. Getränke und Essen vor Ort."
        ],
        "en": [
          "On the edge of the Creuse, country party from 8 pm, fireworks on the river around 11 pm, free dance. Refreshment bar and meal on the spot."
        ],
        "it": [
          "Sulle rive della Creuse, festa in stile country dalle 20.00, fuochi d'artificio sul fiume intorno alle 23.00, ballo gratuito. Bar e pasti in loco."
        ],
        "fr": [
          "En bordure de Creuse, fête champêtre à partir de 20h, feu d'artifice tiré sur la rivière vers 23h, bal gratuit. Buvette et repas sur place."
        ],
        "nl": [
          "Aan de oevers van de Creuse, feest in landelijke stijl vanaf 20 uur, vuurwerk op de rivier rond 23 uur, gratis dans. Versnaperingsbar en maaltijd ter plaatse."
        ],
        "es": [
          "A orillas del Creuse, fiesta campestre a partir de las 20.00 h, fuegos artificiales en el río hacia las 23.00 h, baile gratuito. Bar y comida in situ."
        ]
      },
