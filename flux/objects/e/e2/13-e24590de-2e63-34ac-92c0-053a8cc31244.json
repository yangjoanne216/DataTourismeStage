  "rdfs:comment": {
    "de": [
      "3-Sterne-Hotel - Bistro - Lebensmittelgeschäft mit Terrasse im Herzen des Dorfes Gresse-en-Vercors.\r\nDas Restaurant bietet eine 100 % hausgemachte Küche, bei der frische Bio- und lokale Produkte bevorzugt werden."
    ],
    "en": [
      "3 star hotel - Bistro - Grocery with terrace in the heart of the village of Gresse-en-Vercors.\r\nThe restaurant offers a 100% home-made cuisine, using fresh organic and local products."
    ],
    "it": [
      "Hotel 3 stelle - Bistrot - Drogheria con terrazza nel cuore del villaggio di Gresse-en-Vercors.\r\nIl ristorante offre una cucina 100% casalinga con prodotti freschi, biologici e locali."
    ],
    "fr": [
      "Hôtel 3 étoiles - Bistrot - Epicerie avec terrasse au cœur du village de Gresse-en-Vercors.\r\nLe restaurant propose une cuisine 100 % maison privilégiant les produits frais bios et locaux."
    ],
    "es": [
      "Hotel de 3 estrellas - Bistro - Tienda de comestibles con terraza en el corazón del pueblo de Gresse-en-Vercors.\r\nEl restaurante ofrece una cocina 100% casera con productos frescos, ecológicos y locales."
    ],
    "nl": [
      "3-sterren hotel - Bistro - Kruidenierswinkel met terras in het hart van het dorp Gresse-en-Vercors.\r\nHet restaurant biedt een 100% huisgemaakte keuken met verse biologische en lokale producten."
    ]
  },
