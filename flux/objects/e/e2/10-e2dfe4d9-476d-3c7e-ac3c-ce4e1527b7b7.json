  "rdfs:comment": {
    "de": [
      "Ideal zum Entspannen, Spaß haben, Picknicken oder sogar Angeln (Kinderspielplatz mit Schaukeln, Rutschen, kleiner Hütte usw., Grillplatz mit Picknicktisch und Laube)."
    ],
    "en": [
      "Ideal for relaxing, having fun, picnicking and even fishing (children's playground with swings, slides, small hut, etc., barbecue area with picnic table and arbour)."
    ],
    "it": [
      "Ideale per il relax, il divertimento, i picnic e persino la pesca (area giochi per bambini con altalene, scivoli, casetta, ecc., area barbecue con tavolo da picnic e gazebo)."
    ],
    "fr": [
      "Idéal pour se détendre, s'amuser, pique-niquer, voire même pêcher (aire de jeux pour les enfants, avec balançoires, toboggans, petite cabane, etc., espace barbecue avec table de pique-nique et tonnelle)."
    ],
    "es": [
      "Ideal para relajarse, divertirse, hacer picnics e incluso pescar (zona de juegos para niños con columpios, toboganes, pequeña cabaña, etc., zona de barbacoa con mesa de picnic y cenador)."
    ],
    "nl": [
      "Ideaal voor ontspanning, plezier, picknick en zelfs vissen (kinderspeelplaats met schommels, glijbanen, kleine hut, enz., barbecueplaats met picknicktafel en prieel)."
    ]
  },
