  "rdfs:comment": {
    "de": [
      "Fläche: 7 ha. Fisch: Koi-Karpfen, Hecht, Zander (Löffel im letzten Monat der Saison), Forelle, Schleie, Karpfen und Plötze. Jeder Fisch über 5 kg muss zurückgesetzt werden."
    ],
    "en": [
      "Area: 7 hectares. Fish: Koi carp, pike, zander (spoon in the last month of the season), trout, tench, carp and roach. Any fish over 5 kg must be released."
    ],
    "it": [
      "Superficie: 7 ettari. Pesci: carpa Koi, luccio, luccioperca (cucchiaio nell'ultimo mese della stagione), trota, tinca, carpa e lasca. Tutti i pesci di peso superiore a 5 kg devono essere rilasciati."
    ],
    "fr": [
      "Surface : 7 ha. Poissons : carpes Koï, brochets, sandres (cuillère le dernier mois de la saison), truites, tanches, carpes et gardons. Tout poisson de plus de 5 kg doit être remis à l'eau."
    ],
    "es": [
      "Superficie: 7 ha. Peces: carpa koi, lucio, lucioperca (cuchara en el último mes de la temporada), trucha, tenca, carpa y cucaracha. Todos los peces de más de 5 kg deben ser liberados."
    ],
    "nl": [
      "Oppervlakte: 7 ha. Vis: Koikarper, snoek, snoekbaars (lepel in de laatste maand van het seizoen), forel, zeelt, karper en voorn. Alle vissen van meer dan 5 kg moeten worden teruggezet."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Site de pêche - Plan d'Eau de La Prade"
    ]
  },
