  "rdfs:comment": {
    "de": [
      "Willkommen im La Maison de Lily, einem charmanten Bed & Breakfast in der Nähe des Flusses Allier.\r\nGenießen Sie einen geselligen Aufenthalt in diesem Stadthaus aus dem Jahr 1890, nur 10 Minuten vom Stadtzentrum entfernt."
    ],
    "en": [
      "Welcome to La Maison de Lily, a charming bed and breakfast near the Allier River.\r\nEnjoy a friendly stay in this 1890 townhouse, only 10 minutes from downtown."
    ],
    "it": [
      "Benvenuti a La Maison de Lily, un bed and breakfast di charme vicino al fiume Allier.\r\nGodetevi un soggiorno conviviale in questa casa a schiera del 1890, a soli 10 minuti dal centro città."
    ],
    "fr": [
      "Bienvenue à La Maison de Lily, une charmante chambre d'hôte près de la rivière Allier. \r\nProfitez d'un séjour convivial dans cette maison de ville de 1890, à seulement 10 min du centre-ville."
    ],
    "nl": [
      "Welkom bij La Maison de Lily, een charmante bed and breakfast in de buurt van de rivier de Allier.\r\nGeniet van een vriendelijk verblijf in dit herenhuis uit 1890, op slechts 10 minuten van het stadscentrum."
    ],
    "es": [
      "Bienvenido a La Maison de Lily, un encantador bed and breakfast cerca del río Allier.\r\nDisfrute de una agradable estancia en esta casa adosada de 1890, a sólo 10 minutos del centro de la ciudad."
    ]
  },
