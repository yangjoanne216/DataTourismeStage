  "rdfs:comment": {
    "de": [
      "Im Hafen der Schöpfer wird kreiert, gemalt, gemeißelt, gezeichnet, umgewandelt, gebaut, gesungen, getanzt, interpretiert, performt..."
    ],
    "en": [
      "In the port of the Creators, we create, paint, sculpt, draw, transform, build, sing, dance, perform..."
    ],
    "it": [
      "Al Port des Créateurs si crea, si dipinge, si scolpisce, si disegna, si trasforma, si costruisce, si canta, si balla, si interpreta e si recita."
    ],
    "fr": [
      "Au port des Créateurs, on crée, on peint, on sculpte, on dessine, on transforme, on construit, on chante, on danse, on interprète, on performe..."
    ],
    "nl": [
      "In de Port des Créateurs creëren, schilderen, beeldhouwen, tekenen, transformeren, bouwen, zingen, dansen, interpreteren en optreden."
    ],
    "es": [
      "En el Port des Créateurs, la gente crea, pinta, esculpe, dibuja, transforma, construye, canta, baila, interpreta y actúa."
    ]
