      "dc:description": {
        "de": [
          "Scheune, die zu einem Ferienhaus umgebaut wurde, mit Terrasse, Grill, Parkplatz, Gartenmöbeln, ausgestatteter amerikanischer Küche, Badezimmer, WC, 1 Schlafzimmer (Bett 160), 1 Mezzanine (2 Betten 90), Garten, Gartenmöbel, Grill"
        ],
        "en": [
          "Barn converted into a gîte with terrace, barbecue, parking, garden furniture, American style equipped kitchen, bathroom, WC, 1 bedroom (bed 160), 1 mezzanine (2 beds 90), garden furniture, barbecue"
        ],
        "it": [
          "Fienile trasformato in gîte con terrazza, barbecue, parcheggio, mobili da giardino, cucina all'americana, bagno, WC, 1 camera da letto (letto 160), 1 mezzanino (2 letti 90), mobili da giardino, barbecue"
        ],
        "fr": [
          "Grange aménagée en gîte avec terrasse, barbecue, parking, salon de jardin.Cuisine à l'américaine équipée, salle de bains, WC, 1 chambre (lit 160), 1 mezzanine (2 lits 90), jardin salon de jardin, barbecue"
        ],
        "nl": [
          "Schuur omgebouwd tot gîte met terras, barbecue, parking, tuinmeubelen, ingerichte Amerikaanse keuken, badkamer, WC, 1 slaapkamer (bed 160), 1 mezzanine (2 bedden 90), tuinmeubelen, barbecue"
        ],
        "es": [
          "Granero convertido en casa rural con terraza, barbacoa, aparcamiento, muebles de jardín, cocina americana equipada, baño, WC, 1 habitación (cama 160), 1 altillo (2 camas 90), muebles de jardín, barbacoa"
        ]
      },
