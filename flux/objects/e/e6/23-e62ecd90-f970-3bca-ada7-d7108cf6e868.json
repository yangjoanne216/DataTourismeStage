  "rdfs:comment": {
    "de": [
      "Charakteristisches Haus im Zentrum des Ortes mit Zimmern im Obergeschoss. Ehemaliges Café/Hotel/Restaurant. Sehr schöne unabhängige Veranda zum Entspannen und Essen."
    ],
    "en": [
      "Character house located in the center of town with bedrooms upstairs. Former café/hotel/restaurant. Very nice independent veranda to relax and eat there."
    ],
    "it": [
      "Casa di carattere nel centro del paese con camere da letto al piano superiore. Ex bar/albergo/ristorante. Veranda indipendente molto bella per rilassarsi e mangiare."
    ],
    "fr": [
      "Maison de caractère située en centre bourg avec chambres à l'étage. Ancien café/hôtel/restaurant. Très belle véranda indépendante pour se décontracter et y manger."
    ],
    "nl": [
      "Karakteristiek huis gelegen in het centrum van het dorp met slaapkamers op de verdieping. Voormalig café/hotel/restaurant. Zeer mooie onafhankelijke veranda om te ontspannen en te eten."
    ],
    "es": [
      "Casa de carácter en el centro del pueblo con dormitorios en la planta alta. Antigua cafetería/hotel/restaurante. Veranda independiente muy agradable para relajarse y comer."
    ]
  },
