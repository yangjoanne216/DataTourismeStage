  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/84641cd8-eb05-3e91-b2e0-33b06b42a371",
      "dc:description": {
        "de": [
          "Wohnung F2, 29 m², 1. Stock. Stock. 7 km vom Meer, 400 m vom Dorfzentrum, 100 m von einem Thermalbad entfernt. Separater Eingang, Parkplatz. Private Waschmaschine, Gefrierschrank, Mikrowelle, Fernseher, Heizung. Bett 140 cm Schlafzimmer 1, BZ Wohnzimmer."
        ],
        "en": [
          "Apartment F2, 29 m², 1st floor. At 7km from the seaside, 400m from the village centre, 100m from a thermal bath. Independent entrance, parking. Private washing machine, freezer, microwave oven, television, heating. Bed 140 cm room 1, BZ stay."
        ],
        "it": [
          "Appartamento F2, 29 m², 1° piano. A 7 km dal mare, a 400 m dal centro del paese, a 100 m dalle terme. Ingresso indipendente, parcheggio. Lavatrice privata, congelatore, forno a microonde, televisione, riscaldamento. Letto 140 cm in camera 1, BZ in soggiorno."
        ],
        "fr": [
          "Appartement F2, 29 m², 1er étage. À 7km du bord de mer, à 400m du centre-village, à 100m d'un établissement thermal. Entrée indépendante, parking. Lave linge privatif, congélateur, four à micro-ondes, télévision, chauffage. Lit 140 cm chambre1, BZ séjour."
        ],
        "es": [
          "Piso F2, 29 m², 1er piso. A 7 km del mar, a 400 m del centro del pueblo, a 100 m de un balneario. Entrada independiente, aparcamiento. Lavadora privada, congelador, microondas, televisión, calefacción. Cama de 140 cm en el dormitorio 1, BZ en el salón."
        ],
        "nl": [
          "Flat F2, 29 m², 1e verdieping. Op 7km van de zee, 400m van het dorpscentrum, 100m van een kuuroord. Onafhankelijke ingang, parkeerplaats. Eigen wasmachine, vriezer, magnetron, televisie, verwarming. Bed 140 cm in slaapkamer 1, BZ in woonkamer."
        ]
      },
