  "rdfs:comment": {
    "de": [
      "Alterrenative Kosmetik ist eine handwerkliche Seifenfabrik, die kaltverseifte, überfettete Seifen und Naturkosmetik herstellt."
    ],
    "en": [
      "Alterrenative Cosmetics is a soap factory that manufactures cold-saponified soaps, surgras and natural cosmetics."
    ],
    "it": [
      "Alterrenative Cosmetics è un saponificio tradizionale che produce saponi saponificati a freddo e supergrassi e cosmetici naturali."
    ],
    "fr": [
      "Alterrenative cosmétiques est une savonnerie artisanale qui fabrique des savons saponifiés à froid, surgras et des cosmétiques naturels."
    ],
    "nl": [
      "Alterrenative Cosmetics is een traditionele zeepfabriek die koudgezeepte, supervette zepen en natuurlijke cosmetica produceert."
    ],
    "es": [
      "Alterrenative Cosmetics es una fábrica de jabón tradicional que produce jabones supergrasos saponificados en frío y cosméticos naturales."
    ]
  },
