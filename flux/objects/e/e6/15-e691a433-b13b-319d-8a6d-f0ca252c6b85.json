  "rdfs:comment": {
    "de": [
      "Schattiges Gelände (4 ha), Zugang zu einem 15x5 m großen Swimmingpool, 9 Swin-Golf-Löcher, 6 Disc-Golf-Parcours und 6 Foot-in-Golf-Parcours."
    ],
    "en": [
      "A surprising type of golf which is played with only one 3 three faced club and soft golf balls. In Fontgombault, the course's 9 holes cover a shaded area of 4 hectares. Access to a 15 metres deep nautical basin and to a tennis court."
    ],
    "it": [
      "Terreno ombreggiato (4 ettari), accesso a una piscina di 15x5 metri, 9 buche di Swin-golf, 6 percorsi di Disc-golf e 6 percorsi di Foot-in-Golf."
    ],
    "fr": [
      "Terrain ombragé de (4 ha), accès à une piscine de 15x5 mètres, 9 trous de Swin-golf, 6 parcours de Disc-golf et 6 parcours de Foot-in-Golf."
    ],
    "es": [
      "Terrenos sombreados (4 ha), acceso a una piscina de 15x5 metros, 9 hoyos de Swin-golf, 6 campos de Disc-golf y 6 campos de Foot-in-Golf."
    ],
    "nl": [
      "Schaduwrijk terrein (4 ha), toegang tot een zwembad van 15x5 meter, 9 holes Swin-golf, 6 banen Disc-golf en 6 banen Foot-in-Golf."
    ]
  },
