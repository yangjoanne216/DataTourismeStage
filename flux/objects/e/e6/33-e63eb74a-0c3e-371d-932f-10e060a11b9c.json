  "rdfs:comment": {
    "de": [
      "Audrey, Ser, Glaskünstlerin mit Flamme und Kunstperlerin begleitet Sie, um ihre einzigartigen Kreationen von Glasperlen und handgefertigten Objekten zu entdecken."
    ],
    "en": [
      "Audrey, Ser, flame glass artist and art pearl artist accompanies you to discover her unique creations of glass beads and handmade objects."
    ],
    "it": [
      "Audrey, Ser, artista del vetro e creatrice di perline, vi accompagnerà alla scoperta delle sue creazioni uniche di perle di vetro e oggetti fatti a mano."
    ],
    "fr": [
      "Audrey, Ser, artiste verrier à la flamme, et perlier d'art vous accompagne pour découvrir ses créations uniques de perles en verre et objets fait main."
    ],
    "nl": [
      "Audrey, Ser, glaskunstenares en kralenmaakster, zal u vergezellen om haar unieke creaties van glaskralen en handgemaakte objecten te ontdekken."
    ],
    "es": [
      "Audrey, Ser, artista del vidrio y fabricante de abalorios, le acompañará a descubrir sus creaciones únicas de abalorios y objetos hechos a mano."
    ]
  },
