  "rdfs:comment": {
    "de": [
      "4-Sterne-Hotel mit 64 Zimmern und Suite, im Herzen von Juan les Pins gelegen, 100 Meter von den feinen Sandstränden entfernt, 5 Minuten zu Fuß vom belebten Zentrum des Ortes, den Geschäften und dem Kongresszentrum."
    ],
    "en": [
      "4 star Hotel with 64 rooms and Suites, located in the heart of Juan les Pins, 100 meters from the sandy beaches, 5 minutes walking distance from the lively heart of the city, shops and Convention center."
    ],
    "it": [
      "Hotel 4 stelle con 64 camere e suite, situato nel cuore di Juan les Pins, a 100 metri dalle spiagge sabbiose, a 5 minuti a piedi dal vivace centro della località, dai negozi e dal centro congressi."
    ],
    "fr": [
      "Hôtel 4 étoiles de 64 chambres et suite, situé au cœur de Juan les Pins, à 100 mètres des plages de sable fin, à 5 minutes à pied du centre animé de la station, des magasins, du palais des congrès."
    ],
    "nl": [
      "Viersterrenhotel met 64 kamers en suite, gelegen in het hart van Juan les Pins, op 100 meter van de fijne zandstranden, op 5 minuten lopen van het levendige centrum, de winkels en het congrescentrum."
    ],
    "es": [
      "Hotel de 4 estrellas con 64 habitaciones y suite, situado en el corazón de Juan les Pins, a 100 metros de las playas de arena, a 5 minutos a pie del animado centro de la estación, de las tiendas y del centro de congresos."
    ]
  },
