  "rdfs:comment": {
    "de": [
      "Weinstube, Tapas-Bar. Über 300 Referenzen zu Preisen von Schlössern und Weingütern. Kostenlose Weinproben jeden Tag\nEine Reservierung wird empfohlen\nGanzjährig von 8:30 bis 14:00 Uhr geöffnet."
    ],
    "en": [
      "Wine bar, tapas bar. More than 300 references at the price of châteaux and estates. Free tasting every day\nReservation recommended\nOpen all year round from 8.30am to 2pm."
    ],
    "it": [
      "Wine bar, tapas bar. Più di 300 referenze a prezzi da château e da tenuta. Degustazione gratuita tutti i giorni\nSi consiglia la prenotazione\nAperto tutto l'anno dalle 8.30 alle 14.00."
    ],
    "fr": [
      "Bar à vin, bar à tapas. Plus de 300 références au prix des châteaux et domaines. Dégustation gratuite tous les jours. \nRéservation conseillée. \nOuvert toute l'année de 8h30 à 14h."
    ],
    "nl": [
      "Wijnbar, tapasbar. Meer dan 300 referenties tegen kasteel- en landgoedprijzen. Elke dag gratis proeven\nReservering aanbevolen\nHet hele jaar geopend van 8.30 tot 14.00 uur."
    ],
    "es": [
      "Bar de vinos, bar de tapas. Más de 300 referencias a precios de châteaux y fincas. Degustación gratuita todos los días\nSe recomienda reservar\nAbierto todo el año de 8.30 a 14.00."
    ]
  },
