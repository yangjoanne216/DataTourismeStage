  "rdfs:comment": {
    "de": [
      "Traditionelle regionale Küche, Spezialist für Tartiflette, König der Miesmuscheln mit Pommes frites, Produkte aus dem Champsaur.\r\nAbends auf Reservierung Tartiflette, Raclette am Kamin.\r\nIm Sommer Barbecue."
    ],
    "en": [
      "Traditional local cuisine, specialist in tartiflette, king of mussel and chips Champsaur products.\r\nIn the evening on reservation tartiflette, raclette by the fireplace.\r\nBarbecue in summer."
    ],
    "it": [
      "Cucina tradizionale locale, specialisti della tartiflette, re della frittura di cozze, prodotti Champsaur.\r\nLa sera su prenotazione tartiflette, raclette vicino al camino.\r\nBarbecue in estate."
    ],
    "fr": [
      "Cuisine traditionnelle du terroir, spécialiste de la tartiflette, roi de la moule-frite, produits du Champsaur.\r\nLe soir sur réservation tartiflette, raclette auprès de la cheminée.\r\nBarbecue en été."
    ],
    "nl": [
      "Traditionele lokale keuken, specialist in tartiflette, koning van de mosselen en frietjes, producten uit Champsaur.\r\nS Avonds, tartiflette of raclette bij de open haard, op reservering.\r\nBarbecueën in de zomer."
    ],
    "es": [
      "Cocina tradicional local, especialista en tartiflette, rey de los mejillones y patatas fritas, productos de Champsaur.\r\nPor la noche, tartiflette o raclette junto a la chimenea, previa reserva.\r\nBarbacoa en verano."
    ]
  },
