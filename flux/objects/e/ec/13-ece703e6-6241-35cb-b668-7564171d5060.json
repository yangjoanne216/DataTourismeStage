  "rdfs:comment": {
    "de": [
      "Städtisches Winter- und Sommerschwimmbad. 25m-Innenbecken, 25m-Außenbecken, Planschbecken im Freien und Solarium. Babyschwimmen, Wassergymnastik, Aquaphobie, Schwimmkurse, Tauchen."
    ],
    "en": [
      "Winter and summer municipal swimming centre. 25 m indoor pool, 25 m outdoor pool, open-air paddling pool and sun lounge. Water baby sessions, aquagym, aquaphobia therapy, swimming lessons, diving."
    ],
    "it": [
      "Piscina comunale invernale ed estiva. Piscina interna di 25 m, piscina esterna di 25 m, piscina per bambini all'aperto e solarium. Sessioni di nuoto per bambini, aquagym, aquafobia, lezioni di nuoto, immersioni."
    ],
    "fr": [
      "Piscine municipale d'hiver et d'été. Bassin intérieur de 25m, d'un bassin extérieur de 25m, d'une pataugeoire de plein air et d'un solarium. Séances bébés nageurs, aquagym, aquaphobie, cours de natation, plongée."
    ],
    "es": [
      "Piscina municipal de invierno y de verano. Piscina interior de 25m, una piscina exterior de 25m, una piscina infantil exterior y un solarium. Sesiones de natación para bebés, aquagym, acuafobia, clases de natación, buceo."
    ],
    "nl": [
      "Gemeentelijk winter- en zomerzwembad. Overdekt zwembad van 25m, een buitenzwembad van 25m, een peuterbad en een solarium. Babyzwemsessies, aquagym, aquafobie, zwemlessen, duiken."
    ]
  },
