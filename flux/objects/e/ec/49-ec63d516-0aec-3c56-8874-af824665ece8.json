  "rdfs:comment": {
    "de": [
      "5 Minuten vom Schloss Montreuil-Bellay, 15 Minuten von Saumur und der königlichen Abtei von Fontevraud entfernt, entdecken Sie das Ferienhaus Clair Logis für 2 Personen in Cizay-la-Madeleine."
    ],
    "en": [
      "5 minutes from the castle of Montreuil-Bellay, 15 minutes from Saumur and the Royal Abbey of Fontevraud, discover the Clair Logis gite for 2 persons in Cizay-la-Madeleine."
    ],
    "it": [
      "A 5 minuti dal castello di Montreuil-Bellay, a 15 minuti da Saumur e dall'Abbazia Reale di Fontevraud, scoprite la casa vacanze Clair Logis per 2 persone a Cizay-la-Madeleine."
    ],
    "fr": [
      "A 5 minutes du château de Montreuil-Bellay, 15 minutes de Saumur et de l'Abbaye Royale de Fontevraud, découvrez le gite Clair Logis pour 2 personnes à Cizay-la-Madeleine."
    ],
    "es": [
      "A 5 minutos del castillo de Montreuil-Bellay, a 15 minutos de Saumur y de la Abadía Real de Fontevraud, descubra la casa rural Clair Logis para 2 personas en Cizay-la-Madeleine."
    ],
    "nl": [
      "5 minuten van het kasteel van Montreuil-Bellay, 15 minuten van Saumur en de Koninklijke Abdij van Fontevraud, ontdek de gite Clair Logis voor 2 personen in Cizay-la-Madeleine."
    ]
  },
