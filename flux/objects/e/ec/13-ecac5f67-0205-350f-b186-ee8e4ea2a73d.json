  "rdfs:comment": {
    "de": [
      "Verkauf von Bauernhoferzeugnissen aus der Auvergne, Verköstigung. Verschiedene Käsesorten (Salers, Cantal, Bleu d’Auvergne) und Wurstwaren."
    ],
    "en": [
      "Sale and free tasting session of Auvergne farm produce. Cheeses (Salers,\r\nCantal, bleu d’Auvergne…) and cured hams."
    ],
    "it": [
      "Vendita di prodotti e formaggi d'Alvernia: AOP Cantal, Salers, bleu d'Auvergne, fourme d'Ambert, tome fresche, Saint-Nectaire. Salumi: prosciutto, salsicce e salsicce secche fatte in casa."
    ],
    "fr": [
      "Vente de produits d'Auvergne et de fromages : AOP Cantal, Salers, bleu d'Auvergne, fourme d'Ambert, tome fraîche, Saint-Nectaire. Charcuterie : jambon, saucissons et saucisses sèches, en production maison."
    ],
    "nl": [
      "Verkoop van producten en kazen uit de Auvergne: AOP Cantal, Salers, bleu d'Auvergne, fourme d'Ambert, verse tome, Saint-Nectaire. Charcuterie: ham, worst en droge worst, huisgemaakt."
    ],
    "es": [
      "Venta de productos y quesos de Auvernia: AOP Cantal, Salers, bleu d'Auvergne, fourme d'Ambert, fresh tome, Saint-Nectaire. Charcutería: jamón casero, salchichas y embutidos secos."
    ]
  },
