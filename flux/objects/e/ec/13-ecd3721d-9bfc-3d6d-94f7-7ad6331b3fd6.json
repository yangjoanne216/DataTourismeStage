  "rdfs:comment": {
    "de": [
      "Am Col des Aravis mit Blick auf den Mont-Blanc. Probieren Sie die Spezialitäten Savoyens. Kommunionsfeiern, Taufen, Familienessen und Seminare.  Täglich geöffnet."
    ],
    "en": [
      "Situated at the Col des Aravis, overlooking Mont-Blanc. Come and taste our Savoyard specialities. Communions, baptisms, family meals and seminars. Open every day."
    ],
    "it": [
      "Al Col des Aravis, di fronte al Monte Bianco, venite a gustare le specialità savoiarde. Comunioni, battesimi, pranzi di famiglia e seminari. Aperto tutti i giorni."
    ],
    "fr": [
      "Au Col des Aravis, face au Mont-Blanc, venez déguster les spécialités savoyardes. Communions, baptêmes, repas de familles et séminaires. Ouvert tous les jours."
    ],
    "nl": [
      "Kom op de Col des Aravis, tegenover de Mont Blanc, Savoyaardse specialiteiten proeven. Communies, doopsels, gezinsmaaltijden en seminaries. Elke dag open."
    ],
    "es": [
      "En el Col des Aravis, frente al Mont Blanc, venga a degustar las especialidades saboyanas. Comuniones, bautizos, comidas familiares y seminarios. Abierto todos los días."
    ]
  },
