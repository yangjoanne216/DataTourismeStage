  "rdfs:comment": {
    "de": [
      "Flaschmob mit Vila dance ab 21 Uhr, danach Verteilung von Lampions. Ab 22 Uhr startet der Rückzug in Richtung Port de Charmes, gefolgt von einem Feuerwerk um 23 Uhr."
    ],
    "en": [
      "Flaschmob with Vila dance from 9 p.m., followed by lantern distribution. The retreat starts at 10 p.m. towards the Port de Charmes, followed by fireworks at 11 p.m."
    ],
    "it": [
      "Flaschmob con danza Vila dalle 21:00, seguito dalla distribuzione di lanterne. La ritirata inizia alle 22.00 e si dirige verso il Port de Charmes, seguita dai fuochi d'artificio alle 23.00."
    ],
    "fr": [
      "Flaschmob avec Vila dance à partir de 21h puis distribution de lampions. Départ de la retraite à partir de 22 h en direction du Port de Charmes suivie des feux d'artifice à 23h."
    ],
    "nl": [
      "Flaschmob met Vila-dans vanaf 21.00 uur, gevolgd door het uitdelen van lampionnen. De terugtocht begint om 22.00 uur en gaat richting de Port de Charmes, gevolgd door vuurwerk om 23.00 uur."
    ],
    "es": [
      "Flaschmob con baile de Vila a partir de las 21.00 h, seguido del reparto de farolillos. La retirada comienza a las 22:00 h y se dirige hacia el Port de Charmes, seguida de fuegos artificiales a las 23:00 h."
    ]
  },
