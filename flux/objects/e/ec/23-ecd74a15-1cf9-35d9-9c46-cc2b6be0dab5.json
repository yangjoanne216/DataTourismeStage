  "rdfs:comment": {
    "de": [
      "Kanuabfahrt, Spaziergang auf der Vézère. Ab einem Alter von 8 Jahren. Material für Kanus wird bereitgestellt. Kinder unter 12 Jahren müssen von einem Erwachsenen begleitet werden."
    ],
    "en": [
      "Canoeing on the Vézère river. From 8 years old. Canoe equipment provided. Children under 12 years old must be accompanied by an adult."
    ],
    "it": [
      "Canoa, gita sulla Vézère. A partire da 8 anni. Attrezzatura da canoa fornita. I bambini sotto i 12 anni devono essere accompagnati da un adulto."
    ],
    "fr": [
      "Descente en canoë, balade sur la Vézère. A partir de 8 ans. Matériel canoë fourni. Les enfants de moins de 12 ans doivent être accompagnés par un adulte."
    ],
    "es": [
      "Piragüismo, excursión por el Vézère. A partir de 8 años. Equipo de canoa proporcionado. Los menores de 12 años deben ir acompañados de un adulto."
    ],
    "nl": [
      "Kanoën, tocht op de Vézère. Vanaf 8 jaar. Kano-uitrusting aanwezig. Kinderen jonger dan 12 jaar moeten door een volwassene worden begeleid."
    ]
