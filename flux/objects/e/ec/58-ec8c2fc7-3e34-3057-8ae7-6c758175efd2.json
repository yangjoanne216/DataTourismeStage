  "rdfs:comment": {
    "de": [
      "Diese handwerkliche Keksfabrik und Kaffeerösterei verwöhnt die Geschmacksknospen aller Süßigkeiten- und Kaffeeliebhaber."
    ],
    "en": [
      "This traditional biscuit factory and roastery delights the taste buds of all lovers of sweets and coffee."
    ],
    "it": [
      "Questa torrefazione artigianale di biscotti e caffè delizia il palato di tutti gli amanti dei dolci e del caffè."
    ],
    "fr": [
      "Cette biscuiterie et torréfaction artisanale ravit les papilles de tous les amateurs de douceurs et de café."
    ],
    "es": [
      "Este tostadero artesanal de galletas y café deleita el paladar de todos los amantes de los dulces y el café."
    ],
    "nl": [
      "Deze ambachtelijke koekjes- en koffiebranderij verwent de smaakpapillen van alle liefhebbers van zoetigheid en koffie."
    ]
  },
