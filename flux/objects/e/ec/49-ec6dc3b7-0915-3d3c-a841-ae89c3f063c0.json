  "rdfs:comment": {
    "de": [
      "Willkommen im Jardin, einem ökologisch verantwortungsvollen Ort zum Entdecken, Schaffen, Teilen, Denken und gemeinsamen Genießen... mitten im Herzen von Saint-Nazaire."
    ],
    "en": [
      "Welcome to Le Jardin, an eco-responsible place to discover, create, share, think and enjoy together... in the heart of Saint-Nazaire."
    ],
    "it": [
      "Benvenuti a Le Jardin, un luogo eco-responsabile per scoprire, creare, condividere, pensare e divertirsi insieme... nel cuore di Saint-Nazaire."
    ],
    "fr": [
      "Bienvenue au Jardin, un lieu éco-responsable pour découvrir, créer, partager, penser et savourer ensemble... en plein coeur de Saint-Nazaire."
    ],
    "nl": [
      "Welkom in Le Jardin, een ecologisch verantwoorde plek om samen te ontdekken, creëren, delen, denken en genieten... in het hart van Saint-Nazaire."
    ],
    "es": [
      "Bienvenido a Le Jardin, un lugar eco-responsable para descubrir, crear, compartir, pensar y disfrutar juntos... en el corazón de Saint-Nazaire."
    ]
  },
