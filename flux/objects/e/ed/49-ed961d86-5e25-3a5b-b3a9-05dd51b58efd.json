  "rdfs:comment": {
    "de": [
      "La ferme du bois in Simplé: Äpfel, Birnen, Quitten, Pflaumen, Kirschen, Erdbeeren, Himbeeren, schwarze Johannisbeeren, Johannisbeeren, Brombeeren. Konfitüren, Fruchtpürees und Fruchtsäfte."
    ],
    "en": [
      "La ferme du bois à Simplé : Apples, pears, quinces, plums, cherries, strawberries, raspberries, blackcurrants, redcurrants, blackberries. Jams, fruit purees and fruit juices."
    ],
    "it": [
      "La ferme du bois à Simplé : Mele, pere, mele cotogne, prugne, ciliegie, fragole, lamponi, ribes nero, ribes rosso, more. Marmellate, puree di frutta e succhi di frutta."
    ],
    "fr": [
      "La ferme du bois à Simplé :  Pommes, poires, coings, prunes, cerises, fraises, framboises, cassis, groseilles, mûres. Confitures, purées de fruits et jus de fruits."
    ],
    "es": [
      "La ferme du bois à Simplé : Manzanas, peras, membrillos, ciruelas, cerezas, fresas, frambuesas, grosellas, grosellas rojas, moras. Mermeladas, purés y zumos de frutas."
    ],
    "nl": [
      "La ferme du bois à Simplé : Appels, peren, kweeperen, pruimen, kersen, aardbeien, frambozen, zwarte bessen, rode bessen, bramen. Jam, vruchtenmoes en vruchtensap."
    ]
  },
