  "rdfs:comment": {
    "de": [
      "Im Herzen von La Roquette, am Place Paul Doumer, befindet sich das L'Épicier moderne, das sowohl Weinbar als auch Restaurant ist."
    ],
    "en": [
      "Located in the heart of the Roquette, in the Place Paul Doumer, l'Épicier moderne is both a wine bar and a restaurant."
    ],
    "it": [
      "Situato nel cuore della Roquette, in Place Paul Doumer, l'Épicier moderne è sia un wine bar che un ristorante."
    ],
    "fr": [
      "Situé au cœur de la Roquette, dans la Place Paul Doumer, l’Épicier moderne est à la fois bar à vin, à la fois restaurant."
    ],
    "es": [
      "Situado en el corazón de la Roquette, en la plaza Paul Doumer, l'Épicier moderne es a la vez un bar de vinos y un restaurante."
    ],
    "nl": [
      "Gelegen in het hart van de Roquette, op de Place Paul Doumer, is l'Épicier moderne zowel een wijnbar als een restaurant."
    ]
  },
