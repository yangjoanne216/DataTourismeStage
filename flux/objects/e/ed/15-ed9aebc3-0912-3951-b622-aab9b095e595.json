  "rdfs:comment": {
    "de": [
      "Die Mediathek von Azay-le-Ferron bietet in Zusammenarbeit mit dem CPIE Brenne-Berry eine Animation rund um Spiele im Zusammenhang mit der Natur an."
    ],
    "en": [
      "The media library of Azay-le-Ferron proposes with the CPIE Brenne-Berry an animation around games related to nature."
    ],
    "it": [
      "La mediateca di Azay-le-Ferron, in collaborazione con il CPIE Brenne-Berry, propone un'attività basata su giochi legati alla natura."
    ],
    "fr": [
      "La médiathèque d'Azay-le-Ferron  propose avec le CPIE Brenne-Berry une animation autour de jeux en lien avec la nature."
    ],
    "es": [
      "La mediateca de Azay-le-Ferron, en colaboración con el CPIE Brenne-Berry, propone una actividad basada en juegos relacionados con la naturaleza."
    ],
    "nl": [
      "De mediatheek van Azay-le-Ferron stelt, in samenwerking met de CPIE Brenne-Berry, een activiteit voor die gebaseerd is op spelletjes in verband met de natuur."
    ]
  },
