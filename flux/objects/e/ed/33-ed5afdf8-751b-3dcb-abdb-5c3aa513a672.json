  "rdfs:comment": {
    "de": [
      "Haben Sie schon einmal ein Pferderennen besucht? Dann ist dies die Gelegenheit, dies in Granada zu tun."
    ],
    "en": [
      "Ever been to a horse race before? This is your chance to do it in Granada."
    ],
    "it": [
      "Siete mai stati a una corsa di cavalli? Questa è la vostra occasione per farlo a Granada."
    ],
    "fr": [
      "Vous avez déjà assisté à une course hippique ? C'est l'occasion de le faire à Grenade."
    ],
    "es": [
      "¿Ha asistido alguna vez a una carrera de caballos? Esta es su oportunidad de hacerlo en Granada."
    ],
    "nl": [
      "Ben je ooit naar een paardenrace geweest? Dit is je kans om dat te doen in Granada."
    ]
  },
