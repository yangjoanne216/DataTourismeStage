  "rdfs:comment": {
    "de": [
      "La Table des Granges mit seiner Panoramaterrasse mit Blick auf das Mont-Blanc-Massiv bietet Ihnen authentische Speisen für einen angenehmen Moment in einem sehr gemütlichen Rahmen."
    ],
    "en": [
      "La Table des Granges, with its panoramic terrace overlooking the Mont Blanc massif, offers you an authentic restaurant for a pleasant moment in a very warm setting."
    ],
    "it": [
      "La Table des Granges, con la sua terrazza panoramica sul massiccio del Monte Bianco, offre una cucina autentica per un momento piacevole in un ambiente molto accogliente."
    ],
    "fr": [
      "La Table des Granges, et sa terrasse panoramique sur le massif du Mont Blanc vous propose une restauration authentique pour un agréable moment dans un cadre très chaleureux."
    ],
    "nl": [
      "La Table des Granges, met zijn panoramisch terras met uitzicht op het Mont Blanc-massief, biedt een authentiek diner voor een aangenaam moment in een zeer warm kader."
    ],
    "es": [
      "La Table des Granges, con su terraza panorámica con vistas al macizo del Mont Blanc, ofrece una auténtica cena para pasar un momento agradable en un entorno muy cálido."
    ]
  },
