  "rdfs:comment": {
    "de": [
      "Von Tannenwäldern umgebenes Hotel in der Nähe des Flusses Espezonnette, Angeln. Geschlossener Parkplatz. Beheiztes Schwimmbad im Freien. Restaurant-Label Logis de France: 2 Kokotten"
    ],
    "en": [
      "Hotel in the midst of pine forests near the Espezonnette River, fishing. Private car park. Outdoor heated pool. Logis de France restaurant approved: 2 \"saucepans\"."
    ],
    "it": [
      "Hotel circondato da pinete vicino al fiume Espezonnette, pesca. Parcheggio chiuso. Piscina esterna riscaldata. Etichetta ristorante Logis de France: 2 cocottes"
    ],
    "fr": [
      "Hôtel entouré de forêts de sapins près de la rivière l'Espezonnette, pêche. Parking fermé. Piscine plein air chauffée. Label restauration Logis de France : 2 cocottes"
    ],
    "es": [
      "Hotel rodeado de bosques de pinos cerca del río Espezonnette, pesca. Aparcamiento cerrado. Piscina exterior climatizada. Etiqueta del restaurante Logis de France: 2 cocottes"
    ],
    "nl": [
      "Hotel omgeven door dennenbossen in de buurt van de rivier Espezonnette, vissen. Gesloten parkeerplaats. Verwarmd buitenzwembad. Logis de France restaurant label: 2 cocottes"
    ]
  },
