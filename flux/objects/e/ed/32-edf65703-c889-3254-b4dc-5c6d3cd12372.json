  "rdfs:comment": {
    "de": [
      "Herstellung von Ziegenkäse vom Bauernhof und von Ziegenkäse mit der geschützten Ursprungsbezeichnung Pélardon.\nDer Betrieb zählt 230 Ziegen der Alpenrasse, die in der Garrigue gehalten werden."
    ],
    "en": [
      "Production of farm goat's cheese and Pélardon PDO.\nThe farm has 230 alpine goats reared in the scrubland."
    ],
    "it": [
      "Produzione di formaggio di capra di fattoria e di Pélardon DOP.\nLa fattoria dispone di 230 capre di razza alpina allevate sulle garighe."
    ],
    "fr": [
      "Production de fromage de chèvre fermier et d'AOP pélardon.\nL'exploitation compte 230 chèvres de race alpine élevées sur parcours en garrigues."
    ],
    "nl": [
      "Productie van boerengeitenkaas en BOB Pélardon.\nDe boerderij heeft 230 geiten van het alpenras die op de garrigues worden gehouden."
    ],
    "es": [
      "Producción de queso de cabra de granja y DOP Pélardon.\nLa granja cuenta con 230 cabras de raza alpina criadas en las garrigas."
    ]
  },
