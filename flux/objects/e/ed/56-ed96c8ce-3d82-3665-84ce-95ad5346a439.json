  "rdfs:comment": {
    "de": [
      "Gegenüber dem Meer, 80 Meter vom Strand entfernt, in der Nähe von Geschäften, beheizter Außenpool, Solarium, Privatparkplatz, 50 klimatisierte Zimmer mit allem Komfort."
    ],
    "en": [
      "Facing the sea, 80 meters from the beach, close to shops, heated outdoor swimming pool, solarium, private parking, 50 air-conditioned rooms with every comfort."
    ],
    "it": [
      "Di fronte al mare, a 80 metri dalla spiaggia, vicino ai negozi, piscina esterna riscaldata, solarium, parcheggio privato, 50 camere con aria condizionata e tutti i comfort."
    ],
    "fr": [
      "Face à la mer, à 80 mètres de la plage, proche commerces, piscine extérieure chauffée, solarium, parking privé, 50 chambres climatisées tout confort."
    ],
    "nl": [
      "Met uitzicht op zee, 80 meter van het strand, dicht bij winkels, verwarmd buitenzwembad, solarium, eigen parkeerplaats, 50 kamers met airconditioning en alle comfort."
    ],
    "es": [
      "Frente al mar, a 80 metros de la playa, cerca de los comercios, piscina exterior climatizada, solarium, aparcamiento privado, 50 habitaciones con aire acondicionado y todas las comodidades."
    ]
  },
