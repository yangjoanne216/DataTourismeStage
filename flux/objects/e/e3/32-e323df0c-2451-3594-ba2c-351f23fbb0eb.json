  "rdfs:comment": {
    "de": [
      "15m2 Studio, ausgestattet mit Kochplatte, Top-Ofen / Grill / Nespresso-Maschine, Geschirr. Handtuch, Willkommensartikel. Kleiner, sonniger und ruhiger Innenhof unter Eichen. Garantiert entspannende Atmosphäre."
    ],
    "en": [
      "Studio of 15m2, equipped with a cooking plate, top-oven / barbecue / Nespresso machine, crockery. Towel, welcome product. Small sunny courtyard, quiet, under the oaks. Relaxing atmosphere guaranteed."
    ],
    "it": [
      "Studio di 15 m2, attrezzato con fornelli, forno a legna / barbecue / macchina Nespresso, stoviglie. Asciugamano, prodotto di benvenuto. Piccolo cortile soleggiato, tranquillo, sotto le querce. Atmosfera rilassata garantita."
    ],
    "fr": [
      "Studio de 15m2, équipé d'une plaque de cuisson, top-four / barbecue / machine Nespresso, vaisselleries. Serviette de bain, produit de bienvenu. Petite cour ensoleillée, calme, sous les chênes . Ambiance détente garantie."
    ],
    "nl": [
      "Studio van 15m2, uitgerust met fornuis, top-oven / barbecue / Nespresso machine, serviesgoed. Handdoek, welkom product. Kleine zonnige binnenplaats, rustig, onder de eikenbomen. Ontspannen sfeer gegarandeerd."
    ],
    "es": [
      "Estudio de 15m2, equipado con cocina, top-horno / barbacoa / máquina Nespresso, vajilla. Toalla, producto de bienvenida. Pequeño patio soleado, tranquilo, bajo los robles. Ambiente relajado garantizado."
    ]
  },
