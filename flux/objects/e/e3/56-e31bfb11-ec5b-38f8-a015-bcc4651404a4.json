  "rdfs:comment": {
    "de": [
      "Dorfhaus für 2 Personen. Wohnzimmer, Küchenzeile, im Obergeschoss 1 Schlafzimmer, Duschbad, WC und ein Hof\nIn der Nähe des Stadtzentrums und 15 Minuten von den Thermen entfernt."
    ],
    "en": [
      "Village house for 2 people. Living room, kitchenette, upstairs 1 bedroom, shower room, toilet and a courtyard\nClose to the town centre and 15 minutes from the thermal baths."
    ],
    "it": [
      "Casa di paese per 2 persone. Soggiorno, angolo cottura, al piano superiore 1 camera da letto, bagno con doccia, WC e cortile\nVicino al centro città e a 15 minuti dalle terme."
    ],
    "fr": [
      "Maison de village pour 2 personnes. Séjour, coin cuisine, à l'étage 1 chambre, salle de douche, WC et une cour. \nProche du centre ville et à 15 minutes des thermes."
    ],
    "nl": [
      "Dorpshuis voor 2 personen. Woonkamer, kitchenette, boven 1 slaapkamer, doucheruimte, WC en een binnenplaats\nDicht bij het stadscentrum en 15 minuten van de thermale baden."
    ],
    "es": [
      "Casa de pueblo para 2 personas. Salón, cocina americana, arriba 1 dormitorio, cuarto de ducha, WC y un patio\nCerca del centro de la ciudad y a 15 minutos de los baños termales."
    ]
  },
