  "rdfs:comment": {
    "de": [
      "Saint-Georges Voiles, eine von den französischen Verbänden für Segeln, Strandsegeln, Wandern und Kajak zugelassene Wassersportbasis, befindet sich an der wunderschönen Conche de Saint-Georges-de-Didonne."
    ],
    "en": [
      "Saint-Georges Voiles, a nautical base approved by the French Federations of sailing, sand yachting, hiking and kayaking, is located on the magnificent Conche de Saint-Georges-de-Didonne."
    ],
    "it": [
      "Saint-Georges Voiles, base nautica approvata dalle federazioni francesi di vela, sand yachting, escursionismo e kayak, si trova sulla magnifica Conche di Saint-Georges-de-Didonne."
    ],
    "fr": [
      "Saint-Georges Voiles, base nautique agrée par les Fédérations Françaises de voile, de char à voile, de randonnée et de kayak, est installée sur la magnifique Conche de Saint-Georges-de-Didonne."
    ],
    "nl": [
      "Saint-Georges Voiles, een door de Franse zeil-, strandzeil-, wandel- en kajakfederaties erkende nautische basis, ligt op de prachtige Conche de Saint-Georges-de-Didonne."
    ],
    "es": [
      "Saint-Georges Voiles, base náutica homologada por las federaciones francesas de vela, vela de arena, senderismo y kayak, está situada en la magnífica Conche de Saint-Georges-de-Didonne."
    ]
  },
