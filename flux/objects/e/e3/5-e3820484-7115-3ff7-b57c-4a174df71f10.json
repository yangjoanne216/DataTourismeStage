  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/ab920f02-4591-3722-9011-8bee50609d9a",
      "dc:description": {
        "de": [
          "Voll ausgestattetes Chalet - 1 Schlafzimmer (1 Doppelbett) - Kochecke - Mikrowelle - Backofen - Wohnzimmer mit 1 Schlafcouch für 2 Personen - Esszimmer - TV - Bad (Dusche) - WC - elektrische Heizung - Gartenmöbel - Parkplatz - Grill - Pelletofen - Terrasse - Babybett zur Verfügung - Liegestühle."
        ],
        "en": [
          "Fully equipped chalet - 1 bedroom (1 double bed) - kitchenette - microwave - oven - living room with 1 double sofa bed - dining room - TV - shower room (shower) - WC - electric heating - garden furniture - car park - barbecue - pellet stove - terrace - baby bed available - sun loungers."
        ],
        "it": [
          "Chalet completamente attrezzato - 1 camera da letto (1 letto matrimoniale) - angolo cottura - microonde - forno - soggiorno con 1 divano letto matrimoniale - sala da pranzo - TV - bagno con doccia - WC - riscaldamento elettrico - mobili da giardino - parcheggio - barbecue - stufa a pellet - terrazza - lettino per bambini disponibile - sedie a sdraio"
        ],
        "fr": [
          "Chalet tout équipé - 1 chambre (1 lit 2 pers) - coin cuisine - micro ondes - four - salon avec 1 convertible 2 pers - salle à manger - TV - salle d'eau (douche) - WC - chauffage électrique - salon de jardin - parking - barbecue - poêle à granulés - terrasse - lit bébé à disposition - chaises longues."
        ],
        "nl": [
          "Volledig ingericht chalet - 1 slaapkamer (1 tweepersoonsbed) - kitchenette - magnetron - oven - woonkamer met 1 tweepersoonsslaapbank - eetkamer - TV - doucheruimte - WC - elektrische verwarming - tuinmeubilair - parkeerplaats - barbecue - pelletkachel - terras - babybedje beschikbaar - ligstoelen"
        ],
        "es": [
          "Chalet totalmente equipado - 1 dormitorio (1 cama doble) - cocina americana - microondas - horno - salón con 1 sofá cama doble - comedor - TV - cuarto de ducha - WC - calefacción eléctrica - muebles de jardín - aparcamiento - barbacoa - estufa de pellets - terraza - cuna disponible - tumbonas"
        ]
      },
