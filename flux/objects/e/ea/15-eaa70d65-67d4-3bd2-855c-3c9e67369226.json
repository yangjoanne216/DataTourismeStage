  "rdfs:comment": {
    "de": [
      "Der Campingplatz Ligny, der sich auf dem Freizeitgelände von Ligny befindet, empfängt Sie vom Osterwochenende bis Ende Oktober, allein, mit Freunden oder der Familie."
    ],
    "en": [
      "The Ligny campsite, located on the Ligny leisure base, welcomes you from Easter weekend to the end of October, alone, with friends or with family."
    ],
    "it": [
      "Il campeggio Ligny, situato nel parco divertimenti di Ligny, vi accoglie dal weekend di Pasqua alla fine di ottobre, da soli, con gli amici o in famiglia."
    ],
    "fr": [
      "Le camping de Ligny, situé sur la base de loisirs de Ligny, vous accueille du week-end de Pâques à fin octobre, seul, entre amis ou en famille."
    ],
    "es": [
      "El camping de Ligny, situado en el parque de ocio de Ligny, le acoge desde el fin de semana de Pascua hasta finales de octubre, solo, con amigos o en familia."
    ],
    "nl": [
      "De camping Ligny, gelegen op het recreatiepark Ligny, verwelkomt u vanaf het paasweekend tot eind oktober, alleen, met vrienden of met het gezin."
    ]
  },
