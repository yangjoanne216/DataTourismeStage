  "rdfs:comment": {
    "de": [
      "Ungewöhnliche Hütte ideal für Paare mit nordischem Bad. Genießen Sie den Charme der Landschaft und entdecken Sie unsere Tiere bei Sonnenuntergang."
    ],
    "en": [
      "Unusual cabin ideal for couples with a Nordic bath. Come and enjoy the charm of the countryside and discover our animals at sunset."
    ],
    "it": [
      "Cabina insolita ideale per le coppie con bagno nordico. Venite a godervi il fascino della campagna e a scoprire i nostri animali al tramonto."
    ],
    "fr": [
      "Cabane insolite idéale couple avec bain nordique. Venez profiter du charme de la campagne et découvrir nos animaux au coucher du soleil."
    ],
    "nl": [
      "Ongebruikelijke hut ideaal voor koppels met een Scandinavisch bad. Kom genieten van de charme van het platteland en ontdek onze dieren bij zonsondergang."
    ],
    "es": [
      "Inusual cabaña ideal para parejas con un baño nórdico. Venga a disfrutar del encanto del campo y a descubrir nuestros animales al atardecer."
    ]
  },
