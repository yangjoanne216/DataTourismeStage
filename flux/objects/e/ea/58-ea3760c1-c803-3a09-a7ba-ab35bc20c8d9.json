      "dc:description": {
        "de": [
          "Konzert am 12. August: 21 Uhr ? Aubagne Jazz Band \"Spezial Eddy Mitchell\"\r\nBall am 13. August 20h ? Live-Duo Kaaz\r\nBall vom 14. August 21h ? Lenaprod - Abend der Top 50\r\nShow 15. August 21h - Lenaprod ? Kabarett Feder Strass und Pailletten"
        ],
        "en": [
          "August 12 concert: 9pm ? Aubagne Jazz Band \"Eddy Mitchell special\r\nAugust 13 Ball 8pm ? Kaaz live duo\r\nAugust 14 ball 9pm ? Lenaprod - top 50 evening\r\nAugust 15 show 9pm - Lenaprod ? cabaret feathers rhinestones and sequins"
        ],
        "it": [
          "Concerto del 12 agosto: ore 21.00 ? Aubagne Jazz Band \"Eddy Mitchell special\r\nBallo il 13 agosto ore 20.00 ? Duo live Kaaz\r\nBallo del 14 agosto ore 21.00 ? Lenaprod - serata Top 50\r\nSpettacolo del 15 agosto ore 21.00 - Lenaprod ? cabaret di piume, strass e glitter"
        ],
        "fr": [
          "Concert du 12 août : 21h – Aubagne Jazz Band « spécial Eddy Mitchell »\r\nBal du 13 août 20h – Duo live Kaaz\r\nBal du 14 aout 21h – Lenaprod - soirée top 50\r\nSpectacle 15 août 21h  - Lenaprod – cabaret plumes strass et paillettes"
        ],
        "es": [
          "Concierto del 12 de agosto: 21h ? Aubagne Jazz Band \"Eddy Mitchell especial\r\nBaile el 13 de agosto a las 20h ? Dúo en directo Kaaz\r\nBaile del 14 de agosto 21.00 h ? Lenaprod - Velada Top 50\r\nEspectáculo 15 de agosto 21:00 h - Lenaprod ? cabaret de plumas, strass y purpurina"
        ],
        "nl": [
          "Concert op 12 augustus: 21 uur ? Aubagne Jazz Band \"Eddy Mitchell special\r\nBal op 13 augustus: 20u ? Live duo Kaaz\r\nBal op 14 augustus 21u ? Lenaprod - top 50 avond\r\nShow 15 augustus 21u - Lenaprod ? veren, strass en glitter cabaret"
        ]
      },
