  "rdfs:comment": {
    "de": [
      "Zuflucht Astronomische Sternwarte mit einem Teleskop, abgeschieden in den Bergen oberhalb des Weilers Bouchier. Organisation von Abenden mit Abendessen + Beobachtung (von April bis Oktober auf Reservierung). Übernachtung mit Halbpension."
    ],
    "en": [
      "Refuge Astronomical observatory equipped with a telescope, isolated in the mountains above the hamlet of Bouchier. Dinner and observation evenings (April to October by prior arrangement). Overnight accommodation on a half-board basis."
    ],
    "it": [
      "Rifugio Osservatorio astronomico dotato di telescopio, isolato sulle montagne sopra la frazione di Bouchier. Cena e serate di osservazione (da aprile a ottobre su prenotazione). Pernottamento in mezza pensione."
    ],
    "fr": [
      "Refuge Observatoire astronomique doté d'un téléscope, isolé dans la montagne au-dessus du hameau de Bouchier. Organisation de soirées dîner + observation (d'avril à octobre sur réservation). Accueil pour la nuit en demi-pension."
    ],
    "es": [
      "Refugio Observatorio astronómico equipado con un telescopio, aislado en las montañas por encima de la aldea de Bouchier. Cena y veladas de observación (de abril a octubre con reserva previa). Alojamiento en régimen de media pensión."
    ],
    "nl": [
      "Refuge Astronomisch observatorium uitgerust met een telescoop, geïsoleerd in de bergen boven het gehucht Bouchier. Diner en observatie-avonden (april tot oktober op afspraak). Overnachting op basis van halfpension."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Observatoire astronomique Le Pas du Loup"
    ]
  },
