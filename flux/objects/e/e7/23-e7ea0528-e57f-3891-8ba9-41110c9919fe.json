  "rdfs:comment": {
    "de": [
      "Messe.\n11.30 Uhr Zeremonie am Kriegsdenkmal.\n12 Uhr Ehrenwein, musikalisch umrahmt von Lyre Arzacquoise.\n13 Uhr Südwestliches Essen.\n15 Uhr Pétanque-Turnier mit Schießwettbewerb. Im Doppelpack."
    ],
    "en": [
      "Mass.\n11:30 Ceremony at the war memorial.\n12pm Vin d'honneur hosted by Lyre Arzacquoise.\n1 p.m. Southwestern meal.\n3pm Petanque tournament with shooting competition. Doubles."
    ],
    "it": [
      "Messa.\n11:30 Cerimonia al monumento ai caduti.\n12:00 Aperitivo a cura di Lyre Arzacquoise.\nore 13:00 Pasto del Sud-Ovest.\nore 15:00 Torneo di petanque con gara di tiro. Doppio."
    ],
    "fr": [
      "Messe.\n11h30 Cérémonie au monument aux morts.\n12h Vin d'honneur animé par Lyre Arzacquoise.\n13h Repas du Sud-Ouest.\n15h Tournois de pétanque avec concours de tir. En doublette."
    ],
    "es": [
      "Misa.\n11:30 Ceremonia en el memorial de guerra.\n12h Bebidas ofrecidas por Lyre Arzacquoise.\n13.00 Comida del Suroeste.\n15h Torneo de petanca con tiro al blanco. Dobles."
    ],
    "nl": [
      "Mis.\n11:30 Ceremonie bij het oorlogsmonument.\n12.00 uur Borrel verzorgd door Lyre Arzacquoise.\n13.00 uur Zuid-West maaltijd.\n15.00 uur Petanquetoernooi met schietcompetitie. Dubbelspel."
    ]
  },
