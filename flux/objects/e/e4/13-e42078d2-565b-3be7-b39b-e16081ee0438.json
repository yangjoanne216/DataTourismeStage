  "rdfs:comment": {
    "de": [
      "Facing south and looking onto Mont Blanc, the Flégère area is the departure for classic hikes to magnificent mountain lakes in summer and south-facing skiing   with a view over the Mont Blanc mountain range !"
    ],
    "en": [
      "Facing south and looking onto Mont Blanc, the Flégère area is the departure for classic hikes to magnificent mountain lakes in summer and south-facing skiing   with a view over the Mont Blanc mountain range !"
    ],
    "it": [
      "Facing south and looking onto Mont Blanc, the Flégère area is the departure for classic hikes to magnificent mountain lakes in summer and south-facing skiing   with a view over the Mont Blanc mountain range !"
    ],
    "fr": [
      "Exposé plein sud face à la chaîne du Mont-Blanc, le site de la Flégère est le point de départ de grandes randonnées classiques menant à de magnifiques lacs de montagne en été et ski plein sud face à la chaine du Mont-Blanc en hiver !"
    ],
    "es": [
      "Facing south and looking onto Mont Blanc, the Flégère area is the departure for classic hikes to magnificent mountain lakes in summer and south-facing skiing   with a view over the Mont Blanc mountain range !"
    ],
    "nl": [
      "De site van Flégère, gelegen in de richting van de Mont Blanc-keten, is het vertrekpunt voor grote klassieke wandelingen die in de zomer naar prachtige bergmeren leiden en in de winter naar het zuiden skiën in de richting van de Mont Blanc-keten!"
    ]
  },
