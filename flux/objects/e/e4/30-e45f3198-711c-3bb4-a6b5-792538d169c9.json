  "rdfs:comment": {
    "de": [
      "Panoramahotel gegenüber dem Mont Saint-Michel, inmitten eines Landschaftsparks und blühender Gärten. Wir bieten Ihnen 39 Zimmer; geräumige, helle und raffiniert eingerichtete Suiten. Absolute Ruhe."
    ],
    "en": [
      "Panoramic hotel facing the Mont Saint-Michel, in the middle of a landscaped park and flowery gardens. We offer 39 rooms; spacious, bright and elegantly decorated suites. Absolute calm."
    ],
    "it": [
      "Hotel panoramico di fronte al Mont Saint-Michel, al centro di un parco paesaggistico e di giardini fioriti. Offriamo 39 camere; suite spaziose, luminose e decorate con raffinatezza. Pace e tranquillità assolute."
    ],
    "fr": [
      "Hôtel panoramique face au Mont Saint-Michel, au milieu d'un parc paysager et de jardins fleuris. Nous vous proposons 39 chambres ; des suites spacieuses, lumineuses et décorées avec raffinement. Calme absolu."
    ],
    "nl": [
      "Panoramisch hotel met uitzicht op de Mont Saint-Michel, te midden van een aangelegd park en bloementuinen. Wij bieden 39 kamers; ruime suites, licht en verfijnd ingericht. Absolute rust en stilte."
    ],
    "es": [
      "Hotel panorámico con vistas al Monte Saint-Michel, en medio de un parque paisajístico y jardines de flores. Ofrecemos 39 habitaciones; suites amplias, luminosas y decoradas con refinamiento. Paz y tranquilidad absolutas."
    ]
  },
