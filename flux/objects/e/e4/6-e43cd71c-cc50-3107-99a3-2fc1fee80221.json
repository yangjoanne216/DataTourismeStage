      "dc:description": {
        "de": [
          "Wandern am Fuße des Waldes Haute-Joux, Schneeschuhwandern, Langlauf, Abenteuerparcours, Pferde, Ponys, Mountainbikes, TGV-Bahnhof 17 km, Einkaufsmöglichkeiten 8 km. Coop Brot. Froamge: 2km"
        ],
        "en": [
          "Hiking at the foot of the Haute-Joux forest, snowshoeing, cross-country skiing, adventure trail, horse, pony, mountain bike, TGV station at 17 km, shops at 8 km. Coop bread. froamge : 2km"
        ],
        "it": [
          "Escursioni ai piedi della foresta di Haute-Joux, ciaspolate, sci di fondo, percorso avventura, cavallo, pony, mountain bike, stazione TGV a 17 km, negozi a 8 km. Coop pain. froamge: 2km"
        ],
        "fr": [
          "Rando pédestre au pied de la forêt de la Haute-Joux, raquette, ski de fond, parcours aventure, cheval, poney, VTT, gare TGV à 17 km, commerces à 8 km. Coop pain. froamge : 2km"
        ],
        "nl": [
          "Wandelen aan de voet van het bos van Haute-Joux, sneeuwschoenwandelen, langlaufen, avonturenparcours, paard, pony, mountainbike, TGV-station op 17 km, winkels op 8 km. Coop brood. froamge: 2km"
        ],
        "es": [
          "Senderismo al pie del bosque de Haute-Joux, raquetas de nieve, esquí de fondo, pista de aventura, caballo, poni, bicicleta de montaña, estación de TGV a 17 km, tiendas a 8 km. Coop pain. froamge: 2km"
        ]
