  "rdfs:comment": {
    "de": [
      "Neues Bar-Restaurant Le Tire En Joie. Bistro in einer gemütlichen Atmosphäre, Restaurant mit traditioneller Küche (Tagesmenü, Landmenü, Frösche). Lebensmittelgeschäft, in dem Sie alles und viele lokale Produkte finden."
    ],
    "en": [
      "New bar restaurant Le Tire En Joie. Bistro in a friendly atmosphere, restaurant with traditional cuisine (daily menu, country menu, frogs). Grocery store where you will find everything and a lot of local products."
    ],
    "it": [
      "Nuovo bar ristorante Le Tire En Joie. Bistrot dall'atmosfera accogliente, ristorante con cucina tradizionale (menù del giorno, menù contadino, rane). Negozio di alimentari dove troverete di tutto e molti prodotti locali."
    ],
    "fr": [
      "Nouveau bar restaurant Le Tire En Joie. Bistro dans une ambiance convivial, restaurant avec une cuisine traditionnel (menu du jour, menu campagnard, grenouilles). Épicerie ou vous trouverez de tout et avec beaucoup de produits locaux."
    ],
    "es": [
      "Nuevo bar restaurante Le Tire En Joie. Bistrot en un ambiente acogedor, restaurante de cocina tradicional (menú del día, menú campesino, ranas). Tienda de comestibles donde encontrará de todo y muchos productos locales."
    ],
    "nl": [
      "Nieuwe bar restaurant Le Tire En Joie. Bistro in een gezellige sfeer, restaurant met traditionele keuken (dagmenu, landelijk menu, kikkers). Kruidenierswinkel waar u alles vindt en veel lokale producten."
    ]
  },
