  "rdfs:comment": {
    "de": [
      "Genießen Sie authentische Spezialitäten aus den Höhlenwohnungen des Anjou: Fouées, kleine Brote, die in Holzöfen gebacken werden und die Sie wie früher mit regionalen Produkten belegen."
    ],
    "en": [
      "Savour authentic specialities from the troglodytes of Anjou: fouées, small breads baked in wood-fired ovens, which you will garnish with regional products as in the past."
    ],
    "it": [
      "Assaporate le autentiche specialità dei trogloditi dell'Anjou: le fouées, piccoli pani cotti nel forno a legna, che guarnirete con prodotti regionali come un tempo."
    ],
    "fr": [
      "Savourez d'authentiques spécialités des troglodytes d'Anjou : les fouées, petits pains cuits dans les fours à bois, que vous garnirez de produits régionaux comme autrefois."
    ],
    "es": [
      "Saboree auténticas especialidades de los trogloditas de Anjou: fouées, pequeños panes cocidos en hornos de leña, que aderezará con productos regionales como antaño."
    ],
    "nl": [
      "Proef de authentieke specialiteiten van de troglodieten van Anjou: fouées, kleine broden gebakken in houtovens, die u net als vroeger zult garneren met streekproducten."
    ]
  },
  "rdfs:label": {
