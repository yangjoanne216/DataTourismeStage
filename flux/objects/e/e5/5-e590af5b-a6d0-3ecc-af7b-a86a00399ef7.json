  "rdfs:comment": {
    "de": [
      "Das ganze Dorf Furdenheim wird durch Kunsthandwerk, Kultur und die abendliche Ton- und Lichtshow zum Leben erweckt."
    ],
    "en": [
      "The whole village of Furdenheim comes alive with crafts, culture and a sound and light show in the evening."
    ],
    "it": [
      "L'intero villaggio di Furdenheim si anima con artigianato, cultura e uno spettacolo di suoni e luci la sera."
    ],
    "fr": [
      "Tout le village de Furdenheim s'anime autour de l'artisanat, de la culture et du spectacle son et lumière donné en soirée."
    ],
    "es": [
      "Todo el pueblo de Furdenheim cobra vida con artesanía, cultura y un espectáculo de luz y sonido por la noche."
    ],
    "nl": [
      "Het hele dorp Furdenheim komt tot leven met ambachten, cultuur en een klank- en lichtshow in de avond."
    ]
