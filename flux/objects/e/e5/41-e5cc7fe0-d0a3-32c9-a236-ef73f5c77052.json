  "rdfs:comment": {
    "de": [
      "Les vents d'anges (Orgelkonzert, jeden Samstag im Sommer).\nLuc Antonini (Montpellier) - \"Von Mozart bis Vierne\"\n\nWerke von Mozart, Mendelssohn, Franck, Vierne\n\nAngeboten von \"Orgues en Bastide Saint-Louis et Barbacane\"."
    ],
    "en": [
      "Les vents d'anges (organ concert, every Saturday during the summer).\nLuc Antonini (Montpellier) - \"From Mozart to Vierne\n\nWorks by Mozart, Mendelssohn, Franck, Vierne\n\nOffered by \"Orgues en Bastide Saint-Louis et Barbacane\"."
    ],
    "it": [
      "Les vents d'anges (concerto d'organo, ogni sabato d'estate).\nLuc Antonini (Montpellier) - \"Da Mozart a Vierne\n\nOpere di Mozart, Mendelssohn, Franck e Vierne\n\nProposto da \"Orgues en Bastide Saint-Louis et Barbacane\"."
    ],
    "fr": [
      "Les vents d'anges (concert d'orgue, chaque samedi de l'été).\nLuc Antonini (Montpellier) - \"De Mozart à Vierne\"\n\nOeuvres de Mozart, Mendelssohn, Franck, Vierne\n\nProposé par \"Orgues en Bastide Saint-Louis et Barbacane\"."
    ],
    "nl": [
      "Les vents d'anges (orgelconcert, elke zaterdag in de zomer).\nLuc Antonini (Montpellier) - \"Van Mozart tot Vierne\"\n\nWerken van Mozart, Mendelssohn, Franck, Vierne\n\nVoorgesteld door \"Orgues en Bastide Saint-Louis et Barbacane\"."
    ],
    "es": [
      "Les vents d'anges (concierto de órgano, todos los sábados de verano).\nLuc Antonini (Montpellier) - \"De Mozart a Vierne\n\nObras de Mozart, Mendelssohn, Franck, Vierne\n\nPropuesto por \"Orgues en Bastide Saint-Louis et Barbacane\"."
