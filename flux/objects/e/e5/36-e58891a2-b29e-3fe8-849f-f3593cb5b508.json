  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/524fe9e2-22be-3ebb-895d-4864171d8447",
      "dc:description": {
        "de": [
          "Am Fuß der Pisten gelegen",
          "Studio 5 Plätze - Residenz Gentianes II\nAusrichtung nach Süden - 5\nWohnzimmer mit Sofa - ausgestattete Küchenecke - Schlafbereich mit 1 Doppelbett - Schlafbereich mit 3 Etagenbetten - Badezimmer - separates WC."
        ],
        "en": [
          "Located at the foot of the slopes",
          "Studio 5 places - Résidence Gentianes II\nSouth facing - 5th floor -\nLiving room with sofa - equipped kitchen area - sleeping area with 1 double bed - sleeping area with 3 bunk beds - bathroom - separate toilet."
        ],
        "it": [
          "Situato ai piedi delle piste",
          "Studio 5 posti - Résidence Gentianes II\nEsposizione a sud - 5° piano -\nSoggiorno con divano - angolo cottura attrezzato - zona notte con 1 letto matrimoniale - zona notte con 3 letti a castello - bagno - WC separato."
        ],
        "fr": [
          "Situé en pied de pistes",
          "Studio 5 places - Résidence Gentianes II\nExposition sud - 5ième Etage -\nSéjour avec canapé - coin cuisine équipé - coin nuit avec 1 lit 2 places - coin nuit avec 3 lits superposés - salle de bain - wc séparé."
        ],
        "nl": [
          "Gelegen aan de voet van de pistes",
          "Studio 5 plaatsen - Résidence Gentianes II\nOp het zuiden - 5e verdieping -\nWoonkamer met bank - ingerichte keukenhoek - slaapgedeelte met 1 tweepersoonsbed - slaapgedeelte met 3 stapelbedden - badkamer - apart toilet."
        ],
        "es": [
          "Estudio para 5 personas - Residencia Gentianes II - Orientación Sur con vista a las pistas - 5 planta - \nSalón con 1 sofá - Cocina completamente equipada - Zona de descanso con 1 cama de matrimonio - Otra zona de descanso con 3 literas - Cuarto de baño - WC separados.",
          "Situado al pie de las pistas."
        ]
      },
