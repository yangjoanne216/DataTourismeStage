  "rdfs:comment": {
    "de": [
      "Landwirtschaftlicher Betrieb mit Enten, die im Freien gehalten werden. Verkauf von Confits und Stopfleber ab Hof das ganze Jahr über.  Verkauf von frischen Produkten von November bis April."
    ],
    "en": [
      "Free-range duck farm. Duck confit and foie gras sold year-round at the farm. Fresh produce sold November – April."
    ],
    "it": [
      "Allevamento di anatre all'aperto. Vendita di confit e foies gras in azienda tutto l'anno.  Vendita di prodotti freschi da novembre ad aprile."
    ],
    "fr": [
      "Exploitation agricole de canards élevés en plein air. Vente à la ferme de confits et foies gras toute l'année.  Vente de produits frais de novembre à avril."
    ],
    "nl": [
      "Het houden van eenden met vrije uitloop. Verkoop van confits en foies gras op de boerderij het hele jaar door.  Verkoop van verse producten van november tot april."
    ],
    "es": [
      "Explotación agrícola de patos criados al aire libre. Venta en el caserío de confits y foies-gras todo el año. Venta de productos frescos de noviembre a abril."
    ]
  },
