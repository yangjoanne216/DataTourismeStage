  "rdfs:comment": {
    "de": [
      "Pizzeria im Stadtzentrum von Le Lion d'Angers. Große Auswahl an Pizzas, Salaten und Tagesmenü. Schöne angrenzende Sonnenterrasse gegenüber der Pferderennbahn."
    ],
    "en": [
      "Pizzeria located in the centre of the Lion d'Angers. Wide choice of pizzas, salad and daily menu. Beautiful adjoining sunny terrace facing the racecourse."
    ],
    "it": [
      "Pizzeria situata nel centro di Le Lion d'Angers. Ampia scelta di pizze, insalate e menù del giorno. Splendida terrazza soleggiata affacciata sull'ippodromo."
    ],
    "fr": [
      "Pizzéria située en centre-ville du Lion d'Angers. Large choix de pizzas, salade et menu du jour. Belle terrasse attenante ensoleillée, face à l'hippodrome."
    ],
    "nl": [
      "Pizzeria gelegen in het centrum van Le Lion d'Angers. Ruime keuze aan pizza's, salades en dagmenu. Prachtig zonnig terras met uitzicht op de renbaan."
    ],
    "es": [
      "Pizzería situada en el centro de Le Lion d'Angers. Amplia oferta de pizzas, ensaladas y menú del día. Preciosa terraza soleada con vistas al hipódromo."
    ]
