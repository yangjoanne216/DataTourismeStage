  "rdfs:comment": {
    "de": [
      "Wohnung von sehr hohem Standard, im Herzen des Dorfes in der Nähe aller Geschäfte gelegen, außergewöhnliche Lage gegenüber dem Gletscher von Argentière, unverbaubarer Blick auf das Massiv des Mont-Blanc."
    ],
    "en": [
      "Apartment of very high standing, located in the heart of the village close to all shops, exceptional location facing the Argentière glacier, breathtaking view of the Massif du Mont-Blanc."
    ],
    "it": [
      "Appartamento di alto livello, situato nel cuore del villaggio vicino a tutti i negozi, posizione eccezionale di fronte al ghiacciaio dell'Argentière, vista libera sul massiccio del Monte Bianco."
    ],
    "fr": [
      "Appartement de très haut standing, situé au cœur du village proche tout commerce, emplacement exceptionnel face au glacier d'Argentière, vue imprenable sur le Massif du Mont -Blanc."
    ],
    "es": [
      "Piso de muy alto nivel, situado en el corazón del pueblo cerca de todos los comercios, ubicación excepcional frente al glaciar de Argentière, vista sin obstáculos del macizo del Mont-Blanc."
    ],
    "nl": [
      "Appartement van zeer hoge kwaliteit, gelegen in het hart van het dorp dicht bij alle winkels, uitzonderlijke ligging tegenover de Argentière gletsjer, vrij uitzicht op het Mont-Blanc massief."
    ]
  },
  "rdfs:label": {
    "en": [
      "Appartment le Savoy"
    ],
    "fr": [
      "\"Le Savoy\" appartement de standing au coeur du village"
    ]
  },
