  "rdfs:comment": {
    "de": [
      "Der Daviel-Palast wurde von den Brüdern Gérard zwischen 1743 und 1747 an der Stelle des alten Gerichtsgebäudes errichtet."
    ],
    "en": [
      "The Daviel palace, the work of the Gérard brothers, was built between 1743 and 1747 on the site of the former courthouse."
    ],
    "it": [
      "Il Palazzo Daviel, progettato dai fratelli Gérard, fu costruito tra il 1743 e il 1747 sul sito dell'antico tribunale."
    ],
    "fr": [
      "Le palais Daviel qui est l'oeuvre des frères Gérard a été construit entre 1743 et 1747 sur l'emplacement de l'ancien palais de justice."
    ],
    "nl": [
      "Het paleis Daviel, ontworpen door de gebroeders Gérard, werd tussen 1743 en 1747 gebouwd op de plaats van het voormalige gerechtsgebouw."
    ],
    "es": [
      "El palacio Daviel, diseñado por los hermanos Gérard, se construyó entre 1743 y 1747 en el emplazamiento del antiguo palacio de justicia."
    ]
  },
