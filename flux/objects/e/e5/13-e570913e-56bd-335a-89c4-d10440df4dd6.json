  "rdfs:comment": {
    "de": [
      "Traditionelles Café-Restaurant in der Ortschaft Larajasse, im Herzen der Monts du Lyonnais. Tagesmenü unter der Woche mit lokalen und saisonalen Produkten. Samstags: Teestube und hausgemachte Backwaren. Poststelle."
    ],
    "en": [
      "Traditional café restaurant located in the village of Larajasse, in the heart of the Monts du Lyonnais. Daily menu during the week, with local and seasonal products. Saturday : tea room and home made pastries. Post office."
    ],
    "it": [
      "Caffè-ristorante tradizionale situato nel villaggio di Larajasse, nel cuore dei Monts du Lyonnais. Menu giornaliero durante la settimana, con prodotti locali e di stagione. Sabato: sala da tè e pasticcini fatti in casa. Ufficio postale."
    ],
    "fr": [
      "Café restaurant traditionnel situé dans le bourg de Larajasse, au coeur des Monts du Lyonnais. Menu du jour en semaine, avec les produits locaux et de saison. Samedi : salon de thé et pâtisseries maison. Point poste."
    ],
    "nl": [
      "Traditioneel café-restaurant gelegen in het dorp Larajasse, in het hart van de Monts du Lyonnais. Dagelijks menu tijdens de week, met lokale en seizoensgebonden producten. Zaterdag: theesalon en zelfgemaakt gebak. Postkantoor."
    ],
    "es": [
      "Café-restaurante tradicional situado en el pueblo de Larajasse, en el corazón de los Montes del Lyón. Menú diario durante la semana, con productos locales y de temporada. Sábado: salón de té y pastas caseras. Oficina de correos."
    ]
  },
