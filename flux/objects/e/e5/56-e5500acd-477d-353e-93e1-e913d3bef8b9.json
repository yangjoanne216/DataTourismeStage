  "rdfs:comment": {
    "de": [
      "Wohnung T2 im 2. Stock eines Hauses und besteht aus : 1 Wohnzimmer (Sofa BZ 140) - 1 separate Küche - 1 Schlafzimmer (Bett 140) - 1 Badezimmer/separates WC. Wifi kostenlos. Am Dorfeingang - Nähe zu allen Geschäften."
    ],
    "en": [
      "Apartment T2 located on the 2nd floor of a house and composed of : 1 living room (sofa BZ 140) - 1 independant kitchen - 1 bedroom (bed 140) - 1 shower room/wc separated. Free Wifi. At the entrance of the village - Close to all shops."
    ],
    "it": [
      "Appartamento T2 situato al 2° piano di una casa e composto da : 1 soggiorno (divano BZ 140) - 1 cucina indipendente - 1 camera da letto (letto 140) - 1 bagno con doccia/wc separato. Wifi gratuito. All'ingresso del paese - Vicino a tutti i negozi."
    ],
    "fr": [
      "Appt T2 situé au 2ème ét. d'une maison et composé de : 1 salon (canapé BZ 140) - 1 cuisine indép. - 1 chambre (lit en 140) - 1 salle d'eau/wc séparés. Wifi gratuite. A l'entrée du village - Proche de tous commerces."
    ],
    "nl": [
      "Appartement T2 gelegen op de 2e verdieping van een huis en bestaande uit : 1 woonkamer (bank BZ 140) - 1 onafhankelijke keuken - 1 slaapkamer (bed 140) - 1 doucheruimte/aparte WC. Gratis Wifi. Bij de ingang van het dorp - Dicht bij alle winkels."
    ],
    "es": [
      "Apartamento T2 situado en la 2ª planta de una casa y compuesto por : 1 sala de estar (sofá BZ 140) - 1 cocina independiente - 1 dormitorio (cama 140) - 1 cuarto de ducha/aseo separado. Wifi gratuito. En la entrada del pueblo - Cerca de todos los comercios."
    ]
  },
