  "rdfs:comment": {
    "de": [
      "Les Chavantures - 16. Ausgabe! \r\nDas Animationsteam des Fremdenverkehrsamts des Tals veranstaltet das Event erneut! Ein Tag für Jugendliche von 8 bis 12 Jahren auf dem Gelände der Chavants in Les Houches."
    ],
    "en": [
      "Les Chavantures - 16th Edition ! \r\nThe animation team of the valley's tourist office repeats the event! A day for young people from 8 to 12 years old on the site of Les Chavants, in Les Houches."
    ],
    "it": [
      "Les Chavantures - 16a edizione! \r\nL'équipe di animazione dell'ufficio turistico della valle ripete l'evento! Una giornata per i giovani dagli 8 ai 12 anni sul sito di Les Chavants, a Les Houches."
    ],
    "fr": [
      "Les Chavantures - 16ème Edition ! \r\nL’équipe d'Animation de l'office de tourisme de la vallée réitère l'événement ! Une journée destinée aux jeunes de 8 à 12 ans sur le site des Chavants, aux Houches."
    ],
    "es": [
      "Les Chavantures - ¡16ª edición! \r\n¡El equipo de animación de la oficina de turismo del valle repite evento! Una jornada para jóvenes de 8 a 12 años en el recinto de Les Chavants, en Les Houches."
    ],
    "nl": [
      "Les Chavantures - 16e editie !\r\nHet animatieteam van het toeristenbureau van de vallei herhaalt het evenement! Een dag voor jongeren van 8 tot 12 jaar op het terrein van Les Chavants, in Les Houches."
    ]
