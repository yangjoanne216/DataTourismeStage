  "rdfs:comment": {
    "de": [
      "Schöne Bergwohnung für 6 Personen, um einen Urlaub an der frischen Luft im Herzen des Arbas-Tals und der Pyrenäen zu verbringen!"
    ],
    "en": [
      "Beautiful mountain apartment, for 6 people, to spend vacations in the open air in the heart of the Arbas Valley and the Pyrenees!"
    ],
    "it": [
      "Bellissimo appartamento di montagna, per 6 persone, per trascorrere vacanze all'aria aperta nel cuore della Valle dell'Arbas e dei Pirenei!"
    ],
    "fr": [
      "Bel appartement de montagne, pour 6 personnes, pour passer des vacances au grand air au cœur de la Vallée de l'Arbas et des Pyrénées !"
    ],
    "es": [
      "¡Precioso piso de montaña, para 6 personas, para pasar vacaciones al aire libre en el corazón del Valle de Arbas y los Pirineos!"
    ],
    "nl": [
      "Prachtige bergflat, voor 6 personen, voor een vakantie in de open lucht in het hart van de Arbasvallei en de Pyreneeën!"
    ]
  },
