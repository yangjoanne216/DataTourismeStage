  "rdfs:comment": {
    "de": [
      "Ein saisonaler Gourmet-Popote, der Ihre Geschmacksknospen zum Glühen bringen wird! Also kommen Sie und essen Sie!"
    ],
    "en": [
      "A gourmet and seasonal meal that will tickle your taste buds! So come and sit down without further delay."
    ],
    "it": [
      "Un pasto gourmet stagionale che solleticherà le vostre papille gustative! Quindi venite a sedervi senza ulteriori indugi."
    ],
    "fr": [
      "Une popote gourmande et saisonnière qui va vous émoustiller les papilles!! Alors venez vous attabler sans plus attendre."
    ],
    "es": [
      "Una comida gourmet de temporada que hará las delicias de su paladar Así que venga y siéntese sin más dilación."
    ],
    "nl": [
      "Een seizoensgebonden gastronomische maaltijd die uw smaakpapillen zal prikkelen! Dus kom en ga zitten zonder verder oponthoud."
    ]
  },
