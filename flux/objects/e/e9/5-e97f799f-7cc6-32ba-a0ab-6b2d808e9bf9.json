  "rdfs:comment": {
    "de": [
      "Die Folklore zieht in die Stadt ein! In traditionelle Kleidung gekleidet, werden Tänzer die Hauptstraße zum Klang eines Akkordeons beleben."
    ],
    "en": [
      "Folklore invites itself in the city! Dressed in traditional clothes, dancers will animate the main street to the sound of an accordion."
    ],
    "it": [
      "Il folclore arriva in città! Ballerini in costumi tradizionali animeranno la via principale al suono di una fisarmonica."
    ],
    "fr": [
      "Le folklore s'invite dans la ville ! Parés de tenues traditionnelles, des danseurs animeront la rue principale au son d'un accordéon."
    ],
    "es": [
      "¡El folclore llega a la ciudad! Bailarines ataviados con trajes tradicionales animarán la calle principal al son de un acordeón."
    ],
    "nl": [
      "Folklore komt naar de stad! Dansers gekleed in traditionele kostuums zullen de hoofdstraat animeren op het geluid van een accordeon."
    ]
  },
