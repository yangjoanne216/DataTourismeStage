  "rdfs:comment": {
    "de": [
      "Öffentlicher Garten in Saint-Gaudens, dieser Platz beherbergt die Fragmente der Galerie (Doppelsäulen mit vegetabilen und hakenförmigen Doppelkapitellen), die aus dem ehemaligen Kreuzgang der Bonnefont-Abtei stammen"
    ],
    "en": [
      "Public garden of Saint-Gaudens, this square shelters the fragments of gallery (geminated columns with double capitals with plants and hooks) coming from the old cloister of the abbey of Bonnefont"
    ],
    "it": [
      "Giardino pubblico di Saint-Gaudens, questa piazza accoglie i frammenti di galleria (colonne geminate con doppi capitelli con piante e ganci) provenienti dall'antico chiostro dell'abbazia di Bonnefont"
    ],
    "fr": [
      "Jardin public de Saint-Gaudens, ce square abrite les fragments de galerie (colonnes géminées avec chapiteaux doubles à végétaux et crochets) provenant de l'ancien cloître de l'abbaye de Bonnefont"
    ],
    "nl": [
      "Openbare tuin van Saint-Gaudens, dit plein herbergt de fragmenten van de galerij (geminated zuilen met dubbele kapitelen met planten en haken) afkomstig van het oude klooster van de abdij van Bonnefont"
    ],
    "es": [
      "Jardín público de Saint-Gaudens, esta plaza alberga los fragmentos de galería (columnas geminadas con capiteles dobles con plantas y ganchos) procedentes del antiguo claustro de la abadía de Bonnefont"
    ]
  },
