  "rdfs:comment": {
    "de": [
      "Locavore-Restaurant an einem außergewöhnlichen touristischen Ort 1 km vom Stadtzentrum entfernt in Le Partage des Eaux (direkt gegenüber). Einfache Küche mit Produkten aus unserer Region."
    ],
    "en": [
      "Restaurant located on an exceptional tourist site 1 km from the town centre, sharing the waters. Simple cuisine based on products from our region (grilled meat and fish, fried mussels, fried fish, fresh and stuffed pasta...)."
    ],
    "it": [
      "Ristorante Locavore situato in un sito turistico eccezionale a 1 km dal centro città, presso il Partage des Eaux (proprio di fronte). Una cucina semplice basata sui prodotti della nostra regione."
    ],
    "fr": [
      "Restaurant locavore situé sur un site touristique exceptionnel situé à 1 km du centre-ville, au Partage des Eaux (juste en face). Cuisine simple et à base de produits de notre région."
    ],
    "es": [
      "Restaurante locavore situado en un lugar turístico excepcional a 1 km del centro de la ciudad, en Partage des Eaux (justo enfrente). Cocina sencilla basada en productos de nuestra región."
    ],
    "nl": [
      "Locavore restaurant gelegen op een uitzonderlijke toeristische site op 1 km van het stadscentrum, op Partage des Eaux (recht tegenover). Eenvoudige keuken op basis van producten uit onze streek."
    ]
  },
