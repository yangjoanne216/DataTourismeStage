  "rdfs:comment": {
    "de": [
      "Das Mas d'Icard empfängt Sie in Saintes-Maries-de-la-Mer, 8 km von der Kirche Notre-Dame-de-la-Mer entfernt. Es bietet Ihnen außerdem kostenloses WLAN."
    ],
    "en": [
      "Mas d'Icard welcomes you in Saintes-Maries-de-la-Mer, 8 km from the church of Notre-Dame-de-la-Mer. It also offers free Wi-Fi."
    ],
    "it": [
      "Le Mas d'Icard vi accoglie a Saintes-Maries-de-la-Mer, a 8 km dalla chiesa di Notre-Dame-de-la-Mer. Offre inoltre la connessione Wi-Fi gratuita."
    ],
    "fr": [
      "Le Mas d'Icard vous accueille à Saintes-Maries-de-la-Mer, à 8 km de l'église de Notre-Dame-de-la-Mer. Elle vous propose également une connexion Wi-Fi gratuite."
    ],
    "nl": [
      "Le Mas d'Icard verwelkomt u in Saintes-Maries-de-la-Mer, op 8 km van de kerk Notre-Dame-de-la-Mer. Het biedt ook gratis WiFi."
    ],
    "es": [
      "Le Mas d'Icard le da la bienvenida en Saintes-Maries-de-la-Mer, a 8 km de la iglesia de Notre-Dame-de-la-Mer. También ofrece acceso gratuito a Wi-Fi."
    ]
  },
