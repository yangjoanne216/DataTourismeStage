  "rdfs:comment": {
    "de": [
      "Der Jardin des Arts ist ein Ausstellungsort, der für alle Formen des künstlerischen Ausdrucks offen ist und in dem sowohl professionelle Künstler als auch Amateure zu finden sind."
    ],
    "en": [
      "The Jardin des Arts is an exhibition space open to all forms of artistic expression, where both professional and amateur artists can meet."
    ],
    "it": [
      "Il Jardin des Arts è uno spazio espositivo aperto a tutte le forme di espressione artistica, dove si possono trovare artisti professionisti e non."
    ],
    "fr": [
      "Le Jardin des Arts est un lieu d'exposition ouvert à toutes formes d'expression artistique, où se côtoient des artistes professionnels comme des amateurs."
    ],
    "es": [
      "El Jardin des Arts es un espacio de exposición abierto a todas las formas de expresión artística, en el que se encuentran tanto artistas profesionales como aficionados."
    ],
    "nl": [
      "De Jardin des Arts is een tentoonstellingsruimte die openstaat voor alle vormen van artistieke expressie, waar zowel professionele als amateurkunstenaars te vinden zijn."
    ]
  },
