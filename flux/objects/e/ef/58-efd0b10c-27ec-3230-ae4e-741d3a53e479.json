  "rdfs:comment": {
    "de": [
      "Der Staatsforst von Cadarache befindet sich in Saint Paul les Durance. Sein nördlicher Teil erstreckt sich bis zu den Ufern des Verdon an der Grenze zwischen den Departements Vaucluse und Var."
    ],
    "en": [
      "The Fôret domaniale de Cadarache is located in Saint Paul les Durance. Its northern part extends to the banks of the Verdon, on the border between the Vaucluse and the Var."
    ],
    "it": [
      "La foresta demaniale di Cadarache si trova a Saint Paul les Durance. La sua parte settentrionale si estende fino alle rive del Verdon, al confine tra il Vaucluse e il Var."
    ],
    "fr": [
      "La Fôret domaniale de Cadarache se trouve à Saint Paul les Durance. Sa partie nord s'étend jusqu'aux berges du Verdon, à la frontière entre le Vaucluse et le Var."
    ],
    "nl": [
      "Het staatsbos van Cadarache ligt in Saint Paul les Durance. Het noordelijke deel strekt zich uit tot aan de oevers van de Verdon, op de grens tussen de Vaucluse en de Var."
    ],
    "es": [
      "El bosque estatal de Cadarache se encuentra en Saint Paul les Durance. Su parte norte se extiende hasta las orillas del Verdon, en la frontera entre el Vaucluse y el Var."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Forêt domaniale de Cadarache"
    ]
  },
