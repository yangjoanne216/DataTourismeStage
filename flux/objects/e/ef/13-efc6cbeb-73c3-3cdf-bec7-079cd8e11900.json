  "rdfs:comment": {
    "de": [
      "Wellnessbereich inmitten der Natur, SPA Beheiztes Schwimmbad, Sauna und ungewöhnliche Unterkünfte in Sauve.\r\n\r\nCorine und Jacques haben vor den Toren von Sauve einen Raum inmitten der Natur geschaffen, der dem Wohlbefinden gewidmet ist."
    ],
    "en": [
      "Well-being space in the middle of nature, SPA Heated swimming pool, sauna and unusual lodgings in Sauve.\r\n\r\nCorine and Jacques have created at the gates of Sauve a space dedicated to well-being."
    ],
    "it": [
      "Area benessere nel cuore della natura, SPA Piscina riscaldata, sauna e gîtes insoliti a Sauve.\r\n\r\nCorine e Jacques hanno creato alle porte di Sauve uno spazio naturale dedicato al benessere."
    ],
    "fr": [
      "Espace bien être en pleine nature, SPA Piscine chauffée, sauna et gîtes insolites à Sauve.\r\n\r\nCorine et Jacques ont créé aux portes de Sauve un espace pleine nature dédié au bien-être."
    ],
    "nl": [
      "Wellnessruimte midden in de natuur, SPA Verwarmd zwembad, sauna en bijzondere gîtes in Sauve.\r\n\r\nCorine en Jacques hebben aan de poorten van Sauve een natuurlijke ruimte gewijd aan het welzijn."
    ],
    "es": [
      "Espacio de bienestar en plena naturaleza, SPA Piscina climatizada, sauna y casas rurales insólitas en Sauve.\r\n\r\nCorine y Jacques han creado a las puertas de Sauve un espacio natural dedicado al bienestar."
    ]
  },
