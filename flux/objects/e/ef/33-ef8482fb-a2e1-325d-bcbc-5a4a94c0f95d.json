  "rdfs:comment": {
    "de": [
      "Kleines Hotel-Restaurant in der Nähe des Flusses Garonne in Saint-Martory an der Via Garona (GR861).\nDas Hotel \"Chez Kiki\" stellt Ihnen 6 ausgestattete und ruhige Zimmer zur Verfügung, in denen Sie sich in aller Ruhe erholen können."
    ],
    "en": [
      "Small hotel-restaurant located near the Garonne river in Saint-Martory on the Via Garona (GR861).\nThe Hotel \"Chez Kiki\" offers you 6 equipped and quiet rooms where you can rest in all serenity."
    ],
    "it": [
      "Piccolo hotel-ristorante situato vicino al fiume Garonna a Saint-Martory, sulla Via Garona (GR861).\nL'Hotel \"Chez Kiki\" vi offre 6 camere attrezzate e tranquille dove potrete riposare in tutta serenità."
    ],
    "fr": [
      "Petit hôtel-restaurant implanté près de la Garonne à Saint-Martory sur la Via Garona (GR861).\nL'Hôtel \"Chez Kiki\" met à votre disposition 6 chambres équipées et tranquilles dans lesquelles vous pourrez vous reposer en toute sérénité."
    ],
    "es": [
      "Pequeño hotel-restaurante situado cerca del río Garona, en Saint-Martory, en la Via Garona (GR861).\nEl Hotel \"Chez Kiki\" le ofrece 6 habitaciones equipadas y tranquilas donde podrá descansar en completa serenidad."
    ],
    "nl": [
      "Klein hotel-restaurant gelegen bij de Garonne in Saint-Martory aan de Via Garona (GR861).\nHotel \"Chez Kiki\" biedt u 6 uitgeruste en rustige kamers waar u in alle rust kunt uitrusten."
    ]
  },
