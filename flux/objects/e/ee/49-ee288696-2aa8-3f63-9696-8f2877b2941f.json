  "rdfs:comment": {
    "de": [
      "Bar-Brasserie im Herzen von Saumur. Tagesmenü, hausgemachte Gerichte à la carte... Verschiedene Biere vom Fass und Flaschenbiere, Grillspezialitäten, Salate, vegetarische und vegane Gerichte."
    ],
    "en": [
      "Bar-brewery in the heart of Saumur. Menu of the day, home-made dishes... Various draught and bottled beers, grills, salads, vegetarian and vegan."
    ],
    "it": [
      "Bar-brasserie nel cuore di Saumur. Menu giornaliero, piatti fatti in casa... Varie birre alla spina e in bottiglia, grigliate, insalate, vegetariano e vegano."
    ],
    "fr": [
      "Bar-brasserie au cœur de Saumur. Menu du jour, plats à la carte faits maison... Bières pression et bouteilles variés, grillades, salades, végétarien et végan."
    ],
    "nl": [
      "Bar-brasserie in het hart van Saumur. Dagelijks menu, huisgemaakte gerechten... Diverse bieren van het vat en in flessen, grillades, salades, vegetarisch en veganistisch."
    ],
    "es": [
      "Bar-brasserie en el corazón de Saumur. Menú diario, platos caseros... Varias cervezas de barril y embotelladas, parrilladas, ensaladas, vegetariano y vegano."
    ]
  },
