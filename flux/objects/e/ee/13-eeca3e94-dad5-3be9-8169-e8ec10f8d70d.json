  "rdfs:comment": {
    "de": [
      "Handwerklicher Schmuck in der Gemeinde Mornac-sur-Seudre: \"Inspiriert vom Licht der Insel Oléron kreiere ich Schmuck aus kleinen Kieselsteinen, um mich um Sie zu kümmern\"."
    ],
    "en": [
      "Handcrafted jewelry in Mornac-sur-Seudre: \"Inspired by the light of the island of Oleron, I create jewelry with small stones to take care of you."
    ],
    "it": [
      "Gioielli artigianali a Mornac-sur-Seudre: \"Ispirata dalla luce dell'isola di Oleron, creo gioielli con piccole pietre per prendermi cura di voi\"."
    ],
    "fr": [
      "Bijouterie artisanale sur la commune de Mornac-sur-Seudre : \"Inspirée par la lumière de l'île d'Oléron, je crée des bijoux avec des petits cailloux pour prendre soin de vous\"."
    ],
    "es": [
      "Joyas artesanales en Mornac-sur-Seudre: \"Inspirada por la luz de la isla de Oleron, creo joyas con pequeñas piedras para cuidarte\"."
    ],
    "nl": [
      "Handgemaakte sieraden in Mornac-sur-Seudre: \"Geïnspireerd door het licht van het eiland Oleron, maak ik sieraden met kleine steentjes om je te verzorgen\"."
    ]
  },
