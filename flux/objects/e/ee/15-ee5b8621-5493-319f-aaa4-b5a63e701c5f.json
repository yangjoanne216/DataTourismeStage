  "rdfs:comment": {
    "de": [
      "Machen Sie die Augen weit auf und entdecken Sie die Parks des Loiret mit den Rätselparcours!"
    ],
    "en": [
      "Open your eyes wide and discover the parks of the Loiret with the enigma trails!"
    ],
    "it": [
      "Spalancate gli occhi e scoprite i parchi del Loiret con i sentieri dell'enigma!"
    ],
    "fr": [
      "Ouvrez grand les yeux et partez à la découverte des parcs du Loiret avec les parcours à énigmes !"
    ],
    "nl": [
      "Doe uw ogen wijd open en ontdek de parken van de Loiret met de raadselachtige routes!"
    ],
    "es": [
      "Abra bien los ojos y descubra los parques del Loiret con los senderos del enigma"
    ]
  },
