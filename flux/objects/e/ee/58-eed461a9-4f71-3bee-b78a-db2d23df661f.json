  "rdfs:comment": {
    "de": [
      "Nestou ist eine Familiengeschichte! Das Restaurant liegt nicht weit vom Alten Hafen, dem Strand der Catalans und dem Pharo-Park entfernt und bietet eine Gourmetküche an."
    ],
    "en": [
      "Nestou is a family story! Located not far from the Old Port, the Catalan beach and the Pharo park, it offers a gourmet cuisine."
    ],
    "it": [
      "Nestou è una storia di famiglia! Situato non lontano dal Porto Vecchio, dalla spiaggia catalana e dal parco Pharo, offre una cucina gourmet."
    ],
    "fr": [
      "Nestou, c'est une histoire de famille! Installé non loin du Vieux Port, de la plage des Catalans et du parc du Pharo, il propose une cuisine de gourmet."
    ],
    "nl": [
      "Nestou is een familieverhaal! Het ligt niet ver van de Oude Haven, het Catalaanse strand en het Pharopark en biedt een gastronomische keuken."
    ],
    "es": [
      "Nestou es un negocio familiar. Situado no lejos del Vieux Port, la Plage des Catalans y el Parc du Pharo, ofrece cocina gourmet."
    ]
  },
