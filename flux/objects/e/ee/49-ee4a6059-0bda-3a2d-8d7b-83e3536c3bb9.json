  "rdfs:comment": {
    "de": [
      "Ehemalige königliche Abteikirche Notre Dame de Vaas. Jahrhundert restaurierte Abteikirche (Architektur mit angevinischen Gewölben, die als Plantagenêt bezeichnet werden - Fresken aus dem 14."
    ],
    "en": [
      "Former Royal Abbey of Notre Dame de Vaas. Abbey church restored in the 12th century. (Angevin vault architecture called Plantagenêt - XIVth century frescoes - Recent glass roof and stained glass windows."
    ],
    "it": [
      "Ex abbazia reale di Notre Dame de Vaas. Chiesa abbaziale restaurata nel XII secolo (volta angioina detta Plantageneta - affreschi del XIV secolo - vetri e vetrate recenti."
    ],
    "fr": [
      "Ancienne abbatiale Royale Notre Dame de Vaas. Eglise abbatiale restaurée au XIIème siècle. (Architecture voûtes angevines dites Plantagenêt - Fresques XIVème - Verrière et vitraux récents."
    ],
    "nl": [
      "Voormalige Koninklijke Abdij van Notre Dame de Vaas. Abdijkerk gerestaureerd in de 12e eeuw (Angevin gewelf bekend als Plantagenet - 14e eeuwse fresco's - Recent glas en gebrandschilderde ramen."
    ],
    "es": [
      "Antigua Abadía Real de Notre Dame de Vaas. Iglesia abacial restaurada en el siglo XII (bóveda angevina conocida como Plantagenet - Frescos del siglo XIV - Cristales y vidrieras recientes."
    ]
  },
