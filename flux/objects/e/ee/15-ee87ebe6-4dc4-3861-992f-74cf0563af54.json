  "rdfs:comment": {
    "de": [
      "Empfang von Gruppen aus dem sportlichen oder sozialpädagogischen Umfeld mit einer Zulassung „Jeunesse et Sports“, Ferienzentrum für Landschulheimaufenthalte."
    ],
    "en": [
      "Open for sports teams and school/educational groups with Jeunesse et Sports status. Holiday centres and residential courses."
    ],
    "it": [
      "Ospitare gruppi del movimento sportivo e socio-educativo con l'approvazione di Gioventù e Sport. Centri di vacanza anche per le classi trapiantate."
    ],
    "fr": [
      "Accueil de groupes issus du mouvement sportif et socio-éducatif dotés d'un agrément Jeunesse et Sports. Centres de vacances de classes transplantées également."
    ],
    "nl": [
      "Ontvangst van groepen uit de sport- en sociaal-educatieve beweging met goedkeuring van Jeugd en Sport. Ook vakantiecentra voor overgeplaatste klassen."
    ],
    "es": [
      "Acogida de grupos del movimiento deportivo y socioeducativo con la aprobación de Juventud y Deportes. Centros de vacaciones también para las clases trasplantadas."
    ]
  },
