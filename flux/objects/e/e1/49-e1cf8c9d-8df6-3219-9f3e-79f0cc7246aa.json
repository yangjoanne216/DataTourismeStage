  "rdfs:comment": {
    "de": [
      "Aquarellkurs im Espace Merveilles mit Eliane Le Corre. Material wird nicht zur Verfügung gestellt. Möglichkeit, vor Ort zu Mittag zu essen (Kühlschrank und Mikrowelle stehen zur Verfügung). Alle Niveaus, Spezialkurs Porträt."
    ],
    "en": [
      "Watercolor workshop at Espace Merveilles with Eliane Le Corre. Material not provided. Possibility of lunch on site (fridge and microwave available). All levels, special portrait workshop."
    ],
    "it": [
      "Laboratorio di acquerello all'Espace Merveilles con Eliane Le Corre. Materiale non fornito. Possibilità di pranzare in loco (frigorifero e microonde a disposizione). Tutti i livelli, corso speciale di ritratto."
    ],
    "fr": [
      "Stage d'aquarelle à l'espace Merveilles avec Eliane Le Corre. Matériel non fourni. Possibilité de déjeuner sur place (frigo et micro-ondes à disposition). Tous niveaux, stage spécial portrait."
    ],
    "es": [
      "Taller de acuarela en el Espace Merveilles con Eliane Le Corre. Material no suministrado. Posibilidad de comer in situ (nevera y microondas disponibles). Todos los niveles, curso especial de retrato."
    ],
    "nl": [
      "Aquarel workshop in Espace Merveilles met Eliane Le Corre. Niet voorzien materiaal. Mogelijkheid om ter plaatse te lunchen (koelkast en magnetron aanwezig). Alle niveaus, speciale portretcursus."
    ]
  },
