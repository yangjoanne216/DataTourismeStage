  "rdfs:comment": {
    "de": [
      "Angeschlossenes Reitzentrum FFE und Poney-Club.\n\nBegleiteter Ausritt 1 bis 2 Stunden - Wanderung - Einführungskurs und/oder Perfektionierungskurs - Spezialkurs für Junioren (7-14 Jahre) - Kurse - Pferdepension."
    ],
    "en": [
      "Horse riding center labeled FFE and cheval Club de France)\n\nPony Club (label Pony Club de France)\n1 to 2 hours rides - Hiking - initiation lessons and / or development - Special Training Junior (7-14 years)"
    ],
    "it": [
      "Centro ippico affiliato alla FFE e al Pony-Club.\n\nCavalcate accompagnate di 1 o 2 ore - Escursioni su sentiero - Corsi per principianti e/o avanzati - Corsi speciali per ragazzi (7-14 anni) - Lezioni - Pensione per cavalli."
    ],
    "fr": [
      "Centre équestre affilié FFE et Poney-Club.\n\nBalade 1 à 2h accompagnée - Randonnée - Stage initiation et/ou perfectionnement - Stage spécial junior (7-14 ans) - Cours - Pension chevaux."
    ],
    "nl": [
      "Manege aangesloten bij de FFE en Pony-Club.\n\nRitten van 1 tot 2 uur onder begeleiding - Trailritten - Cursussen voor beginners en/of gevorderden - Speciale juniorcursussen (7-14 jaar) - Lessen - Paardenpension."
    ],
    "es": [
      "Centro ecuestre afiliado a la FFE y al Pony-Club.\n\nPaseos acompañados de 1 a 2 horas - Paseos por senderos - Cursos para principiantes y/o avanzados - Cursos especiales para jóvenes (7-14 años) - Clases - Alojamiento de caballos."
    ]
  },
