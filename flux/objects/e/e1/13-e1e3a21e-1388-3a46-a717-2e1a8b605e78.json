  "rdfs:comment": {
    "de": [
      "L'Antidote bietet Ihnen süße und herzhafte Crêpes, Kindermenü, hausgemachte Desserts, Salate und Fleischgerichte. Durchgehender Service je nach Saison. Bar bis 1 Uhr geöffnet."
    ],
    "en": [
      "L'Antidote offers sweet and salty crepes, children's menu, home-made desserts, salad and meat. Continuous service depending on the season. Bar open until 1 a.m."
    ],
    "it": [
      "L'Antidote offre crepes dolci e salate, menù per bambini, dolci fatti in casa, insalata e carne. Servizio continuo a seconda della stagione. Bar aperto fino all'1 di notte."
    ],
    "fr": [
      "L'Antidote vous propose des crêpes salées et sucrées, menu enfant, desserts maison, salade et viande. Service en continu en fonction de la saison. Bar ouvert jusqu'à 1h."
    ],
    "es": [
      "L'Antidote ofrece crepes dulces y salados, menú infantil, postres caseros, ensalada y carne. Servicio continuo según temporada. Bar abierto hasta la 1 de la madrugada."
    ],
    "nl": [
      "L'Antidote biedt zoete en hartige crêpes, kindermenu, huisgemaakte desserts, salade en vlees. Continue service afhankelijk van het seizoen. Bar open tot 1 uur 's nachts."
    ]
  },
