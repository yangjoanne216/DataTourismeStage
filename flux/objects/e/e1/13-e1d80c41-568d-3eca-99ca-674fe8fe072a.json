  "rdfs:comment": {
    "de": [
      "Hundert Jahre alte Weinkellerei, am Fuße des Tuffsteinfelsens gelegen. Verkauf von Wein und fruchtigem und extra nativem Olivenöl. Gruppenverkostung nach Absprache möglich."
    ],
    "en": [
      "Century-old cellar, located at the foot of the tufa rock. Sale of wine and fruity extra virgin olive oil. Group tasting possible by appointment."
    ],
    "it": [
      "Cantina secolare, situata ai piedi della roccia tufacea. Vendita di vino e olio extravergine di oliva e fruttato. Possibilità di degustazioni di gruppo su appuntamento."
    ],
    "fr": [
      "Cave centenaire, située au pied du rocher de tuf. Vente de vin et d'huile d'olive fruitée et extra vierge. Dégustation de groupe possible sur rendez-vous."
    ],
    "nl": [
      "Eeuwenoude kelder, gelegen aan de voet van de tufsteen. Verkoop van wijn en fruit en extra vergine olijfolie. Groepsproeverij mogelijk op afspraak."
    ],
    "es": [
      "Bodega centenaria, situada al pie de la roca de toba. Venta de vino y aceite de oliva afrutado y virgen extra. Posibilidad de realizar catas en grupo con cita previa."
    ]
  },
