  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/327f40ff-880e-3bb4-b4e1-cf64fd962986",
      "dc:description": {
        "de": [
          "Unabhängige charmante Unterkunft mit gewachster Holzverkleidung im elsässischen Stil. Gemütliche, ruhige und kokette Unterkunft, neu, Etagenbetten für Kinder in Alkoven."
        ],
        "en": [
          "Charming independent gîte, with old-fashioned waxed woodwork, in the Alsatian style. Warm, quiet and cosy gîte, new, bunk beds for children in alcoves."
        ],
        "it": [
          "Incantevole gîte indipendente, con mobili in legno cerato d'epoca, in stile alsaziano. Gîte caldo, tranquillo e accogliente, nuovo, con letti a castello per bambini in nicchie."
        ],
        "fr": [
          "Gîte de charme indépendant, avec boiserie cirée a l'ancienne, au style alsacien. Gîte chaleureux calme et coquet, neuf, lits superposés pour enfants en alcôves."
        ],
        "es": [
          "Encantadora casa rural independiente, con carpintería encerada a la antigua, de estilo alsaciano. Casa rural cálida, tranquila y acogedora, nueva, literas para niños en alcobas."
        ],
        "nl": [
          "Charmante onafhankelijke gîte, met ouderwets gewaxt houtwerk, in Elzasser stijl. Warme, rustige en gezellige gîte, nieuw, stapelbedden voor kinderen in nissen."
        ]
      },
