  "rdfs:comment": {
    "de": [
      "Die Produzenten der Region Sud Vendée Littoral öffnen ihre Türen und geben Einblicke in ihren Beruf. Besuchen Sie die Brauerei SMEELE, die von Jochem betrieben wird."
    ],
    "en": [
      "The producers of the South Vendée Littoral open their doors to make discover their trade. Visit the SMEELE brewery, owned by Jochem."
    ],
    "it": [
      "I produttori del Litorale Sud-Vandea aprono le loro porte per farvi scoprire il loro mestiere. Visitate il birrificio SMEELE, gestito da Jochem."
    ],
    "fr": [
      "Les producteurs du Sud Vendée Littoral ouvrent leur porte pour faire découvrir leur métier. Visitez la brasserie SMEELE, tenue par Jochem."
    ],
    "es": [
      "Los productores del litoral de Sud Vendée le abren sus puertas para que descubra su oficio. Visite la cervecería SMEELE, dirigida por Jochem."
    ],
    "nl": [
      "De producenten van Sud Vendée Littoral openen hun deuren om u hun vak te laten ontdekken. Bezoek de SMEELE brouwerij, gerund door Jochem."
    ]
  },
  "rdfs:label": {
