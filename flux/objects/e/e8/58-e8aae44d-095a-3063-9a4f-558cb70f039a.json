  "rdfs:comment": {
    "de": [
      "Am Eingang des Viertels Roquette, mit Blick auf den Place Antonnelle, entdecken Sie Peach, eine Konditorei und Schnellrestaurants."
    ],
    "en": [
      "At the entrance to the Roquette district, overlooking the Place Antonnelle, come and discover Peach, a pastry shop and fast food restaurant."
    ],
    "it": [
      "All'ingresso del quartiere della Roquette, con vista su Place Antonnelle, venite a scoprire Peach, pasticceria e fast food."
    ],
    "fr": [
      "A l'entrée du quartier de la Roquette, donnant sur la place Antonnelle, venez découvrir Peach, pâtisserie et restauration rapide."
    ],
    "nl": [
      "Bij de ingang van de wijk Roquette, met uitzicht op het Antonnelleplein, ontdekt u Peach, een banketbakkerij en fastfoodrestaurant."
    ],
    "es": [
      "A la entrada del barrio de la Roquette, con vistas a la plaza Antonnelle, venga a descubrir Peach, una pastelería y restaurante de comida rápida."
    ]
  },
