  "rdfs:comment": {
    "de": [
      "Via Cordata in der Ardèche: POSBI wird für Sie eine geeignete Route auswählen und Sie führen, damit Sie die wunderschönen Naturlandschaften der Ardèche entdecken können."
    ],
    "en": [
      "Via cordata in Ardèche : POSBI will help you to choose an itinerary that's right for you. Accompanied outings to ensure you discover the superb natural sites in Ardèche."
    ],
    "it": [
      "Via Cordata nell'Ardèche: POSBI può scegliere l'itinerario che fa per voi e guidarvi attraverso i magnifici siti naturali dell'Ardèche."
    ],
    "fr": [
      "Via Cordata en Ardèche : POSBI saura vous choisir un itinéraire adapté et vous guider pour vous faire découvrir de magnifiques sites naturels Ardéchois."
    ],
    "nl": [
      "Via Cordata in de Ardèche: POSBI kiest een route die bij u past en leidt u door de prachtige natuurgebieden van de Ardèche."
    ],
    "es": [
      "Vía Cordata en Ardèche: POSBI puede elegir un itinerario a su medida y guiarle por los magníficos parajes naturales de Ardèche."
    ]
  },
