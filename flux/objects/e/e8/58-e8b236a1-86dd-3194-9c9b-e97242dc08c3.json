  "rdfs:comment": {
    "de": [
      "Treize en vue bietet Ihnen hausgemachte Gerichte aus frischen Zutaten, ein Angebot an belegten Broten für den kleinen und großen Hunger..."
    ],
    "en": [
      "Treize en vue offers you home-made dishes, made from fresh products, a range of sandwiches for small and big hungers..."
    ],
    "it": [
      "Treize en vue vi offre piatti fatti in casa, a base di prodotti freschi, una gamma di panini per piccoli e grandi affamati..."
    ],
    "fr": [
      "Treize en vue vous propose des plats faits maison, élaborés à partir de produits frais, une offre de tartines pour les petites et grandes faims..."
    ],
    "es": [
      "Treize en vue ofrece platos caseros elaborados con productos frescos, y una gama de bocadillos para satisfacer tanto a pequeños como a grandes..."
    ],
    "nl": [
      "Treize en vue biedt u huisgemaakte gerechten op basis van verse producten, een assortiment broodjes voor kleine en grote honger..."
    ]
  },
