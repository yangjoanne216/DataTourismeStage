  "rdfs:comment": {
    "de": [
      "Konzept einer Bistro-Brauerei :\r\n- Burger vor Ort oder zum Mitnehmen\r\n- Tagesgericht\r\n- brett zum Teilen\r\nIm Bistro ist alles hausgemacht."
    ],
    "en": [
      "Bistro-brasserie concept:\r\n- Burgers on the spot or to go\r\n- Dish of the day\r\n- board to share\r\nEverything is homemade at the bistro."
    ],
    "it": [
      "Concetto di bistrot-brasserie:\r\n- Hamburger sul posto o da asporto\r\n- Piatto del giorno\r\n- tavola da condividere\r\nNel bistrot tutto è fatto in casa."
    ],
    "fr": [
      "Concept de bistrot-brasserie :\r\n- Burgers sur place ou à emporter \r\n- Plat du jour\r\n- planche à partager\r\nTout est fait maison au bistrot."
    ],
    "es": [
      "Concepto de bistró-brasserie:\r\n- Hamburguesas al momento o para llevar\r\n- Plato del día\r\n- tabla para compartir\r\nTodo es casero en el bistró."
    ],
    "nl": [
      "Bistro-brasserie concept:\r\n- Burgers ter plaatse of om mee te nemen\r\n- Schotel van de dag\r\n- bord om te delen\r\nAlles is huisgemaakt in de bistro."
    ]
  },
