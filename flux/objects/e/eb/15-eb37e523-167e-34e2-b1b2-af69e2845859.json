  "rdfs:comment": {
    "de": [
      "Gemütliches Restaurant mit rustikaler Einrichtung. Hier werden Sie eine köstliche Zeit bei guten Produkten und Weinen aus der Region verbringen."
    ],
    "en": [
      "A warm and rustic restaurant where you will be charmed by delicious, regional dishes and wines."
    ],
    "it": [
      "Ristorante caldo con arredamento rustico. Potrete trascorrere un momento delizioso con i buoni prodotti e i vini della regione."
    ],
    "fr": [
      "Restaurant chaleureux au décor rustique. Vous passerez un délicieux moment autour des bons produits et des vins de la région."
    ],
    "nl": [
      "Warm restaurant met een rustieke inrichting. U zult een heerlijke tijd beleven met de goede producten en wijnen van de streek."
    ],
