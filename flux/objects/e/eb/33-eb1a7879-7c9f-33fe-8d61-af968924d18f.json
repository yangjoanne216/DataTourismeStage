  "rdfs:comment": {
    "de": [
      "Das Hôtel Les Capitouls Toulouse Centre liegt im Herzen von Toulouse und ist das perfekte Hotel für Geschäfts- und Freizeitreisen."
    ],
    "en": [
      "Located in the heart of Toulouse, the Hotel Les Capitouls Toulouse Centre is the perfect hotel for your business stays but also for your leisure stays."
    ],
    "it": [
      "Situato nel cuore di Tolosa, l'Hotel Les Capitouls Toulouse Centre è l'albergo perfetto per i vostri soggiorni di lavoro e di piacere."
    ],
    "fr": [
      "Situé en plein cœur de Toulouse, l'Hôtel Les Capitouls Toulouse Centre est l'hôtel parfait pour vos séjours d'affaires mais aussi pour vos séjours loisirs."
    ],
    "es": [
      "Situado en el corazón de Toulouse, el Hotel Les Capitouls Toulouse Centre es el hotel perfecto para sus estancias de negocios y de ocio."
    ],
    "nl": [
      "Hotel Les Capitouls Toulouse Centre, gelegen in het hart van Toulouse, is het perfecte hotel voor uw zaken- en vakantieverblijf."
    ]
  },
