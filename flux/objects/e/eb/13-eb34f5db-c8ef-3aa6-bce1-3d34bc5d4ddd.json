  "rdfs:comment": {
    "de": [
      "Anerkannter Gasthof \"Bienvenue à la ferme\" (Willkommen auf dem Bauernhof). Landestypische Menüs auf der Grundlage von Produkten vom Bauernhof. Spezialitäten: Coq au Vin - Brathähnchen - Rasselbande - hausgemachte Wurstwaren"
    ],
    "en": [
      "Farm inn approved \"Bienvenue à la ferme\". Country-style menus based on farm products. Specialities: coq au vin - roast chicken - grated chicken - home-made charcuterie"
    ],
    "it": [
      "Agriturismo approvato \"Bienvenue à la ferme\". Menù in stile country a base di prodotti agricoli. Specialità: coq au vin - pollo arrosto - pollo grattugiato - salumi fatti in casa"
    ],
    "fr": [
      "Ferme auberge agréée \"Bienvenue à la ferme\". Menus campagnards à base de produits fermiers. Spécialités : coq au vin - poulet rôti - râpée - charcuterie maison"
    ],
    "es": [
      "Posada de la granja aprobada \"Bienvenue à la ferme\". Menús de estilo rural basados en productos de la granja. Especialidades: coq au vin - pollo asado - pollo rallado - charcutería casera"
    ],
    "nl": [
      "Boerderijherberg goedgekeurd \"Bienvenue à la ferme\". Landelijke menu's gebaseerd op producten van de boerderij. Specialiteiten: coq au vin - gebraden kip - geraspte kip - huisgemaakte vleeswaren"
    ]
  },
