  "rdfs:comment": {
    "de": [
      "Picknickplatz, Angeln und Spaziergänge um den See erwarten Sie für einen entspannten Tag.\nBaden verboten."
    ],
    "en": [
      "Picnic area, fishing and walks around the lake await you for a relaxing day.\nSwimming prohibited."
    ],
    "it": [
      "Area picnic, pesca e passeggiata intorno al lago vi aspettano per una giornata di relax.\nNon è consentito nuotare."
    ],
    "fr": [
      "Aire de pique-nique, pêche et promenade autour du lac vous attendent pour une journée détente.\nBaignade interdite."
    ],
    "es": [
      "Zona de picnic, pesca y un paseo por el lago te esperan para pasar un día relajado.\nNo está permitido nadar."
    ],
    "nl": [
      "Picknickplaats, vissen en een wandeling rond het meer wachten op u voor een ontspannende dag.\nZwemmen is niet toegestaan."
    ]
  },
