  "rdfs:comment": {
    "de": [
      "Schattige Terrasse von 100m2. Speisesaal für 30 Gedecke. Klimatisierter Speisesaal mit 50 Gedecken. Einfache und regionale Verpflegung. Tagesmenü von Montag bis Freitag außer an Feiertagen. Speisekarte zur Verfügung."
    ],
    "en": [
      "100m2 shady terrace. Dining room with 30 covers. Air-conditioned dining room providing 50 covers. Simple, regional cuisine : set menu of the day from Monday to Friday, except on public holidays. Full menu available."
    ],
    "it": [
      "Terrazza ombreggiata di 100 m2. Ristorante con 30 posti a sedere. Ristorante climatizzato con 50 posti a sedere. Cucina semplice e regionale. Menu giornaliero dal lunedì al venerdì, esclusi i giorni festivi."
    ],
    "fr": [
      "Terrasse ombragé de 100m2. Salle de restaurant de 30 couverts. Salle de restaurant climatisée de 50 couverts. Restauration simple et régionale. menu du jour du lundi au vendredi hors jours fériés. carte à disposition."
    ],
    "es": [
      "Terraza a la sombra de 100m2. Sala de restaurante de 30 plazas. Sala de restaurante climatizada de 50 plazas. Restauración simple y regional. Menú de lunes a viernes, excepto festivos. Carta disponible."
    ],
    "nl": [
      "Schaduwrijk terras van 100m2. Restaurant met 30 zitplaatsen. Restaurant met airconditioning en 50 zitplaatsen. Eenvoudige en regionale keuken. Dagmenu van maandag tot vrijdag, met uitzondering van feestdagen."
    ]
  },
