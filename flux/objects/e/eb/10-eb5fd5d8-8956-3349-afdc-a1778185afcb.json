  "rdfs:comment": {
    "de": [
      "Restaurant mit 60 Sitzplätzen. Menü zu 15.00 ?, bestehend aus Vorspeise vom Buffet, Hauptgericht und Dessert.  Kindermenü von 9 bis 12.80 ? Spezialitäten: Hausmannskost. Bar. Restaurant zugänglich für Personen mit eingeschränkter Mobilität."
    ],
    "en": [
      "Restaurant with 60 seats. Menu at 15.00 ?, including buffet starter, main course and dessert.  Children's menu from 9 to 12.80 ? Specialities : family cooking. Bar. Restaurant accessible to people with reduced mobility."
    ],
    "it": [
      "Ristorante con 60 posti a sedere. Menu a 15.00 ?, comprensivo di antipasto a buffet, piatto principale e dessert.  Menu per bambini dalle 9 alle 12.80 ? Specialità: cucina casalinga. Bar. Ristorante accessibile alle persone con mobilità ridotta."
    ],
    "fr": [
      "Restaurant de 60 couverts. Menu à 15.00 €, comprenant entrée en buffet, plat et dessert.  Menu enfant de 9 à 12.80 €. Spécialités : cuisine familiale. Bar. Restaurant accessible aux personnes à mobilité réduite."
    ],
    "es": [
      "Restaurante con 60 plazas. Menú a 15.00€, incluyendo entrante buffet, plato principal y postre.  Menú infantil de 9 a 12.80 ? Especialidades: cocina casera. Bar. Restaurante accesible para personas con movilidad reducida."
    ],
    "nl": [
      "Restaurant met 60 zitplaatsen. Menu om 15.00 uur, inclusief voorgerecht, hoofdgerecht en dessert.  Kindermenu van 9 tot 12.80 ? Specialiteiten: thuis koken. Bar. Restaurant toegankelijk voor personen met beperkte mobiliteit."
    ]
