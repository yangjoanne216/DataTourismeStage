  "rdfs:comment": {
    "de": [
      "Kleines Bistro - Traditionelle und Gourmetküche - Hausgemacht.\r\nHerzlicher Empfang, Außenterrasse, Kinderspiele, Bar, Übertragung von ASM-Spielen."
    ],
    "en": [
      "Small bistro - Traditional and gourmet cuisine - Homemade.\r\nWarm welcome, outdoor terrace, children's games, bar, broadcast of ASM matches."
    ],
    "it": [
      "Piccolo bistrot - Cucina tradizionale e gourmet - Cucina casalinga. Prodotti locali.\r\nAccoglienza calorosa, terrazza esterna, giochi per bambini, bar, trasmissione delle partite dell'ASM. Distributore di pizza 24 ore su 24, pizza fatta in casa. Menu trasmessi su Facebook."
    ],
    "fr": [
      "Petit bistrot - Cuisine traditionnelle et gourmande - Fait maison. Produits locaux.\r\nAccueil chaleureux, terrasse extérieure, jeux enfants, bar, diffusion de matchs de l'ASM. Distributeur de pizza 24/24, pizza fait maison. Menus diffusés sur Facebook."
    ],
    "es": [
      "Pequeño bistró - Cocina tradicional y gourmet - Casera. Productos locales.\r\nCálida acogida, terraza exterior, juegos para niños, bar, retransmisión de partidos de la ASM. Dispensador de pizza 24 horas, pizza casera. Menús difundidos en Facebook."
    ],
    "nl": [
      "Kleine bistro - Traditionele en gastronomische keuken - Huisgemaakt. Lokale producten.\r\nWarm onthaal, buitenterras, kinderspelen, bar, uitzending van ASM-wedstrijden. 24-uurs pizza-automaat, zelfgemaakte pizza. Menu's uitgezonden op Facebook."
    ]
  },
