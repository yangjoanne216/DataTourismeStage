  "rdfs:comment": {
    "de": [
      "12 (schwarze Farbe) ? Les Praizes / Typ: Enduro\r\nWunderschöne Enduro-Strecke, die im Herzen des Waldes von Montchavin liegt. Technische Passagen, Haarnadeln und \"pif-paf\" nach Belieben werden die Adrenalinjunkies begeistern."
    ],
    "en": [
      "12 (black route) – Les Praizes/Type: Enduro\r\nFabulous Enduro trail through the heart of Montchavin forest. Technical sections, hairpins and “switchbacks” for real thrills."
    ],
    "it": [
      "12 (colore nero) ? Les Praizes / Tipo: Enduro\r\nMagnifico percorso di enduro immerso nel cuore della foresta del Montchavin. Passaggi tecnici, tornanti e \"pif-paf\" a volontà faranno la gioia degli amanti dell'adrenalina."
    ],
    "fr": [
      "12 (couleur noire) – Les Praizes / Type :  Enduro\r\nMagnifique itinéraire enduro niché au cœur de la forêt de Montchavin. Passages techniques, épingles et « pif-paf » à volonté raviront les admirateurs d’adrénaline."
    ],
    "nl": [
      "12 (zwart) - Les Praizes / Type: Enduro\r\nPrachtige enduroroute door de bossen van Montchavin. De technische stukken, haarspeldbochten en zigzaggen van dit parcours zijn ideaal voor liefhebbers van adrenaline."
    ],
    "es": [
      "12 (color negro) ? Les Praizes / Tipo: Enduro\r\nMagnífica ruta de enduro enclavada en el corazón del bosque de Montchavin. Pasajes técnicos, horquillas y \"pif-paf\" a discreción harán las delicias de los admiradores de la adrenalina."
    ]
  },
  "rdfs:label": {
    "en": [
      "Les Praizes mountain bike trail"
    ],
    "fr": [
      "Itinéraire VTT : Les Praizes"
    ],
    "nl": [
      "Mountainbikeroute: Les Praizes"
    ]
  },
