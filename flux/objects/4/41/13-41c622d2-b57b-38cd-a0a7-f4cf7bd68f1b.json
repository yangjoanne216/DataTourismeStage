  "rdfs:comment": {
    "de": [
      "Produzent von Ziegenkäse. Traditionelle Herstellung ausschließlich mit Ziegenvollmilch. Besichtigung des Bauernhofs und Verkauf von Bauernhofprodukten am Morgen oder nach Vereinbarung."
    ],
    "en": [
      "Producer of farm goat cheeses. Traditional farming exclusively with whole goat milk. Sale of cheese every morning or by appointment."
    ],
    "it": [
      "Produttore di formaggio di capra. Produzione tradizionale esclusivamente con latte intero di capra. Visita dell'azienda e vendita di prodotti agricoli al mattino o su appuntamento."
    ],
    "fr": [
      "Producteur de fromages de chèvre. Fabrication traditionnelle exclusivement avec du lait de chèvre entier. Visite de la ferme et vente de produits fermiers les matins ou sur RDV."
    ],
    "es": [
      "Productor de queso de cabra. Elaboración tradicional exclusivamente con leche entera de cabra. Visita de la granja y venta de productos agrícolas por la mañana o con cita previa."
    ],
    "nl": [
      "Producent van geitenkaas. Traditionele productie uitsluitend met volle geitenmelk. Bezoek aan de boerderij en verkoop van boerderijproducten in de ochtend of op afspraak."
    ]
  },
  "rdfs:label": {
    "fr": [
      "La Chèvre et le paysan"
    ]
  },
