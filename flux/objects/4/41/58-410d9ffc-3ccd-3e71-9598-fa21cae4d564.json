  "rdfs:comment": {
    "de": [
      "Im Herzen des Nationalparks Calanques. Ökologisches Gasthaus. Gemeinsam genutzte Küche. Übernachtungen in Schlafsälen. Rustikaler Komfort."
    ],
    "en": [
      "In the heart of the Calanques National Park. Ecological hostel. Shared kitchen. Sleeping in dormitories. Rustic comfort."
    ],
    "it": [
      "Nel cuore del Parco Nazionale delle Calanques. Ostello ecologico. Cucina in comune. Dormire nei dormitori. Comfort rustico."
    ],
    "fr": [
      "Au coeur du Parc national des Calanques. Auberge écologique. Cuisine partagée. Couchages en dortoirs. Confort rustique."
    ],
    "nl": [
      "In het hart van het Nationaal Park Calanques. Ecologische herberg. Gedeelde keuken. Slapen in slaapzalen. Rustiek comfort."
    ],
    "es": [
      "En el corazón del Parque Nacional de las Calanques. Albergue ecológico. Cocina compartida. Dormir en dormitorios. Confort rústico."
    ]
