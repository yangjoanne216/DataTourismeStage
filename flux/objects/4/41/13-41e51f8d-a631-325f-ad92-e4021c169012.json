  "rdfs:comment": {
    "de": [
      "Herstellung von Abondance AOP, Tomme fermière, Reblochon AOP, Raclette, Frischkäse (mit Knoblauch und Kräutern, mit Gartengemüse, mit Pfeffer, mit getrockneten Tomaten... je nach Saison) und Tommes Blanches."
    ],
    "en": [
      "Production of Abondance PDO farmhouse tomme, Reblochon PDO, raclette, fresh cheeses (coated with garlic and herbs, garden vegetables, pepper, dried tomatoes... depending on the season) and white tommes."
    ],
    "it": [
      "Produzione di Abondance AOP, tomme di fattoria, Reblochon AOP, raclette, formaggi freschi (ricoperti di aglio ed erbe, con verdure dell'orto, con pepe, con pomodoro secco... a seconda della stagione) e tomini bianchi."
    ],
    "fr": [
      "Fabrication de l’Abondance AOP de la tomme fermière, du Reblochon AOP, de la raclette,  de  fromages frais (enrobés ail et herbes, aux légumes du jardin, au poivre, à la tomate séchée... selon la saison.) et tommes blanches."
    ],
    "nl": [
      "Productie van Abondance AOP, hoevetomme, Reblochon AOP, raclette, verse kazen (bedekt met knoflook en kruiden, met tuingroenten, met peper, met gedroogde tomaat... afhankelijk van het seizoen) en witte tommes."
    ],
    "es": [
      "Producción de Abondance AOP, tomme de granja, Reblochon AOP, raclette, quesos frescos (con ajo y hierbas, con verduras de la huerta, con pimienta, con tomate seco... según la temporada) y tommes blancos."
    ]
  },
