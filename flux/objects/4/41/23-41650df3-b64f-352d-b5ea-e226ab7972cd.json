  "rdfs:comment": {
    "de": [
      "Wettbewerb für Mulassiers-Pferde und Poitou-Dohlen.\nVormittag: Wettbewerb der Rassen Mulassière und Merens. Wanderung zu den Pferden.\nMittagessen: Muscheln mit Pommes frites.\nNachmittags: Reitanimationen."
    ],
    "en": [
      "Competition of mulassier horses and Poitou stallions.\nMorning : Competition of the mulassier and Merens breeds. Horseback riding.\nLunch : mussels and French fries.\nAfternoon: equestrian animations."
    ],
    "it": [
      "Concorso di cavalli mulassier e tori Poitou.\nMattina: Concorso di cavalli di razza mulassier e Merens. Passeggiata a cavallo.\nPranzo: cozze e patatine.\nPomeriggio: attività equestri."
    ],
    "fr": [
      "Concours de chevaux mulassiers et baudets du Poitou.\nMatin : Concours des races mulassières et Merens. Randonnée équestre. \nDéjeuner : moules frites. \nAprès midi : animations équestres."
    ],
    "nl": [
      "Wedstrijd van mulassier paarden en Poitou stieren.\nOchtend: Wedstrijd van mulassier en Merens rassen. Paardrijden.\nLunch: mosselen en frieten.\nMiddag: ruiteractiviteiten."
    ],
    "es": [
      "Concurso de caballos mulassier y novillos Poitou.\nMañana: Concurso de razas mulassier y Merens. Paseos a caballo.\nAlmuerzo: mejillones y patatas fritas.\nTarde: actividades ecuestres."
    ]
  },
