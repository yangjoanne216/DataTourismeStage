  "rdfs:comment": {
    "de": [
      "Sicherer Rodelhang oberhalb des Dorfes. Zugänglich für alle, Kinder unter Aufsicht der Eltern. Vorbehaltlich ausreichender Schneeverhältnisse."
    ],
    "en": [
      "Safe sledging slope created above the village.  Open to everyone, children must be accompanied by parents/adults. Subject to sufficient snow cover."
    ],
    "it": [
      "Pista di slittino sicura sopra il villaggio, a Les Puginières. Accesso libero, sotto la supervisione dei genitori. A condizione che l'innevamento sia sufficiente."
    ],
    "fr": [
      "Piste de luge aménagée et sécurisée au dessus du village, aux Puginières. En libre accès, sous la surveillance des parents. Sous réserve d'enneigement suffisant."
    ],
    "es": [
      "Pista de trineo segura por encima del pueblo, en Les Puginières. Acceso libre, bajo la vigilancia de los padres. Sujeto a una capa de nieve suficiente."
    ],
    "nl": [
      "Beveiligde rodelbaan boven het dorp, bij Les Puginières. Gratis toegang, onder ouderlijk toezicht. Onder voorbehoud van voldoende sneeuw."
    ]
  },
