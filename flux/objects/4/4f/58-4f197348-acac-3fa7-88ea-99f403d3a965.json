  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/e47b0fee-86e6-361c-bbf5-2adf6b9571ab",
      "dc:description": {
        "de": [
          "Kochecke, Backofen, Mikrowelle, Geschirrspüler, Waschmaschine, Internetzugang, Wifi-Zugang, Wäschetrockner, Fernseher, Wohnzimmer, elektrische Heizung, Dusche, separates WC, 3 Schlafzimmer, 4 Betten/1 Pers., 1 Bett/2 Pers., 1 Kinderbett, Handtücher und Bettwäsche vorhanden. Privater Parkplatz. Garage. Kurtaxe und Mietnebenkosten inbegriffen."
        ],
        "en": [
          "Kitchenette, oven, microwave, dishwasher, washing machine, internet access, wifi access, tumble dryer, television, living room, electric heating, shower, separate WC, 3 bedrooms, 4 single beds, 1 double bed, 1 child's bed, towels and household linen provided. Private parking. Garage. Tourist tax and rental charges included."
        ],
        "it": [
          "Angolo cottura, forno, microonde, lavastoviglie, lavatrice, accesso internet, accesso wifi, asciugatrice, televisione, soggiorno, riscaldamento elettrico, doccia, WC separato, 3 camere da letto, 4 letti singoli, 1 letto matrimoniale, 1 letto per bambini, asciugamani e biancheria per la casa forniti. Parcheggio privato. Garage. Tassa di soggiorno e spese di noleggio incluse."
        ],
        "fr": [
          "Coin cuisine, four, micro-ondes, lave-vaisselle, lave-linge, accès internet, accès wifi, sèche-linge, télévision, séjour, chauffage électrique, douche, WC séparés, 3 chambres, 4 lits/1 pers., 1 lit/2 pers., 1 lit enfant, linge de toilette et de maison fourni. Parking privé. Garage. Taxe de séjour et charges locatives comprises."
        ],
        "nl": [
          "Keukenhoek, oven, magnetron, vaatwasser, wasmachine, internettoegang, wifi, wasdroger, televisie, woonkamer, elektrische verwarming, douche, aparte WC, 3 slaapkamers, 4 eenpersoonsbedden, 1 tweepersoonsbed, 1 kinderbed, handdoeken en huishoudlinnen aanwezig. Privé parkeerplaats. Garage. Toeristenbelasting en huurkosten inbegrepen."
        ],
        "es": [
          "Cocina americana, horno, microondas, lavavajillas, lavadora, acceso a internet, acceso wifi, secadora, televisión, salón, calefacción eléctrica, ducha, WC separado, 3 dormitorios, 4 camas individuales, 1 cama de matrimonio, 1 cama de niño, toallas y ropa de hogar incluidas. Aparcamiento privado. Garaje. Tasa turística y gastos de alquiler incluidos."
        ]
      },
