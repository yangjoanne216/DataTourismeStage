  "rdfs:comment": {
    "de": [
      "Restaurant im Herzen von Rouffach :\n\nvorschläge des Küchenchefs, regionale Gerichte, traditionelle Gerichte, Tagesgericht zum Mittagessen, salzige Flammkuchen, hausgemachte Desserts.\n\nNeues Konzept: Verkauf von kleinen Snacks zum Mitnehmen und Restaurantgerichten."
    ],
    "en": [
      "Restaurant in the heart of Rouffach :\n\nchef's suggestions, regional dishes, traditional dishes, daily specials at lunchtime, savoury tarts, home-made desserts.\n\nNew concept: take-away food and restaurant dishes."
    ],
    "it": [
      "Ristorante nel cuore di Rouffach :\n\nproposte dello chef, piatti regionali, piatti tradizionali, specialità del giorno a pranzo, torte salate, dessert fatti in casa.\n\nNuovo concetto: cibo da asporto e piatti da ristorante."
    ],
    "fr": [
      "Restaurant au cœur de Rouffach :\n\nsuggestions du chef, plats régionaux, plats traditionnels, plat du jour à midi, tartes flambées salées, desserts maison.\n\nNouveau concept : vente à emporter petite restauration et plats restaurant."
    ],
    "es": [
      "Restaurante en el corazón de Rouffach :\n\nsugerencias del chef, platos regionales, platos tradicionales, platos del día al mediodía, tartas saladas, postres caseros.\n\nNuevo concepto: comida para llevar y platos de restaurante."
    ],
    "nl": [
      "Restaurant in het hart van Rouffach :\n\nsuggesties van de chef, streekgerechten, traditionele gerechten, dagschotels tijdens de lunch, hartige taarten, huisgemaakte desserts.\n\nNieuw concept: afhaalmaaltijden en restaurantgerechten."
    ]
  },
