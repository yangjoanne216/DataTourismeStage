  "rdfs:comment": {
    "de": [
      "Im Rahmen des Welttages der Feuchtgebiete bietet das CPIE eine sensorische Animation zur Entdeckung der Landschaft an."
    ],
    "en": [
      "As part of the World Wetlands Day, the CPIE offers sensory animation to discover the landscape."
    ],
    "it": [
      "Nell'ambito della Giornata mondiale delle zone umide, il CPIE propone un'animazione sensoriale alla scoperta del paesaggio."
    ],
    "fr": [
      "Dans le cadre de la journée mondiale des zones humides, le CPIE propose animation sensorielle à la découverte des paysage."
    ],
    "nl": [
      "In het kader van de Wereld Wetlands Dag biedt het CPIE een zintuiglijke animatie om het landschap te ontdekken."
    ],
    "es": [
      "En el marco del Día Mundial de los Humedales, el CPIE propone una animación sensorial para descubrir el paisaje."
    ]
  },
