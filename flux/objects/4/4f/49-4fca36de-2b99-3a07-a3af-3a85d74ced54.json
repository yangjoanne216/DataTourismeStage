  "COVID19SpecialMeasures": {
    "de": [
      "Lieferung in einem Umkreis von 500 m um das Restaurant und Öffnungszeiten von 10:00 bis 13:00 Uhr."
    ],
    "en": [
      "Delivery within a 500 m radius of the restaurant and open from 10am to 1pm."
    ],
    "it": [
      "Consegna entro un raggio di 500 metri dal ristorante e apertura dalle 10.00 alle 13.00."
    ],
    "fr": [
      "Livraison dans un rayon de 500 m autour du restaurant et ouverture de 10h à 13h."
    ],
    "nl": [
      "Levering binnen een straal van 500 meter van het restaurant en geopend van 10.00 tot 13.00 uur."
    ],
    "es": [
      "Entrega en un radio de 500 metros del restaurante y apertura de 10 a 13 horas."
    ]
