  "rdfs:comment": {
    "de": [
      "Das Atelier Aponi bietet Ihnen eine Einführung in den Siebdruck mit Rahmen an, eine uralte Drucktechnik mit mehreren Schritten, die früher verwendet wurde."
    ],
    "en": [
      "Atelier Aponi offers an introduction to screen printing, an ancient multi-step printing technique used in the past."
    ],
    "it": [
      "Il Workshop Aponi offre un'introduzione alla serigrafia, un'antica tecnica di stampa a più fasi utilizzata in passato."
    ],
    "fr": [
      "L'Atelier Aponi propose de vous initier à la sérigraphie au cadre, technique ancestrale d'impression à plusieurs étapes utilisée autrefois."
    ],
    "es": [
      "El Taller Aponi ofrece una introducción a la serigrafía, una antigua técnica de impresión en varias fases utilizada en el pasado."
    ],
    "nl": [
      "De Aponi Workshop biedt een inleiding in zeefdrukken, een oude druktechniek in meerdere fasen die vroeger werd gebruikt."
    ]
  },
