  "rdfs:comment": {
    "de": [
      "Kulturelle Begegnung bei Einbruch der Dunkelheit : \"Ecoute et vois, l'art parle à l'art\" rund um die Werke von Eliane Karakaya und die gestischen Lieder von Genviève Charras."
    ],
    "en": [
      "Cultural meeting at nightfall: \"Ecoute et vois, l'art parle à l'art\" (Listen and see, art speaks to art), featuring works by Eliane Karakaya and gestural songs by Genviève Charras."
    ],
    "it": [
      "Incontro culturale al calar del sole: \"Ecoute et vois, l'art parle à l'art\" (Ascolta e vedi, l'arte parla all'arte), con opere di Eliane Karakaya e canzoni gestuali di Genviève Charras."
    ],
    "fr": [
      "Rencontre culturelle à la tombée de la nuit : \"Ecoute et vois, l'art parle à l'art\" autour des oeuvres d'Eliane Karakaya et les chants gestués de Genviève Charras."
    ],
    "es": [
      "Encuentro cultural al atardecer: \"Ecoute et vois, l'art parle à l'art\" (Escuche y vea, el arte habla al arte), con obras de Eliane Karakaya y canciones gestuales de Genviève Charras."
    ],
    "nl": [
      "Culturele ontmoeting in de schemering: \"Ecoute et vois, l'art parle à l'art\" (Luister en zie, kunst spreekt tot kunst), met werken van Eliane Karakaya en gebarenliederen van Genviève Charras."
    ]
