  "rdfs:comment": {
    "de": [
      "Freistehendes Haus an den Hängen von Entraygues, 2 km vom Zentrum entfernt. Unverbaubarer Blick auf die Stadt, das Schloss und das Tal des Lot."
    ],
    "en": [
      "Detached 80 m² house located on the hillsides of Entraygues, 2 km from the centre. Stunning view of the town, the château and the Lot valley."
    ],
    "it": [
      "Casa indipendente situata sulle colline di Entraygues, a 2 km dal centro. Vista ininterrotta sulla città, sul castello e sulla valle del Lot."
    ],
    "fr": [
      "Maison indépendante située sur les côteaux d'Entraygues, à 2 km du centre. Vue imprenable sur la ville, le château et la vallée du Lot."
    ],
    "nl": [
      "Vrijstaand huis gelegen op de heuvels van Entraygues, op 2 km van het centrum. Ononderbroken uitzicht op de stad, het kasteel en de vallei van de Lot."
    ],
    "es": [
      "Casa independiente situada en las colinas de Entraygues, a 2 km del centro. Vista ininterrumpida de la ciudad, el castillo y el valle del Lot."
    ]
