  "rdfs:comment": {
    "de": [
      "Gourmet-Herbstmarkt, organisiert von den Journées Gourmandes de Belleville. Zahlreiche Produkte für den Gaumen. Getränke und Speisen vor Ort oder zum Mitnehmen. Wir erwarten Sie zahlreich!"
    ],
    "en": [
      "Autumn Gourmet Market organized by the Journées Gourmandes de Belleville. Numerous food products. Refreshment bar and catering on the spot or to take away. We wait for you numerous!!!"
    ],
    "it": [
      "Mercato gastronomico autunnale organizzato dalle Journées Gourmandes de Belleville. Numerosi prodotti alimentari. Ristoro e cibo sul posto o da asporto. Vi aspettiamo!"
    ],
    "fr": [
      "Marché Gourmand d’Automne organisé par les Journées Gourmandes de Belleville. Nombreux produits de bouche. Buvette et restauration sur place ou à emporter. On vous attend nombreux!!!"
    ],
    "es": [
      "Mercado gastronómico de otoño organizado por las Journées Gourmandes de Belleville. Numerosos productos alimentarios. Refrescos y comida in situ o para llevar. ¡Le esperamos!"
    ],
    "nl": [
      "Herfstmarkt voor fijnproevers georganiseerd door de Journées Gourmandes de Belleville. Talrijke voedingsproducten. Versnaperingen en eten ter plaatse of om mee te nemen. Wij wachten op u!"
    ]
  },
