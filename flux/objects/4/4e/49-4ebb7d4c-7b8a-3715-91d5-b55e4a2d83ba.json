  "rdfs:comment": {
    "de": [
      "Das Vigo befindet sich im Herzen des Stadtzentrums von Le Mans und empfängt Sie täglich von Montag bis Samstag."
    ],
    "en": [
      "Located in the heart of Le Mans city centre, Le Vigo welcomes you every day from Monday to Saturday."
    ],
    "it": [
      "Situato nel cuore del centro di Le Mans, Le Vigo vi accoglie tutti i giorni dal lunedì al sabato."
    ],
    "fr": [
      "Situé en plein cœur du centre ville du Mans, le Vigo vous accueille tous les jours du lundi au samedi."
    ],
    "es": [
      "Situado en el centro de la ciudad de Le Mans, Le Vigo le da la bienvenida todos los días de lunes a sábado."
    ],
    "nl": [
      "Gelegen in het hart van het centrum van Le Mans, verwelkomt Le Vigo u elke dag van maandag tot zaterdag."
    ]
