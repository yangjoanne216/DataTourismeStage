  "rdfs:comment": {
    "de": [
      "Kathedrale Saint-Apollinaire, Maison des Têtes, Maison Dupré-Latour, Pendentif... Entdecken Sie die wichtigsten Gebäude des historischen Erbes von Valencia bei Einbruch der Nacht!"
    ],
    "en": [
      "Saint-Apollinaire Cathedral, Maison des Têtes, Maison Dupré-Latour, Pendentif... discover the essential buildings of Valence's historical heritage at nightfall!"
    ],
    "it": [
      "Cattedrale Saint-Apollinaire, Maison des Têtes, Maison Dupré-Latour, Pendentif... scoprite gli edifici essenziali del patrimonio storico di Valence al calar della sera!"
    ],
    "fr": [
      "Cathédrale Saint-Apollinaire, Maison des Têtes, Maison Dupré-Latour, Pendentif... découvrez les édifices essentiels du patrimoine historique de Valence à la tombée de la nuit !"
    ],
    "nl": [
      "Kathedraal Saint-Apollinaire, Maison des Têtes, Maison Dupré-Latour, Pendentif... ontdek de essentiële gebouwen van het historisch erfgoed van Valence bij het vallen van de avond!"
    ],
    "es": [
      "Catedral de Saint-Apollinaire, Maison des Têtes, Maison Dupré-Latour, Pendentif... ¡descubra los edificios esenciales del patrimonio histórico de Valence al caer la noche!"
    ]
  },
  "rdfs:label": {
