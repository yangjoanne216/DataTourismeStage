  "rdfs:comment": {
    "de": [
      "Ace Hotel Valence ist ein neues Hotel mit 58 Zimmern, das ideal an der Ausfahrt Valence Sud gelegen ist. Zahlreiche Familienzimmer. Empfang rund um die Uhr, kostenloser gesicherter Parkplatz, optimierte Schalldämmung."
    ],
    "en": [
      "Ace Hotel Valence is a new hotel with 58 rooms, ideally located at the exit Valence Sud. Many family rooms. Reception 24/24, free secure parking, optimized soundproofing."
    ],
    "it": [
      "L'Ace Hôtel Valence è un nuovo hotel con 58 camere, situato in posizione ideale all'uscita di Valence Sud. Molte camere per famiglie. Reception 24 ore su 24, parcheggio custodito gratuito, insonorizzazione ottimizzata."
    ],
    "fr": [
      "Ace Hôtel Valence est un hôtel neuf de 58 chambres, idéalement situé à la sortie Valence Sud. Nombreuses chambres familiales. Accueil 24/24, parking sécurisé gratuit, insonorisation optimisée."
    ],
    "es": [
      "El Ace Hôtel Valence es un nuevo hotel de 58 habitaciones, idealmente situado en la salida de Valence Sud. Muchas habitaciones familiares. Recepción 24 horas, aparcamiento seguro gratuito, insonorización optimizada."
    ],
    "nl": [
      "Ace Hôtel Valence is een nieuw hotel met 58 kamers, ideaal gelegen bij de afslag Valence Sud. Veel familiekamers. 24-uurs receptie, gratis beveiligde parkeerplaats, geoptimaliseerde geluidsisolatie."
    ]
  },
