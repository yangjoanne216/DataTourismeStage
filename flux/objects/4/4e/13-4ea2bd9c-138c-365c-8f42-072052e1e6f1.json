  "rdfs:comment": {
    "de": [
      "NACHTROUTE\r\nJeden Dienstag und Freitag in der Wintersaison ist die Orgères-Piste von 17:00 bis 21:00 Uhr für Skitourengeher geöffnet."
    ],
    "en": [
      "NIGHT ROUTE\r\nEvery Tuesday and Friday of the winter season, the Orgères piste is open to ski tourers from 5pm to 9pm."
    ],
    "it": [
      "PERCORSO NOTTURNO\r\nOgni martedì e venerdì durante la stagione invernale, la pista di Orgères è aperta agli scialpinisti dalle 17:00 alle 21:00."
    ],
    "fr": [
      "ITINÉRAIRE NOCTURNE\r\nChaque mardi et vendredi de la saison d’hiver, la piste des Orgères est ouverte aux skieurs de randonnée de 17h à 21h."
    ],
    "es": [
      "RUTA NOCTURNA\r\nTodos los martes y viernes de la temporada de invierno, la pista de Orgères está abierta a los esquiadores de turismo de 17 a 21 horas."
    ],
    "nl": [
      "NACHTELIJKE ROUTE\r\nTijdens het winterseizoen is de piste van Orgères elke dinsdag en vrijdag van 17 tot 21 uur geopend voor skitoeristen."
    ]
  },
