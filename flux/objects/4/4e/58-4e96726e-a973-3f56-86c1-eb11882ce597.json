  "rdfs:comment": {
    "de": [
      "Gemütlicher Ort zwei Minuten vom Alten Hafen entfernt, wo man eine moderne und raffinierte Küche genießt."
    ],
    "en": [
      "Cosy place two minutes from the Old Port where you can enjoy a modern and refined cuisine."
    ],
    "it": [
      "Locale accogliente a due minuti dal Porto Vecchio dove si può gustare una cucina moderna e raffinata."
    ],
    "fr": [
      "Lieu cosy à deux minutes du Vieux Port ou l'on déguste une cuisine moderne et raffinée."
    ],
    "es": [
      "Un lugar acogedor a dos minutos del Puerto Viejo, donde podrá disfrutar de una cocina moderna y refinada."
    ],
    "nl": [
      "Gezellige plek op twee minuten van de Oude Haven waar u kunt genieten van een moderne en verfijnde keuken."
    ]
  },
