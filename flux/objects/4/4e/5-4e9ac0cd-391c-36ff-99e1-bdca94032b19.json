      "dc:description": {
        "de": [
          "Schnellrestaurants und Eisdiele. Schattige Terrasse entlang des Flusses Ill mit Wasserfontänen in 10 Metern Höhe. Punktuelle Organisation von Sonderabenden!"
        ],
        "en": [
          "Fast food and ice cream. Shaded terrace along the Ill with water jets at 10 meters. Occasional organization of special evenings!"
        ],
        "it": [
          "Fast food e gelateria. Terrazza ombreggiata lungo l'Ill con getti d'acqua a 10 metri. Organizzazione occasionale di serate speciali!"
        ],
        "fr": [
          "Restauration rapide et glacier. Terrasse ombragée le long de l'Ill avec jets d'eau à 10 mètres. Organisations ponctuelles de soirées spéciales!"
        ],
        "nl": [
          "Fast food en ijssalon. Schaduwrijk terras langs de Ill met waterstralen op 10 meter. Incidentele organisatie van speciale avonden!"
        ],
        "es": [
          "Comida rápida y heladería. Terraza sombreada a lo largo del Ill con chorros de agua a 10 metros. Organización ocasional de veladas especiales"
