  "rdfs:comment": {
    "de": [
      "\"Benjamin\" befindet sich im Stadtzentrum, unweit des Fremdenverkehrsamtes, und bietet Fertiggerichte, Gebäck und Schokolade zum Mitnehmen an."
    ],
    "en": [
      "\"Benjamin\" is located in the city center, not far from the Tourist Office, and offers take-away meals, pastries and chocolates."
    ],
    "it": [
      "\"Benjamin\" si trova nel centro della città, non lontano dall'Ufficio del Turismo, e offre pasti da asporto, dolci e cioccolatini."
    ],
    "fr": [
      "\"Benjamin\" est situé en centre-ville,  non loin de l'Office de Tourisme, propose la vente à emporter de plats cuisinés, pâtisseries et chocolats."
    ],
    "es": [
      "\"Benjamin\" se encuentra en el centro de la ciudad, no lejos de la Oficina de Turismo, y ofrece comidas para llevar, pasteles y chocolates."
    ],
    "nl": [
      "\"Benjamin\" ligt in het stadscentrum, niet ver van het VVV-kantoor, en biedt afhaalmaaltijden, gebak en bonbons."
    ]
  },
