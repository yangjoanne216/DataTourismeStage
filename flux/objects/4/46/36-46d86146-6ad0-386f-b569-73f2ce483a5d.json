      "dc:description": {
        "de": [
          "2 Schlafzimmer, Küchenzeile / Ruhebereich",
          "55 m² große Wohnung in einem renovierten alten Bauernhaus mit Pool und Lounge im Herzen eines exotischen Gartens, absolute Ruhe!"
        ],
        "en": [
          "2 bedrooms, kitchen area / lounge",
          "55 m² apartment in an ancient farmhouse that has been renovated, with swimming pool and relaxing lounge area in an exotic garden, peace-and-quiet guaranteed!"
        ],
        "it": [
          "2 camere da letto, angolo cottura/sala da pranzo",
          "Appartamento di 55 m² in un antico casale ristrutturato con piscina e salone nel cuore di un giardino esotico, calma assoluta!"
        ],
        "fr": [
          "2 chambres, coin cuisine / repos",
          "Appartement de 55 m² dans un corps de ferme ancien rénové avec piscine et son salon de repos au coeur d'un jardin exotique, le calme absolu!"
        ],
        "nl": [
          "2 slaapkamers, kitchenette/ontbijtkamer",
          "Appartement van 55 m² in een gerenoveerde oude boerderij met zwembad en lounge in het hart van een exotische tuin, absolute rust!"
        ],
        "es": [
          "2 dormitorios, rincón de cocina y de reposo",
          "Apartamento de 55 m² situado en una antigua granja renovada con piscina y un salón de descanso exterior rodeado de un exótico jardín: ¡la paz más absoluta!"
        ]
      },
