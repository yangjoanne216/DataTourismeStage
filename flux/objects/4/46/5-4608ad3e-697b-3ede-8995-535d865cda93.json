  "rdfs:comment": {
    "de": [
      "Entdecken Sie in einer freundlichen und entspannten Atmosphäre, während Sie zwischen den Ständen zirkulieren, die Arbeit der über fünfzig anwesenden Aussteller."
    ],
    "en": [
      "Come and discover, in a friendly and relaxed atmosphere, the work of more than fifty exhibitors in a variety of stands."
    ],
    "it": [
      "Venite a scoprire, in un'atmosfera amichevole e rilassata, il lavoro di oltre cinquanta espositori in una varietà di stand."
    ],
    "fr": [
      "Venez découvrir, dans une ambiance conviviale et décontractée, en circulant entre les stands, le travail de plus de cinquante exposants présents."
    ],
    "es": [
      "Venga a descubrir, en un ambiente agradable y distendido, el trabajo de más de cincuenta expositores en diversos stands."
    ],
    "nl": [
      "Kom in een gezellige en ontspannen sfeer het werk van meer dan vijftig exposanten ontdekken in verschillende stands."
    ]
  },
