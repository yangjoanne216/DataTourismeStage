  "rdfs:comment": {
    "de": [
      "6 km von Gérardmer, 3 km vom Zentrum von Xonrupt entfernt, am Ufer des Sees von Longemer. Empfang für Wohnmobile, Snack, Waschsalon, Brotverkauf. Vermietung von Autos. Lebensmittelgeschäft 5 Minuten zu Fuß entfernt."
    ],
    "en": [
      "6 km from Gérardmer, 3 km from the centre of Xonrupt, on the edge of the lake of Longemer. Reception for camper vans, snack bar, launderette, bread depot. Car rental. Grocery shop 5 minutes walk away."
    ],
    "it": [
      "A 6 km da Gérardmer, a 3 km dal centro di Xonrupt, sulle rive del lago Longemer. Reception per camper, snack bar, lavanderia, deposito pane. Noleggio auto. Negozio di alimentari a 5 minuti a piedi."
    ],
    "fr": [
      "A 6 km de Gérardmer, 3 km du centre de Xonrupt, au bord du lac de Longemer. Accueil camping car, snack, laverie, dépôt de pain. Location de voiture. Epicerie à 5 minutes à pied."
    ],
    "es": [
      "A 6 km de Gérardmer, a 3 km del centro de Xonrupt, a orillas del lago Longemer. Recepción de autocaravanas, snack bar, lavandería, depósito de pan. Alquiler de coches. Tienda de comestibles a 5 minutos a pie."
    ],
    "nl": [
      "6 km van Gérardmer, 3 km van het centrum van Xonrupt, aan de oevers van het meer van Longemer. Receptie voor campers, snackbar, wasserette, brooddepot. Autoverhuur. Kruidenierswinkel op 5 minuten loopafstand."
    ]
  },
