  "rdfs:comment": {
    "de": [
      "Vias Evasion lädt Sie dazu ein, mit der Familie und in aller Sicherheit den Canal du Midi zu entdecken, der von der UNESCO zum Weltkulturerbe erklärt wurde."
    ],
    "en": [
      "Vias Evasion invites you to discover the Canal du Midi, a UNESCO World Heritage site, with your family in complete safety."
    ],
    "it": [
      "Vias Evasion vi invita a scoprire il Canal du Midi, classificato dall'Unesco come patrimonio mondiale, in tutta sicurezza."
    ],
    "fr": [
      "Vias Evasion vous invite à découvrir en famille et en toute sécurité le Canal du Midi, classé par l'Unesco au patrimoine mondial de l'humanité."
    ],
    "nl": [
      "Vias Evasion nodigt je uit om het Canal du Midi, dat op de Werelderfgoedlijst van UNESCO staat, met je gezin in alle veiligheid te ontdekken."
    ],
    "es": [
      "Vias Evasion le invita a descubrir el Canal du Midi, clasificado por la Unesco como patrimonio mundial, con total seguridad."
