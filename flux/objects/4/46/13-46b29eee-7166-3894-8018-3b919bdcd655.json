  "rdfs:comment": {
    "de": [
      "Der TAC, Train de l'Andorge en Cévennes, nimmt einen Teil der alten Departement-Eisenbahnstrecke, die Florac mit Ste Cécile d'Andorge verbindet, durch die gewundenen Täler der Cevennen."
    ],
    "en": [
      "The TAC, Train de l'Andorge en Cévennes, takes part of the old departmental railway linking Florac to Ste Cécile d'Andorge, through the winding Cévennes valleys."
    ],
    "it": [
      "Il TAC, Train de l'Andorge en Cévennes, percorre parte della vecchia ferrovia dipartimentale che collega Florac a Ste Cécile d'Andorge, attraverso le tortuose valli delle Cévennes."
    ],
    "fr": [
      "Le TAC, Train de l'Andorge en Cévennes emprunte une partie de l'ancien chemin de fer départemental reliant Florac à Ste Cécile d'Andorge, par les sinueuses vallées cévenoles."
    ],
    "es": [
      "El TAC, Train de l'Andorge en Cévennes, recorre parte del antiguo ferrocarril departamental que unía Florac con Ste Cécile d'Andorge, a través de los sinuosos valles de las Cévennes."
    ],
    "nl": [
      "De TAC, Train de l'Andorge en Cévennes, neemt een deel van de oude departementale spoorweg die Florac met Ste Cécile d'Andorge verbindt, door de kronkelende valleien van de Cévennes."
    ]
  },
  "rdfs:label": {
    "en": [
      "The Train of Andorge in Cevennes"
    ],
    "fr": [
      "Le Train de l'Andorge en Cévennes"
    ],
    "nl": [
      "De Andorge trein in de Cevennen"
    ]
  },
