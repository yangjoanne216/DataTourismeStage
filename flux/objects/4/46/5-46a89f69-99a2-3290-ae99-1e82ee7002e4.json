  "rdfs:comment": {
    "de": [
      "Kleine Bergherberge in ruhiger Lage. Panoramablick auf den Bergkamm. Typischer und gemütlicher Rahmen. Spezialitäten auf der Grundlage von Produkten des Bauernhofs. Unterbringung in Gîtes und Zimmern."
    ],
    "en": [
      "Small mountain inn in a quiet area. Panoramic view on the ridge. Typical and warm setting. Specialties based on farm products. Accommodation in gites and rooms."
    ],
    "it": [
      "Piccola e tranquilla locanda di montagna. Vista panoramica sul crinale. Ambiente tipico e accogliente. Specialità a base di prodotti di fattoria. Alloggio in gîtes e camere."
    ],
    "fr": [
      "Petite auberge de montagne au calme. Vue panoramique sur la crête. Cadre typique et chaleureux. Spécialités à base des produits de la ferme. Hébergement en gîtes et chambres."
    ],
    "es": [
      "Pequeña y tranquila posada de montaña. Vista panorámica de la cresta. Entorno típico y cálido. Especialidades a base de productos de la granja. Alojamiento en casas rurales y habitaciones."
    ],
    "nl": [
      "Kleine, rustige bergherberg. Panoramisch uitzicht op de bergkam. Typisch en warm kader. Specialiteiten op basis van producten van de boerderij. Accommodatie in gîtes en kamers."
    ]
  },
