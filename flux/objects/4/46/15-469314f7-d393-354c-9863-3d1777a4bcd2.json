  "rdfs:comment": {
    "de": [
      "Eric Montigny ist ein bekannter professioneller Kalligraph und Buchmaler, der seine Kunst in Kursen und Praktika in lateinischer Kalligraphie und Buchmalerei weitergibt. Verkauf von Kalligraphiematerial."
    ],
    "en": [
      "A renowned professional calligrapher and illuminator, Eric Montigny dispenses lessons and courses on Latin calligraphy and illuminations."
    ],
    "it": [
      "Rinomato calligrafo professionista e conferenziere, Eric Montigny insegna la sua arte attraverso corsi e workshop di calligrafia e miniatura latina. Vendita di materiale calligrafico."
    ],
    "fr": [
      "Calligraphe enlumineur professionnel réputé et conférencier, Eric Montigny dispense son art au travers cours et stages de calligraphie latine et d'enluminures. Vente de matériel de calligraphie."
    ],
    "nl": [
      "Eric Montigny is een bekende professionele kalligraaf en docent. Hij onderwijst zijn kunst door middel van cursussen en workshops in Latijnse kalligrafie en illuminatie. Verkoop van kalligrafiemateriaal."
    ],
    "es": [
      "Calígrafo profesional de renombre y conferenciante, Eric Montigny enseña su arte a través de cursos y talleres de caligrafía latina e iluminación. Venta de material de caligrafía."
    ]
  },
