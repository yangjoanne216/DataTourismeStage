  "rdfs:comment": {
    "de": [
      "Familienbetrieb, der auf Charolais-Rinder spezialisiert ist. Auch Erzeuger von Linsen und Kichererbsen. Die Produkte werden im Geschäft Champ'être in Longeville-lès-Saint-Avold verkauft."
    ],
    "en": [
      "Family farm specializing in Charolais cattle. Also producer of lentils and chickpeas. Products are on sale at the Champ'être store in Longeville-lès-Saint-Avold."
    ],
    "it": [
      "Azienda agricola a conduzione familiare specializzata in bovini Charolais. Produttore anche di lenticchie e ceci. I prodotti sono in vendita presso il negozio Champ'être di Longeville-lès-Saint-Avold."
    ],
    "fr": [
      "Ferme familiale spécialisée dans la Charolaise. Également producteur de lentilles et pois chiches. Les produits sont en vente au magasin Champ'être de Longeville-lès-Saint-Avold."
    ],
    "nl": [
      "Familiebedrijf gespecialiseerd in Charolais vee. Ook producent van linzen en kikkererwten. De producten zijn te koop in de winkel van Champ'être in Longeville-lès-Saint-Avold."
    ],
    "es": [
      "Explotación familiar especializada en ganado Charolais. También produce lentejas y garbanzos. Los productos están a la venta en la tienda Champ'être de Longeville-lès-Saint-Avold."
    ]
  },
