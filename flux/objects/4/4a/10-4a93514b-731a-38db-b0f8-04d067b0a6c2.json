  "rdfs:comment": {
    "de": [
      "Entdecken Sie den Wochenmarkt ( Obst, Gemüse, Fleisch, Kleidung ...), auf dem Place du Général Leclerc, freitags von 8.00 bis 13.00 Uhr."
    ],
    "en": [
      "Come and discover the weekly market (fruits, vegetables, meat, clothing ...), on the place du Général Leclerc on Fridays from 8:00 am to 1:00 pm."
    ],
    "it": [
      "Venite a scoprire il mercato settimanale (frutta, verdura, carne, abbigliamento, ecc.), in Place du Général Leclerc il venerdì dalle 8.00 alle 13.00."
    ],
    "fr": [
      "Venez découvrir le marché hebdomadaire ( fruits, légumes, viandes, habillement ...), sur la place du Général Leclerc le vendredi de 8h00 à 13h00."
    ],
    "es": [
      "Venga a descubrir el mercado semanal (frutas, verduras, carne, ropa, etc.), en la plaza del General Leclerc, los viernes de 8 a 13 horas."
    ],
    "nl": [
      "Kom en ontdek de wekelijkse markt (fruit, groenten, vlees, kleding, enz.) op het Place du Général Leclerc op vrijdag van 8 tot 13 uur."
    ]
  },
