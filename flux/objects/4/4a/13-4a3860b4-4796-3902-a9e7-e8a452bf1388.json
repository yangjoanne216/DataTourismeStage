  "rdfs:comment": {
    "de": [
      "Traditionelle Küche mit frischen Produkten aus dem Südwesten\r\n- Burger mit Salaten\r\n- Wein aus Gaillac.\r\n- Fronton und Cahors.\r\nTagesgericht am Mittag unter der Woche."
    ],
    "en": [
      "Traditional cuisine with fresh products from the southwest.\r\n- Salad burgers.\r\n- Gaillac wine.\r\n- Fronton and Cahors.\r\nDish of the day for lunch on weekdays."
    ],
    "it": [
      "Cucina tradizionale con prodotti freschi del sud-ovest\r\n- Hamburger e insalate\r\n- Vino di Gaillac.\r\n- Fronton e Cahors.\r\nPiatto del giorno a pranzo durante la settimana."
    ],
    "fr": [
      "Cuisine traditionnelle avec des produits frais du sud-ouest. \r\n- Burgers salades. \r\n- Vin de Gaillac.\r\n- Fronton et Cahors.\r\nPlat du jour le midi en semaine."
    ],
    "nl": [
      "Traditionele keuken met verse producten uit het zuidwesten\r\n- Burgers en salades\r\n- Wijn uit Gaillac.\r\n- Fronton en Cahors.\r\nSchotel van de dag tijdens de lunch tijdens de week."
    ],
    "es": [
      "Cocina tradicional con productos frescos del suroeste\r\n- Hamburguesas y ensaladas\r\n- Vino de Gaillac.\r\n- Fronton y Cahors.\r\nPlato del día a la hora de comer entre semana."
    ]
  },
