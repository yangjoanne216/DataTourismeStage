  "rdfs:comment": {
    "de": [
      "Dorfhaus T3 ebenerdig und besteht aus : 1 Wohnzimmer/Küchenecke - 2 Schlafzimmer (Bett 140)(Betten 140+90) - 1 Badezimmer/WC. Ein kleiner Garten befindet sich in der Nähe des Hauses."
    ],
    "en": [
      "Semi-detached village house T3 on one level and composed of : 1 living room/kitchenette - 2 bedrooms (bed 140)(beds 140+90) - 1 shower room/wc. Presence of a small garden near the house."
    ],
    "it": [
      "Casa bifamiliare su un unico livello, composta da : 1 soggiorno/angolo cottura - 2 camere da letto (letti 140) (letti 140+90) - 1 bagno con doccia/wc. C'è un piccolo giardino vicino alla casa."
    ],
    "fr": [
      "Maison de village mitoyenne T3 de plain pied et composée de : 1 séjour/coin cuisine - 2 chambres (lit 140)(lits 140+90) - 1 salle d'eau/wc. Présence d'un jardinet à proximité de la maison."
    ],
    "nl": [
      "Halfvrijstaand dorpshuis op één niveau, bestaande uit : 1 woonkamer/keuken - 2 slaapkamers (bed 140)(bedden 140+90) - 1 doucheruimte/wc. Er is een kleine tuin bij het huis."
    ],
    "es": [
      "Casa de pueblo adosada de una sola planta, compuesta de : 1 salón/cocina americana - 2 dormitorios (cama 140)(camas 140+90) - 1 cuarto de ducha/wc. Hay un pequeño jardín cerca de la casa."
    ]
