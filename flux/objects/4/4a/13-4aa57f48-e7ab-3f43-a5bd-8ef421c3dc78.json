  "rdfs:comment": {
    "de": [
      "Ein 18-Loch-Golfplatz mit Bäumen, der sehr technisch ist und sich durch erhöhte Grüns auszeichnet, die von den Bunkern gut verteidigt werden. SSS: 68,6 - Slope: 134."
    ],
    "en": [
      "18-hole tree-lined golf course, very technical and characterized by raised greens well defended by bunkers. SSS: 68.6 - Slope: 134."
    ],
    "it": [
      "Un campo da golf a 18 buche alberato, molto tecnico e caratterizzato da green elevati e ben difesi da bunker. SSS: 68,6 - Pendenza: 134."
    ],
    "fr": [
      "Parcours de golf arboré de 18 trous, très technique et caractérisé par des greens surélevés bien défendus par les bunkers. SSS : 68,6 - Slope : 134."
    ],
    "es": [
      "Un campo de golf de 18 hoyos con árboles, muy técnico y caracterizado por greens elevados y bien defendidos por bunkers. SSS: 68,6 - Pendiente: 134."
    ],
    "nl": [
      "Een 18-holes golfbaan met bomen, zeer technisch en gekenmerkt door verhoogde greens die goed worden verdedigd door bunkers. SSS: 68,6 - Helling: 134."
    ]
  },
