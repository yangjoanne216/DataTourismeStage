  "rdfs:comment": {
    "de": [
      "Vom Bach Gats geformte Schlucht zwischen Châtillon-en-Diois und Glandage. Mögliche Wanderungen auf alten Zufahrtsstraßen zu verlassenen Dörfern."
    ],
    "en": [
      "The Gorges des Gâts are made by the stream des Gâts, between Châtillon en Diois and Glandage. Walks are possible on old access trails to abandoned villages."
    ],
    "it": [
      "Gole formate dal torrente Gats, tra Châtillon-en-Diois e Glandage. Possibili passeggiate su vecchie strade di accesso a villaggi abbandonati."
    ],
    "fr": [
      "Gorges formées par le ruisseau des Gats, entre Châtillon-en-Diois et Glandage. Balades possibles sur d'anciennes routes d'accès à des villages abandonnés."
    ],
    "nl": [
      "Gorges gevormd door het beekje Gats, tussen Châtillon-en-Diois en Glandage. Mogelijkheid om te wandelen over de oude toegangswegen naar de verlaten dorpen."
    ],
    "es": [
      "Gargantas formadas por el arroyo Gats, entre Châtillon-en-Diois y Glandage. Posibles paseos por antiguos caminos de acceso a pueblos abandonados."
    ]
  },
