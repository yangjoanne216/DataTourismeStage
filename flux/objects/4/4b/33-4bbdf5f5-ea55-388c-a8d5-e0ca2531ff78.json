  "rdfs:comment": {
    "de": [
      "Willkommen im Éphémère Guinguette, das Sie in einer neuen Sommersaison für Abende empfängt, von denen einer festlicher ist als der andere, im Schein der Lampions!"
    ],
    "en": [
      "Welcome to l'Éphémère Guinguette, where you can enjoy a new summer season of lantern-lit, festive evenings!"
    ],
    "it": [
      "Benvenuti a l'Éphémère Guinguette, dove vi aspetta una nuova stagione estiva di serate festose illuminate da lanterne!"
    ],
    "fr": [
      "Bienvenue à l'Éphémère Guinguette qui vous accueille pour une nouvelle saison estivale pour des soirées plus festives les unes que les autres, à la lueur des lampions !"
    ],
    "nl": [
      "Welkom bij l'Éphémère Guinguette, waar je je kunt verheugen op een nieuw zomerseizoen van lantaarnverlichte, feestelijke avonden!"
    ],
    "es": [
      "Bienvenido a l'Éphémère Guinguette, donde le espera una nueva temporada estival de veladas festivas iluminadas por farolillos"
    ]
  },
