  "COVID19SpecialMeasures": {
    "de": [
      "Dienstag bis Samstag, abends von 18:30-21:00 Uhr. Jede Woche neue Menüs."
    ],
    "en": [
      "Tuesday to Saturday, evenings from 6:30-9pm. New menus every week."
    ],
    "it": [
      "Dal martedì al sabato, la sera dalle 18.30 alle 21.00. Nuovi menu ogni settimana."
    ],
    "fr": [
      "Du mardi au samedi, le soir de 18h30-21h. Nouveaux menus chaque semaine."
    ],
    "nl": [
      "Dinsdag tot zaterdag, 's avonds van 18.30 tot 21.00 uur. Elke week nieuwe menu's."
    ],
    "es": [
      "De martes a sábado, por las tardes, de 18.30 a 21.00 horas. Nuevos menús cada semana."
    ]
  },
