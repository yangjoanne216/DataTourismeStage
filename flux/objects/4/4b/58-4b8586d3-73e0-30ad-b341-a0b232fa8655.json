  "rdfs:comment": {
    "de": [
      "Die Gastfreundschaft eines Ortes und seiner Gastgeber, die durch die Weine aus einem geschichtsträchtigen Weingut verherrlicht wird."
    ],
    "en": [
      "A friendly place with friendly owners, made even more enjoyable thanks to the superb wines produced on this historic estate."
    ],
    "it": [
      "La convivialità di un luogo e dei suoi ospiti, amplificata dai vini di una tenuta ricca di storia."
    ],
    "fr": [
      "La convivialité d'un lieu et de ses hôtes, magnifiée par les vins issus d'un domaine chargé d'histoire."
    ],
    "es": [
      "La convivencia de un lugar y sus huéspedes, magnificada por los vinos de una finca cargada de historia."
    ],
    "nl": [
      "De gezelligheid van een plek en zijn gasten, versterkt door de wijnen van een landgoed met een rijke geschiedenis."
    ]
  },
