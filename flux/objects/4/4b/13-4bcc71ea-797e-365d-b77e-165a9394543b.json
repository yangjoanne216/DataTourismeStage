  "rdfs:comment": {
    "de": [
      "Tauchen Sie auf eigene Faust von Boulari am Mont-Dore aus und entdecken Sie majestätische Tauchspots im tiefen Süden, die noch weitgehend unbekannt sind!"
    ],
    "en": [
      "Dive unaccompanied from Boulari in Le Mont-Dore and explore some of the most exciting and little-known dive sites in the Great South!"
    ],
    "it": [
      "Immergetevi da soli da Boulari a Mont-Dore e scoprite i maestosi e ancora poco conosciuti punti di immersione del Grand Sud!"
    ],
    "fr": [
      "Plongez en autonomie au départ de Boulari au Mont-Dore et partez à la découverte de majestueux spots de plongées du Grand sud, encore très peu connus !"
    ],
    "nl": [
      "Duik op eigen houtje vanuit Boulari in Mont-Dore en ontdek de majestueuze en nog weinig bekende duikspots van de Grand Sud!"
    ],
    "es": [
      "Bucee por su cuenta desde Boulari, en Mont-Dore, y descubra los majestuosos y aún poco conocidos puntos de inmersión del Gran Sur"
    ]
  },
