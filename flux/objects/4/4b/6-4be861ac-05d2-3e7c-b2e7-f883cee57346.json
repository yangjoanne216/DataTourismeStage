  "rdfs:comment": {
    "de": [
      "Gutes Brot, wie man es liebt, köstliches Gebäck und saftige Kuchen in dieser handwerklichen Bäckerei, die nur 100 m vom Platz in Gy entfernt liegt."
    ],
    "en": [
      "Good bread as we like it, delicious pastries, succulent cakes in this artisanal bakery located at 100m from the square of Gy."
    ],
    "it": [
      "Pane buono come piace a noi, dolci deliziosi, torte succulente in questo panificio artigianale situato a 100 m dalla piazza di Gy."
    ],
    "fr": [
      "Du bon pain comme on l'aime, des viennoiseries délicieuses, des gâteaux succulents dans cette boulangerie artisanale située à 100m de la place de Gy."
    ],
    "nl": [
      "Goed brood zoals we dat graag hebben, heerlijk gebak, sappige taarten in deze ambachtelijke bakkerij op 100m van het plein van Gy."
    ],
    "es": [
      "Buen pan como nos gusta, deliciosa bollería, suculentos pasteles en esta panadería artesanal situada a 100m de la plaza de Gy."
    ]
  },
