  "rdfs:comment": {
    "de": [
      "Chalet. 4 Zimmer, davon 1 Schlafzimmer. Kleine Küche. Schlafzimmer 1: 1 Bett für 2 Personen. Mezzanin: 1 Bett für 1 Person. Wohnzimmer: 1 Clic-Clac 2 Personen. Geschlossener Raum für Skier/Fahrräder/Kinderwagen. Bettwäsche wird kostenlos zur Verfügung gestellt. Großer Garten und Picknicktisch. Gesellschaftsspiele."
    ],
    "en": [
      "Chalet. 4 rooms including 1 bedroom. Kitchenette. Room 1 : 1 double bed. Mezzanine : 1 bed 1 person. Living room : 1 sofa bed for 2 persons. Closed ski/bike/stroller room. Sheets provided free of charge. Large garden and picnic table. Board games."
    ],
    "it": [
      "Chalet. 4 camere di cui 1 camera da letto. Angolo cottura. Camera da letto 1: 1 letto matrimoniale. Mezzanino: 1 letto singolo. Soggiorno: 1 divano letto matrimoniale. Locale chiuso per sci/bici/passeggini. Lenzuola fornite gratuitamente. Ampio giardino e tavolo da picnic. Giochi da tavolo."
    ],
    "fr": [
      "Chalet. 4 pièces dont 1 chambre. Kitchenette. Chambre 1 : 1 lit 2 personnes. Mezzanine : 1 lit 1 personne. Séjour : 1 clic-clac 2 personnes. Local à skis/vélos/poussette fermé. Draps fournis gratuitement. Grand jardin et table de pique-nique. Jeux de société."
    ],
    "nl": [
      "Chalet. 4 kamers waarvan 1 slaapkamer. Kitchenette. Slaapkamer 1: 1 tweepersoonsbed. Mezzanine: 1 eenpersoonsbed. Woonkamer: 1 tweepersoons slaapbank. Gesloten ski-/fiets/wandelwagenruimte. Gratis lakens. Grote tuin en picknicktafel. Gezelschapsspellen."
    ],
    "es": [
      "Chalet. 4 habitaciones de las cuales 1 dormitorio. Rincón cocina. Dormitorio 1: 1 cama doble. Entresuelo: 1 cama individual. Salón: 1 sofá cama doble. Cuarto cerrado para esquís/bicicletas/cochecitos. Sábanas gratuitas. Gran jardín y mesa de picnic. Juegos de mesa."
    ]
  },
