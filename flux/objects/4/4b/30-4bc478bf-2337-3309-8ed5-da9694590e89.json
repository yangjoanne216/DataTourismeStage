  "rdfs:comment": {
    "de": [
      "Neu renovierte Doppelhaushälfte. Wohnzimmer 30 m², 1 Schlafzimmer von 20 m² und 1 Schlafzimmer von 16 m², überdachte Terrasse. Strand 12 km, Einkaufsmöglichkeiten 3 km, Reitstall 5 min, Golf 20 min, Ausflug nach Jersey, Mont Saint-Michel 30 min."
    ],
    "en": [
      "Semi-detached house redone. Living room 30 m², 1 bedroom of 20 m² and 1 bedroom of 16 m², covered terrace. Beach at 12 km, shops at 3 km, equestrian center at 5 min, golf at 20 min, excursion to Jersey, Mont Saint-Michel at 30 min."
    ],
    "it": [
      "Casa bifamiliare ristrutturata. Soggiorno 30 m², 1 camera da letto 20 m² e 1 camera da letto 16 m², terrazza coperta. Spiaggia a 12 km, negozi a 3 km, maneggio a 5 min, golf a 20 min, escursione a Jersey, Mont Saint-Michel a 30 min."
    ],
    "fr": [
      "Maison mitoyenne refaite à neuve. Salle de séjour 30 m², 1 chambre de 20 m² et 1 chambre de 16 m², terrasse couverte. Plage à 12 km, commerces à 3 km, centre équestre à 5 min, golf à 20 min, excursion à Jersey, Mont Saint-Michel à 30 min."
    ],
    "nl": [
      "Halfvrijstaande woning gerenoveerd. Woonkamer 30 m², 1 slaapkamer 20 m² en 1 slaapkamer 16 m², overdekt terras. Strand op 12 km, winkels op 3 km, manege op 5 min, golf op 20 min, excursie naar Jersey, Mont Saint-Michel op 30 min."
    ],
    "es": [
      "Casa adosada reformada. Salón de 30 m², 1 dormitorio de 20 m² y 1 dormitorio de 16 m², terraza cubierta. Playa a 12 km, tiendas a 3 km, escuela de equitación a 5 min, golf a 20 min, excursión a Jersey, Mont Saint-Michel a 30 min."
    ]
  },
