  "rdfs:comment": {
    "de": [
      "Möbliertes Studio in unmittelbarer Nähe eines wunderschönen Parks. Verbringen Sie einen angenehmen Aufenthalt im Herzen der Stadt."
    ],
    "en": [
      "Furnished studio located close to a beautiful park. Come and spend a pleasant stay in the heart of the city."
    ],
    "it": [
      "Monolocale arredato situato vicino a un bellissimo parco. Venite a trascorrere un piacevole soggiorno nel cuore della città."
    ],
    "fr": [
      "Studio meublé situé à deux pas d'un magnifique parc. Venez passer un agréable séjour au cœur de la ville."
    ],
    "nl": [
      "Gemeubileerd studio appartement gelegen naast een mooi park. Kom en breng een aangenaam verblijf door in het hart van de stad."
    ],
    "es": [
      "Estudio amueblado situado cerca de un bonito parque. Venga a pasar una agradable estancia en el corazón de la ciudad."
