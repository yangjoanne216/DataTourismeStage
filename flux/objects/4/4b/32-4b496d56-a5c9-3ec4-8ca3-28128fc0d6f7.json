  "rdfs:comment": {
    "de": [
      "Herstellung von handwerklich hergestellten Konfitüren aus Weinkonfitüre und Zitronenkonfitüre auf der Grundlage lokaler Produkte. Verkauf in Geschäften und Direktverkauf auf Anfrage."
    ],
    "en": [
      "Manufacture of artisanal jams of wine confit, lemon confit based on local products. Sale in stores and direct sale on request."
    ],
    "it": [
      "Produzione di marmellate fatte in casa, marmellate di vino e marmellate di limone a base di prodotti locali. Vendita nei negozi e vendita diretta su richiesta."
    ],
    "fr": [
      "Fabrication de confitures artisanales de confit de vin, de citron confit  à base de produits locaux. Vente en boutiques et en vente directe sur demande."
    ],
    "nl": [
      "Productie van zelfgemaakte jam, wijnjam en citroenjam van lokale producten. Verkoop in winkels en rechtstreekse verkoop op verzoek."
    ],
    "es": [
      "Producción de mermeladas caseras, mermeladas de vino y mermeladas de limón elaboradas con productos locales. Venta en tiendas y venta directa bajo pedido."
    ]
  },
