  "rdfs:comment": {
    "de": [
      "Sehr angenehmes Studio von 30 m2 - 1. Stock - hell und lichtdurchflutet. Wohnzimmer mit Küchenzeile (2 Elektrokochplatten, Elektroherd, Mikrowelle, Kühlschrank), 1 Bett 140 cm, Badezimmer, WC, 2 Etagenbetten im Eingangsbereich, Privatparkplatz, große Loggia."
    ],
    "en": [
      "Very pleasant studio of 30 m2 - 1st floor - Clear and bright. Living room with kitchenette (2 elec. plates, elec. oven, microwave oven, fridge) 1 bed in 140, bathroom, WC, 2 bunk beds in the entrance, private parking, large loggia."
    ],
    "it": [
      "Monolocale molto piacevole di 30 m2 - 1° piano - Luminoso. Soggiorno con angolo cottura (2 piastre elettriche, forno elettrico, forno a microonde, frigorifero), 1 letto in 140, bagno, WC, 2 letti a castello all'ingresso, parcheggio privato, ampia loggia."
    ],
    "fr": [
      "Studio très agréable de 30 m2 - 1er étage - Clair et lumineux . Séjour kitchenette (2 plaques elec., four élec., four micro-onde, frigo top) 1 lit en 140, salle de bain, WC, 2 lits superposés dans l'entrée, parking privé, grande loggia."
    ],
    "nl": [
      "Zeer aangename studio van 30 m2 - 1e verdieping - Licht en helder. Woonkamer met kitchenette (2 kookplaten, elektrische oven, magnetron, koelkast), 1 bed in 140, badkamer, WC, 2 stapelbedden in de entree, eigen parkeerplaats, grote loggia."
    ],
    "es": [
      "Estudio muy agradable de 30 m2 - 1ª planta - Luminoso. Salón con cocina americana (2 placas, horno eléctrico, microondas, nevera), 1 cama en 140, cuarto de baño, WC, 2 literas en la entrada, aparcamiento privado, gran logia."
    ]
  },
