  "rdfs:comment": {
    "de": [
      "Ehemaliges Herrenhaus aus dem 18. Jahrhundert, das Charles-François de Ferres gehörte, einem Nachkommen italienischer Glasbläsermeister, die sich Mitte des 15. Jahrhunderts in Goult niederließen."
    ],
    "en": [
      "Former 18th century mansion belonging to Charles-François de Ferres, a descendant of Italian master glassmakers who settled in Goult in the mid 15th century."
    ],
    "it": [
      "Antica dimora del XVIII secolo appartenente a Charles-François de Ferres, discendente dei maestri vetrai italiani che si stabilirono a Goult a metà del XV secolo."
    ],
    "fr": [
      "Ancien hôtel particulier du XVIIIe appartenant à Charles-François de Ferres, descendant de maîtres verriers italiens installés à Goult au milieu du XVe siècle."
    ],
    "nl": [
      "Een voormalig 18e eeuws herenhuis van Charles-François de Ferres, een afstammeling van Italiaanse meester-glasblazers die zich halverwege de 15e eeuw in Goult vestigden."
    ],
    "es": [
      "Antigua mansión del siglo XVIII perteneciente a Charles-François de Ferres, descendiente de los maestros vidrieros italianos que se instalaron en Goult a mediados del siglo XV."
    ]
  },
