  "rdfs:comment": {
    "de": [
      "Familienrestaurant mit traditioneller Küche, die seit vier Generationen serviert wird. Spezialitäten: Landsuppe, Kalbskopf Ravigote, hausgemachte Wurstwaren, Estofinade, gefülltes Huhn. Warme und angenehme Atmosphäre."
    ],
    "en": [
      "Family restaurant of traditional cuisine, served for four generations. Specialities: country soup, ravigote veal head, homemade charcuterie, estofinade, stuffed chicken. Warm and pleasant atmosphere."
    ],
    "it": [
      "Ristorante a conduzione familiare con cucina tradizionale da quattro generazioni. Specialità: zuppa contadina, tête de veau ravigote, salumi di produzione propria, estofinade, pollo ripieno. Atmosfera calda e piacevole."
    ],
    "fr": [
      "Restaurant familial de cuisine traditionnelle, servie depuis quatre générations. Spécialités : soupe de campagne, tête de veau ravigote, charcuterie maison, estofinade, poule farcie. Ambiance chaude et agréable."
    ],
    "es": [
      "Restaurante familiar de cocina tradicional, servido desde hace cuatro generaciones. Especialidades: sopa de campo, cabeza de ternera cebada, embutidos caseros, estofina, gallina rellena. Ambiancia caliente y agradable."
    ],
    "nl": [
      "Familierestaurant waar al vier generaties lang traditionele gerechten worden geserveerd. Specialiteiten: plattelandssoep, tête de veau ravigote, huisgemaakte vleeswaren, estofinade, gevulde kip. Warme en aangename sfeer."
    ]
  },
