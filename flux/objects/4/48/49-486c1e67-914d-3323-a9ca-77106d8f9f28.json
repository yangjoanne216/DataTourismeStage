  "rdfs:comment": {
    "de": [
      "Verbringen Sie in einer Höhlenumgebung einen angenehmen Abend in der Musikbar Le Lucifer, die sich neben dem Restaurant Les Nobles Fouées befindet."
    ],
    "en": [
      "In a troglodyte setting, come spend a pleasant evening at the musical bar Le Lucifer located next to the restaurant Les Nobles Fouées."
    ],
    "it": [
      "In un ambiente troglodita, venite a trascorrere una piacevole serata al bar musicale Le Lucifer, situato accanto al ristorante Les Nobles Fouées."
    ],
    "fr": [
      "Dans un cadre en troglodyte, venez passer une agréable soirée au bar musical Le Lucifer situé à côté du restaurant Les Nobles Fouées."
    ],
    "nl": [
      "Kom in een troglodieten omgeving een aangename avond doorbrengen in de muzikale bar Le Lucifer, gelegen naast het restaurant Les Nobles Fouées."
    ],
    "es": [
      "En un ambiente troglodita, venga a pasar una agradable velada en el bar musical Le Lucifer, situado junto al restaurante Les Nobles Fouées."
    ]
  },
