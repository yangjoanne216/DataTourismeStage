  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/d4bcbcd1-29cc-3565-9c1c-a3fee255279a",
      "dc:description": {
        "de": [
          "Familiäre Atmosphäre. Das Hotel liegt im Herzen des Dorfes Saintes-Maries-de-la-Mer. 500 m vom Meer entfernt.\r\nRestaurant: Spezialitäten aus der Camargue, Meeresfrüchte, frischer Fisch, Stierfleisch..."
        ],
        "en": [
          "Family atmosphere. Hotel located in the heart of the village of Les Saintes-Maries-de-la-Mer. At 500 meters from the sea.\r\nRestaurant: Camargue specialities, seafood, fresh fish, bull meat..."
        ],
        "it": [
          "Atmosfera familiare. Hotel situato nel cuore del villaggio di Saintes-Maries-de-la-Mer. A 500 metri dal mare.\r\nRistorante: specialità della Camargue, frutti di mare, pesce fresco, carne di toro..."
        ],
        "fr": [
          "Ambiance familiale. Hôtel situé au coeur du village des Saintes-Maries-de-la-Mer. A 500 mètres de la mer.\r\nRestaurant : spécialités camarguaises, fruits de mer, poissons frais, viande de taureau..."
        ],
        "nl": [
          "Familiale sfeer. Hotel gelegen in het hart van het dorp Saintes-Maries-de-la-Mer. Op 500 meter van de zee.\r\nRestaurant: specialiteiten uit de Camargue, zeevruchten, verse vis, stierenvlees..."
        ],
        "es": [
          "Ambiente familiar. Hotel situado en el corazón del pueblo de Saintes-Maries-de-la-Mer. A 500 metros del mar.\r\nRestaurante: especialidades de la Camarga, marisco, pescado fresco, carne de toro..."
        ]
      },
