  "rdfs:comment": {
    "de": [
      "Handwerkliche Herstellung von Gegenständen aus Holz und anderen Materialien. Drechseln von Holz. Geleimte Hölzer, Skulpturen, handgefertigte Einzelstücke. Verkauf in der Boutique in L'Angeline. Vorführungen nach Vereinbarung."
    ],
    "en": [
      "Handcrafted objects in wood and other materials. Turning on wood. Glued wood, sculptures, unique handmade pieces. Sale in the store at L'Angeline. Demonstrations by appointment."
    ],
    "it": [
      "Oggetti artigianali in legno e altri materiali. Tornitura del legno. Legno incollato, sculture, pezzi unici fatti a mano. Vendita nel negozio L'Angeline. Dimostrazioni su appuntamento."
    ],
    "fr": [
      "Fabrication artisanale d'objets en bois et autres matériaux. Tournage sur bois. Bois collés, sculptures, pièces uniques faites main. Vente en boutique à L'Angeline. Démonstrations sur rendez-vous."
    ],
    "es": [
      "Objetos artesanales de madera y otros materiales. Torneado de madera. Madera encolada, esculturas, piezas únicas hechas a mano. Venta en la tienda de L'Angeline. Demostraciones con cita previa."
    ],
    "nl": [
      "Handgemaakte objecten van hout en andere materialen. Houtdraaien. Gelijmd hout, beeldhouwwerken, unieke handgemaakte stukken. Verkoop in de winkel van L'Angeline. Demonstraties op afspraak."
    ]
  },
