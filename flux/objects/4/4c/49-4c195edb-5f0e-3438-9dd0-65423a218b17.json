  "rdfs:comment": {
    "de": [
      "Cidre, Poiré und Aperitif vom Bauernhof aus La Géraudais: eine Produktpalette aus traditionellen Obstgärten, die ohne chemische Behandlungen bewirtschaftet werden und über 100 Jahre alt sind."
    ],
    "en": [
      "The cider, perry and aperitif farm of the Géraudais: a range of products from traditional orchards, without chemical treatments, and centenarians."
    ],
    "it": [
      "Sidro, sidro e aperitivo di fattoria di La Géraudais: una gamma di prodotti provenienti da frutteti tradizionali, senza trattamenti chimici e vecchi di secoli."
    ],
    "fr": [
      "Le cidre, poiré et apéritif fermier de la Géraudais : une gamme de produits issue des vergers traditionnels, sans traitements chimiques, et centenaires."
    ],
    "es": [
      "Sidra, perada y aperitivo de granja de La Géraudais: una gama de productos procedentes de huertos tradicionales, sin tratamientos químicos, y centenarios."
    ],
    "nl": [
      "Cider, perencider en boerenaperitief van La Géraudais: een assortiment producten uit traditionele boomgaarden, zonder chemische behandelingen, en eeuwenoud."
    ]
