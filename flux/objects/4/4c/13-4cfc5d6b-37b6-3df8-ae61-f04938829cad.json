  "rdfs:comment": {
    "de": [
      "La Maison des desserts: hausgemachtes Eis, Crêpes, Waffeln - Cocktails - Wein im Glas, Wurst- und Käsebrett - Exotisches Dekor für diesen versteckten Garten im Herzen der Stadt, schattiger Entspannungsbereich, Sofas."
    ],
    "en": [
      "La Maison des Desserts: homemade ice creams, crêpes, waffles - Cocktails - Wine by the glass, charcuterie and cheese board - Exotic decor for this hidden garden in the heart of the city, shaded relaxation area, sofas."
    ],
    "it": [
      "La Casa dei Dolci: gelati fatti in casa, crêpes, waffles? Cocktail - Vino al bicchiere, tagliere di salumi e formaggi - Arredamento esotico per questo giardino nascosto nel cuore della città, zona relax ombreggiata, divani."
    ],
    "fr": [
      "La Maison des desserts : glaces artisanales, crêpes, gaufres – Cocktails - Vin au verre, planche de charcuterie et fromages - Décor exotique pour ce jardin caché au cœur de la ville, espace détente ombragé, canapés."
    ],
    "nl": [
      "La Maison des Desserts: huisgemaakt ijs, crêpes, wafels - Cocktails - Wijn per glas, charcuterie en kaasplankje - Exotisch decor voor deze verborgen tuin in het hart van de stad, schaduwrijke ontspanningsruimte, sofa's."
    ],
    "es": [
      "La Casa de los Postres: helados caseros, crepes, gofres.. Cócteles - Vino por copas, tabla de embutidos y quesos - Decoración exótica para este jardín escondido en el corazón de la ciudad, zona de descanso a la sombra, sofás."
    ]
  },
