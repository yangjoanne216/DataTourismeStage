  "rdfs:comment": {
    "de": [
      "Der Werkstattladen der Chocolaterie de Puyricard befindet sich direkt an der Produktionsstätte in Puyricard, etwa 15 Autominuten vom Zentrum von Aix-en-Provence entfernt."
    ],
    "en": [
      "The Chocolaterie de Puyricard workshop shop is directly located on the manufacturing site in Puyricard, about fifteen minutes by car from the centre of Aix-en-Provence."
    ],
    "it": [
      "Il negozio-laboratorio della Chocolaterie de Puyricard si trova direttamente nel sito di produzione di Puyricard, a circa quindici minuti di auto dal centro di Aix-en-Provence."
    ],
    "fr": [
      "La boutique d'atelier de la Chocolaterie de Puyricard est directement implantée sur le site de fabrication à Puyricard, à une quinzaine de minutes en voiture du centre d'Aix-en-Provence."
    ],
    "es": [
      "La tienda-taller de la Chocolaterie de Puyricard está situada directamente en el lugar de producción de Puyricard, a unos quince minutos en coche del centro de Aix-en-Provence."
    ],
    "nl": [
      "De atelierwinkel van Chocolaterie de Puyricard bevindt zich direct op het productieterrein in Puyricard, op ongeveer een kwartiertje rijden van het centrum van Aix-en-Provence."
    ]
  },
