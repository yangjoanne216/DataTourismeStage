  "rdfs:comment": {
    "de": [
      "Genießen Sie einen angenehmen Spaziergang auf dem See von Pierrefonds und einen atemberaubenden Blick auf das Schloss."
    ],
    "en": [
      "Come and enjoy a pleasant stroll on the Pierrefonds Lake and a breathtaking view of the Castle."
    ],
    "it": [
      "Venite a godervi una piacevole passeggiata sul lago di Pierrefonds e una vista mozzafiato sul castello."
    ],
    "fr": [
      "Venez profiter d'une agréable balade sur le Lac de Pierrefonds et d'une vue imprenable sur le Château."
    ],
    "es": [
      "Venga a disfrutar de un agradable paseo por el lago de Pierrefonds y de una impresionante vista del Castillo."
    ],
    "nl": [
      "Kom genieten van een aangename wandeling op het meer van Pierrefonds en een adembenemend uitzicht op het kasteel."
    ]
  },
