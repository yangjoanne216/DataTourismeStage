  "rdfs:comment": {
    "de": [
      "Der Campingplatz Les Asphodèles, ein Naturgebiet am Col de Portet d'Aspet, umgeben von einem schönen Tannenwald, empfängt Sie für Ihren Urlaub an der frischen Luft auf einem seiner 25 Stellplätze!"
    ],
    "en": [
      "The Camping Les Asphodèles, a natural area at the Col de Portet d'Aspet, surrounded by a beautiful fir wood, welcomes you for your vacations in the open air on one of its 25 pitches!"
    ],
    "it": [
      "Il Camping Les Asphodèles, un'area naturale sul Col de Portet d'Aspet, circondata da una splendida pineta, vi accoglie per le vostre vacanze all'aria aperta in una delle sue 25 piazzole!"
    ],
    "fr": [
      "Le Camping Les Asphodèles, aire naturelle au Col de Portet d'Aspet, entouré d'un joli bois de sapin, vous accueille pour vos vacances au grand air sur un de ses 25 emplacements !"
    ],
    "es": [
      "El camping Les Asphodèles, un espacio natural en el Col de Portet d'Aspet, rodeado de un hermoso pinar, le da la bienvenida para sus vacaciones al aire libre en una de sus 25 parcelas"
    ],
    "nl": [
      "Camping Les Asphodèles, een natuurgebied op de Col de Portet d'Aspet, omgeven door een prachtig dennenbos, verwelkomt u voor uw vakantie in de buitenlucht op een van zijn 25 staanplaatsen!"
    ]
  },
  "rdfs:label": {
