  "rdfs:comment": {
    "de": [
      "6 km von Sarlat entfernt. Ehemaliger restaurierter Bauernhof im Perigordine. Park mit Bäumen und Blumen, viel Charme, Picknickmöglichkeit, Gartenmöbel. Frühstück wird je nach Saison auf der Terrasse serviert. 3 Zimmer verfügbar."
    ],
    "en": [
      "6 km from Sarlat. Old restored Perigord farmhouse. Park with trees and flowers, a lot of charm, possibility of picnic, garden furniture. Breakfasts served on the terrace depending on the season. 3 rooms available."
    ],
    "it": [
      "6 km da Sarlat. Antica casa colonica perigordina ristrutturata. Parco con alberi e fiori, tanto fascino, possibilità di picnic, mobili da giardino. Colazione servita in terrazza a seconda della stagione. 3 camere disponibili."
    ],
    "fr": [
      "A 6 km de Sarlat. Ancienne ferme périgourdine réstaurée. Parc arboré et fleuri, beaucoup de charme, possibilité de pique-nique, salon de jardin. Petits déjeuners servis en terrasse selon saison. 3 chambres disponibles."
    ],
    "es": [
      "A 6 km de Sarlat. Antiguo caserío perigordiano restaurado. Parque con árboles y flores, mucho encanto, posibilidad de picnic, muebles de jardín. El desayuno se sirve en la terraza según la temporada. 3 habitaciones disponibles."
    ],
    "nl": [
      "6 km van Sarlat. Oude gerestaureerde Perigordiaanse boerderij. Park met bomen en bloemen, veel charme, mogelijkheid tot picknick, tuinmeubelen. Ontbijt geserveerd op het terras afhankelijk van het seizoen. 3 kamers beschikbaar."
    ]
  },
