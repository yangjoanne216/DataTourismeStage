  "rdfs:comment": {
    "de": [
      "Pädagogischer Mini-Bauernhof, Eselreiten, Tapetenwechsel inmitten der Natur, 1,5 km vom Zentrum von Torreilles Village entfernt. Getränkestand und Laden mit lokalen und handwerklichen Produkten."
    ],
    "en": [
      "Educational mini-farm, donkey rides, a change of scenery in the middle of nature, 1.5 km from the center of Torreilles village. Refreshment bar and store of local and artisanal products."
    ],
    "it": [
      "Mini-fattoria didattica, passeggiate con gli asini, un cambio di scenario in mezzo alla natura, a 1,5 km dal centro del villaggio di Torreilles. Bar e negozio di prodotti locali e artigianali."
    ],
    "fr": [
      "Mini-ferme pédagogique, balades à dos d'ânes, dépaysement en pleine nature à 1,5km du centre de Torreilles village. Buvette et boutique de produits locaux et artisanaux."
    ],
    "es": [
      "Minigranja educativa, paseos en burro, un cambio de aires en plena naturaleza, a 1,5 km del centro del pueblo de Torreilles. Bar de refrescos y tienda de productos locales y artesanales."
    ],
    "nl": [
      "Educatieve mini-boerderij, ezeltochten, een verandering van omgeving midden in de natuur, op 1,5 km van het centrum van Torreilles. Bar en winkel met lokale en ambachtelijke producten."
    ]
  },
