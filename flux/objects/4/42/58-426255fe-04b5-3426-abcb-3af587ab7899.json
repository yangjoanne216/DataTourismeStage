  "rdfs:comment": {
    "de": [
      "Italienische Spezialitäten mit hausgemachten Nudeln, Broten und Desserts, frischen oder importierten Produkten aus Italien. Eine Reservierung wird empfohlen. Restaurant mit dem Gütesiegel \"Ospitalita Italianna\"."
    ],
    "en": [
      "Italian specialities with homemade pasta, breads and desserts, fresh products or imported from Italy. Reservation recommended. Restaurant labelled Ospitalita Italianna."
    ],
    "it": [
      "Specialità italiane con pasta fatta in casa, pane e dolci, prodotti freschi o importati dall'Italia. Si consiglia la prenotazione. Ristorante con marchio Ospitalita Italiana."
    ],
    "fr": [
      "Spécialités italiennes avec des pâtes, pains est desserts faits maison, des produits frais ou importés d'Italie. Réservation recommandée. Restaurant labellisé Ospitalita Italianna."
    ],
    "nl": [
      "Italiaanse specialiteiten met zelfgemaakte pasta, brood en desserts, verse producten of geïmporteerd uit Italië. Reservering aanbevolen. Restaurant met het label Ospitalita Italianna."
    ],
    "es": [
      "Especialidades italianas con pasta, panes y postres caseros, productos frescos o importados de Italia. Se recomienda reservar. Restaurante con la etiqueta Ospitalita Italianna."
    ]
  },
