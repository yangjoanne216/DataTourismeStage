  "rdfs:comment": {
    "de": [
      "In Noidans-le-Ferroux können Sie unter der Woche mittags oder am Wochenende abends einen Zwischenstopp einlegen, um etwas zu essen und zu trinken."
    ],
    "en": [
      "In Noidans-le-Ferroux, take advantage of a lunch stop during the week or an evening stop at the weekend to have a meal."
    ],
    "it": [
      "A Noidans-le-Ferroux, godetevi una sosta per il pranzo durante la settimana o una sosta serale nel fine settimana."
    ],
    "fr": [
      "A Noidans-le-Ferroux, profitez d'une halte le midi en semaine ou le soir le week-end pour vous restaurer."
    ],
    "nl": [
      "Profiteer in Noidans-le-Ferroux van een lunchstop tijdens de week of een avondstop in het weekend voor een maaltijd."
    ],
    "es": [
      "En Noidans-le-Ferroux, disfrute de una parada para comer durante la semana o una parada nocturna el fin de semana."
    ]
