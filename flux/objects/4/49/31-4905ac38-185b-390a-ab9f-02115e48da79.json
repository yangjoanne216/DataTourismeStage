  "rdfs:comment": {
    "de": [
      "Schatzsuche, Orientierungslauf, Rätselrallye oder einfache Einführung in das Wandern - Evasion bietet Ihnen verschiedene Möglichkeiten, die Orientierung zu entdecken."
    ],
    "en": [
      "Treasure hunt, trails and orientation course, enigma course or simple beginners' walks, Evasion offers diverse packages for discovering orientation."
    ],
    "it": [
      "Cacce al tesoro, percorsi e gare di orienteering, gare di enigmistica o semplicemente un'introduzione all'escursionismo, Evasion vi offre diverse opzioni per scoprire l'orienteering."
    ],
    "fr": [
      "Chasse au trésor, parcours et course d'orientation, course à énigmes, ou simple initiation à la randonnée, Evasion vous propose diverses formules pour découvrir l'orientation."
    ],
    "nl": [
      "Schattenjachten, oriëntatielopen en -wedstrijden, enigmawedstrijden, of gewoon een kennismaking met wandelen, Evasion biedt verschillende formules om oriëntatielopen te ontdekken."
    ],
    "es": [
      "Búsqueda del tesoro, recorridos y carreras de orientación, carreras de enigma, o simplemente una introducción al senderismo, Evasión le ofrece varias opciones para descubrir la orientación."
    ]
  },
