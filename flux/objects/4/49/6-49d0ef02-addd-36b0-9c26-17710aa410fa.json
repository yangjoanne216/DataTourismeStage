  "rdfs:comment": {
    "de": [
      "Im Herzen des Seenlandes im Jura, auf 2 Hektar Wald, können Sie mit Ihrer Familie auf Entdeckungsreise gehen und rund 30 Holzspiele ausprobieren!"
    ],
    "en": [
      "In the heart of the Pays des Lacs, in the Jura, in 2 hectares of forest, discover with your family about thirty wooden games!"
    ],
    "it": [
      "Nel cuore del Pays des Lacs, nel Giura, in 2 ettari di foresta, scoprite con la vostra famiglia una trentina di giochi in legno!"
    ],
    "fr": [
      "Au cœur du Pays des Lacs, dans le Jura, sur 2 hectares de forêt, partez à la découverte en famille d'une trentaine de jeux en bois !"
    ],
    "nl": [
      "In het hart van het Pays des Lacs, in de Jura, in 2 hectare bos, ontdekt u met uw gezin een dertigtal houten spelen!"
    ],
    "es": [
      "En el corazón del Pays des Lacs, en el Jura, en 2 hectáreas de bosque, ¡descubra en familia una treintena de juegos de madera!"
    ]
  },
