  "rdfs:comment": {
    "de": [
      "Abendmahl aus Keramik, das nach einem Werk von Leonardo da Vinci geschnitzt wurde. Sie befindet sich auf dem alten befestigten Friedhof (Oelberg)."
    ],
    "en": [
      "Ceramic Holy Communion, carved after the work of Leonardo da Vinci. It is located in the old fortified cemetery (Oelberg)."
    ],
    "it": [
      "Santa Comunione in ceramica, scolpita secondo l'opera di Leonardo da Vinci. Si trova nell'ex cimitero fortificato (Oelberg)."
    ],
    "fr": [
      "Sainte-Cène en céramique, sculptée d'après l'oeuvre de Léonard de Vinci. Elle est située à l'ancien cimetière fortifié (Oelberg)."
    ],
    "es": [
      "Santa Comunión de cerámica, tallada según la obra de Leonardo da Vinci. Se encuentra en el antiguo cementerio fortificado (Oelberg)."
    ],
    "nl": [
      "Keramisch Heilig Avondmaal, gesneden naar het werk van Leonardo da Vinci. Het is gelegen op de voormalige versterkte begraafplaats (Oelberg)."
    ]
