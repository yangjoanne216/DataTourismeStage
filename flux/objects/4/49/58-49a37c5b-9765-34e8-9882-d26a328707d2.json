  "rdfs:comment": {
    "de": [
      "Fly-Provence befindet sich in Aix-en-Provence, am Fuße des Berges Sainte-Victoire und in der Nähe der Calanques. Fly-Provence ist eine Flugschule mit menschlicher Dimension."
    ],
    "en": [
      "Fly-Provence is located in Aix-en-Provence, at the foot of the Sainte-Victoire and near the Calanques. Fly-Provence is a flight school on a human dimension."
    ],
    "it": [
      "Fly-Provence si trova ad Aix-en-Provence, ai piedi della montagna Sainte-Victoire e vicino alle Calanques. Fly-Provence è una scuola di volo a dimensione umana."
    ],
    "fr": [
      "Fly-Provence est située à Aix-en-Provence, au pied de la Sainte-Victoire et à proximité des Calanques. Fly-Provence est une école de pilotage à dimension humaine."
    ],
    "es": [
      "Fly-Provence se encuentra en Aix-en-Provence, a los pies de la montaña Sainte-Victoire y cerca de las Calanques. Fly-Provence es una escuela de vuelo con dimensión humana."
    ],
    "nl": [
      "Fly-Provence ligt in Aix-en-Provence, aan de voet van de berg Sainte-Victoire en vlakbij de Calanques. Fly-Provence is een vliegschool met een menselijke dimensie."
    ]
  },
