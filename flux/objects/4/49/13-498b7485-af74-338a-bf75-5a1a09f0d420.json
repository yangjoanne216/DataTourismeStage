  "rdfs:comment": {
    "de": [
      "Donnerstag, 13. Juli, ab 20.00 Uhr, traditionelles Picknick aus dem Rucksack jedes Einzelnen an den Quais de l'Aumance in Hérisson.\r\nNach dem Picknick folgt der Tanz in der Festhalle.\r\nKostenlos."
    ],
    "en": [
      "Thursday, July 13, from 20:00, traditional picnic drawn from the bag of each, on the Quays of the Aumance, in Hérisson.\r\nThe picnic will be followed by a ball at the village hall.\r\nFree of charge."
    ],
    "it": [
      "Giovedì 13 luglio, dalle 20:00, tradizionale picnic sul Quais de l'Aumance, a Hérisson.\r\nIl picnic sarà seguito da un ballo nella sala del villaggio.\r\nGratuito."
    ],
    "fr": [
      "Jeudi 13 Juillet, à partir de 20h00, traditionnel pique-nique tiré du sac de chacun, sur les Quais de l'Aumance, à Hérisson.\r\nLe pique-nique sera suivi du bal à la salle des fêtes.\r\nGratuit."
    ],
    "es": [
      "Jueves 13 de julio, a partir de las 20.00 h, comida campestre tradicional en el Quais de l'Aumance, en Hérisson.\r\nTras el picnic se celebrará un baile en la sala del pueblo.\r\nEntrada gratuita."
    ],
    "nl": [
      "Donderdag 13 juli, vanaf 20.00 uur, traditionele picknick op de Quais de l'Aumance, in Hérisson.\r\nDe picknick wordt gevolgd door een dans in het dorpshuis.\r\nGratis."
    ]
  },
