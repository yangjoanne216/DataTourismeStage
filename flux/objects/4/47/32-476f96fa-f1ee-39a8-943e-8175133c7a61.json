  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/a4e2070e-2481-3528-872e-2c46e9cdb5e2",
      "dc:description": {
        "de": [
          "Wohnung Studio Mezzanine, 35 m², 2. Stock Asc. 50m von einem Thermalbad, 200m vom Stadtzentrum, 7km vom Meer entfernt. Parkplatz, Loggia, Gartenmöbel. Fernseher, Telefon, Mikrowelle, Gefrierschrank, Heizung. Bett 140 cm Wohnzimmer, Bett 140 cm Mezzanin."
        ],
        "en": [
          "Apartment studio mezzanine, 35 m², 2nd floor Asc. At 50m from a thermal establishment, at 200m from the city center, at 7km from the seaside. Parking, loggia, garden furniture. Television, telephone, microwave oven, freezer, heating. Bed 140 cm living room, bed 140 cm mezzanine."
        ],
        "it": [
          "Monolocale con soppalco, 35 m², 2° piano Asc. A 50 m da uno stabilimento termale, a 200 m dal centro città, a 7 km dal mare. Parcheggio, loggia, mobili da giardino. Televisione, telefono, forno a microonde, congelatore, riscaldamento. Letto 140 cm soggiorno, letto 140 cm mezzanino."
        ],
        "fr": [
          "Appartement  studio mezzanine, 35 m², 2ème étage Asc. À 50m d'un établissement thermal, à 200m du centre-ville, à 7km du bord de mer. Parking, loggia, salon de jardin. Télévision, téléphone, four à micro-ondes, congélateur, chauffage. Lit 140 cm séjour, lit 140 cm mezzanine."
        ],
        "nl": [
          "Studio met mezzanine, 35 m², 2e verdieping Asc. 50m van een thermaal bad, 200m van het stadscentrum, 7km van de zee. Parkeerplaats, loggia, tuinmeubelen. Televisie, telefoon, magnetron, diepvriezer, verwarming. Bed 140 cm woonkamer, bed 140 cm mezzanine."
        ],
        "es": [
          "Estudio con altillo, 35 m², 2ª planta Asc. A 50 m de un balneario, a 200 m del centro de la ciudad, a 7 km del mar. Aparcamiento, logia, muebles de jardín. Televisión, teléfono, microondas, congelador, calefacción. Cama 140 cm salón, cama 140 cm entresuelo."
        ]
      },
