  "rdfs:comment": {
    "de": [
      "Das Hotel-Restaurant befindet sich im Herzen des Skiortes Auris. Das Hotel bietet einen Panoramablick auf das Massif des Écrins."
    ],
    "en": [
      "The Hotel-Restaurant is located in the heart of Auris resort. The hotel has doorstep skiing and panoramic views over the Ecrins mountains."
    ],
    "it": [
      "L'hotel-ristorante si trova nel cuore del resort di Auris. L'hotel gode di una vista panoramica sul massiccio degli Écrins."
    ],
    "fr": [
      "L'Hôtel-restaurant se situe au cœur de la station d'Auris. Ski au pied l’hôtel bénéficie d’une vue panoramique sur le massif des Écrins."
    ],
    "nl": [
      "Het hotel-restaurant is gelegen in het hartje van Auris en geniet van een panoramisch zicht op het massief van de Ecrins.  Vanuit het hotel kunt u direct op de ski's stappen."
    ],
    "es": [
      "El hotel-restaurante está situado en el corazón de la estación de Auris. El hotel tiene una vista panorámica del macizo de los Écrins."
    ]
  },
