  "rdfs:comment": {
    "de": [
      "Eric Fossard bietet Ihnen die Möglichkeit, Folgendes zu teilen: Bergsteigen, Klettern, Skitouren, Skifahren abseits der Pisten, Canyoning, Klettersteige, Schneeschuhwanderungen."
    ],
    "en": [
      "Eric Fossard will guide you in mountain climbing, rock climbing, cross country skiing, off piste skiing, canyoning, via ferrata, and snowshoeing."
    ],
    "it": [
      "Eric Fossard vi propone di condividere: alpinismo, arrampicata, sci alpinismo, sci fuori pista, canyoning, vie ferrate, ciaspole."
    ],
    "fr": [
      "Eric Fossard vous propose de partager : alpinisme, escalade, ski de randonnée, ski hors pistes, canyoning, via ferrata, randonnées en raquettes."
    ],
    "es": [
      "Eric Fossard le propone compartir: alpinismo, escalada, esquí de travesía, esquí fuera de pista, barranquismo, vía ferrata, raquetas de nieve."
    ],
    "nl": [
      "Eric Fossard biedt zijn diensten aan voor: alpinisme, klimmen, langlaufen, off track skiën, canyoning, via ferrata, trektochten en sneeuwraketten."
    ]
  },
