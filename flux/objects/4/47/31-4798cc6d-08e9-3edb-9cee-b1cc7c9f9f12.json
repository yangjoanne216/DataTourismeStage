  "rdfs:comment": {
    "de": [
      "In St-Georges-de-Luzençon, ganz in der Nähe von Millau, Panoramablick auf das Viadukt, im Regionalpark Grands Causses, zwischen Larzac und Tarn-Tal, auf dem Hügel gegenüber dem Dorf Peyre."
    ],
    "en": [
      "Saint-Georges-de-Luzençon. Near Millau, this gite has a panoramic view of the Viaduct, in the Regional Park of Grands Causses, between Larzac and the Tarn valley, on the hill opposite the village of Peyre."
    ],
    "it": [
      "A St-Georges-de-Luzençon, vicino a Millau, vista panoramica del Viadotto, nel Parc Régional des Grands Causses, tra Larzac e Vallée du Tarn, sulla collina di fronte al villaggio di Peyre."
    ],
    "fr": [
      "A St-Georges-de-Luzençon, tout près de Millau, vue panoramique sur le Viaduc, dans le Parc Régional des Grands Causses, entre Larzac et Vallée du Tarn, sur la colline en face du village de Peyre."
    ],
    "nl": [
      "In St-Georges-de-Luzençon, bij Millau, panoramisch uitzicht op het Viaduct, in het Parc Régional des Grands Causses, tussen Larzac en de vallei van de Tarn, op de heuvel tegenover het dorp Peyre."
    ],
    "es": [
      "En St-Georges-de-Luzençon, cerca de Millau, vista panorámica del Viaducto, en el Parque Regional des Grands Causses, entre Larzac y Vallée du Tarn, en la colina frente al pueblo de Peyre."
    ]
  },
