  "rdfs:comment": {
    "de": [
      "Die Unterkunft \"Gîte des Blinières\" empfängt Sie inmitten der Natur, zwei Schritte vom Ortszentrum entfernt."
    ],
    "en": [
      "The gîte des Blinières welcomes you in the middle of nature, a stone's throw from the centre of the village."
    ],
    "it": [
      "Il gîte des Blinières vi accoglie nel cuore della natura, a due passi dal centro del paese."
    ],
    "fr": [
      "Le gîte des Blinières vous accueille en pleine nature,  deux pas du centre bourg."
    ],
    "es": [
      "La casa rural des Blinières le acoge en plena naturaleza, a dos pasos del centro del pueblo."
    ],
    "nl": [
