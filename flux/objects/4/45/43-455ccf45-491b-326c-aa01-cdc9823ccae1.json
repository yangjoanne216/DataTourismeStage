  "rdfs:comment": {
    "de": [
      "Sie nehmen den GR65, gehen am Château de Madirac vorbei, erblicken den Brunnen von Coumet, durchqueren den Wald von Broustés und kehren über den Waschplatz zurück."
    ],
    "en": [
      "You will take the GR65, pass in front of the Castle of Madirac, see the fountain of Coumet, cross the wood of Broustés and return by the wash-house."
    ],
    "it": [
      "Si percorre il GR65, si passa davanti al castello di Madirac, si vede la fontana Coumet, si attraversa il bosco di Broustés e si ritorna passando per il lavatoio."
    ],
    "fr": [
      "Vous emprunterez le GR65, passerez devant le Château de Madirac, apercevrez la fontaine de Coumet, traverserez le bois du Broustés et reviendrez par le lavoir."
    ],
    "nl": [
      "U neemt de GR65, komt voor het kasteel van Madirac langs, ziet de fontein van Coumet, steekt het bos van Broustés over en keert terug via het washuis."
    ],
    "es": [
      "Tomará el GR65, pasará por delante del castillo de Madirac, verá la fuente de Coumet, atravesará el bosque de Broustés y regresará por el lavadero."
    ]
  },
