      "dc:description": {
        "de": [
          "Carnac-Rouffiac: Samstag, 15. Juli\nSauzet: Freitag, 28. Juli\nCambayrac: Freitag, 11. August\nVillesèque: Donnerstag, 17. August\n\nBitte bringen Sie Ihr eigenes Besteck mit."
        ],
        "en": [
          "Carnac-Rouffiac: Saturday, July 15\nSauzet: Friday, July 28\nCambayrac: Friday August 11\nVillesèque: Thursday, August 17\n\nPlease bring your own cutlery."
        ],
        "it": [
          "Carnac-Rouffiac: sabato 15 luglio\nSauzet: venerdì 28 luglio\nCambayrac: venerdì 11 agosto\nVillesèque: giovedì 17 agosto\n\nSi prega di portare le proprie posate."
        ],
        "fr": [
          "Carnac-Rouffiac: samedi 15 juillet\nSauzet: vendredi 28 juillet\nCambayrac: vendredi 11 août\nVillesèque: jeudi 17 août\n\nMerci d'apporter vos couverts."
        ],
        "nl": [
          "Carnac-Rouffiac: zaterdag 15 juli\nSauzet: vrijdag 28 juli\nCambayrac: vrijdag 11 augustus\nVillesèque: donderdag 17 augustus\n\nGelieve uw eigen bestek mee te nemen."
        ],
        "es": [
          "Carnac-Rouffiac: sábado 15 de julio\nSauzet: viernes 28 de julio\nCambayrac: viernes 11 de agosto\nVillesèque: jueves 17 de agosto\n\nPor favor, traiga sus propios cubiertos."
        ]
      },
