  "rdfs:comment": {
    "de": [
      "Privates Museum im Untergeschoss der Weinkellerei von Ribeauvillé. Erinnerung an den Beruf des Winzers, das Pfropfen der Rebstöcke, die Weinlese, das Keltern und die Weinherstellung."
    ],
    "en": [
      "Private museum located in the basement of the Ribeauvillé wine cellar. Evokes the profession of winemaker, grafting of vine plants, harvesting, pressing and winemaking."
    ],
    "it": [
      "Museo privato situato nel seminterrato della cantina Ribeauvillé. Evocazione del mestiere del viticoltore, innesto delle piante di vite, vendemmia, pigiatura e vinificazione."
    ],
    "fr": [
      "Musée privé situé dans le sous-sol de la cave vinicole de Ribeauvillé. Évocation du métier de vigneron, du greffage des plants de vigne, des vendanges, du pressurage et de la vinification."
    ],
    "nl": [
      "Privé-museum in de kelder van de wijnkelder van Ribeauvillé. Het beroep van wijnbouwer, het enten van de wijnplanten, het oogsten, het persen en de wijnbereiding."
    ],
    "es": [
      "Museo privado situado en el sótano de la bodega de Ribeauvillé. Evocación del oficio de viticultor, injerto de las plantas de vid, cosecha, prensado y vinificación."
    ]
  },
