  "rdfs:comment": {
    "de": [
      "Geführte Besichtigung ab 7 Jahren.\r\nEine Einladung, mit der ganzen Familie die Schätze des Altbestands der Bibliothek zu entdecken.\r\nDauer 30 min.\r\nAnmeldung Bibliothek."
    ],
    "en": [
      "Guided tour from 7 years old.\r\nAn invitation to discover with your family the treasures of the library's ancient collections.\r\nDuration 30 min.\r\nRegistration for the library."
    ],
    "it": [
      "Visita guidata a partire dai 7 anni.\r\nUn invito a scoprire i tesori delle antiche collezioni della biblioteca con la famiglia.\r\nDurata 30 minuti.\r\nIscrizione alla biblioteca."
    ],
    "fr": [
      "Visite guidée dès 7 ans.\r\nUne invitation à découvrir en famille les trésors des fonds anciens de la bibliothèque.\r\nDurée 30 min.\r\nInscription bibliothèque."
    ],
    "es": [
      "Visita guiada a partir de 7 años.\r\nUna invitación a descubrir en familia los tesoros de los fondos antiguos de la biblioteca.\r\nDuración 30 min.\r\nInscripción en la biblioteca."
    ],
    "nl": [
      "Rondleiding vanaf 7 jaar.\r\nEen uitnodiging om met het gezin de schatten van de oude collecties van de bibliotheek te ontdekken.\r\nDuur 30 min.\r\nInschrijving voor de bibliotheek."
    ]
  },
