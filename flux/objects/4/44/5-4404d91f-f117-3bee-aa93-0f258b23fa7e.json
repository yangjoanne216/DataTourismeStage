  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/6d5b510c-4b60-3a5a-914c-3f1b0535cea0",
      "dc:description": {
        "de": [
          "Werk der Pariser Bildhauer Vermare und Moreau-Vauthier zum Gedenken an die französischen Jäger (Krieg von 1914-1918). Zugang: Route des Crêtes (D 431), 20 Minuten zu Fuß vom Grand Ballon entfernt."
        ],
        "en": [
          "Work by the Parisian sculptors Vermare and Moreau-Vauthier in memory of the French hunters (1914-1918 war). Access : Route des Crêtes (D 431) 20 mn walk from the Grand Ballon."
        ],
        "it": [
          "Opera degli scultori parigini Vermare e Moreau-Vauthier in memoria dei cacciatori francesi (guerra 1914-1918). Accesso: Route des Crêtes (D 431) a 20 minuti a piedi dal Grand Ballon."
        ],
        "fr": [
          "Oeuvre des sculpteurs parisiens Vermare et Moreau-Vauthier à la mémoire des chasseurs français (Guerre de 1914-1918). Accès : Route des Crêtes (D 431) à 20 mn à pied du Grand Ballon."
        ],
        "nl": [
          "Werk van de Parijse beeldhouwers Vermare en Moreau-Vauthier ter nagedachtenis aan de Franse jagers (oorlog 1914-1918). Bereikbaarheid: Route des Crêtes (D 431) op 20 minuten lopen van de Grand Ballon."
        ],
        "es": [
          "Obra de los escultores parisinos Vermare y Moreau-Vauthier en recuerdo de los cazadores franceses (guerra de 1914-1918). Acceso: Route des Crêtes (D 431) a 20 minutos a pie del Grand Ballon."
        ]
      },
