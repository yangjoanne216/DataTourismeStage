  "rdfs:comment": {
    "de": [
      "Christophe Gay, ehemaliger Rugbyspieler aus Aix-en-Provence und von Beruf Koch, betreibt in Lanta das Bar-Restaurant Entre XIII... et 15. Hier findet man die Atmosphäre eines Dorfbistros."
    ],
    "en": [
      "Christophe Gay, a former rugby player from Aix-en-Provence and a chef by profession, runs the bar-restaurant Entre XIII... et 15 in Lanta. It has a village bistro atmosphere."
    ],
    "it": [
      "Christophe Gay, ex giocatore di rugby di Aix-en-Provence e chef di professione, gestisce il bar-ristorante Entre XIII... et 15 a Lanta. L'atmosfera è quella di un bistrot di paese."
    ],
    "fr": [
      "Christophe Gay, ex rugbyman d'Aix-en-Provence et cuisinier de profession, tient à  Lanta le bar restaurant Entre XIII... et 15. On y retrouve une ambiance bistrot de village."
    ],
    "nl": [
      "Christophe Gay, voormalig rugbyspeler uit Aix-en-Provence en kok van beroep, runt bar-restaurant Entre XIII... et 15 in Lanta. Het heeft een dorpse bistrosfeer."
    ],
    "es": [
      "Christophe Gay, antiguo jugador de rugby de Aix-en-Provence y chef de profesión, dirige el bar-restaurante Entre XIII... et 15 en Lanta. Tiene un ambiente de bistró de pueblo."
    ]
  },
