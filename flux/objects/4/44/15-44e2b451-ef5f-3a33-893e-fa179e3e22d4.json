  "rdfs:comment": {
    "de": [
      "Diversifizierte Getreidefarm mit biologischer Pilzproduktion: Shiitake, Austernpilze... In frischer und verarbeiteter Form: getrocknete Pilze, Austernpilzsuppe."
    ],
    "en": [
      "Diversified cereal farm with organic production of mushrooms: shiitake, oyster mushrooms... Fresh and processed: dehydrated mushrooms, oyster mushroom soup."
    ],
    "it": [
      "Azienda agricola cerealicola diversificata con produzione biologica di funghi: shiitake, funghi ostrica... Freschi e trasformati: funghi disidratati, zuppa di funghi ostrica."
    ],
    "fr": [
      "Ferme céréalière diversifiée avec de la production biologique de champignons : shiitaké, pleurotes... En frais et transformés : champignons déshydratés, velouté de pleurotes."
    ],
    "es": [
      "Granja de cereales diversificada con producción ecológica de setas: shiitake, setas ostra... Frescas y procesadas: setas deshidratadas, sopa de setas ostra."
    ],
    "nl": [
      "Gediversifieerde graanboerderij met biologische productie van champignons: shiitake, oesterzwammen... Vers en verwerkt: gedroogde champignons, oesterzwammen soep."
    ]
  },
