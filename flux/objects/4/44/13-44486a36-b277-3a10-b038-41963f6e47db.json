  "rdfs:comment": {
    "de": [
      "Das Park Hôtel Suisse &amp; Spa befindet sich im Zentrum von Chamonix gegenüber dem Fremdenverkehrsamt, 50 m von den Pendelbussen entfernt, die alle Skipisten bedienen."
    ],
    "en": [
      "The Park Hotel Suisse &amp; Spa is located in the center of Chamonix in front of the Tourist Office, 50 meters from the shuttle buses serving all the ski slopes."
    ],
    "it": [
      "Il Park Hotel Suisse &amp; Spa si trova nel centro di Chamonix, di fronte all'Ufficio del Turismo, a 50 metri dai bus navetta per tutte le piste da sci."
    ],
    "fr": [
      "Le Park Hôtel Suisse & Spa est situé au centre de Chamonix en face de l'Office du Tourisme, à 50 mètres des navettes desservant toutes les pistes de ski."
    ],
    "nl": [
      "Het Park Hotel Suisse & Spa ligt in het centrum van Chamonix tegenover het toeristenbureau, op 50 meter van de pendelbussen naar alle skipistes."
    ],
    "es": [
      "El Park Hotel Suisse &amp; Spa está situado en el centro de Chamonix, frente a la Oficina de Turismo, a 50 metros de los autobuses que llevan a todas las pistas de esquí."
    ]
  },
