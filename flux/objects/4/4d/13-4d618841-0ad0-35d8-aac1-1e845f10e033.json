  "rdfs:comment": {
    "de": [
      "Aufzucht in Ecocert-Biolandwirtschaft - Kuh-, Ziegen- und Schafskäse, Joghurt - Hanf - Mehl aus Roggen, Hanf... Biomarkt auf dem Bauernhof mit anderen lokalen Produzenten am Freitag von 16:30 bis 18:30 Uhr."
    ],
    "en": [
      "Organic farming Ecocert - Cow, goat and sheep cheese, yoghurt - Hemp - Rye flour, hemp... Organic market at the farm with other local producers on Fridays from 4:30 to 6:30 pm."
    ],
    "it": [
      "Agricoltura biologica Ecocert - Formaggio di mucca, capra e pecora, yogurt - Canapa - Farina di segale e canapa... Mercato biologico in fattoria con altri produttori locali il venerdì dalle 16.30 alle 18.30."
    ],
    "fr": [
      "Elevage en agriculture biologique Ecocert - Fromages de vache, chèvre et brebis, yaourts - Chanvre - Farine seigle, chanvre... Marché bio à la ferme avec d'autres producteurs locaux le vendredi de 16h30  à 18h30h."
    ],
    "nl": [
      "Ecocert biologische landbouw - Kaas van koeien, geiten en schapen, yoghurt... Biologische markt op de boerderij met andere lokale producenten op vrijdag van 16.30 tot 18.30 uur."
    ],
    "es": [
      "Agricultura ecológica Ecocert - Queso de vaca, cabra y oveja, yogur - Cáñamo - Harina de centeno y cáñamo... Mercado ecológico en la granja con otros productores locales los viernes de 16:30 a 18:30."
    ]
