  "rdfs:comment": {
    "de": [
      "Der Heilige Siffrein, Bischof von Carpentras, soll eine erste Kapelle zu Ehren des Heiligen Antoninus errichtet haben. Diese stand neben dem heutigen Standort."
    ],
    "en": [
      "Saint Siffrein, bishop of Carpentras, would have had a first chapel built in Saint Antoninus. This one was built next to the present site."
    ],
    "it": [
      "Si dice che San Siffrein, vescovo di Carpentras, abbia costruito una prima cappella a Sant'Antonino. Questa cappella sorgeva accanto al sito attuale."
    ],
    "fr": [
      "Saint Siffrein, évêque de Carpentras, aurait fait élevé une première chapelle à Saint Antonin. Celle-ci s'élevait à côté de l'emplacement actuel."
    ],
    "es": [
      "Se dice que San Siffrein, obispo de Carpentras, construyó una primera capilla a San Antonino. Esta capilla se encontraba junto al emplazamiento actual."
    ],
    "nl": [
      "De heilige Siffrein, bisschop van Carpentras, zou een eerste kapel hebben gebouwd voor de heilige Antoninus. Deze kapel stond naast de huidige plek."
    ]
  },
