  "rdfs:comment": {
    "de": [
      "Schloss aus dem 13. Jahrhundert in einem Park mit zweihundertjährigen Linden, in dem 1629 Ludwig XIII. und Richelieu logierten. Geräumige Traumzimmer, davon 6 mit Balneotherapie. Gourmet-Restaurant."
    ],
    "en": [
      "A 13th century chateau in a park lined with two-centuries-old lime trees, where Louis XIII and Richelieu stayed in 1629. Traditional restaurant."
    ],
    "it": [
      "Un castello del XIII secolo immerso in un parco con tigli bicentenari, dove Luigi XIII e Richelieu soggiornarono nel 1629. Ampie camere da sogno, di cui 6 con balneoterapia. Ristorante gastronomico."
    ],
    "fr": [
      "Château du XIIIe siècle situé dans un parc aux tilleuls bicentenaires où ont logé, en 1629, Louis XIII et Richelieu. Chambres de rêve spacieuses dont 6 avec balnéothérapie. Restaurant gastronomique."
    ],
    "es": [
      "Un castillo del siglo XIII situado en un parque con tilos bicentenarios donde se alojaron Luis XIII y Richelieu en 1629. Amplias habitaciones de ensueño, incluidas 6 con balneoterapia. Restaurante gastronómico."
    ],
    "nl": [
      "Kasteel uit de XIIIe eeuw, gelegen in een park met tweehonderd jaar oude lindebomen en waar Lodewijk XIII en Richelieu logeerden in 1629. Traditioneel restaurant."
    ]
  },
