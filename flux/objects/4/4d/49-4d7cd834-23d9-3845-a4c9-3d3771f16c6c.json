  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/9a1203d8-327e-3021-a91c-801c816322ce",
      "dc:description": {
        "de": [
          "Menhir mit den Maßen 2,60 m x 1,20 m und 1,10 m Dicke.\n\nPitre de Lisle nannte ihn in seinem Dictionnaire achéologique von 1880 Menhir des Devonnières."
        ],
        "en": [
          "Menhir of 2m60 by 1m20 and 1m10 thick.\n\nNote that Pitre de Lisle in his 1880 archaeological dictionary calls it Menhir des Devonnières."
        ],
        "it": [
          "Menhir di 2m60 per 1m20 e 1m10 di spessore.\n\nSi noti che Pitre de Lisle, nel suo dizionario archeologico del 1880, lo chiama Menhir des Devonnières."
        ],
        "fr": [
          "Menhir de 2m60 sur 1m20 et 1m10 d’épaisseur.\n\nA noter que Pitre de Lisle dans son dictionnaire achéologique de 1880 l’appelle Menhir des Devonnières."
        ],
        "nl": [
          "Menhir van 2m60 bij 1m20 en 1m10 dik.\n\nOpgemerkt zij dat Pitre de Lisle in zijn archeologisch woordenboek van 1880 de Menhir des Devonnières noemt."
        ],
        "es": [
          "Menhir de 2m60 por 1m20 y 1m10 de grosor.\n\nCabe señalar que Pitre de Lisle, en su diccionario arqueológico de 1880, lo denomina Menhir des Devonnières."
        ]
      },
