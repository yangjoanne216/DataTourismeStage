  "rdfs:comment": {
    "de": [
      "Ein einfach zu erreichendes Hotel mit Grünanlage, ideal für Ihre Urlaubs- oder Geschäftsreisen. Das Best Western liegt 10 Minuten von Saint-Etienne, dem Stade G. Guichard sowie 2 Minuten vom Château de Bouthéon entfernt."
    ],
    "en": [
      "This easily accessible hotel is in a natural setting making it ideal for both business trips and holidays. The Best Western is 10 minutes from Saint-Etienne and G. Guichard Stadium and 2 minutes from the Château de Bouthéon."
    ],
    "it": [
      "Best Western Porte du Forez Restaurant By Georgette: il luogo ideale per pranzare, bere, divertirsi, lavorare o riposare tutto l'anno, a 15 minuti dal centro di Saint Etienne."
    ],
    "fr": [
      "Best Western Porte du Forez Restaurant By Georgette : l'endroit rêvé pour un déjeuner, boire un verre, s'amuser, travailler ou se reposer toute l'année, à 15min du centre de Saint Etienne."
    ],
    "es": [
      "Best Western Porte du Forez Restaurant By Georgette: el lugar perfecto para comer, tomar una copa, divertirse, trabajar o descansar durante todo el año, a 15 minutos del centro de Saint Etienne."
    ],
    "nl": [
      "Best Western Porte du Forez Restaurant By Georgette: de perfecte plek voor lunch, borrel, plezier, werk of rust het hele jaar door, op 15 minuten van het centrum van Saint Etienne."
    ]
  },
