  "rdfs:comment": {
    "de": [
      "In L'Épicerie im Herzen von La Borne finden Sie Produkte \"Made In Berry\", die Sie mit nach Hause nehmen können."
    ],
    "en": [
      "You will find at L'Épicerie in the heart of the Borne \"Made In Berry\" products to take home."
    ],
    "it": [
      "Presso L'Épicerie, nel cuore di La Borne, troverete prodotti \"Made In Berry\" da portare a casa."
    ],
    "fr": [
      "Vous trouverez à L’Épicerie en plein coeur de la Borne des produits \"Made In Berry\" à rapporter à la maison."
    ],
    "es": [
      "En L'Épicerie, en el corazón de La Borne, encontrará productos \"Made In Berry\" para llevar a casa."
    ],
    "nl": [
      "Bij L'Épicerie in het hart van La Borne vindt u producten \"Made In Berry\" om mee naar huis te nemen."
    ]
