  "rdfs:comment": {
    "de": [
      "Großer Wasserskilift mit einer Länge von 650 m!\r\nFreizeitzentrum mit Aquapark, Wakeboard, Wasserski, gezogener Boje, Paddle...\r\n450 m2 Beachfeld: Fußball, Volleyball, Rugby, Tennis, Petanque.\r\nLa Guinguette: Restaurant/Bar, Abendveranstaltungen, Hängematte und Strand."
    ],
    "en": [
      "Large water ski lift of 650m !\r\nLeisure centre with aquapark, wakeboard, water-ski, towed buoy, paddle...\r\n450 m2 of beach area : football, volleyball, rugby, tennis, petanque.\r\nLa Guinguette : Restaurant/Bar, parties, hammock and beach."
    ],
    "it": [
      "Grande impianto di risalita per sci nautico di 650 m!\r\nCentro ricreativo con acquapark, wakeboard, sci nautico, boa trainata, paddle...\r\n450 m2 di spiaggia: calcio, pallavolo, rugby, tennis, bocce.\r\nLa Guinguette: ristorante/bar, feste, amaca e spiaggia."
    ],
    "fr": [
      "Grand Téléski Nautique de 650m !\r\nBase de loisirs avec aquapark, wakeboard, ski nautique, bouée tractée, paddle...\r\n450 m2 de terrain beach : foot, volley, rugby, tennis, pétanque.\r\nLa Guinguette : Restauration/Bar, soirées, hamac et plage."
    ],
    "nl": [
      "Grote waterskilift van 650m !\r\nRecreatiecentrum met aquapark, wakeboard, waterski, sleepboei, paddle...\r\n450 m2 strand: voetbal, volleybal, rugby, tennis, petanque.\r\nLa Guinguette : Restaurant/Bar, feesten, hangmat en strand."
    ],
    "es": [
      "Gran telesilla de 650 m\r\nCentro de ocio con aquapark, wakeboard, esquí acuático, boya remolcada, paddle...\r\n450 m2 de zona de playa: fútbol, voleibol, rugby, tenis, petanca.\r\nLa Guinguette : Restaurante/Bar, fiestas, hamaca y playa."
    ]
  },
