  "rdfs:comment": {
    "de": [
      "Genießen Sie die Badefreuden, die das städtische Schwimmbad in Boussens bietet."
    ],
    "en": [
      "Enjoy swimming at the Boussens municipal pool."
    ],
    "it": [
      "Godetevi una nuotata rinfrescante nella piscina comunale di Boussens."
    ],
    "fr": [
      "Profitez des joies de la baignade offertes par la piscine municipale de Boussens."
    ],
    "nl": [
      "Geniet van een verfrissende duik in het gemeentelijk zwembad van Boussens."
    ],
    "es": [
      "Disfrute de un refrescante baño en la piscina municipal de Boussens."
    ]
