  "rdfs:comment": {
    "de": [
      "Donnerstags wird getanzt! Der Verein Altaïr schlägt Ihnen vor, sich auf dem Belvedere des Hafens zu treffen, um einen festlichen Abend zu verbringen, der dem Tanz gewidmet ist."
    ],
    "en": [
      "Dancing Thursday! The Altaïr association invites you to join them on the harbor belvedere for a festive evening of dancing."
    ],
    "it": [
      "Giovedì danzante! L'associazione Altaïr vi invita a ritrovarvi sul belvedere del porto per una serata di ballo."
    ],
    "fr": [
      "Le jeudi ça danse ! L'association Altaïr vous propose de se retrouver sur le belvédère du port, pour une soirée festive dédiée à la danse."
    ],
    "es": [
      "Jueves de baile La asociación Altaïr le invita a reunirse en el mirador del puerto para una noche festiva de baile."
    ],
    "nl": [
      "Dansende donderdag! De vereniging Altaïr nodigt u uit voor een feestelijke dansavond op de havenbelvedère."
    ]
  },
