  "rdfs:comment": {
    "de": [
      "Japanisches Restaurant.\nDer Chefkoch bietet Ihnen zahlreiche Spezialitäten: Sushi, Sashimi, Chirachi, Yakitori, Maki vor Ort und mit 5% Rabatt auf den Verkauf zum Mitnehmen. Kostenlose Lieferung nach Hause."
    ],
    "en": [
      "Japanese restaurant.\nThe chef offers a wide range of specialities: Sushi, Sashimi, Chirachi, Yakitori, Maki on the premises and with 5% discount on takeaway. Free home delivery."
    ],
    "it": [
      "Ristorante giapponese.\nLo chef offre una vasta gamma di specialità: Sushi, Sashimi, Chirachi, Yakitori, Maki sul posto e con uno sconto del 5% per l'asporto. Consegna a domicilio gratuita."
    ],
    "fr": [
      "Restaurant Japonais.\nLe chef vous propose de nombreuses spécialités: Sushi, Sashimi, Chirachi, Yakitori, Maki sur place et avec 5% de remise sur la vente à emporter. Livraison gratuite à domicile."
    ],
    "es": [
      "Restaurante japonés.\nEl chef ofrece una amplia gama de especialidades: Sushi, Sashimi, Chirachi, Yakitori, Maki en el local y con un 5% de descuento para llevar. Entrega a domicilio gratuita."
    ],
    "nl": [
      "Japans restaurant.\nDe chef biedt een breed scala aan specialiteiten: Sushi, Sashimi, Chirachi, Yakitori, Maki ter plaatse en met 5% afhaalkorting. Gratis thuisbezorging."
    ]
  },
