  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/f29144ed-eb38-3fc0-8806-47bd4f36c453",
      "dc:description": {
        "de": [
          "Verkostung lokaler Bauernprodukte: Heilpflanzen, Fruchtsäfte und Nektare, Saisonfrüchte, Gemüse, Marmeladen, Sirup, Käse, Eier, Fleisch, Honig, Feldfrüchte, Oliven, Olivenöl, Fisch, Kekse, Kastanien, Spirulina."
        ],
        "en": [
          "Tasting of local farm products: medicinal plants, fruit juices and nectars, seasonal fruits, vegetables, jams, syrups, cheeses, eggs, meat, honey, field crops, olives, olive oils, fish, cookies, chestnuts, spirulina."
        ],
        "it": [
          "Degustazione di prodotti agricoli locali: piante officinali, succhi e nettari di frutta, frutta di stagione, ortaggi, marmellate, sciroppi, formaggi, uova, carne, miele, colture di campo, olive, oli di oliva, pesce, biscotti, castagne, spirulina."
        ],
        "fr": [
          "Dégustation de produits paysans locaux : plantes médicinales, jus de fruits et nectars, fruits de saison, légumes, confitures, sirops, fromages, oeufs, viande, miel, grandes cultures, olives, huiles d'olive, poisson, biscuits, châtaignes, spiruline."
        ],
        "nl": [
          "Proeverij van lokale landbouwproducten: geneeskrachtige planten, vruchtensappen en nectars, seizoensfruit, groenten, jam, siroop, kaas, eieren, vlees, honing, akkerbouwgewassen, olijven, olijfolie, vis, koekjes, kastanjes, spirulina."
        ],
        "es": [
          "Degustación de productos agrícolas locales: plantas medicinales, zumos y néctares de frutas, frutas de temporada, verduras, mermeladas, siropes, quesos, huevos, carne, miel, cultivos de campo, aceitunas, aceites de oliva, pescado, galletas, castañas, espirulina."
        ]
      },
