  "rdfs:comment": {
    "de": [
      "Familiencampingplatz am Flussufer mit Stellplätzen, Mobilheimen, Chalets und Luxus-Chalets mit eigenem Jacuzzi. Wasserpark auf 1200m² mit 7 Rutschen bis 10m, 1 Splashpark. Animationen & Sporthalle, 2 synthetische Multisportbereiche."
    ],
    "en": [
      "Family-friendly riverside campsite with pitches, mobile homes, chalets and luxury chalets with private jacuzzi. 1200m² water park with 7 slides and a splash park. \r\nEntertainment and sports hall plus 2 synthetic multi-sports pitches."
    ],
    "it": [
      "Campeggio familiare situato sulla riva del fiume con piazzole, case mobili, chalet e chalet di lusso con jacuzzi privata. Parco acquatico di 1200 m² con 7 scivoli fino a 10 m, 1 splashpark. Sala spettacoli e sport, 2 aree polisportive sintetiche."
    ],
    "fr": [
      "Camping familial situé en bord de rivière avec emplacements, mobilhomes, chalets et chalets luxe avec jacuzzi privatif. Parc aquatique sur 1200m² avec 7 toboggans jusqu'à 10m, 1 splashpark. Animations & salle de sport, 2 espaces multisports synthétiques."
    ],
    "nl": [
      "Familiecamping gelegen aan de rivieroever met staanplaatsen, stacaravans, chalets en luxe chalets met eigen jacuzzi. Waterpark op 1200m² met 7 glijbanen tot 10m, 1 splashpark. Animatie- en sporthal, 2 synthetische multisportterreinen."
    ],
    "es": [
      "Camping familiar situado en la orilla del río con parcelas, casas móviles, chalets y chalets de lujo con jacuzzi privado. Parque acuático de 1200m² con 7 toboganes de hasta 10m, 1 splashpark. Sala de entretenimiento y deportes, 2 áreas sintéticas multideportivas."
    ]
  },
