  "rdfs:comment": {
    "de": [
      "La Vague bietet Frühstück, Brunch, Mittagessen, Snacks, die aus frischen & lokalen Produkten zubereitet werden. Aber auch vegetarische, glutenfreie und vegane Gerichte."
    ],
    "en": [
      "La Vague offers breakfasts, brunches, lunches, snacks made from fresh & local products. But also vegetarian, gluten-free and vegan cuisine."
    ],
    "it": [
      "La Vague offre colazioni, brunch, pranzi e spuntini a base di prodotti freschi e locali. Ma anche cucina vegetariana, senza glutine e vegana."
    ],
    "fr": [
      "La Vague propose des petits déjeuners, brunchs, déjeuners, goûters élaborés à base de produits frais & locaux. Mais également une cuisine végétarienne, sans gluten et Végan."
    ],
    "es": [
      "La Vague ofrece desayunos, almuerzos, comidas y aperitivos elaborados con productos frescos y locales. Pero también cocina vegetariana, sin gluten y vegana."
    ],
