  "rdfs:comment": {
    "de": [
      "Einzigartiger Abenteuerparcours im Herzen des Waldes von Marsanne: 3 Hektar angelegte Fläche, 142 Spiele, 15 Parcours. Niveaus grün, blau, rot, schwarz. 3 Stunden unvergessliche Aktivitäten! Ab 2 Jahren."
    ],
    "en": [
      "A unique adventure course in the heart of the Marsanne forest: 3 hectares fitted out, 142 games, 15 courses. Green, blue, red, black levels. 3 hours of unforgettable activities! From 2 years old."
    ],
    "it": [
      "Percorso avventura unico nel cuore della foresta di Marsanne: 3 ettari di aree paesaggistiche, 142 giochi, 15 percorsi. Livelli verde, blu, rosso e nero. 3 ore di attività indimenticabili! A partire da 2 anni."
    ],
    "fr": [
      "Parcours d'aventure unique au coeur de la forêt de Marsanne : 3 hectares aménagés, 142 jeux, 15 parcours. Niveaux vert, bleu, rouge, noir. 3 heures d'activités inoubliables ! A partir de 2 ans."
    ],
    "nl": [
      "Uniek avonturenparcours in het hart van het bos van Marsanne: 3 hectare aangelegde terreinen, 142 spelen, 15 parcours. Groene, blauwe, rode en zwarte niveaus. 3 uur lang onvergetelijke activiteiten! Vanaf 2 jaar."
    ],
    "es": [
      "Un recorrido de aventura único en el corazón del bosque de Marsanne: 3 hectáreas de zonas ajardinadas, 142 juegos, 15 recorridos. Niveles verde, azul, rojo y negro. ¡3 horas de actividades inolvidables! A partir de los 2 años."
    ]
  },
