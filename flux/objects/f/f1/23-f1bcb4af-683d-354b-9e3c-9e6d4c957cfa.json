  "rdfs:comment": {
    "de": [
      "Handgemachte Pizzas aus dem Holzofen, Bruschetta, Salate, hausgemachte Desserts. In der Saison 7/7 geöffnet,. Außerhalb der Saison dienstags und sonntags mittags geschlossen."
    ],
    "en": [
      "Homemade pizzas cooked over a wood fire, bruschettas, salads, homemade desserts. Open 7/7 in season. Closed Tuesday and Sunday lunchtime out of season."
    ],
    "it": [
      "Pizze fatte in casa cotte a legna, bruschette, insalate, dolci fatti in casa. Aperto 7/7 in stagione. Chiuso il martedì e la domenica a pranzo fuori stagione."
    ],
    "fr": [
      "Pizzas artisanales cuites au feu de bois, bruschettas, salades, desserts maison. Ouvert 7/7 en saison,. Fermé le mardi et dimanche midi hors saison."
    ],
    "nl": [
      "Zelfgemaakte pizza's op houtvuur, bruschetta's, salades, huisgemaakte desserts. Open 7/7 in het seizoen. Buiten het seizoen gesloten op dinsdag en zondagmiddag."
    ],
    "es": [
      "Pizzas caseras cocinadas a leña, bruschettas, ensaladas, postres caseros. Abierto 7/7 en temporada. Cerrado los martes y domingos al mediodía fuera de temporada."
    ]
