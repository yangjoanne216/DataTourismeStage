  "rdfs:comment": {
    "de": [
      "Ein Campingplatz mit 2 Swimmingpools, Rutschen (davon eine mit Sensationen!), Spa, Animationen, Aktivitäten, Spielplatz... Alles für Ihren Familienurlaub. Strand in 900 m Entfernung. Vermietung von Mobilheimen und Chalets."
    ],
    "en": [
      "A campsite with 2 swimming pools, slides (one of which is sensational!), spa, activities, playground... Everything for your family holidays. Beach at 900 m. Mobile homes and chalets for rent."
    ],
    "it": [
      "Un campeggio con 2 piscine, scivoli (di cui uno sensazionale!), spa, animazione, attività, parco giochi? Tutto per le vostre vacanze in famiglia. Spiaggia a 900 m. Case mobili e chalet in affitto."
    ],
    "fr": [
      "Un camping avec 2 piscines, toboggans (dont un à sensations !), spa, animations, activités, aire de jeux… Tout pour vos vacances en famille. Plage à 900 m. Locations de mobil-homes et de chalets."
    ],
    "es": [
      "Un camping con 2 piscinas, toboganes (¡uno de ellos es una sensación!), spa, animación, actividades, parque infantil.. Todo para sus vacaciones en familia. Playa a 900 m. Alquiler de casas móviles y chalets."
    ],
    "nl": [
      "Een camping met 2 zwembaden, glijbanen (waarvan één sensationeel!), spa, animatie, activiteiten, speeltuin... Alles voor uw gezinsvakantie. Strand op 900 m. Stacaravans en chalets te huur."
    ]
  },
