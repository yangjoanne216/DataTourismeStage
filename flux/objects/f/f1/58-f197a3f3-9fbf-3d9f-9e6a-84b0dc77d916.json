  "rdfs:comment": {
    "de": [
      "Mitten in der Natur empfängt Sie der Park des Château de Florans für vier Wochen Musik im Herzen der Provence."
    ],
    "en": [
      "In the heart of nature, the park of Château de Florans welcomes you for 4 weeks of music in the heart of Provence."
    ],
    "it": [
      "Nel cuore della natura, il parco dello Château de Florans vi accoglie per 4 settimane di musica nel cuore della Provenza."
    ],
    "fr": [
      "En pleine nature, le parc du Château de Florans vous accueille pour 4 semaines de musique au cœur de la Provence."
    ],
    "nl": [
      "Midden in de natuur heet het park van Château de Florans u welkom voor 4 weken muziek in het hart van de Provence."
    ],
    "es": [
      "En plena naturaleza, el parque del Château de Florans le acoge durante 4 semanas de música en el corazón de la Provenza."
    ]
  },
