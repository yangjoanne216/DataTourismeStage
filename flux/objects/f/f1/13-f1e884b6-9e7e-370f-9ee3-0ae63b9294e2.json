  "rdfs:comment": {
    "de": [
      "Aiglun liegt 15 Minuten vom Zentrum von Digne-les-Bains entfernt, in der Nähe der Route Napoléon. Der Vieil Aiglun, der schönste Aussichtspunkt des Dorfes, erreicht eine Höhe von 830 m über dem Meeresspiegel."
    ],
    "en": [
      "Aiglun is located 15 minutes from the centre of Digne-les-Bains, close to the Route Napoleon. Le Vieil Aiglun, the most beautiful viewpoint of the village, culminates at 830 m of altitude."
    ],
    "it": [
      "L'Aiglun si trova a 15 minuti dal centro di Digne-les-Bains, vicino alla Route Napoléon. Il Vieil Aiglun, il punto panoramico più bello del villaggio, culmina a 830 m di altitudine."
    ],
    "fr": [
      "Aiglun se situe à 15 minutes du centre de Digne-les-Bains, à proximité de la Route Napoléon. Le Vieil Aiglun, le plus beau point de vue du village, culmine à 830 m d’altitude."
    ],
    "es": [
      "Aiglun se encuentra a 15 minutos del centro de Digne-les-Bains, cerca de la Route Napoléon. El Vieil Aiglun, el más bello mirador del pueblo, culmina a 830 m de altitud."
    ],
    "nl": [
      "Aiglun ligt op 15 minuten van het centrum van Digne-les-Bains, vlakbij de Route Napoléon. De Vieil Aiglun, het mooiste uitzichtpunt van het dorp, culmineert op 830 m hoogte."
    ]
  },
  "rdfs:label": {
