  "rdfs:comment": {
    "de": [
      "Ehemaliges Offizierskasino, das 1892 erbaut wurde und später Sitz der Ecole Communautaire de Musique de l'Agglomération de Forbach, Porte de France (Musikschule der Agglomeration Forbach, Porte de France) wurde."
    ],
    "en": [
      "Former officers' casino, built in 1892, which later became the headquarters of the Community Music School of the Forbach conurbation, Porte de France."
    ],
    "it": [
      "Ex casinò degli ufficiali, costruito nel 1892, che in seguito divenne la sede dell'Ecole Communautaire de Musique de l'Agglomération de Forbach, Porte de France."
    ],
    "fr": [
      "Ancien casino des officiers, construit en 1892, qui devint plus tard le siège de l'Ecole Communautaire de Musique de l’Agglomération de Forbach, Porte de France."
    ],
    "nl": [
      "Voormalig officierscasino, gebouwd in 1892, dat later de zetel werd van de Ecole Communautaire de Musique de l'Agglomération de Forbach, Porte de France."
    ],
    "es": [
      "Antiguo casino de oficiales, construido en 1892, que posteriormente se convirtió en la sede de la Escuela Comunitaria de Música de la Aglomeración de Forbach, Porte de France."
    ]
  },
  "rdfs:label": {
    "fr": [
