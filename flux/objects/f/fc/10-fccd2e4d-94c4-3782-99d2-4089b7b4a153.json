  "rdfs:comment": {
    "de": [
      "Ein guter Tisch in einem hübschen Marktflecken. Traditionelle Küche, Marktküche, Bankette und Familienessen. Montag ganztägig und Sonntagabend geschlossen."
    ],
    "en": [
      "A good table in a nice town. Traditional cuisine, market, banquet and family meals. Closed Monday all day and Sunday evening."
    ],
    "it": [
      "Un buon ristorante in un grazioso villaggio. Cucina tradizionale e di mercato, banchetti e pasti in famiglia. Chiuso tutto il lunedì e la domenica sera."
    ],
    "fr": [
      "Une bonne table dans un joli bourg. Cuisine traditionnelle, du marché, banquet et repas de famille. Fermeture lundi toute la journée et dimanche soir."
    ],
    "es": [
      "Un buen restaurante en un bonito pueblo. Cocina tradicional y de mercado, banquetes y comidas familiares. Cerrado todo el lunes y el domingo por la noche."
    ],
    "nl": [
      "Een goed restaurant in een mooi dorp. Traditionele en marktkeuken, banketten en gezinsmaaltijden. Maandag en zondagavond de hele dag gesloten."
    ]
  },
