  "rdfs:comment": {
    "de": [
      "Restaurant in einem 8 Hektar großen, baumbestandenen Park eines Hotelguts in La Pommeraye. 5 Minuten von der Loire entfernt. 30 Minuten von Angers entfernt. Helle Terrasse mit Blick auf eine Wasserfläche."
    ],
    "en": [
      "Restaurant located on a wooded park of 8 hectares of a hotel estate in La Pommeraye. 5 minutes from the Loire River. 30 minutes from Angers. Luminous terrace on the lake."
    ],
    "it": [
      "Ristorante situato in un parco alberato di 8 ettari di una tenuta alberghiera a La Pommeraye. A 5 minuti dalla Loira. 30 minuti da Angers. Luminosa terrazza con vista sul lago."
    ],
    "fr": [
      "Restaurant situé sur un parc arboré de 8 hectares d'un domaine hôtelier à la Pommeraye. A 5 minutes de la Loire. A 30 minutes d'Angers. Terrasse lumineuse sur plan d'eau."
    ],
    "es": [
      "Restaurante situado en un parque arbolado de 8 hectáreas de una finca hotelera en La Pommeraye. A 5 minutos del Loira. A 30 minutos de Angers. Terraza luminosa con vistas al lago."
    ],
    "nl": [
      "Restaurant gelegen op een bebost park van 8 hectare van een hoteldomein in La Pommeraye. 5 minuten van de Loire. 30 minuten van Angers. Helder terras met uitzicht op het meer."
    ]
  },
