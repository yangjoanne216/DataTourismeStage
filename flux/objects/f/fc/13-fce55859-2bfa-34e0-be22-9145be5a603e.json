  "rdfs:comment": {
    "de": [
      "Kommen Sie vorbei, um bestimmte Tiere zu entdecken, zu füttern und zu knuddeln. Myocastors, Murmeltiere, Lamas, Alpakas, Kängurus, Präriehunde, Rentiere....12 Tierarten."
    ],
    "en": [
      "Come discover, feed, cuddle animals. Myocastors, marmots, kangaroos, prairie dogs, reindeer .... More than 10 animal species."
    ],
    "it": [
      "Venite a scoprire, nutrire e coccolare alcuni animali. Miocastori, marmotte, lama, alpaca, canguri, cani della prateria, renne....12 specie animali."
    ],
    "fr": [
      "Venez découvrir, nourrir, câliner certains animaux. Myocastors, marmottes, lamas, alpagas, kangourous, chiens de prairies, rennes....12 espèces animales."
    ],
    "es": [
      "Venga a descubrir, alimentar y abrazar a algunos animales. Miocastores, marmotas, llamas, alpacas, canguros, perros de la pradera, renos....12 especies animales."
    ],
    "nl": [
      "Kom dieren ontdekken, voeren en knuffelen. Myocastors, marmotten, lama's, alpaca's, kangoeroes, prairiehonden, rendieren....12 diersoorten."
    ]
  },
