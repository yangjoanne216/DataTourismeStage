  "rdfs:comment": {
    "de": [
      "Erwecken Sie Ihre Sinne auf einem herrlichen Baumkletterparcours in natürlicher Umgebung, um und über einem Teich."
    ],
    "en": [
      "Awaken your senses in the heart of a magnificent accrobranche course in a natural environment, around and above a pond."
    ],
    "it": [
      "Risvegliate i vostri sensi nel cuore di un magnifico percorso di tree climbing in un ambiente naturale, intorno e sopra un laghetto."
    ],
    "fr": [
      "Éveillez vos sens au cœur d'un magnifique parcours d'accrobranche en milieu naturel, autour et au-dessus d'un étang."
    ],
    "nl": [
      "Prikkel uw zintuigen in het hart van een prachtig boomklimparcours in een natuurlijke omgeving, rond en over een vijver."
    ],
    "es": [
      "Despierte sus sentidos en el corazón de un magnífico recorrido de escalada de árboles en un entorno natural, alrededor y sobre un estanque."
    ]
