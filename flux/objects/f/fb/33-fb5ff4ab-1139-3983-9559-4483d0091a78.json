  "rdfs:comment": {
    "de": [
      "Christine bietet Ihnen ihre biologischen, leckeren und engagierten Kekse an, die in Launac hergestellt werden!"
    ],
    "en": [
      "Christine offers you her organic, gourmet and committed cookies, made in Launac!"
    ],
    "it": [
      "Christine vi propone i suoi biscotti biologici, gourmet e impegnati, prodotti a Launac!"
    ],
    "fr": [
      "Christine vous propose ses biscuits bios, gourmands et engagés, fabriqués à Launac !"
    ],
    "es": [
      "Christine le ofrece sus galletas ecológicas, gourmet y comprometidas, ¡hechas en Launac!"
    ],
    "nl": [
      "Christine biedt u haar biologische, gastronomische en geëngageerde koekjes aan, gemaakt in Launac!"
    ]
  },
