      "dc:description": {
        "de": [
          "Elementare Reitschule (Ecole Elémentaire d'Equitation). 1 Reitlehrer. 20 Pferde, 10 Ponys, 2 Reithallen, 1 Steinbruch. Praktika: Woche. Spaziergang, Kleine Wanderung (2 Tage), Große Wanderung (Woche, zwei Wochen oder mehr)."
        ],
        "en": [
          "Elementary Riding School. 1 instructor. 20 horses, 10 ponies, 2 rides, 1 arena. Training courses : week. Walking, short ride (2 days), long ride (week, fortnight or more)."
        ],
        "it": [
          "Scuola elementare di equitazione. 1 istruttore. 20 cavalli, 10 pony, 2 maneggi, 1 arena. Corsi di formazione: settimana. Cavalcare, brevi corse (2 giorni), lunghe corse (settimana, quindici giorni o più)."
        ],
        "fr": [
          "Ecole Elémentaire d'Equitation. 1 moniteur. 20 chevaux, 10 poneys, 2 manèges, 1 carrière. Stages : semaine. Promenade, Petite Randonnée (2 jours), Grande randonnée (semaine, quinzaine ou plus)."
        ],
        "es": [
          "Escuela primaria de equitación. 1 instructor. 20 caballos, 10 ponis, 2 picaderos, 1 pista. Cursos de formación : semana. Paseos, paseos cortos (2 días), paseos largos (semana, quincena o más)."
        ],
        "nl": [
          "Basisschool. 1 instructeur. 20 paarden, 10 pony's, 2 maneges, 1 arena. Opleidingen: week. Paardrijden, korte ritten (2 dagen), lange ritten (week, twee weken of meer)."
        ]
      },
