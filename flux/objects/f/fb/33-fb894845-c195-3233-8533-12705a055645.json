  "rdfs:comment": {
    "de": [
      "Wunderschönes Restaurant mit traditioneller Küche, das bis zu 300 Gedecke aufnehmen kann, mit einer sehr großen Terrasse im Schatten eines tausendjährigen Olivenbaums."
    ],
    "en": [
      "Superb restaurant of traditional cuisine, with a capacity of up to 300 diners, with a very large terrace in the shade of a thousand-year-old olive tree."
    ],
    "it": [
      "Superbo ristorante con cucina tradizionale, fino a 300 posti a sedere, con un'ampia terrazza all'ombra di un ulivo millenario."
    ],
    "fr": [
      "Superbe restaurant de cuisine traditionnelle, pouvant accueillir jusqu'à 300 couverts, avec une très grande terrasse à l'ombre d'un olivier millénaire."
    ],
    "nl": [
      "Schitterend restaurant met traditionele keuken, voor maximaal 300 personen, met een zeer groot terras in de schaduw van een duizend jaar oude olijfboom."
    ],
    "es": [
      "Magnífico restaurante de cocina tradicional, con capacidad para 300 comensales y una amplísima terraza a la sombra de un olivo milenario."
    ]
  },
