  "rdfs:comment": {
    "de": [
      "Schönes, vollständig renoviertes Bauernhaus auf der Achse Nantes, Angers, in der Nähe von Candé, das als Ferienhaus und Gästezimmer dienen kann."
    ],
    "en": [
      "Beautiful farmhouse completely renovated, on the axis Nantes, Angers, near Candé, which can accommodate in lodging and guest rooms."
    ],
    "it": [
      "Splendida fattoria ristrutturata, sull'asse Nantes-Angers, vicino a Candé, che può essere utilizzata come gîte o bed and breakfast."
    ],
    "fr": [
      "Belle longère entièrement rénovée, sur l'axe Nantes, Angers, près de Candé, pouvant accueillir en gîte et chambres d'hôtes."
    ],
    "nl": [
      "Prachtig gerenoveerde boerderij, op de as Nantes-Angers, vlakbij Candé, die gebruikt kan worden als gîte of bed and breakfast."
    ],
    "es": [
      "Hermosa granja renovada, en el eje Nantes-Angers, cerca de Candé, que puede utilizarse como casa rural o bed and breakfast."
    ]
  },
