  "rdfs:comment": {
    "de": [
      "Besuchen Sie das Dorf Beaumont und begeben Sie sich dann auf eine bukolische Wanderung mit schönen Ausblicken auf die befestigte Kirche."
    ],
    "en": [
      "Visit the village of Beaumont then go for a bucolic hike with beautiful panoramas of the fortified church."
    ],
    "it": [
      "Visitate il villaggio di Beaumont e poi fate una passeggiata bucolica con una splendida vista sulla chiesa fortificata."
    ],
    "fr": [
      "Visitez le village de Beaumont puis partez pour une randonnée bucolique avec de beaux panoramas sur l'église fortifiée."
    ],
    "es": [
      "Visite el pueblo de Beaumont y luego dé un bucólico paseo con hermosas vistas de la iglesia fortificada."
    ],
    "nl": [
      "Bezoek het dorp Beaumont en maak vervolgens een bucolische wandeling met prachtig uitzicht op de versterkte kerk."
    ]
  },
