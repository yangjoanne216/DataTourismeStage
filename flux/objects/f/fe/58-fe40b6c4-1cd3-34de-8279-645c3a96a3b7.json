  "rdfs:comment": {
    "de": [
      "Das Restaurant La Familia liegt nur wenige Schritte vom Rathaus entfernt und bietet traditionelle Küche aus dem Holzofen."
    ],
    "en": [
      "A stone's throw from the City Hall, the La Familia restaurant serves traditional cuisine cooked on a wood fire."
    ],
    "it": [
      "Situato a pochi passi dal Municipio, il ristorante La Familia offre una cucina tradizionale a legna."
    ],
    "fr": [
      "Située à quelques pas de l'Hôtel de Ville, le restaurant La Familia propose une cuisine traditionnelle au feu de bois."
    ],
    "es": [
      "Situado a pocos pasos del Ayuntamiento, el restaurante La Familia ofrece cocina tradicional de leña."
    ],
    "nl": [
      "Restaurant La Familia ligt op een steenworp afstand van het stadhuis en biedt traditionele houtgestookte gerechten."
