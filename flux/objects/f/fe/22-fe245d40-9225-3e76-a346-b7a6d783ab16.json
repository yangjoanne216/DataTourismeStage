  "rdfs:comment": {
    "de": [
      "Der Verein Chapeau! organisiert einen Je-azz-Abend in Ségala, einen Abend mit Jazzimprovisationen (Sax-Piano/Trompete). Aperitif und Bretter (Wurst/Käse) werden vor Ort angeboten."
    ],
    "en": [
      "The association Chapeau! organizes, an evening of Je-azz in Ségala, evening of Impro Jazzistique (Sax-piano-trumpet). Aperitif and boards (cold cuts/cheese) proposed on the spot."
    ],
    "it": [
      "L'associazione Chapeau! organizza una serata di improvvisazione jazz a Ségala (Sax-pianoforte-tromba). Aperitivo e assi (salumi/formaggi) proposti sul posto."
    ],
    "fr": [
      "L'association Chapeau! organise,  une soirée Je-azz en Ségala, soirée d'Impro Jazzistique (Sax-piano-trompette). Apéro et planches (charcuterie/fromage) proposés sur place."
    ],
    "es": [
      "La asociación Chapeau! organiza una velada de improvisación jazzística en Ségala (Saxo-piano-trompeta). Aperitivo y tablas (embutidos/queso) propuestos in situ."
    ],
    "nl": [
      "De vereniging Chapeau! organiseert een avond jazzimprovisatie in Ségala (Sax-piano-trompet). Aperitief en planken (vleeswaren/kaas) ter plaatse aangeboden."
    ]
  },
