  "rdfs:comment": {
    "de": [
      "T2 de 40 m² im Haus (1. Stock) mit Wohnzimmer (1 Doppelbett 140 cm), Küchenecke (elektrische Kochplatte, Ofen und Mikrowelle), Schlafzimmer mit 1 Bett für 2 Pers. 140 cm, elektrisches Vélux und ein Badezimmer mit WC. Waschmaschine und Fernseher. Zugang zum Gemeinschaftsgarten mit Gartenmöbeln und Grill. Privater Parkplatz."
    ],
    "en": [
      "T2 of 40 m² in house (1st floor) with living room (1 BZ in 140 cm), a kitchenette (electric hob, oven and microwave), a bedroom with 1 double bed in 140 cm, electric velux and a bathroom with toilet. Washing machine and TV. Access to the communal garden with garden furniture and barbecue. Private car park."
    ],
    "it": [
      "T2 di 40 m² in casa (1° piano) con soggiorno (1 BZ 140 cm), angolo cottura (piastra elettrica, forno e microonde), camera con 1 letto matrimoniale (140 cm), velux elettrico e bagno con doccia e WC. Lavatrice e TV. Accesso al giardino in comune con mobili da giardino e barbecue. Parcheggio privato."
    ],
    "fr": [
      "T2 de 40 m² dans maison (1er étage) avec salon (1 BZ en 140 cm), un coin cuisine (plaque électrique, four et micro-ondes), une chambre avec 1 lit de 2 pers. en 140 cm, vélux électrique et une salle d'eau avec WC. Lave-linge et TV. Accès au jardin commun avec salon de jardin et barbecue. Parking privé."
    ],
    "nl": [
      "T2 van 40 m² in huis (1e verdieping) met woonkamer (1 BZ 140 cm), een kitchenette (elektrisch fornuis, oven en magnetron), een slaapkamer met 1 tweepersoonsbed (140 cm), elektrische velux en een doucheruimte met WC. Wasmachine en TV. Toegang tot de gemeenschappelijke tuin met tuinmeubelen en barbecue. Privé parkeerplaats."
    ],
    "es": [
      "T2 de 40 m² en casa (1er piso) con sala de estar (1 BZ 140 cm), una cocina americana (placa eléctrica, horno y microondas), un dormitorio con 1 cama doble (140 cm), velux eléctrico y un cuarto de ducha con WC. Lavadora y televisión. Acceso al jardín compartido con muebles de jardín y barbacoa. Aparcamiento privado."
    ]
  },
