  "rdfs:comment": {
    "de": [
      "Das Clip'n Climb Grand Avignon bietet unterhaltsame Herausforderungen – genau das Richtige für Familien, Ausflüge mit Freunden, Betriebsausflüge, Firmenevents und vieles mehr ...!"
    ],
    "en": [
      "Clip'n'Climb Grand Avignon: challenging fun. Super for families, for activities among friends, to celebrate a special occasion, for company team-building and all kinds of other reasons!"
    ],
    "it": [
      "Palestra di arrampicata reinventata accessibile a tutti, dai 6 anni ai 90 anni e oltre, su una trentina di pareti colorate di 12 m di altezza e in totale sicurezza; merenda a disposizione."
    ],
    "fr": [
      "Salle d'escalade réinventée accessible à tous de 6 ans à 90 ans et plus, sur une trentaine de murs colorés de 12 m de haut et en toute sécurité; snack à disposition."
    ],
    "nl": [
      "Heruitgevonden klimhal toegankelijk voor iedereen vanaf 6 jaar tot 90 jaar, op een dertigtal gekleurde wanden van 12 m hoog en in alle veiligheid; snack beschikbaar."
    ],
    "es": [
      "Clip'n'Climb Gran Aviñón: desafíos divertidos, perfectos para las familias, salidas entre amigos, fiestas, actividades de empresa y ¡mucho más!"
    ]
  },
