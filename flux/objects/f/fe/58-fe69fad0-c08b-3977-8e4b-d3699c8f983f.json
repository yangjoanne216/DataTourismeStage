  "rdfs:comment": {
    "de": [
      "Das ideal am Cours Mirabeau gelegene Kino Le Renoir ermöglicht es Ihnen, einen kulturellen Ort im Stadtzentrum von Aix-en-Provence zu genießen."
    ],
    "en": [
      "Ideally located on the Cours Mirabeau, the cinema Le Renoir allows you to enjoy a cultural place in downtown Aix-en-Provence."
    ],
    "it": [
      "Situato in posizione ideale sul Cours Mirabeau, il cinema Renoir offre uno spazio culturale nel centro di Aix-en-Provence."
    ],
    "fr": [
      "Idéalement situé sur le cours Mirabeau, le cinéma Le Renoir permet de profiter d'un lieu culturel en centre-ville d'Aix-en-Provence."
    ],
    "es": [
      "Idealmente situado en el Cours Mirabeau, el cine Renoir ofrece un espacio cultural en el centro de Aix-en-Provence."
    ],
    "nl": [
      "De Renoir bioscoop is ideaal gelegen aan de Cours Mirabeau en biedt een cultureel trefpunt in het centrum van Aix-en-Provence."
    ]
  },
