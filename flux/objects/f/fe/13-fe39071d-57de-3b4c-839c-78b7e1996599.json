  "rdfs:comment": {
    "de": [
      "Standorte Schule in der Domaine de Bachat-Bouloud (Village du Bachat)\nFelsriegel, der sich aufgrund seiner engen Ausstattung und des Niveaus der recht einfachen Routen sehr gut für Anfänger eignet."
    ],
    "en": [
      "Bachat-Bouloud school climbing site (Village du Bachat)\r\nRocky ridge appropriate for beginners thanks to its close holds and its easy routes level."
    ],
    "it": [
      "Siti scolastici del dominio Bachat-Bouloud (villaggio Bachat)\r\nUna barra rocciosa che si presta molto bene all'iniziazione grazie alla sua attrezzatura ravvicinata e al livello delle vie abbastanza facili."
    ],
    "fr": [
      "Sites école du domaine de Bachat-Bouloud (Village du Bachat)\r\nBarre rocheuse qui se prête très bien à l'initiation de par son équipement rapproché et le niveau des voies assez faciles."
    ],
    "nl": [
      "Schoolplaatsen van het domein Bachat-Bouloud (dorp Bachat)\r\nEen rotsachtige bar die zich zeer goed leent voor initiatie vanwege de nauwe uitrusting en het niveau van de vrij gemakkelijke routes."
    ],
    "es": [
      "Lugares escolares del dominio Bachat-Bouloud (pueblo de Bachat)\r\nUna barra rocosa que se presta muy bien a la iniciación por su equipamiento cercano y el nivel de las rutas bastante fáciles."
    ]
  },
