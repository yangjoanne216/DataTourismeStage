  "rdfs:comment": {
    "de": [
      "Gourmet-Burger, hausgemachte Pommes frites, Tiramisu und viele andere Überraschungen. 100% französisches Fleisch vom Metzger, Rohkost vom Gemüsehändler und frisches Brot ohne Konservierungsstoffe."
    ],
    "en": [
      "Gourmet burgers, home fries, tiramisu and many other surprises. 100% French meat from the butcher, raw vegetables from the greengrocer and fresh bread without preservatives."
    ],
    "it": [
      "Hamburger gourmet, patatine fritte, tiramisù e molte altre sorprese. Carne 100% francese del macellaio, verdure crude del fruttivendolo e pane fresco senza conservanti."
    ],
    "fr": [
      "Burgers gourmets, frites maison, tiramisu et plein d'autres surprises. De la Viande 100% française du boucher, des crudités du primeur et du pain frais sans conservateur."
    ],
    "es": [
      "Hamburguesas gourmet, patatas fritas caseras, tiramisú y muchas otras sorpresas. Carne 100% francesa del carnicero, verduras crudas de la frutería y pan fresco sin conservantes."
    ],
    "nl": [
      "Gourmet burgers, friet, tiramisu en vele andere verrassingen. 100% Frans vlees van de slager, rauwe groenten van de groenteboer en vers brood zonder conserveringsmiddelen."
    ]
  },
