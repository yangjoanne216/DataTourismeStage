  "rdfs:comment": {
    "de": [
      "Campiingplatz am Rand der Ardèche im Schatten. Restaurant, Bar, hausgemachte Pizzas, Kinderbereich, Hüpfburg, Seilrutsche, Mobilitätwerkstatt für die kleinsten. Es besteht die Möglichkeit, Kanutouren in die Ardèche zu machen, Musikabends."
    ],
    "en": [
      "Shaded campsite situated at the riverbank. Restaurant, bar, homemade pizzas, children area, bouncy castle, zypline, mobility games for the youngest. It's possible to go down the Gorges de l'Ardèche by canoe, musical evenings."
    ],
    "it": [
      "Campeggio ben ombreggiato situato ai margini dell'Ardèche. Ristorante, bar, pizza fatta in casa, area per bambini, castello gonfiabile, teleferica.\r\nPossibilità di scendere l'Ardèche in canoa, serate musicali."
    ],
    "fr": [
      "Camping situé en bordure d'Ardèche et bien ombragé. Restaurant, bar, pizza maison, espace pour enfants, château gonflable, tyrolienne.\r\nPossibilité de faire la descente de l'Ardèche en canoë, soirées musicales."
    ],
    "nl": [
      "Schaduwrijke camping aan de rand van de Ardèche. Restaurant, bar, huisgemaakte pizza, kinderzone, springkasteel, tyrolietraverse.\r\nMogelijkheid tot kanovaren op de Ardèche, muziekavonden."
    ],
    "es": [
      "Camping bien sombreado situado al borde del Ardèche. Restaurante, bar, pizza casera, zona infantil, castillo hinchable, tirolina.\r\nPosibilidad de descender el Ardèche en canoa, veladas musicales."
    ]
  },
