  "rdfs:comment": {
    "de": [
      "Saal mit 215 Plätzen, 11 Meter breite Leinwand, digitaler Ton.\r\nFilme in nationalen Kinos während der Ferien.\r\nDigitale Projektion in 2D und 3D.\r\nFilmvorführungen für Gruppen auf Anfrage."
    ],
    "en": [
      "Hall with 215 seats, 11-metre-wide screen, digital sound.\r\nFilms released nationally during the holidays.\r\nDigital 2D and 3D projection.\r\nFilm screenings for groups on request."
    ],
    "it": [
      "Teatro da 215 posti, schermo panoramico di 11 metri, audio digitale.\r\nUscite nazionali durante le festività.\r\nProiezione digitale in 2D e 3D.\r\nProiezioni di gruppo su richiesta."
    ],
    "fr": [
      "Salle de 215 places, écran de 11 mètres de large, son numérique.\r\nFilms en sortie nationale pendant les vacances.\r\nProjection numérique en 2D et 3D.\r\nSéances cinéma pour les groupes sur demande."
    ],
    "es": [
      "Teatro con 215 asientos, pantalla ancha de 11 metros, sonido digital.\r\nLanzamientos nacionales durante las vacaciones.\r\nProyección digital en 2D y 3D.\r\nProyecciones para grupos previa solicitud."
    ],
    "nl": [
      "Bioscoop met 215 zitplaatsen, 11 meter breedbeeldscherm, digitaal geluid.\r\nFilms die nationaal worden uitgebracht tijdens de feestdagen.\r\nDigitale projectie in 2D en 3D.\r\nFilmvoorstellingen voor groepen op aanvraag."
    ]
  },
