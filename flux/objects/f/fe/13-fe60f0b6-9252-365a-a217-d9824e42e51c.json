  "rdfs:comment": {
    "de": [
      "5 Minuten von Forcalquier entfernt. Diese authentische Auberge, die in einem Bauernhof aus dem späten 18. Jahrhundert untergebracht ist, bietet in einer grünen Umgebung eine qualitativ hochwertige, traditionelle Küche."
    ],
    "en": [
      "5 minutes from Forcalquier. This authentic inn, installed in a farm from the end of the 18th century, offers a quality, traditional cuisine in a green setting."
    ],
    "it": [
      "A 5 minuti da Forcalquier. Questa autentica locanda, situata in una casa colonica della fine del XVIII secolo, offre una cucina tradizionale e di qualità in un ambiente verde."
    ],
    "fr": [
      "A 5 minutes de Forcalquier. Cette auberge authentique, installée dans une ferme de la fin du XVIIIè siècle, propose dans un cadre de verdure une cuisine de qualité, de tradition."
    ],
    "es": [
      "A 5 minutos de Forcalquier. Esta auténtica posada, situada en una granja de finales del siglo XVIII, ofrece una cocina tradicional de calidad en un entorno verde."
    ],
    "nl": [
      "5 minuten van Forcalquier. Deze authentieke herberg, gevestigd in een boerderij uit het einde van de 18e eeuw, biedt een traditionele kwaliteitskeuken in een groene omgeving."
    ]
  },
