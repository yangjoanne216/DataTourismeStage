  "rdfs:comment": {
    "de": [
      "Mehrzweckraum, in dem alle Arten von Veranstaltungen stattfinden können: Aufführungen, Messen, Mahlzeiten, Lotto..."
    ],
    "en": [
      "Multi-purpose room that can accommodate all kinds of events: shows, exhibitions, meals, bingo..."
    ],
    "it": [
      "Sala polivalente che può ospitare ogni tipo di evento: spettacoli, mostre, pasti, bingo, ecc."
    ],
    "fr": [
      "Salle polyvalente pouvant accueillir toutes sortes de manifestation : spectacle, salon, repas, loto..."
    ],
    "es": [
      "Sala polivalente que puede acoger todo tipo de eventos: espectáculos, exposiciones, comidas, bingos, etc."
    ],
    "nl": [
      "Multifunctionele zaal die geschikt is voor allerlei evenementen: voorstellingen, tentoonstellingen, maaltijden, bingo, enz."
