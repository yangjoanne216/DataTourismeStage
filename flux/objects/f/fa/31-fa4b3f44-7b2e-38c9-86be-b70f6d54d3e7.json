  "rdfs:comment": {
    "de": [
      "Besuchen Sie das Aveyron mit dem Taxi. Taxi Sébastien Bou begleitet Sie zu den verschiedenen Orten, die Sie in aller Ruhe erkunden möchten."
    ],
    "en": [
      "Visit Aveyron by cab. Cab Sébastien Bou will accompany you to the different sites you wish to discover in all serenity."
    ],
    "it": [
      "Visitate l'Aveyron in taxi. Il taxi Sébastien Bou vi accompagnerà nei vari siti che desiderate scoprire in tutta serenità."
    ],
    "fr": [
      "Visitez l'Aveyron en taxi. Taxi Sébastien Bou vous accompagne vers les différents sites que vous souhaitez découvrir en toute sérénité."
    ],
    "es": [
      "Visite el Aveyron en taxi. Taxi Sébastien Bou le acompañará a los distintos lugares que desee descubrir con toda serenidad."
    ],
    "nl": [
      "Bezoek Aveyron per taxi. Taxi Sébastien Bou begeleidt u in alle rust naar de verschillende plaatsen die u wilt ontdekken."
    ]
