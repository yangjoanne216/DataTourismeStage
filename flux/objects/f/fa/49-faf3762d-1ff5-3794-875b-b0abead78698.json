  "rdfs:comment": {
    "de": [
      "Entdecken Sie den Park \"historisches Monument\" des Schlosses Lathan in Breil"
    ],
    "en": [
      "Discover the park \"historical monument\" of the castle of Lathan in Breil"
    ],
    "it": [
      "Scoprite il parco \"monumento storico\" del Castello di Lathan a Breil"
    ],
    "fr": [
      "Découvrez le Parc \"monument historique\" du château de Lathan à Breil"
    ],
    "nl": [
      "Ontdek het \"historisch monument\" park van het Château de Lathan in Breil"
    ],
