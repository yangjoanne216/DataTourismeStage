  "rdfs:comment": {
    "de": [
      "Escape Game Sévérac ... Die Zeit läuft Ihnen davon, um das Geheimnis des Alchemisten zu lüften, sonst... werden Sie in den Mauern des Museums zu Fossilien."
    ],
    "en": [
      "Escape Game Sévérac... Time is running out for you to discover the secret of the Alchemist, otherwise... you will become fossils in the walls of the museum."
    ],
    "it": [
      "Gioco di fuga Sévérac... Il tempo stringe per scoprire il segreto dell'Alchimista, altrimenti... diventerete un fossile tra le mura del museo."
    ],
    "fr": [
      "Escape Game Sévérac... Le temps vous est compté pour percer le secret de l'Alchimiste, sinon... vous deviendrez fossiles dans les murs du musée."
    ],
    "nl": [
      "Ontsnappingsspel Sévérac... De tijd dringt om het geheim van de Alchemist te ontdekken, anders... word je een fossiel binnen de muren van het museum."
    ],
    "es": [
      "Juego de Escape Sévérac... Se te acaba el tiempo para descubrir el secreto del Alquimista, de lo contrario... te convertirás en un fósil entre las paredes del museo."
    ]
