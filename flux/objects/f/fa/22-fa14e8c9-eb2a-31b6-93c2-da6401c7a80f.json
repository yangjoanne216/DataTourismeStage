  "rdfs:comment": {
    "de": [
      "Lassen Sie sich an noch zeitgenössischen Fertigkeiten teilhaben: Vorführungen von Spinnen und Weben, Korbflechten, Töpfern, dem Beruf des Buchdruckers.."
    ],
    "en": [
      "Come and share contemporary know-how: demonstrations of spinning and weaving, basketry, pottery, the trade of the typographer printer.."
    ],
    "it": [
      "Venite a condividere le vostre abilità contemporanee: dimostrazioni di filatura e tessitura, cesteria, ceramica, stampa tipografica?"
    ],
    "fr": [
      "Venez partager des savoir-faire encore contemporains : démonstrations de filage et de tissage, de vannerie, de poterie, du métier de l’imprimeur typographe…"
    ],
    "es": [
      "Venga a compartir sus habilidades contemporáneas: demostraciones de hilado y tejido, cestería, cerámica, impresión tipográfica.."
    ],
    "nl": [
      "Kom delen in de vaardigheden van hedendaagse ambachtslieden: demonstraties van spinnen en weven, mandenvlechten, pottenbakken, boekdrukkunst.."
    ]
  },
