  "rdfs:comment": {
    "de": [
      "Vom Feld auf den Teller! Die Gelegenheit für einen Ausflug ins Freie, um Groß und Klein eine Freude zu machen."
    ],
    "en": [
      "From field to plate! An opportunity to enjoy an outdoor outing for young and old alike."
    ],
    "it": [
      "Dal campo al piatto! Un'occasione per godersi una gita all'aperto per grandi e piccini."
    ],
    "fr": [
      "Du champ à l'assiette ! L’occasion d'une sortie en plein air pour faire plaisir aux petits et aux grands."
    ],
    "es": [
      "¡Del campo al plato! Una oportunidad para disfrutar de una excursión al aire libre para jóvenes y mayores."
    ],
    "nl": [
      "Van het veld naar de plaat! Een gelegenheid voor een uitje in de buitenlucht waar jong en oud plezier aan beleven."
    ]
  },
