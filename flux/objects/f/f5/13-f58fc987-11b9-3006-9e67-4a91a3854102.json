  "rdfs:comment": {
    "de": [
      "Strategischer Vorsprung, ein echter Wächter, der den Durchgang nach Italien kontrolliert. Das Fort d'Aiton wurde 1875-1880 auf den Ruinen eines Bischofspalastes errichtet."
    ],
    "en": [
      "Strategic prominence, a veritable sentinel controlling the passage to Italy. The Fort of Aiton was built in 1875-1880, on the ruins of an episcopal palace."
    ],
    "it": [
      "Una posizione strategica, una vera e propria sentinella che controlla il passaggio verso l'Italia. Il Forte di Aiton fu costruito nel 1875-1880, sulle rovine di un palazzo episcopale."
    ],
    "fr": [
      "Proéminence stratégique, véritable sentinelle contrôlant le passage vers l'Italie. Le Fort d'Aiton a été construit en 1875-1880, sur les ruines d'un palais épiscopal."
    ],
    "es": [
      "Una prominencia estratégica, un verdadero centinela que controla el paso hacia Italia. El Fuerte de Aiton fue construido en 1875-1880, sobre las ruinas de un palacio episcopal."
    ],
    "nl": [
      "Strategische positie, een echte schildwacht die de doorgang naar Italië controleert. Het Aiton Fort werd gebouwd in 1875-1880, op de ruïnes van een bisschoppelijk paleis."
    ]
  },
