  "rdfs:comment": {
    "de": [
      "32 Aussteller :\n\n- handgemachte Nudeln, Produkte von fetten Enten, Gewürze, Honig, Wein, Bier, Walnussöl, Haselnüsse...\n\n- schmuck, Glasgravur, Holzarbeiten, Seifen, Modeaccessoires, Taschen, Spiele, Bücher\n\nDonnerstag: Vormittags Signierstunden lokaler Autoren"
    ],
    "en": [
      "32 exhibitors:\n\n- handmade pasta, duck products, spices, honey, wines, beers, walnut oils, hazelnuts...\n\n- jewelry, glass engraving, woodwork, soaps, fashion accessories, bags, games, books\n\nThursday: signing sessions with local authors"
    ],
    "it": [
      "32 espositori:\n\n- pasta fatta in casa, prodotti di anatra, spezie, miele, vino, birra, olio di noci, nocciole...\n\n- gioielli, incisioni su vetro, lavori in legno, saponi, accessori moda, borse, giochi, libri, ecc\n\nGiovedì: sessioni di autografi con autori locali"
    ],
    "fr": [
      "32 exposants :\n\n- pâtes artisanales, produits de canards gras, épices, miel, vins, bières, huiles de noix, noisettes...\n\n- bijoux, gravure sur verre, travail du bois, savons, accessoires de mode, sacs, jeux, livres \n\nJeudi : matinée dédicaces des auteurs locaux"
    ],
    "es": [
      "32 expositores:\n\n- pasta casera, productos de pato, especias, miel, vino, cerveza, aceite de nuez, avellanas...\n\n- bisutería, grabado en vidrio, carpintería, jabones, accesorios de moda, bolsos, juegos, libros, etc\n\nJueves: sesiones de firmas con autores locales"
    ],
    "nl": [
      "32 exposanten:\n\n- huisgemaakte pasta, eendenproducten, kruiden, honing, wijn, bier, walnootolie, hazelnoten...\n\n- juwelen, glasgravure, houtbewerking, zeep, modeaccessoires, tassen, spelletjes, boeken, enz\n\nDonderdag: signeersessies met lokale auteurs"
    ]
  },
