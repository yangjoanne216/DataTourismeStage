  "rdfs:comment": {
    "de": [
      "Louis Retaud bietet Ihnen einen Einblick in seine Familien-Fischzucht."
    ],
    "en": [
      "Louis Retaud invites you to discover his family fish farm."
    ],
    "it": [
      "Louis Retaud vi invita a scoprire la sua azienda ittica di famiglia."
    ],
    "fr": [
      "Louis Retaud vous propose de découvrir sa pisciculture familiale."
    ],
    "nl": [
      "Louis Retaud nodigt u uit om zijn familie viskwekerij te ontdekken."
    ],
    "es": [
      "Louis Retaud le invita a descubrir su piscifactoría familiar."
    ]
  },
