  "rdfs:comment": {
    "de": [
      "Nehmen Sie an einer Themenwanderung teil! Wanderung durch den Wald und Vorführung der Weidenbearbeitung durch die Korbflechterin Sonia Maupas."
    ],
    "en": [
      "Come and participate in a themed hike! Walk in the heart of the forest and demonstration of wicker work by Sonia Maupas, basket maker."
    ],
    "it": [
      "Partecipate a una passeggiata a tema! Passeggiata nel cuore della foresta e dimostrazione di lavorazione del vimini da parte di Sonia Maupas, cestaia."
    ],
    "fr": [
      "Venez participer à une randonnée à thème ! Balade au cœur de la forêt et démonstration du travail de l'osier par Sonia Maupas, vannière."
    ],
    "es": [
      "¡Venga a participar en un paseo temático! Paseo por el corazón del bosque y demostración de cestería a cargo de Sonia Maupas, cestera."
    ],
    "nl": [
      "Kom deelnemen aan een themawandeling! Wandeling in het hart van het bos en demonstratie van vlechtwerk door Sonia Maupas, mandenmaker."
    ]
  },
