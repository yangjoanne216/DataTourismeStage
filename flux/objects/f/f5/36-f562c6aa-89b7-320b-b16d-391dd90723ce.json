  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/02ea2212-a1aa-3699-a787-015ee5bd490b",
      "dc:description": {
        "de": [
          "Verkauf von lokalen Produkten: Porc Noir de Bigorre (Schinken, Wurst, Pastete...), Garbure, Daube, Noire d'Astarac de Bigorre, Gewürze, Safrans, Marmeladen, Kuchen am Spieß, lokale Biere, lokale Aperitifs, lokale Weine, Liköre aus den Pyrenäen."
        ],
        "en": [
          "Sale of local products : Porc Noir de Bigorre (ham, sausage, pâté...), Garbure, Daube, Noire d'Astarac de Bigorre, condiments, saffron, jams, spit cakes, local beers, local aperitifs, local wines, liqueurs from the Pyrenees."
        ],
        "it": [
          "Vendita di prodotti locali: Porc Noir de Bigorre (prosciutto, salsiccia, pâté...), Garbure, Daube, Noire d'Astarac de Bigorre, condimenti, zafferano, marmellate, dolci allo spiedo, birre locali, aperitivi locali, vini locali, liquori pirenaici"
        ],
        "fr": [
          "Vente de produits locaux : Porc Noir de Bigorre (jambon, saucisson, pâté...), Garbure, Daube, Noire d'Astarac de Bigorre, condiments, safrans, confitures gâteaux à la broche, bières locales, apéritifs locaux, vins locaux, liqueurs des Pyrénées."
        ],
        "nl": [
          "Verkoop van lokale producten: Porc Noir de Bigorre (ham, worst, paté...), Garbure, Daube, Noire d'Astarac de Bigorre, specerijen, saffraan, jam, spiesgebak, lokale bieren, lokale aperitieven, lokale wijnen, likeuren uit de Pyreneeën..."
        ],
        "es": [
          "Venta de productos locales: Porc Noir de Bigorre (jamón, salchicha, paté...), Garbure, Daube, Noire d'Astarac de Bigorre, condimentos, azafrán, mermeladas, pasteles asados, cervezas locales, aperitivos locales, vinos locales, licores pirenaicos"
        ]
      },
