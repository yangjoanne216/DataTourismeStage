  "rdfs:comment": {
    "de": [
      "Alexis und Baptiste bewirtschaften in der Gemeinde Teulat 30 ha biologisch. Der auf dem Land erzeugte Weizen wird für die Herstellung von Sauerteigbrot verwendet."
    ],
    "en": [
      "Alexis and Baptiste cultivate 30 ha in organic farming in the commune of Teulat. The wheat produced on the land is used to make sourdough bread."
    ],
    "it": [
      "Alexis e Baptiste coltivano 30 ettari di terreno in agricoltura biologica nel comune di Teulat. Il grano prodotto in questa terra viene utilizzato per produrre pane a lievitazione naturale."
    ],
    "fr": [
      "Alexis et Baptiste cultivent 30 ha en agriculture biologique sur la commune de Teulat. Le blé produit sur les terres sert à la fabrication de pain au levain."
    ],
    "es": [
      "Alexis y Baptiste cultivan 30 hectáreas en agricultura ecológica en el municipio de Teulat. El trigo producido en la tierra se utiliza para hacer pan de masa madre."
    ],
    "nl": [
      "Alexis en Baptiste bewerken 30 hectare grond in biologische landbouw in de gemeente Teulat. Van de op het land geproduceerde tarwe wordt zuurdesembrood gemaakt."
    ]
  },
