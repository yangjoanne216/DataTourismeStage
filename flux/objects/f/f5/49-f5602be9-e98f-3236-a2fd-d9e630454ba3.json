  "rdfs:comment": {
    "de": [
      "Die Domaine des Pierres Jumelles ist ein Ökotourismus-Projekt: Die Schönheit des Umwelterbes soll durch ethisches Reiten erlebbar gemacht werden."
    ],
    "en": [
      "The Domaine des Pierres Jumelles is an eco-tourism project: to make people discover the beauty of the environmental heritage through ethical horse riding."
    ],
    "it": [
      "Il Domaine des Pierres Jumelles è un progetto di ecoturismo: scoprire la bellezza del nostro patrimonio ambientale attraverso l'equitazione etica."
    ],
    "fr": [
      "Le Domaine des Pierres Jumelles est un projet d'éco-tourisme : faire découvrir la beauté du patrimoine environnemental par le biais de l'équitation éthique."
    ],
    "nl": [
      "Het Domaine des Pierres Jumelles is een ecotoeristisch project: het ontdekken van de schoonheid van ons ecologisch erfgoed door middel van ethisch verantwoord paardrijden."
    ],
    "es": [
      "El Domaine des Pierres Jumelles es un proyecto de ecoturismo: descubrir la belleza de nuestro patrimonio medioambiental a través de la equitación ética."
    ]
  },
