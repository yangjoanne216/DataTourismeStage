  "rdfs:comment": {
    "de": [
      "Große, neue und moderne 2-Zimmer-Wohnung im Erdgeschoss, 4 km von den Thermen entfernt. Im Herzen des Dorfes, gemütlicher und ruhiger Ort, kleiner Außenbereich. 1 Bett 140+ elektrische Matratze."
    ],
    "en": [
      "Large new and modern T2 on the ground floor, 4km from the thermal baths. In the heart of the village, cosy and quiet place, small outside. 1 bed 140+ electric mattress."
    ],
    "it": [
      "Grande T2 nuovo e moderno al piano terra, a 4 km dalle terme. Nel cuore del villaggio, luogo accogliente e tranquillo, esterno piccolo. 1 letto 140+ materasso elettrico."
    ],
    "fr": [
      "Grand T2 neuf et moderne RDC, 4km des Thermes. Au coeur du village, endroit douillet et calme, petit extérieur. 1 lit 140+ matelas électrique."
    ],
    "nl": [
      "Grote nieuwe en moderne T2 op de begane grond, 4km van de thermale baden. In het hart van het dorp, gezellige en rustige plaats, kleine buitenkant. 1 bed 140+ elektrische matras."
    ],
    "es": [
      "Gran T2 nueva y moderna en la planta baja, a 4 km de los baños termales. En el corazón del pueblo, lugar acogedor y tranquilo, exterior pequeño. colchón eléctrico de 1 cama 140+."
    ]
  },
