  "rdfs:comment": {
    "de": [
      "Typischer Bauernhof in den Vogesen mit Verarbeitung und Verkauf von Produkten (Münsterkäse, Tomme des Vosges, ...), die von Kühen der Vogesenrasse stammen, vor Ort.  Gemolken wird von 16:00 bis 18:00 Uhr."
    ],
    "en": [
      "Typical Vosges farm with on-site processing and sale of products (munster, Vosges tomme, etc.) from Vosges cows.  Milking is from 4pm to 6pm."
    ],
    "it": [
      "Tipica fattoria dei Vosgi con lavorazione e vendita in loco dei prodotti (Munster, tomme Vosges, ecc.) delle mucche dei Vosgi.  La mungitura avviene dalle 16.00 alle 18.00."
    ],
    "fr": [
      "Ferme typique des Vosges avec transformation et vente sur place de produits (munster, tomme des Vosges, ...) issus des vaches de race vosgienne.  La traite s'effectue de 16h à 18h."
    ],
    "nl": [
      "Typische Vogezische boerderij met verwerking en verkoop ter plaatse van producten (Munster, Vogezische tomme, enz.) van Vogezische koeien.  Het melken vindt plaats van 16.00 tot 18.00 uur."
    ],
    "es": [
      "Granja típica de los Vosgos con transformación y venta in situ de productos (Munster, tomme de los Vosgos, etc.) procedentes de vacas de los Vosgos.  El ordeño tiene lugar de 16:00 a 18:00."
    ]
  },
