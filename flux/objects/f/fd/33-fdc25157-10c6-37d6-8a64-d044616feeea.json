  "rdfs:comment": {
    "de": [
      "In Portet-sur-Garonne bietet Ihnen das Team des Café Paul eine traditionelle Küche, in der Grillspezialitäten aus dem Holzofen eine wichtige Rolle spielen."
    ],
    "en": [
      "In Portet-sur-Garonne, the team of the café Paul offers you a traditional cuisine that gives pride of place to wood-fired grills."
    ],
    "it": [
      "A Portet-sur-Garonne, il team del Café Paul offre una cucina tradizionale con particolare attenzione alle grigliate a legna."
    ],
    "fr": [
      "A Portet-sur-Garonne, l'équipe du café Paul vous propose une cuisine traditionnelle qui fait la part belle aux grillades au feu de bois."
    ],
    "nl": [
      "In Portet-sur-Garonne biedt het team van Café Paul een traditionele keuken met de nadruk op houtgestookte grillgerechten."
    ],
    "es": [
      "En Portet-sur-Garonne, el equipo del Café Paul ofrece una cocina tradicional centrada en las parrilladas de leña."
    ]
  },
