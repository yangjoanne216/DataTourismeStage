      "dc:description": {
        "de": [
          "300m von den Thermen entfernt",
          "Das Haus liegt in Bagnères de Bigorre, in der Nähe aller Geschäfte.",
          "Im Erdgeschoss befindet sich eine separate Küche mit einem Bett (140 cm) und einem Sofa."
        ],
        "en": [
          "300m from the thermal baths",
          "Located in Bagnères de Bigorre, close to all shops.",
          "On the ground floor, independent kitchen, with a bed in 140 and a sofa."
        ],
        "it": [
          "A 300 m dalle terme",
          "Al piano terra, cucina indipendente, con letto matrimoniale e divano.",
          "Situato a Bagnères de Bigorre, vicino a tutti i negozi."
        ],
        "fr": [
          "A 300m des thermes",
          "Au rez de chaussée, cuisine indépendante, avec un lit en 140 et un canapé.",
          "Situé à Bagnères de Bigorre, à proximité de tous les commerces."
        ],
        "es": [
          "A 300 metros de los baños termales",
          "En la planta baja, cocina independiente, con una cama doble y un sofá.",
          "Situado en Bagnères de Bigorre, cerca de todos los comercios."
        ],
        "nl": [
          "300m van de thermale baden",
          "Gelegen in Bagnères de Bigorre, dicht bij alle winkels.",
          "Op de begane grond, onafhankelijke keuken, met een tweepersoonsbed en een bank."
        ]
      },
