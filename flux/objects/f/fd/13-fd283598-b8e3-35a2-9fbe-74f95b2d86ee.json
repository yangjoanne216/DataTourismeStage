  "rdfs:comment": {
    "de": [
      "Les Aventurières du Goût, das sind Brigitte und Odile, zwei leidenschaftliche Pflanzenliebhaberinnen und Liebhaber der natürlichen Küche, die Sie einladen, essbare Wildpflanzen und -blumen zu entdecken."
    ],
    "en": [
      "Les Aventurières du Goût (\"the Taste Adventurers\") are Brigitte and Odile, two fans of plants and natural cooking who invite you to learn about edible\r\nwildflowers and plants."
    ],
    "it": [
      "Les Aventurières du Goût sono Brigitte e Odile, due appassionate di piante e cucina naturale che vi invitano a scoprire piante commestibili e fiori selvatici."
    ],
    "fr": [
      "Les Aventurières du Goût, ce sont Brigitte et Odile, deux passionnées de plantes et de cuisine naturelle qui vous invitent à découvrir les plantes et les fleurs sauvages comestibles."
    ],
    "nl": [
      "Les Aventurières du Goût zijn Brigitte en Odile, twee liefhebbers van planten en natuurlijk koken die u uitnodigen om eetbare planten en wilde bloemen te ontdekken."
    ],
    "es": [
      "Les Aventurières du Goût son Brigitte y Odile, dos entusiastas de las plantas y la cocina natural que le invitan a descubrir las plantas comestibles y las flores silvestres."
    ]
  },
  "rdfs:label": {
