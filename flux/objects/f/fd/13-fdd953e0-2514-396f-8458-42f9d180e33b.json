  "rdfs:comment": {
    "de": [
      "Villlefranche-d'Allier verfügt über ein Aqua-Recreation-Center, das Groß und Klein begeistert: offenes Becken, Wasserrutsche, Planschbecken, Duschpilz."
    ],
    "en": [
      "Villlefranche-d'Allier has an aqua-recreational centre that will delight young and old alike: open-air pool, water slide, paddling pool, mushroom shower."
    ],
    "it": [
      "Villlefranche-d'Allier dispone di un centro acquatico-ricreativo che farà la gioia di grandi e piccini: piscina all'aperto, scivolo, piscina per bambini, doccia a fungo."
    ],
    "fr": [
      "Villlefranche-d'Allier s'est doté d'un centre aqua-récréatif qui ravira petits et grands : bassin découvert, toboggan aquatique, pataugeoire, champignon douche."
    ],
    "nl": [
      "Villlefranche-d'Allier heeft een waterrecreatiecentrum dat jong en oud zal bekoren: openluchtzwembad, waterglijbaan, peuterbad, paddenstoelendouche."
    ],
    "es": [
      "Villlefranche-d'Allier cuenta con un centro acuático-recreativo que hará las delicias de grandes y pequeños: piscina al aire libre, tobogán acuático, piscina infantil, ducha de setas."
    ]
  },
  "rdfs:label": {
    "fr": [
