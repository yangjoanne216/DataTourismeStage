  "rdfs:comment": {
    "de": [
      "Begeben Sie sich mit Freunden oder Sportlern auf eine 22 km lange Strecke, um die verschiedenen Landschaften der Loire zu entdecken!"
    ],
    "en": [
      "Take a 22 km trip with friends or sportsmen to discover the different landscapes of the Loire!"
    ],
    "it": [
      "Fate una gita di 22 km con gli amici o gli sportivi per scoprire i diversi paesaggi della Loira!"
    ],
    "fr": [
      "Faites un parcours de 22 km entre amis ou entre sportifs pour découvrir les différents paysages de la Loire !"
    ],
    "es": [
      "Realice una excursión de 22 km con amigos o deportistas para descubrir los diferentes paisajes del Loira"
    ],
    "nl": [
      "Maak een tocht van 22 km met vrienden of sportievelingen om de verschillende landschappen van de Loire te ontdekken!"
    ]
