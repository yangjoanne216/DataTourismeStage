  "rdfs:comment": {
    "de": [
      "Hotel - Restaurant mit Terrasse am Ufer der Creuse (Staudamm von Eguzon), mit Blick auf die Ruinen von Crozant. Verleih von Kajaks, Kanus und Paddles. Abfahrt des Ausflugsboots auf dem See von Eguzon."
    ],
    "en": [
      "Bar/hotel/restaurant beside Lake Eguzon, with views over the Crozant ruins. Terrace; group reception."
    ],
    "it": [
      "Hotel - Ristorante con terrazza sulle rive della Creuse (diga di Eguzon), con vista sulle rovine di Crozant. Noleggio di kayak, canoe e pagaie. Gita in barca sul lago di Eguzon."
    ],
    "fr": [
      "Hôtel - Restaurant avec terrasse au bord de la Creuse (retenue du barrage d'Eguzon), avec vue sur les ruines de Crozant. Location kayaks canoës et paddles. Départ bateau-promenade sur le lac d'Eguzon."
    ],
    "es": [
      "Hotel - Restaurante con terraza a orillas del Creuse (presa de Eguzon), con vistas a las ruinas de Crozant. Alquiler de kayaks, canoas y remos. Paseo en barco por el lago de Eguzon."
    ],
    "nl": [
      "Hotel - Restaurant met terras aan de oever van de Creuse (stuwdam van Eguzon), met uitzicht op de ruïnes van Crozant. Kajak-, kano- en peddelverhuur. Boottocht op het meer van Eguzon."
    ]
  },
