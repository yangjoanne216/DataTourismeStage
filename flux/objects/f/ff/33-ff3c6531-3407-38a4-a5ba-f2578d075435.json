  "rdfs:comment": {
    "de": [
      "Äthiopisches Restaurant in Toulouse. Wir bieten traditionelle Gerichte mit Gemüse, Hülsenfrüchten und Fleisch an, die auf einem großen Fladenbrot namens Injera serviert werden."
    ],
    "en": [
      "Ethiopian restaurant in Toulouse. We offer traditional dishes based on vegetables, legumes and meats served on a large wafer called Injera."
    ],
    "it": [
      "Ristorante etiope a Tolosa. Offriamo piatti tradizionali a base di verdure, legumi e carni serviti su una grande cialda chiamata Injera."
    ],
    "fr": [
      "Restaurant Ethiopien sur Toulouse. Nous proposons des plats traditionnels à base de légumes , légumineuses et viandes servis sur une grande galette nommée Injera."
    ],
    "es": [
      "Restaurante etíope en Toulouse. Ofrecemos platos tradicionales a base de verduras, legumbres y carnes servidos en una gran oblea llamada Injera."
    ],
    "nl": [
      "Ethiopisch restaurant in Toulouse. Wij bieden traditionele gerechten op basis van groenten, peulvruchten en vlees geserveerd op een grote wafel genaamd Injera."
    ]
