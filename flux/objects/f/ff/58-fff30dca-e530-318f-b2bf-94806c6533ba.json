  "rdfs:comment": {
    "de": [
      "Die Synagoge von Aix-en-Provence befindet sich im Herzen der Stadt, nur wenige Schritte von der Rotonde in Richtung Busbahnhof entfernt."
    ],
    "en": [
      "The Synagogue of Aix-en-Provence is located in the heart of the city, a few steps from the Rotunda in the direction of the bus station."
    ],
    "it": [
      "La Sinagoga di Aix-en-Provence si trova nel cuore della città, a pochi passi dalla Rotonda verso la stazione degli autobus."
    ],
    "fr": [
      "La Synagogue d'Aix-en-Provence est située au coeur de la ville, à quelques pas de la Rotonde en direction de la gare routière."
    ],
    "es": [
      "La Sinagoga de Aix-en-Provence está situada en el corazón de la ciudad, a pocos pasos de la Rotonda hacia la estación de autobuses."
    ],
    "nl": [
      "De Synagoge van Aix-en-Provence ligt in het hart van de stad, op een steenworp afstand van de Rotunda naar het busstation."
    ]
  },
