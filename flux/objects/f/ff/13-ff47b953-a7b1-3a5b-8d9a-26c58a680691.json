  "rdfs:comment": {
    "de": [
      "Sport-Pub, einzigartiges Konzept in der Drôme Ardèche! Brasserie mittags und abends. Sportübertragungen auf Großbildschirmen. 500 m² Vergnügen zum Teilen! An 6 Tagen in der Woche mittags und abends geöffnet!"
    ],
    "en": [
      "Sports pub, unique concept in Drôme Ardèche! Brasserie for lunch and dinner. Sports broadcasts on giant screens. 500 m² of pleasure to share! Open 6 days a week for lunch and dinner!"
    ],
    "it": [
      "Pub sportivo, concetto unico nella Drôme Ardèche! Brasserie per pranzo e cena. Trasmissioni sportive su schermi giganti. 500 m² di piacere da condividere! Aperto 6 giorni alla settimana per pranzo e cena!"
    ],
    "fr": [
      "Pub sportif, concept unique en Drôme Ardèche ! Brasserie à midi et soir. Retransmissions sportives sur écrans géants. 500 m² de plaisirs à partager ! Ouvert 6 jours sur 7 midi et soir !"
    ],
    "nl": [
      "Sportcafé, uniek concept in de Drôme Ardèche! Brasserie voor lunch en diner. Sportuitzendingen op gigantische schermen. 500 m² plezier om te delen! 6 dagen per week geopend voor lunch en diner!"
    ],
    "es": [
      "Pub deportivo, ¡concepto único en la Drôme Ardèche! Brasserie para el almuerzo y la cena. Retransmisiones deportivas en pantallas gigantes. ¡500 m² de placer para compartir! Abierto 6 días a la semana para comer y cenar"
    ]
  },
