      "dc:description": {
        "de": [
          "1 Wohnküche mit Mikrowelle, Waschmaschine, Fernseher, DVD-Player, CD-Radio. 1 Schlafzimmer mit 1 Bett 140. 1 Badezimmer mit Dusche. Zentralheizung mit Gas. Zugang für Behinderte. Haustiere erlaubt."
        ],
        "en": [
          "Kitchen/living room with microwave, washing machine, TV, DVD player, radio CD. 1 bedroom with double bed, shower room, gas central heating. Disabled access. Pets accepted."
        ],
        "it": [
          "1 soggiorno cucina con microonde, lavatrice, televisione, lettore DVD, radio CD. 1 camera con 1 letto matrimoniale. 1 bagno con doccia. Riscaldamento centrale a gas. Accesso per i disabili. Animali accettati."
        ],
        "fr": [
          "1 cuisine séjour avec micro-ondes, lave-linge, télévision, lecteur DVD, radio CD. 1 chambre avec 1 lit 140. 1 salle d'eau avec douche. Chauffage central gaz. Accès handicapés. Animaux acceptés."
        ],
        "nl": [
          "1 woonkamer keuken met magnetron, wasmachine, televisie, DVD-speler, radio CD. 1 slaapkamer met 1 tweepersoonsbed. 1 badkamer met douche. Centrale verwarming op gas. Toegang voor gehandicapten. Dieren geaccepteerd."
        ],
        "es": [
          "1 cocina salon con microondas, lavadora, television, dvd, radio. 1 habitacion con 1 cama 140. 1 baño con ducha. Animales admitidos."
        ]
      },
