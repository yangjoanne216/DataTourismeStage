  "rdfs:comment": {
    "de": [
      "Christine Ladevèze, Bildhauerin-Keramikerin in Igny, kreiert Gebrauchs- und Dekorationsgegenstände aus Steinzeug mit hoher Temperatur: Schalen, Vasen, Teller. sowie Skulpturen."
    ],
    "en": [
      "Located in Igny, Christine Ladevèze creates a lot of items, either useful of décorative, in high temperature stoneware."
    ],
    "it": [
      "Christine Ladevèze, scultrice-ceramista con sede a Igny, crea oggetti utilitari e decorativi in gres ad alta temperatura: ciotole, vasi, piatti e sculture."
    ],
    "fr": [
      "Christine Ladevèze, Sculpteur-Céramiste installée à Igny, crée des objets utilitaires et décoratifs en grès de haute température : bols, vases, plats. ainsi que des sculptures."
    ],
    "es": [
      "Christine Ladevèze, escultora-ceramista afincada en Igny, crea objetos utilitarios y decorativos en gres de alta temperatura: cuencos, jarrones, platos, así como esculturas."
    ],
    "nl": [
      "Christine Ladevèze, beeldhouwster-keramiste uit Igny, creëert gebruiks- en siervoorwerpen in hoge temperatuur steengoed: kommen, vazen, schalen, alsook sculpturen."
    ]
  },
