  "rdfs:comment": {
    "de": [
      "Das Café Columbus liegt im Herzen des historischen Zentrums von Aix-en-Provence und bietet eine große Auswahl an Kaffee, amerikanisch inspiriertem Gebäck und herzhaften Sandwiches und Bagels."
    ],
    "en": [
      "Located in the heart of the historic center of Aix-en-Provence, the Columbus café offers a wide selection of coffees, American-inspired pastries and savory sandwiches and bagels."
    ],
    "it": [
      "Situato nel cuore del centro storico di Aix-en-Provence, il caffè Columbus offre un'ampia scelta di caffè, dolci di ispirazione americana, panini salati e bagel."
    ],
    "fr": [
      "Situé au coeur du centre historique d'Aix-en-Provence, le café Columbus propose un large choix de cafés, de pâtisseries aux inspirations américaines et de sandwichs et bagels salés."
    ],
    "es": [
      "Situado en el corazón del centro histórico de Aix-en-Provence, el café Columbus ofrece una amplia selección de cafés, bollería de inspiración americana y sabrosos sándwiches y bagels."
    ],
    "nl": [
      "Het Columbus café ligt in het hart van het historische centrum van Aix-en-Provence en biedt een ruime keuze aan koffie, Amerikaans geïnspireerd gebak en hartige sandwiches en bagels."
    ]
  },
