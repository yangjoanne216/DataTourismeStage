  "COVID19SpecialMeasures": {
    "de": [
      "Maskenpflicht ,Desinfektion der Zimmer nach jeder Vermietung, verstärkte Reinigung von Bettwäsche und Handtüchern."
    ],
    "en": [
      "Mandatory mask, disinfection of rooms after each rental, cleaning of bed linen and towels reinforced."
    ],
    "it": [
      "Maschera obbligatoria, disinfezione delle camere dopo ogni affitto, pulizia della biancheria da letto e degli asciugamani rinforzata."
    ],
    "fr": [
      "Masque obligatoire ,désinfection de chambres après chaque location, nettoyage du linge de lit et de toilette renforcé."
    ],
    "nl": [
      "Verplicht masker, desinfectie van de kamers na elke verhuur, schoonmaak van beddengoed en handdoeken versterkt."
    ],
    "es": [
      "Mascarilla obligatoria, desinfección de las habitaciones después de cada alquiler, refuerzo de la limpieza de la ropa de cama y las toallas."
    ]
  },
