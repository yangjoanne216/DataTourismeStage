  "rdfs:comment": {
    "de": [
      "Ferienhaus auf 2 Ebenen (63 m²/4 Pers.), unabhängig, in CORCELLES (Gemeinde Champdor-Corcelles)\r\nEG: Küche, Badezimmer, WC. Etage: 2 Schlafzimmer (1 Doppelbett, 2 Etagenbetten). Elektrische Heizung mit Heizstrahler. Restauration in 4 km Entfernung."
    ],
    "en": [
      "Gite on 2 levels (63 m²/4 pers), independent, in CORCELLES (commune Champdor-Corcelles)\r\nGround floor : Kitchen, shower room, WC. Floor : 2 bedrooms (1 double bed, 2 bunk beds). Radiant electric heating. Restaurant at 4 km."
    ],
    "it": [
      "Gîte su 2 livelli (63 m²/4 pers), indipendente, a CORCELLES (comune di Champdor-Corcelles)\r\nPiano terra: cucina, bagno con doccia, WC. Primo piano: 2 camere da letto (1 letto matrimoniale, 2 letti a castello). Riscaldamento elettrico radiante. Ristorante a 4 km."
    ],
    "fr": [
      "Gîte sur 2 niveaux (63 m²/4 pers), indépendant, à CORCELLES (commune Champdor-Corcelles) \r\nRDC : Cuisine, salle d'eau, WC. Etage : 2 chambres (1 lit 2 pers, 2 lits superposés). Chauffage électrique rayonnant. Restauration à 4 km."
    ],
    "nl": [
      "Gîte op 2 niveaus (63 m²/4 pers), onafhankelijk, in CORCELLES (gemeente Champdor-Corcelles)\r\nBegane grond: keuken, doucheruimte, WC. Eerste verdieping: 2 slaapkamers (1 tweepersoonsbed, 2 stapelbedden). Elektrische verwarming. Restaurant op 4 km."
    ],
    "es": [
      "Casa rural en 2 niveles (63 m²/4 pers), independiente, en CORCELLES (commune Champdor-Corcelles)\r\nPlanta baja: cocina, cuarto de ducha, WC. Primera planta: 2 habitaciones (1 cama doble, 2 literas). Calefacción eléctrica radiante. Restaurante a 4 km."
    ]
  },
