  "rdfs:comment": {
    "de": [
      "In Argenton-sur-Creuse begrüßen Sie Josépha und Frédéric seit dem 1. September 2013 in einem neuen Reitzentrum : L'Ecurie de l'äge\" im Tal der Creuse."
    ],
    "en": [
      "In Argenton-sur-Creuse, Josépha and Frédéric have been welcoming visitors to their new horse riding centre, “L'Ecurie de l'äge\", in the valley of the Creuse since 1 September 2013."
    ],
    "it": [
      "Ad Argenton-sur-Creuse, Josépha e Frédéric vi accolgono dal 1° settembre 2013 in un nuovo centro equestre: L'Ecurie de l'äge\" nella valle della Creuse."
    ],
    "fr": [
      "A Argenton-sur-Creuse, Josépha et Frédéric vous accueillent depuis le 1er septembre 2013, dans un nouveau centre équestre : L'Ecurie de l'äge\" dans la vallée de la Creuse."
    ],
    "nl": [
      "In Argenton-sur-Creuse verwelkomen Josépha en Frédéric u sinds 1 september 2013 in een nieuwe manege: L'Ecurie de l'äge\" in de Creuse vallei."
    ],
    "es": [
      "En Argenton-sur-Creuse, Josépha y Frédéric le dan la bienvenida desde el 1 de septiembre de 2013, en un nuevo centro ecuestre: L'Ecurie de l'äge\" en el valle de Creuse."
    ]
  },
