  "rdfs:comment": {
    "de": [
      "Kommen Sie und entdecken Sie die Tiere auf dem Bauernhof. Die Besichtigung ist frei. Verkauf von Bauernhofprodukten Eier, Enten - Foie gras, Entenbrust, Confits, Pasteten, Rillettes"
    ],
    "en": [
      "Come and discover the farm's animals. Free admission. Sale of farm products eggs, ducks - foie gras, magrets, confits, pâtés, rillettes, etc."
    ],
    "it": [
      "Venite a scoprire gli animali della fattoria. Visita gratuita. Vendita di prodotti di fattoria: uova, anatre, foie gras, petto d'anatra, confit, paté, rillettes"
    ],
    "fr": [
      "Venez découvrir les animaux de la ferme. Visite libre. Vente de produits fermiers œufs, canards – foie gras, magrets, confits, pâtés, rillettes"
    ],
    "es": [
      "Ven a descubrir los animales de la granja. Visita gratuita. Venta de productos de granja: huevos, patos, foie gras, pechuga de pato, confits, patés, rillettes"
    ],
    "nl": [
      "Kom en ontdek de dieren van de boerderij. Gratis bezoek. Verkoop van boerderijproducten eieren, eenden - foie gras, magrets, confits, patés, rillettes"
    ]
  },
  "rdfs:label": {
