  "rdfs:comment": {
    "de": [
      "\"Le Romarin\" befindet sich im Herzen des Campingplatzes Les Philippons. Er empfängt Sie auf seiner Terrasse mit einem atemberaubenden Blick auf das Esterel-Gebirge."
    ],
    "en": [
      "\"Le Romarin\" is installed in the heart of Camping Les Philippons. He welcomes you on his terrace with a breathtaking view of the Esterel."
    ],
    "it": [
      "\"Le Romarin\" si trova nel cuore del campeggio Les Philippons. Vi accoglie sulla sua terrazza con una vista mozzafiato sull'Esterel."
    ],
    "fr": [
      "\"Le Romarin\" est installé au coeur du Camping Les Philippons. Il vous accueille sur sa terrasse à la vue imprenable sur l'Estérel."
    ],
    "nl": [
      "\"Le Romarin\" ligt in het hart van camping Les Philippons. Het verwelkomt u op zijn terras met een adembenemend uitzicht op de Esterel."
    ],
    "es": [
      "\"Le Romarin\" está situado en el corazón del camping Les Philippons. Le da la bienvenida en su terraza con una vista impresionante del Esterel."
    ]
  },
