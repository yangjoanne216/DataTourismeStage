  "rdfs:comment": {
    "de": [
      "Bowling-Bar-Restaurant-Pizza-Komplex mit Holzfeuer. Restaurant mit Terrasse auf dem Land. Restaurant mit einer großen Auswahl an Pizzen aus dem Holzofen und einer abwechslungsreichen Karte für traditionelle Küche."
    ],
    "en": [
      "Bowling-Bar-Restaurant-Pizza Complex with wood fire. Restaurant with terrace in the countryside. Restaurant with a wide range of wood-fired pizzas and a varied menu for traditional cuisine."
    ],
    "it": [
      "Complesso bowling-bar-ristorante-pizza con fuoco a legna. Ristorante con terrazza in campagna. Ristorante con un'ampia scelta di pizze cotte a legna e un variegato menu di cucina tradizionale."
    ],
    "fr": [
      "Complexe Bowling-Bar-Restaurant-Pizza aux feu de bois. Restaurant avec terrasse en campagne. Restaurant avec une large gamme de pizzas au feu de bois et une carte variée pour la cuisine traditionnelle."
    ],
    "es": [
      "Complejo de bolos-bar-restaurante-pizza con fuego de leña. Restaurante con terraza en el campo. Restaurante con una amplia gama de pizzas al horno de leña y una variada carta de cocina tradicional."
    ],
    "nl": [
      "Bowling-Bar-Restaurant-Pizza complex met houtvuur. Restaurant met terras op het platteland. Restaurant met een breed scala aan houtgestookte pizza's en een gevarieerd menu voor de traditionele keuken."
    ]
  },
