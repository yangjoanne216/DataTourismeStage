  "rdfs:comment": {
    "de": [
      "Erfrischen Sie sich mit (un)tödlichen Nektaren: Entdecken Sie eine große Auswahl an köstlichen Cocktails und eine Auswahl der besten Whiskeys aus allen Teilen der Welt."
    ],
    "en": [
      "Refresh yourself with (un)deadly nectars: discover a wide choice of delicious cocktails and a selection of the best whiskeys from all over the world."
    ],
    "it": [
      "Rinfrescatevi con nettari (non) mortali: scoprite un'ampia scelta di deliziosi cocktail e una selezione dei migliori whisky di tutto il mondo."
    ],
    "fr": [
      "Rafraîchissez-vous de nectars (im)mortels : découvrez un large choix de cocktails délicieux et une sélection des meilleurs whiskeys des quatre coins du monde."
    ],
    "nl": [
      "Verfris u met (on)dodelijke nectar: ontdek een ruime keuze aan heerlijke cocktails en een selectie van de beste whisky's uit de hele wereld."
    ],
    "es": [
      "Refrésquese con néctares (in)mortales: descubra una amplia gama de deliciosos cócteles y una selección de los mejores whiskys de todo el mundo."
    ]
  },
