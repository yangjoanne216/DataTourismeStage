  "rdfs:comment": {
    "de": [
      "Familienrestaurant mit Blick auf das mittelalterliche Dorf Le Crestet und den Mont Ventoux. Eine authentische und saisonale Küche in einer einzigartigen Umgebung ist das Versprechen des Restaurants l'Epine."
    ],
    "en": [
      "Family restaurant with a view on the medieval village of Crestet and the Mont Ventoux. An authentic and seasonal cuisine in a unique setting is the promise of the restaurant l'Epine."
    ],
    "it": [
      "Ristorante familiare con vista sul borgo medievale di Le Crestet e sul Mont Ventoux. Una cucina autentica e stagionale in un ambiente unico è la promessa del ristorante l'Epine."
    ],
    "fr": [
      "Restaurant familial avec vue sur le village médiéval du Crestet et sur le Mont Ventoux. Une cuisine authentique et de saison dans un cadre unique c’est la promesse du restaurant l’Epine."
    ],
    "es": [
      "Restaurante familiar con vistas al pueblo medieval de Le Crestet y al Mont Ventoux. Una cocina auténtica y de temporada en un entorno único es la promesa del restaurante l'Epine."
    ],
    "nl": [
      "Familierestaurant met uitzicht op het middeleeuwse dorp Le Crestet en de Mont Ventoux. Authentieke en seizoensgebonden gerechten in een uniek kader is de belofte van restaurant l'Epine."
    ]
  },
