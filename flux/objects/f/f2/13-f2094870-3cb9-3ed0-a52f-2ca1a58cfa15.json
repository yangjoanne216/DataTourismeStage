  "rdfs:comment": {
    "de": [
      "Beratender Weinhändler und Weinrestaurant in Saint-Jean-de-Maurienne. Auswahl an Weinen ausschließlich von Winzern, hauptsächlich aus biologischem Anbau. Auswahl an vorwiegend lokalen Bieren und ausgewählten Spirituosen."
    ],
    "en": [
      "Wine merchant and wine restaurant in Saint-Jean-de-Maurienne. Selection of wines from wine growers only, mainly organic. Selection of mainly local beers and selected spirits."
    ],
    "it": [
      "Consulente enologico e ristorante a Saint-Jean-de-Maurienne. Selezione di vini provenienti esclusivamente da viticoltori, prevalentemente biologici. Selezione di birre prevalentemente locali e di alcolici selezionati."
    ],
    "fr": [
      "Caviste conseil et restaurant à vins à Saint-Jean-de-Maurienne. Sélection de vins de vignerons uniquement, principalement bio. Sélection de bières principalement locales et de spiritueux sélectionnés."
    ],
    "nl": [
      "Wijnadviseur en wijnrestaurant in Saint-Jean-de-Maurienne. Selectie van wijnen van uitsluitend wijnboeren, voornamelijk biologisch. Selectie van voornamelijk lokale bieren en geselecteerde sterke dranken."
    ],
    "es": [
      "Asesor de vinos y restaurante de vinos en Saint-Jean-de-Maurienne. Selección de vinos procedentes exclusivamente de viticultores, principalmente ecológicos. Selección de cervezas, principalmente locales, y licores selectos."
    ]
  },
