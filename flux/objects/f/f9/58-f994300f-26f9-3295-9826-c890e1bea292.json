  "rdfs:comment": {
    "de": [
      "Von Cornillon-Confoux und über 3 Kilometer in Richtung Lançon de Provence erreicht man den Weinkeller, ein bemerkenswertes Gebäude im Herzen des Weinbergs."
    ],
    "en": [
      "From Cornillon-Confoux, and for 3 kilometres in the direction of Lançon de Provence, you reach the cellar, a remarkable building in the heart of the vineyard."
    ],
    "it": [
      "Da Cornillon-Confoux, dopo 3 chilometri in direzione di Lançon de Provence, si raggiunge la cantina, un edificio notevole nel cuore del vigneto."
    ],
    "fr": [
      "De Cornillon-Confoux, et sur 3 kilomètres en direction de Lançon de Provence, on atteint la cave, bâtiment remarquable, au coeur du vignoble."
    ],
    "nl": [
      "Vanuit Cornillon-Confoux, en na 3 kilometer in de richting van Lançon de Provence, bereikt u de kelder, een opmerkelijk gebouw in het hart van de wijngaard."
    ],
    "es": [
      "Desde Cornillon-Confoux, y tras 3 kilómetros en dirección a Lançon de Provence, se llega a la bodega, un edificio notable en el corazón del viñedo."
    ]
  },
