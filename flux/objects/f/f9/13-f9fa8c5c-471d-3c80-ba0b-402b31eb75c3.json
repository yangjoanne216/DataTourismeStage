  "rdfs:comment": {
    "de": [
      "Sonntagsmarkt in Trans en Provence - Geschmack von Italien, Schmorgerichte, lokale Honigsorten, Gartengemüse, Nüsse, Kastanien, Olivenöl, Weine aus der Provence, regionale Wurstwaren."
    ],
    "en": [
      "Sunday market in Trans en Provence - Flavors from Italy, simmered dishes, local honeys, garden vegetables, nuts, chestnuts, olive oil, wines from Provence, regional delicatessen."
    ],
    "it": [
      "Mercato domenicale a Trans en Provence - sapori italiani, piatti in umido, mieli locali, verdure dell'orto, noci, castagne, olio d'oliva, vini provenzali, salumi regionali."
    ],
    "fr": [
      "Marché le dimanche à Trans en Provence - Saveurs d'Italie, plats mijotés, miels locaux, légumes du jardin, noix, châtaignes, huile d'olive, vins de Provence, charcuterie régionale."
    ],
    "nl": [
