  "rdfs:comment": {
    "de": [
      "Traditionelles Restaurant, hausgemachte Küche, frische Produkte.\nGibt es zu entdecken. Veranda und Terrasse für die schönen Tage.\n5 Min. von Verdun und den Schlachtfeldern entfernt."
    ],
    "en": [
      "Traditional restaurant, home cooking, fresh products.\nTo be discovered. Veranda and terrace for the beautiful days.\n5 min from Verdun and the Battlefields."
    ],
    "it": [
      "Ristorante tradizionale, cucina casalinga, prodotti freschi.\nDa scoprire. Veranda e terrazza per le giornate di sole.\nA 5 minuti da Verdun e dai campi di battaglia."
    ],
    "fr": [
      "Restaurant traditionnel, cuisine maison, produits frais.\nÀ découvrir. Véranda et terrasse pour les beaux jours.\nA 5 min de Verdun et des Champs de bataille."
    ],
    "nl": [
      "Traditioneel restaurant, huis keuken, verse producten.\nOm te ontdekken. Veranda en terras voor zonnige dagen.\n5 minuten van Verdun en de slagvelden."
    ],
    "es": [
      "Restaurante tradicional, cocina casera, productos frescos.\nPara descubrir. Veranda y terraza para los días soleados.\nA 5 minutos de Verdún y de los campos de batalla."
    ]
  },
