  "rdfs:comment": {
    "de": [
      "Jérémy, ein Önologie-Ingenieur, baut alte und lokale Rebsorten (Verdesse - Altesse - Mondeuse blanche - Etraire - Douce noire - Persan - Mondeuse noire) an den steilen Hängen von Brion an."
    ],
    "en": [
      "Jérémy, an oenological engineer, cultivates old and local grape varieties (Verdesse - Altesse - Mondeuse blanche - Etraire - Douce noire - Persan - Mondeuse noire) on the steep slopes of Brion."
    ],
    "it": [
      "Jérémy, ingegnere enologo, coltiva vitigni antichi e locali (Verdesse - Altesse - Mondeuse blanche - Etraire - Douce noire - Persan - Mondeuse noire) sui ripidi pendii di Brion."
    ],
    "fr": [
      "Jérémy, ingénieur oenologue, cultive des cépages anciens et locaux (Verdesse - Altesse - Mondeuse blanche - Etraire - Douce noire - Persan - Mondeuse noire) sur les côteaux escarpés de Brion."
    ],
    "es": [
      "Jérémy, ingeniero enológico, cultiva variedades de uva antiguas y locales (Verdesse - Altesse - Mondeuse blanche - Etraire - Douce noire - Persan - Mondeuse noire) en las empinadas laderas de Brion."
    ],
    "nl": [
      "Jérémy, een oenologisch ingenieur, verbouwt oude en lokale druivenrassen (Verdesse - Altesse - Mondeuse blanche - Etraire - Douce noire - Persan - Mondeuse noire) op de steile hellingen van Brion."
    ]
  },
