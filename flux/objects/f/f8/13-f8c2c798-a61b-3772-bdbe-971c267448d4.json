  "rdfs:comment": {
    "de": [
      "Diese Radtour führt rund um das Naturschutzgebiet des Marais de Lavours und durch die Winzerdörfer im Süden von Bugey."
    ],
    "en": [
      "This cycling circuit goes around the Lavours marsh nature reserve and crosses the atypical wine-growing villages of the South of Bugey."
    ],
    "it": [
      "Questo tour in bicicletta si snoda intorno alla riserva naturale della palude di Lavours e attraverso i villaggi vinicoli del Bugey meridionale."
    ],
    "fr": [
      "Ce circuit vélo fait le tour de la réserve naturelle du marais de Lavours et traverse les villages vignerons du Sud du Bugey."
    ],
    "es": [
      "Esta excursión en bicicleta rodea la reserva natural de la marisma de Lavours y atraviesa los pueblos vitivinícolas del sur del Bugey."
    ],
    "nl": [
      "Deze fietstocht gaat rond het natuurgebied van het Lavours moeras en door de wijndorpjes van de zuidelijke Bugey."
    ]
  },
