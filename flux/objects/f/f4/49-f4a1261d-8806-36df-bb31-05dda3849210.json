  "rdfs:comment": {
    "de": [
      "Produkte des Bauernhofs: Strickgarne, 100% französische Mohair-Konfektion: Pullover, Mützen, Baskenmützen, Socken, Schals, Spitze,... Originelle handwerkliche Kreationen, die auf dem Bauernhof gestrickt werden."
    ],
    "en": [
      "Farm products : knitting yarns, 100% French mohair : pullovers, hats, berets, socks, scarves, lace,... original hand-made creations knitted on the farm."
    ],
    "it": [
      "Prodotti della fattoria: filati per maglieria, abbigliamento in mohair 100% francese: maglioni, cappelli, berretti, calze, sciarpe, pizzi, ecc. Creazioni originali fatte a mano in fattoria."
    ],
    "fr": [
      "Produits de la ferme : fils à tricoter, confection mohair 100% française : pulls, bonnets, bérets, chaussettes, écharpes, dentelle,... créations artisanales originales tricotées sur la ferme."
    ],
    "es": [
      "Productos de la granja: hilos para tejer, prendas de vestir de mohair 100% francés: jerseys, sombreros, boinas, calcetines, bufandas, encajes, etc. Originales creaciones artesanales tejidas en la granja."
    ],
    "nl": [
      "Producten van de boerderij: breigarens, kleding van 100% Frans mohair: truien, mutsen, baretten, sokken, sjaals, kant, enz. Originele handgemaakte creaties gebreid op de boerderij."
    ]
  },
