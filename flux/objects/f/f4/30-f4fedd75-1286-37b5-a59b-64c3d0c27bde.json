  "rdfs:comment": {
    "de": [
      "500m vom Stadtzentrum entfernt, ebenerdiges Haus mit Veranda, geschlossenem Privatgrundstück und Terrasse (Gartenmöbel, Liegestühle, Grill). Erdgeschoss: Wohnzimmer (TV, 2 Sofas, Kamin), amerikanische Küche (MW, Backofen, Kochplatte, LV, Gefrierkombi),..."
    ],
    "en": [
      "400m from city center, semi-detached house on basement with private closed ground and terrace (bathroom, barbecue, portico with slide). First floor : living room (TV, 2 clic-clac, fireplace), US kitchen (MO, LV, combi-congel), shower room + WC...."
    ],
    "it": [
      "a 500 m dal centro del paese, casa a un piano con veranda, terreno privato recintato e terrazza (mobili da giardino, sedie a sdraio, barbecue). Piano terra: soggiorno (TV, 2 divani, camino), cucina americana (MO, forno, piano cottura, LV, combi-congelatore),..."
    ],
    "fr": [
      "500m centre ville, maison de plain- pied avec Véranda , terrain privatif clos et terrasse (Salon de jardin,transats, barbecue,). Rez-de-chaussée : séjour-salon (TV, 2 canapés, cheminée), cuisine US (MO, Four, Plaque de cuisson, LV, combi-congel),..."
    ],
    "es": [
      "a 500 m del centro de la ciudad, casa de una sola planta con porche, recinto privado y terraza (muebles de jardín, tumbonas, barbacoa). Planta baja: salón (TV, 2 sofás, chimenea), cocina americana (MO, horno, placa de cocción, LV, congelador combinado),..."
    ],
    "nl": [
      "500m van het stadscentrum, gelijkvloerse woning met veranda, omheind privéterrein en terras (tuinmeubelen, ligstoelen, barbecue). Begane grond: woonkamer (TV, 2 banken, open haard), Amerikaanse keuken (MO, oven, kookplaat, LV, combi-vriezer),..."
    ]
  },
