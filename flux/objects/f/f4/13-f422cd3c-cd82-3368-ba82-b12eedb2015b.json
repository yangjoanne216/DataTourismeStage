  "rdfs:comment": {
    "de": [
      "Büro des Fremdenverkehrsamts des Val d'Arly in Notre Dame de Bellecombe.\r\nPublikumsempfang, Auskünfte, Reservierungen und Buchungen, Shop, Ticketverkauf, Kundenservice ...\r\nZögern Sie nicht, bei uns vorbeizuschauen!"
    ],
    "en": [
      "Val d'Arly Tourist Office in Notre Dame de Bellecombe. Open to the public, information, bookings, sales, ticket office. Please do come and see us!"
    ],
    "it": [
      "Ufficio del Turismo della Val d'Arly a Notre Dame de Bellecombe\r\nAccoglienza del pubblico, informazioni, prenotazioni, vendite, biglietteria, servizi...\r\nNon esitate a venirci a trovare."
    ],
    "fr": [
      "Bureau de l'Office de Tourisme du Val d'Arly à Notre Dame de Bellecombe\r\nAccueil du public, renseignements, réservations, ventes, billetterie, services...\r\nN'hésitez pas à venir nous rendre visite."
    ],
    "es": [
      "Oficina de Turismo de Val d'Arly en Notre Dame de Bellecombe\r\nRecepción del público, información, reservas, venta, venta de entradas, servicios...\r\nNo dude en venir a visitarnos."
    ],
    "nl": [
      "Kantoor van het toeristenbureau van Val d'Arly in Notre Dame de Bellecombe\r\nOntvangst van het publiek, informatie, reserveringen, verkoop, kaartverkoop, diensten...\r\nAarzel niet om ons te komen bezoeken."
    ]
  },
