  "rdfs:comment": {
    "de": [
      "Wohnmobilstellplatz in einer ländlichen Umgebung in geselliger Runde in der Nähe der Grotte des Demoiselles."
    ],
    "en": [
      "Motorhome parking area in a friendly country setting near the Grotte des Demoiselles."
    ],
    "it": [
      "Area di sosta per camper in un'accogliente zona di campagna vicino alla Grotte des Demoiselles."
    ],
    "fr": [
      "Aire de stationnement de camping-car dans un cadre champêtre en toute convivialité à proximité de la Grotte des Demoiselles."
    ],
    "es": [
      "Área de estacionamiento de autocaravanas en un agradable entorno campestre cerca de la Grotte des Demoiselles."
    ],
    "nl": [
      "Parkeerplaats voor campers in een vriendelijke, landelijke omgeving in de buurt van de Grotte des Demoiselles."
    ]
  },
