  "rdfs:comment": {
    "de": [
      "Neapolitanische Handwerkspizzeria, Produkte von höchster Qualität, Fleisch vom Metzger, frisches Gemüse vom Bauern und renommierten Frühsortierer, italienisches Mehl und Tomaten, Castellano-Mozzarella, hergestellt in Aix-en-Provence."
    ],
    "en": [
      "Neapolitan home-made pizzeria, superior quality products, meat from the butcher, fresh vegetables from the farmer and renowned fresh produce, Italian flour and tomato, mozzarella Castellano made in Aix-en-Provence."
    ],
    "it": [
      "Pizzeria artigianale napoletana, prodotti di alta qualità, carne del macellaio, verdure fresche del contadino e del rinomato fruttivendolo, farina e pomodori italiani, mozzarella Castellano prodotta ad Aix-en-Provence."
    ],
    "fr": [
      "Pizzeria artisanale Napolitaine, produits de qualités supérieur, viande du boucher, légumes frais du paysan et primeur de renom, farine et tomate italienne, mozzarella Castellano fabriquée à Aix-en-Provence."
    ],
    "es": [
      "Pizzería napolitana artesanal, productos de alta calidad, carne del carnicero, verduras frescas del agricultor y verdulero de renombre, harina y tomates italianos, mozzarella castellana hecha en Aix-en-Provence."
    ],
    "nl": [
      "Ambachtelijke Napolitaanse pizzeria, producten van hoge kwaliteit, vlees van de slager, verse groenten van de boer en gerenommeerde groenteboer, Italiaans meel en tomaten, Castellano mozzarella gemaakt in Aix-en-Provence."
    ]
  },
