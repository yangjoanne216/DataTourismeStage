  "rdfs:comment": {
    "de": [
      "Zwischen Indre und Loir-et-Cher können Sie sich am Ufer des Teichs von Faverolles-en-Berry entspannen und angeln."
    ],
    "en": [
      "Between Indre and Loir-et-Cher come relax and fish at the pond of Faverolles-en-Berry."
    ],
    "it": [
      "Tra l'Indre e il Loir-et-Cher, venite a rilassarvi e a pescare ai bordi dello stagno di Faverolles-en-Berry."
    ],
    "fr": [
      "Entre Indre et Loir-et-Cher venez vous détendre et pêcher au bord de l'étang de Faverolles-en-Berry."
    ],
    "nl": [
      "Tussen Indre en Loir-et-Cher, kom ontspannen en vissen aan de rand van de vijver van Faverolles-en-Berry."
    ],
    "es": [
      "Entre Indre y Loir-et-Cher, venga a relajarse y a pescar al borde del estanque de Faverolles-en-Berry."
    ]
