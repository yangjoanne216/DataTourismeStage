      "dc:description": {
        "de": [
          "T2 im 3. Stock eines Gebäudes im Stadtzentrum von Laguiole, ohne Aufzug. Kochnische, Wohnzimmer, Esszimmer, ein Schlafzimmer im Zwischengeschoss. Mikrowelle, Fernseher."
        ],
        "en": [
          "T2 on the 3rd floor of a building in downtown Laguiole, without elevator. Kitchenette, living room, dining room, one bedroom in mezzanine. Microwave, TV."
        ],
        "it": [
          "T2 al 3° piano di un edificio nel centro di Laguiole, senza ascensore. Angolo cottura, soggiorno, sala da pranzo, una camera da letto sul mezzanino. Microonde, TV."
        ],
        "fr": [
          "T2 au 3ème étage d'un immeuble du centre-ville de Laguiole, sans ascenseur. Coin cuisine, salon, salle à manger, une chambre en mezzanine. Micro-ondes, TV."
        ],
        "nl": [
          "T2 op de 3e verdieping van een gebouw in het centrum van Laguiole, zonder lift. Kitchenette, woonkamer, eetkamer, een slaapkamer op de mezzanine. Magnetron, TV."
        ],
        "es": [
          "T2 en el 3er piso de un edificio en el centro de Laguiole, sin ascensor. Cocina americana, salón, comedor, una habitación en el entresuelo. Microondas, TV."
        ]
      },
