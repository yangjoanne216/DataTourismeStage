  "rdfs:comment": {
    "de": [
      "150 m vom Strand entfernt, Familienhotel in der Nähe von Aktivitäten. Ruhige Straße, kostenloser öffentlicher Parkplatz, Terrasse - Verpflegung 1/2 Pension - Gesellige Atmosphäre - Attraktive Preise."
    ],
    "en": [
      "150 m from the beach, family hotel close to activities. Quiet street, free public parking, terrace - 1/2 board catering - Friendly atmosphere - Attractive rates."
    ],
    "it": [
      "A 150 m dalla spiaggia, hotel familiare vicino alle attività. Strada tranquilla, parcheggio pubblico gratuito, terrazza - ristorante con mezza pensione - atmosfera amichevole - tariffe vantaggiose."
    ],
    "fr": [
      "A 150 m de la plage, hôtel familial proche des activités. Rue calme, parking public gratuit, terrasse - restauration 1/2 Pension - Ambiance conviviale - Tarifs attractifs."
    ],
    "es": [
      "A 150 m de la playa, hotel familiar cerca de las actividades. Calle tranquila, aparcamiento público gratuito, terraza - restaurante con media pensión - ambiente agradable - tarifas atractivas."
    ],
    "nl": [
      "150 m van het strand, familiehotel dichtbij activiteiten. Rustige straat, gratis openbare parkeerplaats, terras - restaurant met halfpension - vriendelijke sfeer - aantrekkelijke tarieven."
    ]
