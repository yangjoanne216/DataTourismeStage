  "rdfs:comment": {
    "de": [
      "Restaurant mit marktfrischer Küche.\nVerkauf von handgemachten Pizzen und hausgemachten Leckereien.\nCrêpes Waffeln Snaking Sandwiches Paninis ?\nPlancha je nach Veranstaltung\nUnd Rotisserie\nTraiteur-Gericht"
    ],
    "en": [
      "Market cuisine restaurant.\nSales of homemade pizzas and homemade delicacies.\nPancakes Waffles Snaking sandwiches Paninis ?\nPlancha according to event\nAnd Rotisserie\nCatering dish"
    ],
    "it": [
      "Ristorante di cucina di mercato.\nVendita di pizze fatte in casa e di prelibatezze artigianali.\nPancakes Waffles Panini da sgranocchiare Panini ?\nPlancha in base all'evento\nE rosticceria\nPiatti per il catering"
    ],
    "fr": [
      "Restaurant de cuisine du marché.\nVentes de pizzas artisanales et de gourmandises faites maison.\nCrêpes Gaufres Snaking sandwichs Paninis …\nPlancha selon événement \nEt Rôtisserie \nPlat traiteur"
    ],
    "es": [
      "Restaurante de cocina de mercado.\nVenta de pizzas caseras y delicias caseras.\nTortitas Gofres Bocadillos Paninis ?\nPlancha según evento\nY Asador\nPlatos para catering"
    ],
    "nl": [
      "Markt keuken restaurant.\nVerkoop van zelfgemaakte pizza's en huisgemaakte lekkernijen.\nPannenkoeken Wafels Broodjes Panini's ?\nPlancha volgens evenement\nEn Rotisserie\nCatering gerechten"
    ]
  },
