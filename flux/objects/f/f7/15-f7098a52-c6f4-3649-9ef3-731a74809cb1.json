  "rdfs:comment": {
    "de": [
      "La Pantoufle du Berry: Französischer Hersteller von qualitativ hochwertigen und originellen Hausschuhen, Charentaises und Pantoffeln made in France im Herzen von Berry."
    ],
    "en": [
      "La Pantoufle du Berry: a French slipper manufacturer that makes genuine, quality Charantaise slippers and other types of slipper in the heart of Berry."
    ],
    "it": [
      "La Pantoufle du Berry: produttore francese di pantofole originali e di qualità, realizzate in Francia nel cuore del Berry."
    ],
    "fr": [
      "La Pantoufle du Berry : fabricant français de chaussons, charentaises et pantoufles de qualité et originaux made in France au coeur du Berry."
    ],
