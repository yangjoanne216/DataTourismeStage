  "rdfs:comment": {
    "de": [
      "Täglich geöffnete Bar-Brasserie auf dem Cours Victor Hugo in Saint Rémy de Provence. Traditionelle Küche mit hausgemachten Produkten und der Spezialität Lammkeule, die am Knochen gebraten wird!"
    ],
    "en": [
      "Bar-Brewery open every day on the Cours Victor Hugo, in Saint Rémy de Provence. Traditional cuisine with homemade products and the speciality of leg of lamb roasted on the bone!"
    ],
    "it": [
      "Bar-brasserie aperto tutti i giorni sul Cours Victor Hugo, a Saint Rémy de Provence. Cucina tradizionale con prodotti fatti in casa e la specialità del cosciotto di agnello arrostito con l'osso!"
    ],
    "fr": [
      "Bar-Brasserie ouverte tous les jours sur le cours Victor Hugo, à Saint Rémy de Provence. Cuisine traditionnelle avec des produits maison et la spécialité du gigot d'agneau rôti sur l'os !"
    ],
    "nl": [
      "Bar-Brasserie dagelijks geopend op de Cours Victor Hugo, in Saint Rémy de Provence. Traditionele keuken met huisgemaakte producten en de specialiteit van lamsbout geroosterd op het bot!"
    ],
    "es": [
      "Bar-Brasserie abierto todos los días en el Cours Victor Hugo, en Saint Rémy de Provence. Cocina tradicional con productos caseros y la especialidad de la pierna de cordero asada con hueso"
    ]
  },
