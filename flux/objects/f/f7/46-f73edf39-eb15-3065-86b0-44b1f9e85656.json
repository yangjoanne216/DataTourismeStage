  "rdfs:comment": {
    "de": [
      "Awen, National Guide Conferencière, zugelassen vom Ministerium für Kultur und Tourismus, bietet ihre Dienste als Fremdenführerin, Dolmetscherin und Reisebegleiterin auf Französisch, Englisch und Spanisch an."
    ],
    "en": [
      "Awen, National Tour Guide, approved by the Ministry of Culture and Tourism, offers you her services as a tour guide, interpreter and guide in French, English and Spanish."
    ],
    "it": [
      "Awen, Guida Turistica Nazionale, certificata dal Ministero della Cultura e del Turismo, vi offre i suoi servizi di guida turistica, guida interprete e guida accompagnatrice in francese, inglese e spagnolo."
    ],
    "fr": [
      "Awen, Guide Conférencière Nationale, agréée du Ministère de la Culture et du Tourisme, vous propose ses services de guide conférencière, de guide interprète et de guide accompagnatrice en français, anglais et espagnol."
    ],
    "nl": [
      "Awen, nationale reisleidster, gecertificeerd door het Ministerie van Cultuur en Toerisme, biedt u haar diensten aan als reisleidster, tolkgids en begeleider in het Frans, Engels en Spaans."
    ],
    "es": [
      "Awen, Guía Nacional de Turismo, certificada por el Ministerio de Cultura y Turismo, le ofrece sus servicios como guía turística, guía intérprete y guía acompañante en francés, inglés y español."
    ]
  },
