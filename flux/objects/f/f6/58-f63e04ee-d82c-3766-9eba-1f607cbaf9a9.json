  "rdfs:comment": {
    "de": [
      "Massimo Birindelli, ehemaliger Chefkoch der Restaurants La Grande Italia und Via Mermoz, hat im Erdgeschoss einer Bastide aus dem 18. Jahrhundert im Stadtteil Blancarde ein Restaurant eröffnet."
    ],
    "en": [
      "Massimo Birindelli, former chef of the restaurants La Grande Italia and Via Mermoz has opened a restaurant on the first floor of an 18th century bastide in the Blancarde district."
    ],
    "it": [
      "Massimo Birindelli, già chef dei ristoranti La Grande Italia e Via Mermoz, ha aperto un ristorante al piano terra di una bastide del XVIII secolo nel quartiere Blancarde."
    ],
    "fr": [
      "Massimo Birindelli, ancien chef des restaurants La Grande Italia et Via Mermoz a ouvert un restaurant au rez-de-chaussée d'une bastide du XVIIIe siècle, dans le quartier de la Blancarde."
    ],
    "nl": [
      "Massimo Birindelli, voormalig chef-kok van de restaurants La Grande Italia en Via Mermoz heeft een restaurant geopend op de begane grond van een 18e eeuwse bastide in de wijk Blancarde."
    ],
    "es": [
      "Massimo Birindelli, antiguo chef de La Grande Italia y Via Mermoz, ha abierto un restaurante en la planta baja de una bastida del siglo XVIII en el barrio de Blancarde."
    ]
  },
  "rdfs:label": {
    "fr": [
      "La Bastide Massimo"
    ]
  },
