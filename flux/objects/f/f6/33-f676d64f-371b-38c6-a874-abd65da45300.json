  "rdfs:comment": {
    "de": [
      "Treffen Sie sich am Fuß der Windkraftanlagen in Avignonet-Lauragais und erfahren Sie, wie diese Maschinen funktionieren."
    ],
    "en": [
      "Meet at the foot of the wind turbines of Avignonet-Lauragais to discover how these machines work."
    ],
    "it": [
      "Incontro ai piedi delle turbine eoliche di Avignonet-Lauragais per scoprire il funzionamento di queste macchine."
    ],
    "fr": [
      "Rendez-vous au pied des éoliennes d'Avignonet-Lauragais pour découvrir  le fonctionnement  de ces machines."
    ],
    "es": [
      "Reúnase al pie de los aerogeneradores de Avignonet-Lauragais para descubrir el funcionamiento de estas máquinas."
    ],
    "nl": [
      "Ontmoet aan de voet van de windmolens in Avignonet-Lauragais om te ontdekken hoe deze machines werken."
    ]
