  "rdfs:comment": {
    "de": [
      "Familiäre und gesellige Atmosphäre. Traditionelles Restaurant, das hausgemachte Speisen aus frischen Produkten anbietet. Tagesgericht jeden Mittag unter der Woche (außer montags)."
    ],
    "en": [
      "Family and friendly atmosphere. Traditional restaurant with home-made products. Dish of the day every lunchtime during the week (except Monday)."
    ],
    "it": [
      "Atmosfera familiare e amichevole. Ristorante tradizionale con prodotti fatti in casa. Piatto del giorno tutti i giorni feriali a pranzo (tranne il lunedì)."
    ],
    "fr": [
      "Cadre familial et convivial. Restaurant traditionnel pratiquant le fait-maison à base de produits frais. Plat du jour tous les midis en semaine (sauf le lundi)."
    ],
    "nl": [
      "Familiale en vriendelijke sfeer. Traditioneel restaurant met huisgemaakte producten. Schotel van de dag elke middag op weekdagen (behalve maandag)."
    ],
    "es": [
      "Ambiente familiar y acogedor. Restaurante tradicional con productos caseros. Plato del día todos los almuerzos de los días laborables (excepto el lunes)."
    ]
  },
