  "rdfs:comment": {
    "de": [
      "Nachtkarpfenstrecke des Canal Marans / La Rochelle (rechtes Ufer)\r\nVom Siphon des Canal de la Brune bis zum \"Siphon des bois\" (Ende des asphaltierten Weges)\r\n8.200 m\r\nGemeinden Marans, Charron, Andiily, Villedoux und Saint Ouen d'Aunis"
    ],
    "en": [
      "Night carp course of the Canal Marans / La Rochelle (right bank)\r\nFrom the siphon of the Canal de la Brune to the \"siphon des bois\" (end of the asphalt road)\r\n8 200 meters\r\nCommunes of Marans, Charron, Andiily, Villedoux and Saint Ouen d'Aunis"
    ],
    "it": [
      "Percorso notturno del Canal Marans / La Rochelle (riva destra)\r\nDal sifone del Canal de la Brune al \"sifone dei bois\" (fine della strada asfaltata)\r\n8 200 metri\r\nComuni di Marans, Charron, Andiily, Villedoux e Saint Ouen d'Aunis"
    ],
    "fr": [
      "Parcours carpe de nuit du Canal Marans / La Rochelle (rive droite)\r\nDu siphon du Canal de la Brune jusqu'au \"siphon des bois\" (fin du chemin bitumé)\r\n8 200 mètres\r\nCommunes de Marans, Charron, Andiily, Villedoux et Saint Ouen d'Aunis"
    ],
    "nl": [
      "Nachtkarperloop van het kanaal Marans / La Rochelle (rechteroever)\r\nVan de sifon van het Canal de la Brune tot de \"sifon des bois\" (einde van de asfaltweg)\r\n8 200 meter\r\nGemeenten Marans, Charron, Andiily, Villedoux en Saint Ouen d'Aunis"
    ],
    "es": [
      "Curso de carpa nocturna del Canal de Marans / La Rochelle (orilla derecha)\r\nDesde el sifón del Canal de la Brune hasta el \"sifón des bois\" (final de la carretera asfaltada)\r\n8 200 metros\r\nMunicipios de Marans, Charron, Andiily, Villedoux y Saint Ouen d'Aunis"
    ]
  },
