  "rdfs:comment": {
    "de": [
      "La Boca Loca, das ist Mexiko in Aix-en-Provence. Von den traditionellen Burritos bis hin zu Quesadillas und Tacos, gönnen Sie sich einen lateinamerikanischen Ausflug."
    ],
    "en": [
      "La Boca Loca is Mexico in Aix-en-Provence. From traditional Burritos to Quesadillas and Tacos, treat yourself to a Latin American getaway."
    ],
    "it": [
      "La Boca Loca è il Messico di Aix-en-Provence. Dai tradizionali Burritos alle Quesadillas e ai Tacos, concedetevi una fuga in America Latina."
    ],
    "fr": [
      "La Boca Loca, c'est le Mexique à Aix-en-Provence. Des traditionnels Burritos aux Quesadillas et aux Tacos, offrez-vous une escapade Latino-Américaine."
    ],
    "nl": [
      "La Boca Loca is Mexico in Aix-en-Provence. Van traditionele burrito's tot quesadillas en taco's, trakteer uzelf op een Latijns-Amerikaans uitje."
    ],
    "es": [
      "La Boca Loca es México en Aix-en-Provence. Desde los tradicionales burritos hasta las quesadillas y los tacos, regálate una escapada latinoamericana."
    ]
  },
