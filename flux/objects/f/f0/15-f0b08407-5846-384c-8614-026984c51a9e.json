  "rdfs:comment": {
    "de": [
      "Das Tal der Geoden befindet sich am touristischen Standort der Grottes du Foulon, den Paris am nächsten gelegenen Höhlen, und ist ein Panoramasaal, der sich zum Tal des Loir hin öffnet."
    ],
    "en": [
      "Located on the tourist site of the Grottes du Foulon, the closest caves to Paris, the Valley of the Geodes is a panoramic room open to the Loir Valley."
    ],
    "it": [
      "Situata nel sito turistico delle Grottes du Foulon, le grotte più vicine a Parigi, la Valle delle Geodi è una sala panoramica aperta sulla valle del Loir."
    ],
    "fr": [
      "Située sur le site touristique des Grottes du Foulon, grottes les plus proche de Paris, la Vallée des Géodes est une salle panoramique ouverte sur la vallée du Loir."
    ],
    "es": [
      "Situado en el enclave turístico de las Grutas del Foulon, las más cercanas a París, el Valle de las Geodas es una sala panorámica abierta al valle del Loir."
    ],
    "nl": [
      "Gelegen op de toeristische site van de Grottes du Foulon, de dichtstbijzijnde grotten van Parijs, is de Vallei van de Geoden een panoramische ruimte die uitkijkt op de vallei van de Loir."
    ]
  },
