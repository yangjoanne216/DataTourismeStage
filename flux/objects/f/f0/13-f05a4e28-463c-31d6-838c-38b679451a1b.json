  "rdfs:comment": {
    "de": [
      "Genießen Sie auf Inlineskates oder Skates die Ufer der Isère, während die Vögel singen und das Wasser fließt."
    ],
    "en": [
      "On rollerblades or skateboards, enjoy the banks of the Isère to the sound of birdsong and flowing water."
    ],
    "it": [
      "Pattinando in linea o con lo skateboard, godetevi le rive dell'Isère al suono del canto degli uccelli e dell'acqua che scorre."
    ],
    "fr": [
      "En roller ou en skate, profitez des berges de l’Isère au son du chant des oiseaux et de l’eau qui coule."
    ],
    "nl": [
      "Skeeleren of skateboarden, genieten aan de oevers van de Isère bij het geluid van vogelgezang en stromend water."
    ],
    "es": [
      "Patinando en línea o en monopatín, disfrute de las orillas del Isère al son del canto de los pájaros y del agua que fluye."
    ]
