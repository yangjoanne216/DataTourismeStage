  "rdfs:comment": {
    "de": [
      "Botanischer Ausflug, um die Pflanzen des Waldes von Gâvre zu entdecken, von 13:45 bis 17:00 Uhr (geänderte Zeiten aufgrund des Erlasses der Präfektur, der den Zugang zum Wald auf den Vormittag beschränkt) - Reservierungen: 02 40 87 15 11"
    ],
    "en": [
      "Botanical tour, to discover the plants of the forest of Le Gâvre, from 1:45 pm to 5 pm (schedule modified due to the prefectural decree restricting access to the forest in the morning) - Reservations: 02 40 87 15 11"
    ],
    "it": [
      "Uscita botanica alla scoperta delle piante della foresta di Gâvre, dalle 13.45 alle 17.00 (orario modificato a causa del decreto prefettizio che limita l'accesso alla foresta al mattino) - Prenotazioni: 02 40 87 15 11"
    ],
    "fr": [
      "Sortie botanique, à la découverte des végétaux de la forêt du Gâvre, de 13h45 à 17h (horaires modifiés en raison de l'arrêté préfectoral restreignant l'accès à la forêt à la matinée) - Réservations : 02 40 87 15 11"
    ],
    "es": [
      "Salida botánica para descubrir las plantas del bosque de Gâvre, de 13.45 a 17.00 h (horario modificado debido al decreto de la prefectura que restringe el acceso al bosque por la mañana) - Reservas: 02 40 87 15 11"
    ],
    "nl": [
      "Botanische uitstap om de planten van het bos van Gâvre te ontdekken, van 13.45 tot 17.00 uur (tijdschema gewijzigd als gevolg van het prefectuurbesluit dat de toegang tot het bos 's ochtends beperkt) - Reserveringen: 02 40 87 15 11"
    ]
  },
