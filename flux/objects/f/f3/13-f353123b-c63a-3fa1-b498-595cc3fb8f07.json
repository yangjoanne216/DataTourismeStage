  "rdfs:comment": {
    "de": [
      "Abenteuerpark im Vallon Pont d'Arc: Seilpark für Kinder und Erwachsene, 300 m Seilbahn, Baby-Parcours ab 2 Jahren. Xplore-Spiel für Kinder: ein digitales Escape-Game im Freien für Kinder ab 5 Jahren."
    ],
    "en": [
      "Trail for children aged 2 and over: The trail is 1m from the ground and little adventurers are kitted out to the max with harnesses, ropes and snap hooks. They can take their time tackling the action. Giant 300m zip-line!"
    ],
    "it": [
      "Parco avventura a Vallon Pont d'Arc: Accrobranche per bambini e adulti, traversata tirolese di 300 m, percorso baby a partire da 2 anni Xplore game per bambini: un gioco di fuga digitale all'aperto a partire da 5 anni"
    ],
    "fr": [
      "Parc aventure à Vallon Pont d'Arc: Accrobranche enfant et adultes, Tyrolienne de 300 m, parcours baby dès 2 ans Xplore game enfant: un escape game numérique en extérieur dès 5 ans"
    ],
    "es": [
      "Parque de aventuras en Vallon Pont d'Arc: Accrobranche para niños y adultos, tirolina de 300 m, recorrido para bebés a partir de 2 años Juego Xplore para niños: un juego de escape digital al aire libre a partir de 5 años"
    ],
    "nl": [
      "Een avonturenpark in Vallon Pont d'Arc: tokkelbanen voor jong en oud."
    ]
  },
