  "rdfs:comment": {
    "de": [
      "Campingplatz mit Grasboden, schattig, am Flussufer mit ganztägigem Angeln.\nGeöffnet von Mitte April bis Ende September.\nPreise auf der Website einsehbar."
    ],
    "en": [
      "Grassy, shaded, riverside campground with day fishing.\nOpen from mid-April to the end of September.\nRates available on the website."
    ],
    "it": [
      "Un campeggio erboso e ombreggiato in riva al fiume con possibilità di pesca in giornata.\nAperto da metà aprile a fine settembre.\nI prezzi sono disponibili sul sito web."
    ],
    "fr": [
      "Camping au sol herbeux, ombragé, bord de rivière avec pêche à la journée.\nOuvert de mi-avril à fin septembre.\nTarifs consultables sur le site internet."
    ],
    "es": [
      "Un camping herboso y sombreado junto al río, con pesca por día.\nAbierto desde mediados de abril hasta finales de septiembre.\nPrecios disponibles en el sitio web."
    ],
    "nl": [
      "Een grasrijke, schaduwrijke camping aan de rivier met vissen per dag.\nGeopend van half april tot eind september.\nPrijzen beschikbaar op de website."
    ]
  },
