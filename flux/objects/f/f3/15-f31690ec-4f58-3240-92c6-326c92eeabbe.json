  "rdfs:comment": {
    "de": [
      "Konzert in den Gärten des Schlosses Azay-Le-Ferron, die von den berühmten Landschaftsgärtnern, den Brüdern Bühler, neu gestaltet wurden und als \"Jardin remarquable\" (bemerkenswerter Garten) eingestuft wurden."
    ],
    "en": [
      "Concert in the gardens of the castle of Azay-Le-Ferron, redesigned by the Bühler brothers, famous landscape designers, and classified as a \"remarkable garden\"."
    ],
    "it": [
      "Concerto nei giardini dello Château d'Azay-Le-Ferron, ridisegnati dai fratelli Bühler, famosi paesaggisti, e classificati come \"giardino notevole\"."
    ],
    "fr": [
      "Concert dans les jardins du château d'Azay-Le-Ferron, réaménagés par les frères Bühler, célèbres paysagistes, et classés « Jardin remarquable »."
    ],
    "es": [
      "Concierto en los jardines del castillo de Azay-Le-Ferron, rediseñados por los hermanos Bühler, famosos paisajistas, y clasificados como \"jardín notable\"."
    ],
    "nl": [
      "Concert in de tuinen van het Château d'Azay-Le-Ferron, heringericht door de gebroeders Bühler, beroemde landschapsontwerpers, en geklasseerd als \"opmerkelijke tuin\"."
    ]
  },
