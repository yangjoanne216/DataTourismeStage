  "rdfs:comment": {
    "de": [
      "Wunderschönes Blockhaus auf unserem Weiler gelegen. Vollständig und gemütlich ausgestattet. Mit Holz befeuerter Ofen. Eigene Terrasse mit Tischen und Bänken, Liegestühlen und Grill."
    ],
    "en": [
      "Superb log cabin located in our hamlet. Fully and warmly equipped. Wood stove. Private terrace with tables and benches, deck chairs and barbecue."
    ],
    "it": [
      "Splendida casetta di legno situata nel nostro borgo. Completamente e calorosamente attrezzato. Stufa a legna. Terrazza privata con tavoli e panche, sedie a sdraio e barbecue."
    ],
    "fr": [
      "Superbe chalet en rondins situé sur notre hameau. Entièrement et chaleureusement équipé. Poêle à bois. Terrasse privative avec tables et bancs, chaises longues et barbecue."
    ],
    "nl": [
      "Schitterende blokhut gelegen in ons gehucht. Volledig en warm uitgerust. Houtkachel. Privé-terras met tafels en banken, ligstoelen en barbecue."
    ],
    "es": [
      "Magnífica cabaña de madera situada en nuestra aldea. Totalmente y cálidamente equipado. Estufa de leña. Terraza privada con mesas y bancos, tumbonas y barbacoa."
    ]
  },
