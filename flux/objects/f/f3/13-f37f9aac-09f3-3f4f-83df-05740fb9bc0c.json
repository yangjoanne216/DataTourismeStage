  "rdfs:comment": {
    "de": [
      "Restaurant mit lokaler Küche in der Bas Gambetta/Promenade des Anglais. Qualität, frische, lokale und saisonale Produkte sind hier an der Tagesordnung, alles ist hausgemacht. Gutes Preis-Leistungs-Verhältnis."
    ],
    "en": [
      "Restaurant of local cuisine located at the bottom of Gambetta/Promenade des Anglais. Quality, fresh, local and seasonal products, everything is home-made. Good quality/price ratio."
    ],
    "it": [
      "Ristorante di cucina locale situato in fondo a Gambetta/Promenade des Anglais. Prodotti di qualità, freschi, locali e di stagione, tutto fatto in casa. Buon rapporto qualità/prezzo."
    ],
    "fr": [
      "Restaurant de cuisine locale situé bas Gambetta/Promenade des Anglais. Qualité, produits frais, locaux et de saison sont au rendez-vous, tout est fait maison. Bon rapport qualité/prix."
    ],
    "es": [
      "Restaurante de cocina local situado al pie de Gambetta/Promenade des Anglais. Productos de calidad, frescos, locales y de temporada, todo casero. Buena relación calidad-precio."
    ],
    "nl": [
      "Restaurant met lokale keuken, gelegen onderaan de Gambetta/Promenade des Anglais. Kwaliteit, verse, lokale en seizoensgebonden producten, alles is huisgemaakt. Goede prijs-kwaliteitverhouding."
    ]
  },
