  "rdfs:comment": {
    "de": [
      "Bar-Brasserie in einem Einkaufszentrum am Eingang von Lion d'Angers, nahe der Achse Segré - Angers mit verschiedenen Mittagsangeboten, vom Sandwich zum Mitnehmen bis zum Arbeitermenü."
    ],
    "en": [
      "Bar-brewery in a shopping center at the entrance of Le Lion d'Angers, near the Segré - Angers axis with different formulas at lunchtime, from sandwiches to take away to the working class menu."
    ],
    "it": [
      "Bar-brasserie in un centro commerciale all'ingresso di Le Lion d'Angers, vicino all'asse Segré - Angers, con diverse formule per il pranzo, dai panini al take-away fino al menu dei lavoratori."
    ],
    "fr": [
      "Bar-brasserie dans un centre commercial à l'entrée du Lion d'Angers, proche de l'axe Segré - Angers avec différentes formules le midi, du sandwich à emporter au menu ouvrier."
    ],
    "nl": [
      "Bar-brasserie in een winkelcentrum aan de ingang van Le Lion d'Angers, dicht bij de as Segré - Angers, met verschillende lunchformules, van broodjes tot afhaalmaaltijden en een arbeidersmenu."
    ],
    "es": [
      "Bar-brasserie en un centro comercial a la entrada de Le Lion d'Angers, cerca del eje Segré - Angers, con diferentes fórmulas para el almuerzo, desde sándwiches hasta comida para llevar y un menú obrero."
    ]
  },
