  "rdfs:comment": {
    "de": [
      "Wohnung F3, 38 m², 1. Stock. Am Meer , Meerblick, 1. Linie, im Stadtzentrum, 300m vom Hafen entfernt. Privater Parkplatz, Balkon, Gartenmöbel. Heizung, Gefrierschrank, Mikrowelle, private Waschmaschine, Telefon, Fernseher. Bett 140 cm Schlafzimmer 1, Bett 140 cm Schlafzimmer 2."
    ],
    "en": [
      "Apartment F3, 38 m², 1st floor. At the seaside, sea view, 1st line, in the city center, 300m from the port. Private parking, balcony, garden furniture. Heating, freezer, microwave oven, private washing machine, telephone, television. Bed 140 cm bedroom1, bed 140 cm bedroom2."
    ],
    "it": [
      "Appartamento F3, 38 m², 1° piano. Sul lungomare, vista mare, 1a linea, nel centro città, a 300 m dal porto. Parcheggio privato, balcone, mobili da giardino. Riscaldamento, congelatore, forno a microonde, lavatrice privata, telefono, televisione. Letto 140 cm in camera 1, letto 140 cm in camera 2."
    ],
    "fr": [
      "Appartement  F3, 38 m², 1er étage. Au bord de la mer , vue mer, 1ère ligne, en centre ville, à 300m du port. Parking privé, balcon, salon de jardin. Chauffage, congélateur, four à micro-ondes, lave linge privatif, téléphone, télévision. Lit 140 cm chambre1, lit 140 cm chambre2."
    ],
    "es": [
      "Piso F3, 38 m², 1ª planta. En primera línea de mar, con vistas al mar, en el centro de la ciudad, a 300 metros del puerto. Aparcamiento privado, balcón, muebles de jardín. Calefacción, congelador, microondas, lavadora privada, teléfono, televisión. Cama de 140 cm en la habitación 1, cama de 140 cm en la habitación 2."
    ],
    "nl": [
      "Flat F3, 38 m², 1e verdieping. Aan de kust, uitzicht op zee, 1e lijn, in het stadscentrum, 300 m van de haven. Eigen parkeerplaats, balkon, tuinmeubilair. Verwarming, vriezer, magnetron, eigen wasmachine, telefoon, televisie. Bed 140 cm in kamer 1, bed 140 cm in kamer 2."
    ]
  },
