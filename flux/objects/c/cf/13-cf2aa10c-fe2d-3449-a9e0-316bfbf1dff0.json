  "rdfs:comment": {
    "de": [
      "Reitstunden mit Ponys/Pferden.\r\nAufnahme von Kindern in Praktika während der Schulferien mit Halb- oder Vollpension.\r\nSpielerische Erziehung für alle Altersgruppen ab 1 Jahr."
    ],
    "en": [
      "Pony/horse riding sessions.\r\nReception of children on training courses during school holidays with half or full board.\r\nPlayful education for all ages, from 1 year old."
    ],
    "it": [
      "Sessioni di equitazione su pony/cavalli.\r\nCorsi per bambini durante le vacanze scolastiche, in mezza pensione o pensione completa.\r\nEducazione divertente per tutte le età, a partire da 1 anno."
    ],
    "fr": [
      "Séances d'équitation poney/cheval.\r\nAccueil des enfants en stage pendant les vacances scolaires en demi-pension ou pension complète.\r\nEducation ludique pour tous les âges, à partir de 1 an."
    ],
    "es": [
      "Sesiones de equitación.\r\nCursos para niños durante las vacaciones escolares, con media pensión o pensión completa.\r\nEducación divertida para todas las edades, a partir de 1 año."
    ],
    "nl": [
      "Pony/paardrij sessies.\r\nKindercursussen tijdens de schoolvakanties, halfpension of volpension.\r\nLeuk onderwijs voor alle leeftijden, vanaf 1 jaar."
    ]
  },
