  "rdfs:comment": {
    "de": [
      "GRIMMLAND® bietet Ihnen ein einzigartiges Erlebnis: Übernachten Sie inmitten eines Freizeitparks in trauter Zweisamkeit und mit dem Kopf in den Sternen!"
    ],
    "en": [
      "GRIMMLAND® offers you a unique experience, sleeping in the heart of an amusement park with your head in the stars!"
    ],
    "it": [
      "GRIMMLAND® vi offre un'esperienza unica, dormire nel cuore di un parco divertimenti con la testa tra le stelle!"
    ],
    "fr": [
      "GRIMMLAND® vous propose de vivre une expérience unique, dormir au cœur d'un parc de loisirs en amoureux et la tête dans les étoiles !"
    ],
    "es": [
      "GRIMMLAND® te ofrece una experiencia única, dormir en el corazón de un parque de atracciones con la cabeza en las estrellas"
    ],
    "nl": [
      "GRIMMLAND® biedt u een unieke ervaring, slapen in het hart van een recreatiepark met uw hoofd in de sterren!"
    ]
  },
