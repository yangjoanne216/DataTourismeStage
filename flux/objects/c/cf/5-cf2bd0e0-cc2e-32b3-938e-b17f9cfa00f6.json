  "rdfs:comment": {
    "de": [
      "Jacky Bletzacker wird Feinschmecker mit seinen Fundstücken verwöhnen, eine Küche, die sich je nach Markt und Jahreszeit weiterentwickelt."
    ],
    "en": [
      "Jacky Bletzacker will delight gourmets with his discoveries, a cuisine that evolves according to the market and the seasons."
    ],
    "it": [
      "Jacky Bletzacker delizierà i buongustai con le sue scoperte, una cucina che si evolve in base al mercato e alle stagioni."
    ],
    "fr": [
      "Jacky Bletzacker régalera les gourmets de ses trouvailles, une cuisine évolutive selon le marché et les saisons."
    ],
    "nl": [
      "Jacky Bletzacker zal de fijnproevers verrukken met zijn vondsten, een keuken die evolueert volgens de markt en de seizoenen."
    ],
    "es": [
      "Jacky Bletzacker deleitará a los gourmets con sus hallazgos, una cocina que evoluciona según el mercado y las estaciones."
    ]
  },
