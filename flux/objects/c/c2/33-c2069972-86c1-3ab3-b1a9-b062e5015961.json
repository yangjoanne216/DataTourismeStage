  "rdfs:comment": {
    "de": [
      "Entdecken Sie lokale Aromen und den guten Geschmack der neapolitanischen Pizzen in Granada!"
    ],
    "en": [
      "Discover local flavors and the great taste of Granada's Neapolitan pizzas!"
    ],
    "it": [
      "Scoprite i sapori locali e il grande gusto della pizza napoletana di Granada!"
    ],
    "fr": [
      "Découvrez des saveurs locales et le bon goût des pizzas Napolitaines de Grenade !"
    ],
    "nl": [
      "Ontdek de lokale smaken en de geweldige smaak van de Napolitaanse pizza's van Granada!"
    ],
    "es": [
      "Descubra los sabores locales y el gran sabor de las pizzas napolitanas de Granada"
    ]
  },
