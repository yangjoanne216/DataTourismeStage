  "rdfs:comment": {
    "de": [
      "Gehen Sie mit dem Komponisten Jim Petit auf eine ganz neue Entdeckungsreise am Altenweiher. Halten Sie die Ohren offen, um Ihre Schritte zu lenken!"
    ],
    "en": [
      "Discover the Altenweiher lake with the composer Jim Petit. Listen carefully to guide your steps!"
    ],
    "it": [
      "Scoprite il lago Altenweiher in compagnia del compositore Jim Petit. Ascoltate attentamente e seguite i vostri passi!"
    ],
    "fr": [
      "Partez à la découverte inédite du lac d’Altenweiher en compagnie du compositeur Jim Petit. Tendez l'oreille pour guider vos pas !"
    ],
    "nl": [
      "Ontdek het Altenweiher meer in gezelschap van componist Jim Petit. Luister goed en volg uw stappen!"
    ],
    "es": [
      "Descubra el lago Altenweiher en compañía del compositor Jim Petit. ¡Escucha con atención y sigue tus pasos!"
    ]
