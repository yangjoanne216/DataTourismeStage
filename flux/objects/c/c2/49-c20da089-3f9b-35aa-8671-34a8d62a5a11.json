  "rdfs:comment": {
    "de": [
      "Dominique lädt Sie ein, seine BIO-Produktion von Kürbissen, Kürbiskonfitüren, Apfelsaft, Kartoffeln, Karotten und Kürbissuppen zu probieren."
    ],
    "en": [
      "Dominique invites you to taste her ORGANIC production of squash, pumpkin jam, apple juice, potato, carrot and squash soup."
    ],
    "it": [
      "Dominique vi invita a degustare la sua produzione biologica di zucca, la marmellata di zucca, il succo di mela, la zuppa di patate, carote e zucca."
    ],
    "fr": [
      "Dominique vous invite à déguster sa production BIO de courges, confitures de citrouilles, de jus de pomme, pomme de terre, carottes et soupes de courges."
    ],
    "nl": [
      "Dominique nodigt u uit om te proeven van haar biologische pompoenproductie, pompoenjam, appelsap, aardappel-, wortel- en pompoensoep."
    ],
    "es": [
      "Dominique le invita a degustar su producción de calabaza ecológica, la mermelada de calabaza, el zumo de manzana y la sopa de patata, zanahoria y calabaza."
    ]
  },
