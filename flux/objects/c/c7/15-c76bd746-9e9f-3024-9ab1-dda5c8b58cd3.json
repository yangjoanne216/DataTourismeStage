  "rdfs:comment": {
    "de": [
      "Anglerfreunde, willkommen im Etang des Peupliers. Genießen Sie eine Umgebung mit Bäumen, in der sich auch die Kinder mit den Spielgeräten vergnügen können."
    ],
    "en": [
      "Fishermen friends welcome to the pond of Reboursin, enjoy a wooded setting where children can also have fun with games."
    ],
    "it": [
      "I pescatori, benvenuti al laghetto Peupliers, godono di un ambiente boscoso dove anche i bambini possono divertirsi con i giochi."
    ],
    "fr": [
      "Amis pêcheurs, bienvenue à l'étang des Peupliers, profitez d'un cadre arboré où les enfants pourront aussi s'amuser avec les jeux."
    ],
    "nl": [
      "Vissers, welkom bij de Peupliers vijver, genieten van een bosrijke omgeving waar ook kinderen zich kunnen vermaken met de spelletjes."
    ],
    "es": [
      "Pescadores, bienvenidos al estanque Peupliers, disfruten de un entorno arbolado donde los niños también pueden divertirse con los juegos."
    ]
  },
