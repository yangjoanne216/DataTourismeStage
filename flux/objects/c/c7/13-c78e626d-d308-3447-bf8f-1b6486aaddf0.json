  "rdfs:comment": {
    "de": [
      "Neu! Bergerie la Palette (Schäferei).\r\nChalet Restaurant und Bar auf der Höhe im Herzen der Natur.\r\nLokale und hausgemachte Küche, Bar, lebendige Weine, warme Atmosphäre und gesellige Musik."
    ],
    "en": [
      "New! Bergerie la Palette.\r\nChalet restaurant and mountain bar in the heart of nature.\r\nLocal and homemade cuisine, bar, warm atmosphere and friendly music."
    ],
    "it": [
      "Nuovo! Bergerie la Palette.\r\nRistorante e bar dello chalet nel cuore della natura.\r\nCucina locale e casalinga, bar, vini dal vivo, atmosfera calda e musica amichevole."
    ],
    "fr": [
      "Nouveau ! Bergerie la Palette.\r\nChalet restaurant et bar d'altitude au coeur de la nature.\r\nCuisine locale et faite maison, bar, vins vivants, ambiance chaleureuse et musique conviviale."
    ],
    "es": [
      "Novedad Bergerie la Palette.\r\nChalet restaurante y bar en plena naturaleza.\r\nCocina local y casera, bar, vinos en directo, ambiente cálido y música agradable."
    ],
    "nl": [
      "Nieuw! Bergerie la Palette.\r\nChalet restaurant en bar in het hart van de natuur.\r\nLokale en huisgemaakte keuken, bar, live wijnen, warme sfeer en vriendelijke muziek."
    ]
  },
