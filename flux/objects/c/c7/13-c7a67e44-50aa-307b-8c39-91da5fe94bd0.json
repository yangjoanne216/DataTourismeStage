  "rdfs:comment": {
    "de": [
      "Aqua Leman Nautique bietet Windsurfausrüstung (JP , BIC, Neilpride ) für alle Niveaus, zu mieten, vom Drift für Anfänger zum funboard für erfahrene Windsurfer."
    ],
    "en": [
      "Aqua Léman Nautique offers to rent windsurfing equipment for all levels, drift board for beginner to the board of funboard for experienced windsurfers."
    ],
    "it": [
      "Aqua Léman Nautique, Recommended Red Paddle Centre, offre attrezzature da windsurf a noleggio (JP, BIC, Neilpride) per tutti i livelli, dalle tavole da drifting per i principianti alle funboard per i windsurfisti esperti."
    ],
    "fr": [
      "Aqua Léman Nautique, Centre Recommandé Red Paddle, vous propose à la location du matériel de windsurf (JP, BIC, Neilpride) pour tous les niveaux, de la planche à dérive pour débutant à la planche de funboard pour les windsurfers expérimentés."
    ],
    "nl": [
      "Aqua Léman Nautique, Recommended Red Paddle Centre, verhuurt windsurfmateriaal (JP, BIC, Neilpride) voor alle niveaus, van driftboards voor beginners tot funboards voor ervaren windsurfers."
    ],
    "es": [
      "Aqua Léman Nautique, Centro de Paddle Recomendado, ofrece alquiler de material de windsurf (JP, BIC, Neilpride) para todos los niveles, desde tablas de deriva para principiantes hasta funboards para windsurfistas experimentados."
    ]
  },
