  "rdfs:comment": {
    "de": [
      "Besuchen Sie die Zucht von Kühen der Eringerrasse. Direktverkauf vom Erzeuger auf Ihren Teller. Kalbfleisch und Fleisch von Jungrindern. Einmachen in Gläsern."
    ],
    "en": [
      "Visit of the Hérens cow farm. Direct sale from the producer to your plate. Veal and young bovine meat. Canning in jars."
    ],
    "it": [
      "Visita all'allevamento di mucche di Hérens. Vendita diretta dal produttore al vostro piatto. Carne di vitello e di giovani bovini. Inscatolamento in barattoli."
    ],
    "fr": [
      "Visite de l'élevage de vache de la race Hérens. Vente directe du producteur dans votre assiette. Viande de veau et de jeune bovin. Conserverie en bocaux."
    ],
    "es": [
      "Visita a la granja de vacas Hérens. Venta directa del productor a su plato. Carne de ternera y de vacuno joven. Conservas en tarros."
    ],
    "nl": [
      "Bezoek aan de koeienboerderij Hérens. Rechtstreekse verkoop van de producent op uw bord. Vlees van kalfsvlees en jongvee. Inblikken in potten."
    ]
  },
