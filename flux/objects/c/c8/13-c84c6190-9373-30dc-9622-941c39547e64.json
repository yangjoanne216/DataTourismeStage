  "rdfs:comment": {
    "de": [
      "Food Truck (fahrender Anhänger): Bio-Restaurants vor Ort und zum Mitnehmen, auf Holzfeuer gekocht. Pizzas, Empanadas, Falafel, pflanzliche Fladenbrote, Blätterteiggebäck, Suppen, Salate, Desserts."
    ],
    "en": [
      "Food truck (mobile trailer): organic food on the spot and to take away, cooked on wood fire. Pizzas, empanadas, falafels, vegetable patties, soups, salads, desserts."
    ],
    "it": [
      "Food truck (rimorchio mobile): cibo biologico sul posto e da asporto, cucinato a legna. Pizze, empanadas, falafel, torte di verdure, sfogliatine, zuppe, insalate, dessert."
    ],
    "fr": [
      "Food truck (remorque ambulante) : restauration bio sur place et à emporter, cuisson au feu de bois. Pizzas, empanadas, falafels, galettes végétales, feuilletés, soupes, salades, desserts."
    ],
    "es": [
      "Food truck (remolque móvil): comida ecológica in situ y para llevar, cocinada con fuego de leña. Pizzas, empanadas, falafels, pasteles vegetales, hojaldres, sopas, ensaladas, postres."
    ],
    "nl": [
      "Food truck (mobiele trailer): biologisch voedsel ter plaatse en om mee te nemen, gekookt op houtvuur. Pizza's, empanadas, falafels, groentetaarten, bladerdeeg, soepen, salades, desserts."
    ]
  },
