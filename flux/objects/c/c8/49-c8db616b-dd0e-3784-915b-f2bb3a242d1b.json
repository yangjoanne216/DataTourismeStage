  "rdfs:comment": {
    "de": [
      "Crêperie Pizzeria im Ortszentrum von Champigné, wenige Minuten von Châteauneuf sur Sarthe und Le Lion d'Angers entfernt, mit verschiedenen Mittags- und Abendangeboten, vor Ort oder zum Mitnehmen."
    ],
    "en": [
      "Creperie-pizzeria in the centre of Champigné, a few minutes from Châteauneuf sur Sarthe and the Lion d'Angers with different lunch and dinner options, on site or to take away."
    ],
    "it": [
      "Creperie pizzeria nel centro di Champigné, a pochi minuti da Châteauneuf sur Sarthe e Le Lion d'Angers, con varie formule di mezzogiorno e sera, in loco o da asporto."
    ],
    "fr": [
      "Crêperie pizzeria en centre-bourg de Champigné, à quelques minutes de Châteauneuf sur Sarthe et du Lion d'Angers avec différentes formules midis et soirs, sur place ou à emporter."
    ],
    "es": [
      "Pizzería crepería en el centro de Champigné, a pocos minutos de Châteauneuf sur Sarthe y de Le Lion d'Angers, con diversas fórmulas de mediodía y de noche, en el lugar o para llevar."
    ],
    "nl": [
      "Creperie pizzeria in het centrum van Champigné, op enkele minuten van Châteauneuf sur Sarthe en Le Lion d'Angers, met verschillende middag- en avondformules, ter plaatse of om mee te nemen."
    ]
  },
