  "rdfs:comment": {
    "de": [
      "Am Tor zu den Gorges du Verdon, schattige Stellplätze mit Pool, Lebensmittelgeschäft, Snackbar und Bäckerei ( Tennis 200 m entfernt). Schöne Mobilheime mit allem Komfort ab 2 Nächten."
    ],
    "en": [
      "At the gates of the Gorges du Verdon, shady pitches with swimming pool, grocery store, snack bar and bakery. ( Tennis court at 200 m ). Beautiful mobilhomes with all comfort from 2 nights."
    ],
    "it": [
      "Alle porte delle Gole del Verdon, piazzole ombreggiate con piscina, negozio di alimentari, snack bar e panetteria (campo da tennis a 200 m). Case mobili belle e confortevoli a partire da 2 notti."
    ],
    "fr": [
      "Aux portes des Gorges du Verdon, emplacements ombragés avec piscine, épicerie, Snack-bar et boulangerie. ( Tennis à 200 m). Beaux mobilhomes tout confort à partir de 2 nuits."
    ],
    "nl": [
      "Aan de poort van de Gorges du Verdon, schaduwrijke plaatsen met zwembad, kruidenier, snackbar en bakker (tennisbaan op 200 m). Mooie en comfortabele stacaravans vanaf 2 nachten."
    ],
    "es": [
      "A las puertas de las Gargantas del Verdon, parcelas sombreadas con piscina, tienda de comestibles, snack bar y panadería (pista de tenis a 200 m). Hermosas y confortables casas móviles a partir de 2 noches."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Camping Chateau de l'Eouvière"
    ]
  },
