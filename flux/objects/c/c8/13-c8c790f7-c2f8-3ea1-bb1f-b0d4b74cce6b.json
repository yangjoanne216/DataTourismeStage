  "rdfs:comment": {
    "de": [
      "Paragliding-Schule im Süden der Ardèche mit Sitz im Herzen des Regionalparks Monts d'Ardèche. Die Schule bietet verschiedene Taufen, Ausbildungskurse und den Verkauf von Material an. Für alle Altersgruppen und ohne Altersbeschränkung."
    ],
    "en": [
      "Paragliding school of the South Ardèche based in the heart of the Monts d'Ardèche regional park. The school offers various baptisms, training courses and the sale of equipment. For all public and without age limit."
    ],
    "it": [
      "Scuola di parapendio dell'Ardèche meridionale con sede nel cuore del parco regionale dei Monts d'Ardèche. La scuola offre diversi primi voli, corsi di formazione e vendita di attrezzature. Per tutte le età."
    ],
    "fr": [
      "Ecole de Parapente du Sud Ardèche basée au cœur du parc régional des Monts d'Ardèche. L'école propose différents baptêmes, des stages de formation et la vente de matériel. Pour tout public et sans limite d'âge."
    ],
    "nl": [
      "Paragliding school van de Zuid-Ardèche gevestigd in het hart van het regionale park Monts d'Ardèche. De school biedt verschillende eerste vluchten, opleidingen en de verkoop van apparatuur. Voor alle leeftijden."
    ],
    "es": [
      "Escuela de parapente del sur de Ardèche con sede en el corazón del parque regional de los Montes de Ardèche. La escuela ofrece varios primeros vuelos, cursos de formación y venta de equipos. Para todas las edades."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Parapente avec Taranis"
    ]
  },
  "COVID19SpecialMeasures": {
    "de": [
      "Bitte um das Tragen von Maske, Brille und Handschuhen. Wir befinden uns die ganze Zeit in der Natur"
    ],
    "en": [
      "Request to wear a mask, glasses and gloves. We are always in the middle of nature"
    ],
    "it": [
      "Chiedere di indossare maschera, occhiali e guanti. Siamo sempre in mezzo alla natura"
    ],
    "fr": [
      "Demande du port du masque, lunettes et gants. Nous sommes tout le temps en pleine nature"
    ],
    "nl": [
      "Verzoek om een masker, bril en handschoenen te dragen. We staan altijd midden in de natuur"
    ],
    "es": [
      "Solicita llevar máscara, gafas y guantes. Siempre estamos en medio de la naturaleza"
    ]
  },
