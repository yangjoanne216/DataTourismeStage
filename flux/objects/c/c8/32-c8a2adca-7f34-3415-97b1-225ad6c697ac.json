  "rdfs:comment": {
    "de": [
      "Alain Barrault, Antiquitätenhändler, Dekorateur und Trendsetter, empfängt Sie in seinem Geschäft, in dem sich Gegenstände und Möbel in Hülle und Fülle ohne falsche Noten stapeln."
    ],
    "en": [
      "Antique dealer, decorator and trend finder, Alain Barrault welcomes you to his store where objects and furniture are piled up in profusion without false note."
    ],
    "it": [
      "Antiquario, decoratore e trend scout, Alain Barrault vi dà il benvenuto nel suo negozio dove oggetti e mobili sono accatastati a profusione, senza alcuna nota falsa."
    ],
    "fr": [
      "Antiquaire, décorateur et dénicheur de tendances, Alain Barrault vous accueille au sein de sa boutique où les objets et le mobilier s’empilent à profusion sans fausse note."
    ],
    "nl": [
      "Antiquair, decorateur en trendscout, Alain Barrault verwelkomt u in zijn winkel waar voorwerpen en meubels in overvloed zijn opgestapeld zonder valse noten."
    ],
    "es": [
      "Anticuario, decorador y buscador de tendencias, Alain Barrault le da la bienvenida a su tienda, donde los objetos y los muebles se amontonan con profusión y sin notas falsas."
    ]
  },
