  "rdfs:comment": {
    "de": [
      "David und Patou begrüßen Sie in einer warmen und freundlichen Atmosphäre. um ihre hausgemachten Gerichte im Herzen des Dorfes Le Beausset zu genießen."
    ],
    "en": [
      "David and Patou welcome you in a warm and friendly atmosphere to taste their homemade dishes in the heart of the village of Le Beausset."
    ],
    "it": [
      "David e Patou vi accolgono in un'atmosfera calda e amichevole per farvi gustare i loro piatti fatti in casa, nel cuore del villaggio di Le Beausset."
    ],
    "fr": [
      "David et Patou vous accueillent dans une ambiance chaleureuse et conviviale. pour déguster leurs plats faits maison, au cœur du village du Beausset."
    ],
    "es": [
      "David y Patou le dan la bienvenida en un ambiente cálido y acogedor para degustar sus platos caseros, en el corazón del pueblo de Le Beausset."
    ],
    "nl": [
      "David en Patou verwelkomen u in een warme en vriendelijke sfeer om hun huisgemaakte gerechten te proeven, in het hart van het dorp Le Beausset."
    ]
  },
