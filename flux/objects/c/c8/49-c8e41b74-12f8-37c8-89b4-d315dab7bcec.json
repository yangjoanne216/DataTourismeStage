  "rdfs:comment": {
    "de": [
      "Zimmer mit atemberaubendem Blick auf die Loire. Verpflegung vor Ort möglich: frische Produkte (Fisch aus der Loire) und saisonale Produkte."
    ],
    "en": [
      "Rooms with a breathtaking view of the Loire. Catering on site possible: fresh products (Loire fish) and seasonal products."
    ],
    "it": [
      "Camere con vista mozzafiato sulla Loira. Possibilità di ristorazione in loco: prodotti freschi (pesce della Loira) e di stagione."
    ],
    "fr": [
      "Chambres avec vue imprenable sur la Loire. Restauration sur place possible : produits frais (poissons de Loire) et de saison."
    ],
    "nl": [
      "Kamers met een adembenemend uitzicht op de Loire. Catering ter plaatse mogelijk: verse (Loire-vis) en seizoensproducten."
    ],
    "es": [
      "Habitaciones con una impresionante vista del Loira. Posibilidad de restauración in situ: productos frescos (pescado del Loira) y de temporada."
    ]
  },
