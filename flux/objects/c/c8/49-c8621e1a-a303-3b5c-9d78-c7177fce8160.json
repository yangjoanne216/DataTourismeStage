  "rdfs:comment": {
    "de": [
      "Das Herrenhaus aus dem XV-XVIII. Jahrhundert überragt einen zweiteiligen Garten, der eine große Rasenfläche einrahmt."
    ],
    "en": [
      "The Manor House XV-XVIII th century overlooks a garden in two parts framing a vast lawn."
    ],
    "it": [
      "La casa padronale del XVIII secolo si affaccia su un giardino in due parti che incornicia un vasto prato."
    ],
    "fr": [
      "Le Manoir XV-XVIII ème surplombe un jardin en deux parties encadrant une vaste pelouse."
    ],
    "es": [
      "La casa solariega del siglo XVIII da a un jardín en dos partes que enmarca un amplio césped."
    ],
    "nl": [
      "Het 18e eeuwse landhuis kijkt uit op een tuin in twee delen die een groot grasveld omlijsten."
    ]
  },
