  "rdfs:comment": {
    "de": [
      "Charcuterie auf dem Land mitten im historischen Zentrum von Aix-en-Provence. Die Familie Bianco führt die Geschäfte seit über 30 Jahren."
    ],
    "en": [
      "A country delicatessen in the heart of the historic center of Aix-en-Provence, the Bianco family has been running the place for over 30 years."
    ],
    "it": [
      "La famiglia Bianco gestisce da oltre 30 anni questa gastronomia di campagna nel cuore del centro storico di Aix-en-Provence."
    ],
    "fr": [
      "Charcuterie de campagne en plein centre historique d'Aix-en-Provence, la famille Bianco tient les lieux depuis plus de 30 ans."
    ],
    "es": [
      "La familia Bianco regenta desde hace más de 30 años esta charcutería campestre en pleno centro histórico de Aix-en-Provence."
    ],
    "nl": [
      "De familie Bianco runt al meer dan 30 jaar deze landelijke delicatessenzaak in het hart van het historische centrum van Aix-en-Provence."
    ]
  },
