  "rdfs:comment": {
    "de": [
      "Familiengeführtes Hotel-Restaurant mit komfortablen, klimatisierten Zimmern mit eigenem Bad für bis zu 5 Personen. Restaurant für alle geöffnet, jeden Abend von 19:00 bis 21:30 Uhr mit traditioneller und regionaler Küche, die hausgemacht ist."
    ],
    "en": [
      "Family hotel-restaurant offering comfortable air-conditioned rooms all with private bathrooms for one, two, three, four and even five people."
    ],
    "it": [
      "Albergo-ristorante a conduzione familiare. Offre camere confortevoli, climatizzate e con bagno privato per una, due, tre, quattro e anche cinque persone."
    ],
    "fr": [
      "Hôtel-Restaurant familial proposant des chambres confortables climatisées avec salles de bain privatives jusqu'à 5 personnes. Restaurant ouvert à tous, tous les soirs de 19h à 21h30 proposant une cuisine traditionnelle et régionale faite maison."
    ],
    "es": [
      "Hotel-restaurante de gestión familiar que ofrece cómodas habitaciones con aire acondicionado y baño privado para un máximo de 5 personas. Restaurante abierto a todos, todas las noches de 19:00 a 21:30 que ofrece cocina casera tradicional y regional."
    ],
    "nl": [
      "Familiehotel-restaurant met comfortabele kamers met airconditioning en eigen badkamer voor maximaal 5 personen. Restaurant open voor iedereen, elke avond van 19.00 tot 21.30 uur met traditionele en regionale huisgemaakte gerechten."
    ]
  },
