      "dc:description": {
        "de": [
          "Sonntag, 6. August: PARGUES - Flohmarkt, ab 6:30 Uhr für Aussteller, 8 ? für Besucher. 1,50 ? pro ml oder 5 ? für 5 ml. 60-80 Aussteller. Organisiert von Culture et Animation à Pargues. Kontakt: +33 (0)6 24 97 11 00 - cultureetanimationapargues@gmail.com"
        ],
        "en": [
          "Sunday August 6: PARGUES - Garage sale, from 6.30 am for exhibitors, €8 for visitors. €1.50 per ml or €5 for 5 ml. 60-80 exhibitors. Organized by Culture et Animation in Pargues. Contact: +33 (0)6 24 97 11 00 - cultureetanimationapargues@gmail.com"
        ],
        "it": [
          "Domenica 6 agosto : PARGUES - Mercato delle pulci, dalle ore 6.30 per gli espositori, 8 ? per i visitatori. 1,50 ? per ml o 5 ? per 5 ml. 60-80 espositori. Organizzato da Culture et Animation à Pargues. Contatto: +33 (0)6 24 97 11 00 - cultureetanimationapargues@gmail.com"
        ],
        "fr": [
          "Dimanche 6 août : PARGUES - Vide grenier, à partir de 6h30 pour les exposants, 8 € pour les visiteurs. 1,50 € le ml ou 5 € les 5 ml. 60-80 exposants. Organisé par Culture et Animation à Pargues. Contact : +33 (0)6 24 97 11 00 - cultureetanimationapargues@gmail.com"
        ],
        "nl": [
          "Zondag 6 augustus : PARGUES - Vlooienmarkt, vanaf 6.30 uur voor standhouders, 8 ? voor bezoekers. 1,50 ? per ml of 5 ? per 5 ml. 60-80 standhouders. Georganiseerd door Culture et Animation à Pargues. Contact: +33 (0)6 24 97 11 00 - cultureetanimationapargues@gmail.com"
        ],
        "es": [
          "Domingo 6 de agosto : PARGUES - Rastro, a partir de las 6h30 para los expositores, 8€ para los visitantes. 1,50€ por ml o 5€ por 5 ml. 60-80 expositores. Organizado por Culture et Animation à Pargues. Contacto: +33 (0)6 24 97 11 00 - cultureetanimationapargues@gmail.com"
        ]
      },
