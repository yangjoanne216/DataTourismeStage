  "rdfs:comment": {
    "de": [
      "Dieser in La Pouëze gelegene Bauernhof bietet Ihnen Geflügelprodukte (Hühner, Perlhühner und Topfhühner) sowie Hühnereier an."
    ],
    "en": [
      "Located in La Pouëze, this farm offers poultry products (chickens, guinea fowl and potted hens) as well as hen eggs."
    ],
    "it": [
      "Situata a La Pouëze, questa fattoria offre prodotti avicoli (polli, faraone e galline da carne) e uova di gallina."
    ],
    "fr": [
      "Située à la Pouëze, cette ferme vous propose des produits volailles (poulets, pintades et poules au pot) ainsi que des oeufs de poule."
    ],
    "es": [
      "Situada en La Pouëze, esta granja ofrece productos avícolas (pollos, gallinas de Guinea y gallinas de marmita), así como huevos de gallina."
    ],
    "nl": [
      "Deze boerderij in La Pouëze biedt pluimveeproducten (kippen, parelhoenders en plofkippen) en kippeneieren."
    ]
  },
