  "rdfs:comment": {
    "de": [
      "Eine authentische Küche aus Laos und Thailand, in der Sie unsere TOM YUM KUNG (Garnelensuppe) entdecken können, die das ganze Jahr über jeden Abend angeboten wird."
    ],
    "en": [
      "An authentic cuisine from Laos and Thailand, where you can discover our TOM YUM KUNG (shrimp soup) proposed every evening of the year."
    ],
    "it": [
      "Autentica cucina laotiana e thailandese, dove potrete scoprire il nostro TOM YUM KUNG (zuppa di gamberi) offerto ogni sera dell'anno."
    ],
    "fr": [
      "Une cuisine authentique du Laos et de la Thailande, où vous pourrez découvrir notre TOM YUM KUNG (soupe de crevettes) proposée chaque soir de l'année."
    ],
    "nl": [
      "Authentieke Laotiaanse en Thaise keuken, waar u onze TOM YUM KUNG (garnalensoep) kunt ontdekken die elke avond van het jaar wordt aangeboden."
    ],
    "es": [
      "Auténtica cocina laosiana y tailandesa, donde podrá descubrir nuestro TOM YUM KUNG (sopa de gambas) que se ofrece todas las noches del año."
    ]
  },
