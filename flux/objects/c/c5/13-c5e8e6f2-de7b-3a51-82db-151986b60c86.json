  "rdfs:comment": {
    "de": [
      "WIRD BIS NOVEMBER 2023 RENOVIERT.\r\nSie wurde am 26. Oktober 1208 in Anwesenheit des Prinzen Wilhelm von Les Baux Unserer Lieben Frau von Nazareth geweiht."
    ],
    "en": [
      "UNDER CONSTRUCTION UNTIL NOVEMBER 2023.\r\nOrange’s cathedral was dedicated to Our Lady of Nazareth on the 26th October 1208, in the presence of Guillaume des Baux, Prince of Orange."
    ],
    "it": [
      "IN COSTRUZIONE FINO AL NOVEMBRE 2023.\r\nFu consacrata il 26 ottobre 1208 a Nostra Signora di Nazareth alla presenza del principe Guillaume des Baux."
    ],
    "fr": [
      "EN TRAVAUX JUSQU'EN NOVEMBRE 2023.\r\nElle est consacrée le 26 octobre 1208 à Notre Dame de Nazareth en présence du prince Guillaume des Baux."
    ],
    "nl": [
      "IN AANBOUW TOT NOVEMBER 2023.\r\nHet werd op 26 oktober 1208 gewijd aan Onze Lieve Vrouw van Nazareth in aanwezigheid van prins Guillaume des Baux."
    ],
    "es": [
      "EN CONSTRUCCIÓN HASTA NOVIEMBRE DE 2023.\r\nFue consagrada el 26 de octubre de 1208 a Nuestra Señora de Nazaret en presencia del príncipe Guillaume des Baux."
    ]
  },
