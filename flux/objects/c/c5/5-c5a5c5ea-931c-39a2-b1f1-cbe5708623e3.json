  "rdfs:comment": {
    "de": [
      "Kaffeerösterei Schreiber, gegründet 193, eine der letzten handwerklichen Röstereien in der Region. 16 Kaffeesorten, Damann-Tees, Süßwaren, gefüllte Körbe, Maschinen und Zubehör."
    ],
    "en": [
      "Schreiber coffee roastery, founded in 193, one of the last artisanal roasteries in the region. 16 kinds of coffee, Damann teas, confectionery, filled baskets, machines and accessories."
    ],
    "it": [
      "Torrefazione Schreiber, fondata nel 193, una delle ultime torrefazioni artigianali della regione. 16 tipi di caffè, tè Damann, dolciumi, cesti regalo, macchine e accessori."
    ],
    "fr": [
      "Torréfaction de café Schreiber, fondée en 193, l'une des dernières torréfactions artisanales de la région. 16 sortes de cafés, thés Damann, confiseries, paniers garnis, machines et accessoires."
    ],
    "es": [
      "Tostadero de café Schreiber, fundado en 193, uno de los últimos tostaderos artesanales de la región. 16 tipos de café, tés Damann, confitería, cestas de regalo, máquinas y accesorios."
    ],
    "nl": [
      "Koffiebranderij Schreiber, opgericht in 193, een van de laatste ambachtelijke branderijen in de regio. 16 soorten koffie, Damann-thee, zoetwaren, geschenkmanden, machines en accessoires."
    ]
  },
