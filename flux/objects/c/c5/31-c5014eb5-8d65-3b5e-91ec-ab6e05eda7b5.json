  "rdfs:comment": {
    "de": [
      "Möblierte Unterkunft in Cransac-les-Thermes, dem einzigen Kurort im Aveyron und der Stadt des Wohlbefindens. Unabhängiges Haus auf dem Land, 2 km von der Stadt entfernt."
    ],
    "en": [
      "Furnished accommodation in the Aveyronnaise spa resort of Cransac. Fully equipped detached house in the country 2 km from the town centre."
    ],
    "it": [
      "Alloggio turistico ammobiliato situato a Cransac-les-Thermes, unico centro termale dell'Aveyron e città del benessere. Casa indipendente situata in campagna a 2 km dalla città."
    ],
    "fr": [
      "Meublé de tourisme situé à Cransac-les-Thermes, l'unique station thermale aveyronnaise et la ville de bien-être. Maison indépendante située en campagne à 2 km de la ville."
    ],
    "nl": [
      "Gemeubileerde toeristische accommodatie gelegen in Cransac-les-Thermes, het enige kuuroord in Aveyron en de stad van het welzijn. Vrijstaand huis gelegen op het platteland op 2 km van de stad."
    ],
    "es": [
      "Amueblado de turismo en Cransac-les-Thermes, el unico centro termal en Aveyron y la ciudad del bienestar. Amueblado en una casa."
    ]
  },
