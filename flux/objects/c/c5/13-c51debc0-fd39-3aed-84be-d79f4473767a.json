  "rdfs:comment": {
    "de": [
      "Entdecken Sie, versteckt in einer Seitengasse, das Musée de l'Etrange. Die Führung durch das Museum richtet sich an Gruppen von 4 bis 8 Personen und dauert etwa 2 Stunden.Reservierung erforderlich."
    ],
    "en": [
      "Come to discover, nested at the bottom of an alley, the Strange Museum."
    ],
    "it": [
      "Venite a scoprire il Museo dello Strano, nascosto in un vicolo. La visita guidata al museo è riservata a gruppi di 4-8 persone e dura circa 2 ore."
    ],
    "fr": [
      "Venez découvrir, niché au fond d'une ruelle, le Musée de l'Etrange. La visite guidée du musée s'adresse aux groupes de 4 à 8 personnes et dure environ 2h.Réservation obligatoire."
    ],
    "es": [
      "Venga a descubrir el Museo de lo Extraño, enclavado en un callejón. La visita guiada al museo es para grupos de 4 a 8 personas y dura unas 2 horas."
    ],
    "nl": [
      "Kom en ontdek het Museum van het Vreemde, genesteld in een steegje. De rondleiding door het museum is voor groepen van 4 tot 8 personen en duurt ongeveer 2 uur."
    ]
  },
