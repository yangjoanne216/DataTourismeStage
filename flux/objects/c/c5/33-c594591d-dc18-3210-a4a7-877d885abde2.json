  "rdfs:comment": {
    "de": [
      "Die Freizeitanlage La Ramée empfängt Sie, um Ihre Tage spielerischer, sportlicher und angenehmer zu gestalten!"
    ],
    "en": [
      "The leisure center of La Ramée welcomes you to make your days more playful, sporty and pleasant!"
    ],
    "it": [
      "Il centro ricreativo La Ramée vi accoglie per rendere le vostre giornate più divertenti, sportive e piacevoli!"
    ],
    "fr": [
      "La base de loisirs de la Ramée vous accueille pour rendre vos journées plus ludiques, sportives et agréables !"
    ],
    "nl": [
      "Het recreatiecentrum La Ramée verwelkomt u om uw dagen leuker, sportiever en leuker te maken!"
    ],
    "es": [
      "El centro de ocio de La Ramée le da la bienvenida para que sus días sean más divertidos, deportivos y agradables"
    ]
  },
