  "rdfs:comment": {
    "de": [
      "Libanesisches Restaurant und Buffet, frische Hausmannskost, samstags Abendunterhaltung mit Klavier und Synthesizer. Libanesische Musik und orientalischer Tanz. Gegenüber dem Archipel-Theater."
    ],
    "en": [
      "Lebanese restaurant and buffet, fresh family cuisine, entertainment on Saturday nights with piano and synthesizer. Lebanese music and oriental dance. Opposite the Archipel theatre."
    ],
    "it": [
      "Ristorante e buffet libanese, cucina fresca e familiare, intrattenimento il sabato sera con pianoforte e sintetizzatore. Musica libanese e danza orientale. Di fronte al teatro Archipel."
    ],
    "fr": [
      "Restaurant et buffet libanais, cuisine familiale fraiche, animation les samedis soir au piano et au synthétiseur. Musique libanaise et danse orientale. Face au théâtre de l'Archipel."
    ],
    "es": [
      "Restaurante y buffet libanés, cocina familiar fresca, animación los sábados por la noche con piano y sintetizador. Música libanesa y danza oriental. Frente al teatro Archipel."
    ],
    "nl": [
      "Libanees restaurant en buffet, verse familiekeuken, animatie op zaterdagavond met piano en synthesizer. Libanese muziek en oosterse dans. Tegenover het Archipel theater."
    ]
  },
