  "rdfs:comment": {
    "de": [
      "Entdecken Sie Strike, eine Bar / Bowlingbahn im ersten Stock des Balneotherapiezentrums. Die Bowlingbahn ist für Kinder ab 5 Jahren (1 Rampe \"Kugelwerfer\" vorhanden) in Begleitung eines Erwachsenen zugänglich.\r\nKeine Anmeldung für ein Spiel."
    ],
    "en": [
      "Discover the Strike, bar / bowling alley on the first floor of the Balneotherapy Center. Bowling is accessible to children from 5 years old (1 \"ball launcher\" ramp available), accompanied by an adult.\r\nNo registration for a game."
    ],
    "it": [
      "Scoprite lo Strike, un bar/pista da bowling al primo piano del Centro di Balneoterapia. La pista da bowling è accessibile ai bambini a partire dai 5 anni (è disponibile 1 rampa \"lanciapalle\"), accompagnati da un adulto.\r\nNessuna registrazione per una partita."
    ],
    "fr": [
      "Découvrez le Strike, bar / bowling au premier étage du Centre de Balnéothérapie. Le bowling est accessible aux enfants à partir de 5 ans (1 rampe «lance-boule» disponible), accompagnés d’un adulte.\r\nAucune inscription pour une partie."
    ],
    "nl": [
      "Ontdek Strike, een bar/kegelbaan op de eerste verdieping van het Balneotherapiecentrum. De bowlingbaan is toegankelijk voor kinderen vanaf 5 jaar (1 \"ball launcher\" helling beschikbaar), onder begeleiding van een volwassene.\r\nGeen registratie voor een spel."
    ],
    "es": [
      "Descubra Strike, un bar / bolera en la primera planta del Centro de Balneoterapia. La bolera es accesible a los niños a partir de 5 años (1 rampa de \"lanzamiento de bolas\" disponible), acompañados por un adulto.\r\nNo hay que registrarse para un juego."
    ]
  },
  "rdfs:label": {
    "en": [
      "The Strike - Bowling and bar"
    ],
    "fr": [
      "Le Strike - Bar / Bowling"
    ],
    "nl": [
      "Strike - Bowling en Bar"
    ]
  },
