  "rdfs:comment": {
    "de": [
      "Die Seifenfabrik in Cardaillac öffnet ihre Türen für Sie! Entdecken Sie die Geheimnisse der Herstellung von kaltverseifter Seife!"
    ],
    "en": [
      "The Cardaillac soap factory opens its doors to you! Come and discover the secrets of cold process soap making!"
    ],
    "it": [
      "Il saponificio Cardaillac vi apre le sue porte! Venite a scoprire i segreti della fabbricazione del sapone a freddo!"
    ],
    "fr": [
      "La savonnerie de Cardaillac vous ouvre ses portes ! Venez découvrir les secrets de la fabrication du savon saponifié à froid !"
    ],
    "es": [
      "La jabonería Cardaillac le abre sus puertas Venga a descubrir los secretos de la fabricación del jabón en frío"
    ],
    "nl": [
      "De zeepfabriek Cardaillac opent haar deuren voor u! Kom en ontdek de geheimen van het koud bewerken van zeep!"
    ]
  },
