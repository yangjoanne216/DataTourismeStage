  "rdfs:comment": {
    "de": [
      "Restaurant Brasserie L'Atelier du Jardin in L'Isle sur la Sorgue empfängt Sie mittags und abends auf seiner Terrasse unter einer wunderschönen Glyzinie, um Gourmetgerichte zu genießen."
    ],
    "en": [
      "Restaurant brasserie L'Atelier du Jardin in L'Isle sur la Sorgue welcomes you at lunchtime and in the evening on its terrace, under a magnificent wisteria, to enjoy gourmet dishes."
    ],
    "it": [
      "Il ristorante brasserie L'Atelier du Jardin a L'Isle sur la Sorgue vi accoglie a pranzo e la sera sulla sua terrazza, sotto un magnifico glicine, per gustare piatti gourmet."
    ],
    "fr": [
      "Restaurant brasserie L'Atelier du Jardin à L'Isle sur la Sorgue vous accueille le midi et le soir sur sa terrasse, sous une magnifique glycine pour déguster des plats gourmands."
    ],
    "nl": [
      "Restaurant brasserie L'Atelier du Jardin in L'Isle sur la Sorgue verwelkomt u 's middags en 's avonds op het terras, onder een prachtige blauwe regen, om te genieten van gastronomische gerechten."
    ],
    "es": [
      "El restaurante brasserie L'Atelier du Jardin, en L'Isle sur la Sorgue, le recibe a la hora de comer y por la noche en su terraza, bajo una magnífica glicina, para disfrutar de platos gourmet."
    ]
  },
