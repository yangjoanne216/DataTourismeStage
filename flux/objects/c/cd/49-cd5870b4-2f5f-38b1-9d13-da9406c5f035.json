  "rdfs:comment": {
    "de": [
      "Entdecken Sie verschiedene Aktivitäten: Reitunterricht, Pferdepension, Horse Ball, Ponyreiten, Ausritte an den Ufern der Loire."
    ],
    "en": [
      "Come and discover different activities: horseback riding lessons, horse boarding, horse ball, pony rides, horseback riding on the banks of the Loire."
    ],
    "it": [
      "Venite a scoprire diverse attività: lezioni di equitazione, pensione per cavalli, horse ball, passeggiate su pony, passeggiate a cavallo sulle rive della Loira."
    ],
    "fr": [
      "Venez découvrir différentes activités : cours d'équitation, pension équine, horse ball, promenade à poneys, balade à cheval sur les bords de Loire."
    ],
    "es": [
      "Venga a descubrir diferentes actividades: clases de equitación, pupilaje, horse ball, paseos en poni, paseos a caballo a orillas del Loira."
    ],
    "nl": [
      "Kom en ontdek verschillende activiteiten: paardrijlessen, paardenpension, paardenbal, ponyrijden, paardrijden aan de oevers van de Loire."
    ]
  },
