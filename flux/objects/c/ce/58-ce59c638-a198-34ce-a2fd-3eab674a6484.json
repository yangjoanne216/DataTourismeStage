  "rdfs:comment": {
    "de": [
      "Seit 2017 öffnet das Atelier d'Andréa montags bis samstags von 9 bis 18 Uhr seine Türen für Frauen und Männer."
    ],
    "en": [
      "Since 2017, Andrea's Workshop has opened its doors to women and men Monday through Saturday from 9am to 6pm."
    ],
    "it": [
      "Dal 2017, l'Officina di Andréa apre le sue porte a donne e uomini dal lunedì al sabato dalle 9.00 alle 18.00."
    ],
    "fr": [
      "Depuis 2017, l'Atelier d'Andréa ouvre ses portes aux femmes et aux hommes du lundi au samedi de 9h à 18h."
    ],
    "nl": [
      "Sinds 2017 opent Andréa's Workshop haar deuren voor vrouwen en mannen van maandag tot en met zaterdag van 9.00 tot 18.00 uur."
    ],
    "es": [
      "Desde 2017, el Taller de Andréa abre sus puertas a mujeres y hombres de lunes a sábado de 9:00 a 18:00 horas."
    ]
