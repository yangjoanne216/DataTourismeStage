  "rdfs:comment": {
    "de": [
      "A la ferme des p'tits bergers: Besichtigung des Bauernhofs durch unseren kleinen Hirten Noa: eine Ziege, ein Schaf - was frisst das?"
    ],
    "en": [
      "A la ferme des p'tits berger : Visit of the farm by our little shepherd Noa : what does a goat or a sheep eat ?"
    ],
    "it": [
      "A la ferme des pits bergers : Visita della fattoria da parte del nostro piccolo pastore Noa: una capra, una pecora, cosa mangiano?"
    ],
    "fr": [
      "A la ferme des p’tits bergers : Visite de la ferme par notre p’tit berger Noa : une chèvre, un mouton ça mange quoi ?"
    ],
    "es": [
      "A la ferme des p?tits bergers : Visita de la granja por nuestro pequeño pastor Noa : una cabra, una oveja, ¿qué comen?"
    ],
    "nl": [
      "A la ferme des p'tits bergers : Bezoek aan de boerderij door onze kleine herder Noa : een geit, een schaap, wat eten ze ?"
    ]
  },
