  "rdfs:comment": {
    "de": [
      "Pizza zum Mitnehmen. Dienstags und freitags in Aveizieux, Place du Bourg; samstags in L'Etrat, gegenüber der Kirche und sonntags in Saint-Médard en Forez, Place de l'église."
    ],
    "en": [
      "Pizza to go. Tuesdays and Fridays in Aveizieux, place du Bourg; Saturdays in L'Etrat, opposite the church and Sundays in Saint-Médard en Forez, place de l'église."
    ],
    "it": [
      "Pizza da asporto. Il martedì e il venerdì ad Aveizieux, place du Bourg; il sabato a L'Etrat, di fronte alla chiesa e la domenica a Saint-Médard en Forez, place de l'église."
    ],
    "fr": [
      "Pizza à emporter. Les mardis et vendredis à Aveizieux, place du Bourg ; les samedis à L'Etrat, face à l'église et les dimanches à Saint-Médard en Forez, place de l'église."
    ],
    "es": [
      "Pizza para llevar. Martes y viernes en Aveizieux, place du Bourg; sábados en L'Etrat, frente a la iglesia y domingos en Saint-Médard en Forez, place de l'église."
    ],
    "nl": [
      "Pizza om mee te nemen. Dinsdag en vrijdag in Aveizieux, place du Bourg; zaterdag in L'Etrat, tegenover de kerk en zondag in Saint-Médard en Forez, place de l'église."
    ]
  },
