  "rdfs:comment": {
    "de": [
      "Escape Hunt Toulouse, nur wenige Minuten vom Stadtzentrum von Toulouse entfernt. Lösen Sie einen originellen Fall und entfliehen Sie in eine zeitlose Kulisse!"
    ],
    "en": [
      "Escape Hunt Toulouse, a few minutes from downtown Toulouse. Come and solve an original investigation, and escape in a timeless setting!"
    ],
    "it": [
      "Escape Hunt Toulouse, a pochi minuti dal centro di Tolosa. Venite a risolvere un'indagine originale e a fuggire in un'ambientazione senza tempo!"
    ],
    "fr": [
      "Escape Hunt Toulouse, à quelques minutes du centre ville de Toulouse. Venez résoudre une enquête originale, et vous évader dans un décors hors du temps !"
    ],
