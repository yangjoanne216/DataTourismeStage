  "COVID19SpecialMeasures": {
    "de": [
      "Physische Distanzierung; Bereitstellung von alkoholischem Wassergel; kontaktlose Kreditkartenzahlung; Zugang und Verkostung im Freien, unter Dach."
    ],
    "en": [
      "Physical distance; provision of hydro-alcoholic gel; contactless payment by credit card; access and tasting outside, under cover."
    ],
    "it": [
      "Distanza fisica; fornitura di gel idroalcolico; pagamento contactless con carta di credito; accesso e degustazione al coperto."
    ],
    "fr": [
      "Distanciation physique ; mise à disposition de gel hydro alcoolique ; paiement carte bancaire sans contact ; accès et dégustation en extérieur, sous abri."
    ],
    "nl": [
      "Fysieke afstand; verstrekking van hydro-alcoholische gel; contactloze betaling met kredietkaart; toegang en proeven overdekt."
    ],
    "es": [
      "Distancia física; suministro de gel hidroalcohólico; pago sin contacto con tarjeta de crédito; acceso y degustación a cubierto."
    ]
  },
