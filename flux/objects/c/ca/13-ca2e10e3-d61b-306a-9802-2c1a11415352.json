  "rdfs:comment": {
    "de": [
      "Dem Heiligen Eloi gewidmetes Gebäude im Wald. Geschützt durch ein schmiedeeisernes Gitter. Gebogener Ziegelsturz aus abwechselnd roten und weißen Ziegelsteinen."
    ],
    "en": [
      "Building dedicated to Saint Eloi, in the wood. Protected by a wrought iron gate. Arched brick lintel, made of alternating red and white bricks."
    ],
    "it": [
      "Edificio dedicato a Sant'Eloi, nel bosco. Protetto da un cancello in ferro battuto. Architrave in mattoni ad arco con mattoni rossi e bianchi alternati."
    ],
    "fr": [
      "Edifice dédié à Saint-Eloi, dans le bois. Protégé par une grille de fer forgé. Linteau de briques arqué, constitué d’une alternance de briques rouges et de briques blanches."
    ],
    "nl": [
      "Gebouw gewijd aan Sint Eloi, in het bos. Beschermd door een smeedijzeren hek. Gebogen bakstenen latei met afwisselend rode en witte bakstenen."
    ],
    "es": [
      "Edificio dedicado a San Eloy, en el bosque. Protegido por una puerta de hierro forjado. Dintel arqueado de ladrillo con alternancia de ladrillos rojos y blancos."
    ]
  },
