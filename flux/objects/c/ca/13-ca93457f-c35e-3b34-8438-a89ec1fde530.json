  "rdfs:comment": {
    "de": [
      "Land Art Festival \"Le Tacot, Voyage éphémère\": Kunstausstellung unter freiem Himmel\r\n acht Kunstwerke von acht Künstlern, die von der Gemeinde ausgewählt wurden, in zeitgenössischem Stil zum Thema \"Seien wir realistisch, schaffen wir das Unmögliche\"."
    ],
    "en": [
      "Festival Land'art \"Le Tacot, Voyage éphémère\": Open air art exhibition\r\n of eight works of art, representing eight artists chosen by the municipality, of contemporary style, on the theme \"Let us be realistic, let us create the impossible\"."
    ],
    "it": [
      "Festival della Land'art \"Le Tacot, Voyage éphémère\": mostra d'arte all'aria aperta\r\n di otto opere d'arte, rappresentanti otto artisti scelti dal Comune, di stile contemporaneo, sul tema \"Siamo realisti, creiamo l'impossibile\"."
    ],
    "fr": [
      "Festival Land'art \"Le Tacot, Voyage éphémère\": Exposition artistique à ciel ouvert \r\n de huit œuvres d'art, représentant huit artistes choisis par la commune, de style contemporain, sur le thème «Soyons réaliste, créons l’impossible»."
    ],
    "nl": [
      "Land'art festival \"Le Tacot, Voyage éphémère\": Openlucht kunsttentoonstelling\r\n van acht kunstwerken, die acht door de gemeente gekozen kunstenaars vertegenwoordigen, in hedendaagse stijl, met als thema \"Laten we realistisch zijn, laten we het onmogelijke creëren\"."
    ],
    "es": [
      "Land'art festival \"Le Tacot, Voyage éphémère\": Exposición de arte al aire libre\r\n de ocho obras de arte, que representan a ocho artistas elegidos por el municipio, de estilo contemporáneo, sobre el tema \"Seamos realistas, creemos lo imposible\"."
    ]
  },
