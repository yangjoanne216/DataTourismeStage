  "rdfs:comment": {
    "de": [
      "MONATLICHE MESSE IN MARVEJOLS :\n\n    Place du Soubeyran: lokale Produkte, Obst, Gemüse, Käse, Fleisch, Blumen, Brot, Gebäck, Gewürze, Honig...\n    Place Henri Cordesse: Kleidung,\n    Place du Barry und Place de l'Esplanade: Material, Hüte..."
    ],
    "en": [
      "MONTHLY MARVEJOL FAIR:\n\n    Place du Soubeyran : local products, fruits, vegetables, cheeses, meat, flowers, bread, pastries, spices, honey...\n    Place Henri Cordesse : clothes,\n    Places of Barry and Esplanade: equipment, hats..."
    ],
    "it": [
      "FIERA MENSILE MARVEJOLS :\n\n    Place du Soubeyran: prodotti locali, frutta, verdura, formaggio, carne, fiori, pane, pasticceria, spezie, miele, ecc.\n    Piazza Henri Cordesse: abbigliamento,\n    Place du Barry e Place de l'Esplanade: attrezzature, cappelli, ecc."
    ],
    "fr": [
      "FOIRE MENSUELLE DE MARVEJOLS :\n\n    Place du Soubeyran : produits locaux, fruits, légumes, fromages, viande, fleurs, pain, viennoiseries, épices, miel...\n    Place Henri Cordesse : vêtements,\n    Places du Barry et de l’Esplanade : matériel, chapeaux..."
    ],
    "nl": [
      "MARVEJOLS MAANDELIJKSE BEURS :\n\n    Place du Soubeyran: streekproducten, fruit, groenten, kaas, vlees, bloemen, brood, gebak, kruiden, honing, enz.\n    Place Henri Cordesse: kleding,\n    Place du Barry en Place de l'Esplanade: uitrusting, hoeden, enz."
    ],
    "es": [
      "FERIA MENSUAL MARVEJOLS :\n\n    Place du Soubeyran: productos locales, fruta, verdura, queso, carne, flores, pan, bollería, especias, miel, etc.\n    Place Henri Cordesse: ropa,\n    Place du Barry y Place de l'Esplanade: equipamiento, sombreros, etc."
    ]
  },
