  "rdfs:comment": {
    "de": [
      "Koch zu sein bedeutet, Freude zu bereiten, essen Sie mit Glück und seien Sie gemeinsam glücklich."
    ],
    "en": [
      "To be a cook is to give pleasure, eat happily and be happy together."
    ],
    "it": [
      "Essere un cuoco significa dare piacere, mangiare con felicità ed essere felici insieme."
    ],
    "fr": [
      "Etre cuisinier, c'est donner du plaisir, mangez avec bonheur et soyez heureux ensemble."
    ],
    "es": [
      "Ser cocinero es dar placer, comer con alegría y ser felices juntos."
    ],
    "nl": [
      "Kok zijn is plezier geven, gelukkig eten en samen gelukkig zijn."
    ]
  },
