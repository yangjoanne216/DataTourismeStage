  "rdfs:comment": {
    "de": [
      "Angenehme Unterkunft in ruhiger Lage auf einem Weingut mit privatem Hof. Blick auf den Weinberg. Persönlicher Empfang. Möglichkeit zur Besichtigung des Weinkellers und Weinprobe."
    ],
    "en": [
      "Pleasant gîte situated in a quiet location on a wine-growing estate with private courtyard. View on the vineyard. Personalized welcome. Possibility of cellar visit and wine tasting."
    ],
    "it": [
      "Piacevole gîte situato in posizione tranquilla in una tenuta vinicola con cortile privato. Vista sul vigneto. Accoglienza personalizzata. Possibilità di visita della cantina e degustazione di vini."
    ],
    "fr": [
      "Gîte agréable situé au calme sur un domaine viticole avec cour privée. Vue sur le vignoble. Accueil personnalisé. Possibilité de visite de cave et dégustation de vins."
    ],
    "es": [
      "Agradable casa rural situada en un lugar tranquilo en una finca vinícola con patio privado. Vista del viñedo. Bienvenida personalizada. Posibilidad de visita a bodega y degustación de vinos."
    ],
    "nl": [
      "Aangename gîte gelegen op een rustige locatie op een wijndomein met privé binnenplaats. Uitzicht op de wijngaard. Persoonlijke ontvangst. Mogelijkheid tot kelderbezoek en wijnproeven."
    ]
  },
