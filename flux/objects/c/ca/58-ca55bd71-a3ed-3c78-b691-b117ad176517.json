  "rdfs:comment": {
    "de": [
      "Asiatische Küche. Kleines, gemütliches Familienrestaurant, in dem Sie einen Blick auf die Köchin erhaschen können, die die Gerichte auf Wunsch zubereitet: knusprige Frühlingsrollen, gedämpfte Ravioli, kantonesischer Reis..."
    ],
    "en": [
      "Asian cuisine. Small family restaurant, very warm where you can see the cook preparing dishes on demand: crispy egg rolls, steamed ravioli, Cantonese rice..."
    ],
    "it": [
      "Cucina asiatica. Piccolo ristorante a conduzione familiare, molto accogliente, dove si può vedere il cuoco che prepara i piatti su richiesta: involtini croccanti, ravioli al vapore, riso alla cantonese..."
    ],
    "fr": [
      "Cuisine asiatique. Petit restaurant familial, très chaleureux où vous pouvez apercevoir la cuisinière qui prépare les plats à la demande : nems croustillants, raviolis vapeur, riz cantonais..."
    ],
    "es": [
      "Cocina asiática. Pequeño restaurante familiar, muy cálido donde se puede ver al cocinero preparar platos a la carta: rollitos crujientes, raviolis al vapor, arroz cantonés..."
    ],
    "nl": [
      "Aziatische keuken. Klein familierestaurant, zeer warm waar je de kok op verzoek gerechten ziet bereiden: knapperige loempia's, gestoomde ravioli, Kantonese rijst..."
    ]
  },
