  "rdfs:comment": {
    "de": [
      "Im Herzen des Comminges entdecken Sie diese angenehme Einrichtung, die am Waldrand inmitten der Natur liegt."
    ],
    "en": [
      "In the heart of the Comminges, discover this pleasant establishment located on the edge of the forest in the middle of nature."
    ],
    "it": [
      "Nel cuore delle Comminges, scoprite questa piacevole struttura situata ai margini di una foresta in mezzo alla natura."
    ],
    "fr": [
      "Au cœur du Comminges, découvrez cet agréable établissement situé en lisière de forêt en pleine nature."
    ],
    "nl": [
      "Ontdek in het hart van de Comminges dit aangename etablissement gelegen aan de rand van een bos midden in de natuur."
    ],
    "es": [
      "En el corazón de las Comminges, descubra este agradable establecimiento situado al borde de un bosque en plena naturaleza."
    ]
  },
