  "rdfs:comment": {
    "de": [
      "Regionale Snacks, Eis, Sandwiches, Pommes frites, Soda/Bier, Fish & Chips, Rohkost- und Obstkompositionen. Grüner Park mit schattenspendenden Bäumen, atemberaubender Blick auf den See. Beaufsichtigtes Baden in der Sommersaison."
    ],
    "en": [
      "Local snack, ice creams, sandwiches, fries, soda/beers, fish & chips, mix of raw vegetables and fruits. \r\nGrass area with some shadow, great view over the lake.\r\nLifeguard on duty during the summer season."
    ],
    "it": [
      "Snack locali, gelati, panini, patatine, bibite/birra, fish & chips, composizioni di verdure crude e frutta. Parco verde con ombra, vista mozzafiato sul lago. Nuoto sorvegliato durante la stagione estiva."
    ],
    "fr": [
      "Snacking terroir, glaces, sandwiches, frites, soda/bière, fish & chips, compositions de crudités et fruits. Parc de verdure avec ombrage, vue imprenable sur le lac. Baignade surveillée en saison d'été."
    ],
    "nl": [
      "Lokale snacks, ijsjes, broodjes, chips, frisdrank/bier, fish & chips, rauwkost- en fruitsamenstellingen. Groen park met schaduw, adembenemend uitzicht op het meer. Zwemmen onder toezicht in het zomerseizoen."
    ],
    "es": [
      "Aperitivos locales, helados, sándwiches, patatas fritas, refrescos/cerveza, pescado y patatas fritas, composiciones de verduras y frutas crudas. Parque verde con sombra, vista impresionante del lago. Natación supervisada en la temporada de verano."
    ]
  },
  "rdfs:label": {
    "en": [
      "Snack of the Sougey beach"
    ],
    "fr": [
      "Snack de la plage du Sougey"
    ]
  },
