  "rdfs:comment": {
    "de": [
      "Entdecken Sie Marseille durch den Besuch verschiedener unumgänglicher Museen der Stadt: ein Tag, der ganz der Kultur gewidmet ist!"
    ],
    "en": [
      "Discover Marseille by visiting some of the city's must-see museums: a day dedicated entirely to culture!"
    ],
    "it": [
      "Scoprite Marsiglia attraverso la visita ai vari musei imperdibili della città: una giornata interamente dedicata alla cultura!"
    ],
    "fr": [
      "Découvrez Marseille au travers de la visite de différents musées incontournables de la ville : une journée dédiée entièrement à la culture !"
    ],
    "nl": [
      "Ontdek Marseille via een bezoek aan de verschillende must-see musea van de stad: een dag volledig gewijd aan cultuur!"
    ],
    "es": [
      "Descubra Marsella visitando algunos de los museos imprescindibles de la ciudad: ¡un día dedicado por completo a la cultura!"
    ]
  },
  "rdfs:label": {
    "fr": [
      "Visiter Marseille en 1 jour - Circuit culturel"
    ]
  },
