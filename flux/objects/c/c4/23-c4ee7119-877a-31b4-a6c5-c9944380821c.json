  "rdfs:comment": {
    "de": [
      "Transportfahrzeug mit Chauffeur.\nGewerbliche Personenbeförderung, Transport über alle Entfernungen, zu SNCF-Bahnhöfen und Flughäfen, 7 Tage pro Woche, 24 Stunden pro Tag.\nReservierungen unter 06 50 77 76 04."
    ],
    "en": [
      "Vehicle of Transport with Driver.\nTransport of people for a fee, transport all distances, to train stations and airports, 7 days a week, 24 hours a day.\nReservations at 06 50 77 76 04."
    ],
    "it": [
      "Veicolo da trasporto con autista.\nTrasporto di persone a pagamento, trasporto su tutte le distanze, fino alle stazioni ferroviarie e agli aeroporti, 7 giorni su 7, 24 ore al giorno.\nPrenotazioni al numero 06 50 77 76 04."
    ],
    "fr": [
      "Véhicule de Transport avec Chauffeur.\nTransport de personnes à titre onéreux, transport toutes distances, vers gares SNCF et aéroports,  7j/7, 24h/24.\nRéservations au 06 50 77 76 04."
    ],
    "es": [
      "Vehículo de transporte con conductor.\nTransporte de personas previo pago, transporte a todas las distancias, a estaciones de ferrocarril y aeropuertos, 7 días a la semana, 24 horas al día.\nReservas en el 06 50 77 76 04."
    ],
    "nl": [
      "Transportvoertuig met chauffeur.\nVervoer van personen tegen betaling, vervoer over alle afstanden, naar stations en luchthavens, 7 dagen per week, 24 uur per dag.\nReserveringen op 06 50 77 76 04."
    ]
  },
