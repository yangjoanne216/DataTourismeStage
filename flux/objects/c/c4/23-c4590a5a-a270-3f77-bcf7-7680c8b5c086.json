  "rdfs:comment": {
    "de": [
      "10 Uhr: Bouleturnier, Vorführung der Feuerwehr von Navailles-Angos.\n11 Uhr: Essen und Trinken im Freien.\n15 Uhr: Hüpfburgen, Entenangeln und Gewehrschießen.\n21h/2h: Essen mit anschließendem Tanz."
    ],
    "en": [
      "10am: Petanque tournament, demonstration by the Navailles-Angos firefighters.\n11am: Catering and outdoor refreshment area.\n3pm: Bouncy castles, duck fishing and rifle shooting.\n9 p.m./2 a.m.: Dinner followed by a dance."
    ],
    "it": [
      "ore 10: torneo di bocce, dimostrazione dei vigili del fuoco di Navailles-Angos.\nore 11.00: Catering e area di ristoro all'aperto.\nore 15.00: castelli gonfiabili, pesca delle anatre e tiro con la carabina.\nore 21.00/2.00: Pasto seguito da un ballo."
    ],
    "fr": [
      "10h : Tournoi de pétanque, démonstration des pompiers de Navailles-Angos.\n11h : Restauration et buvette extérieure.\n15h : Châteaux gonflables, pêche aux canards et tir à la carabine.\n21h/2h : Repas suivi du bal."
    ],
    "nl": [
      "10u: Petanquetoernooi, demonstratie door de brandweer van Navailles-Angos.\n11.00 uur: catering en verfrissing in de open lucht.\n15.00 uur: Opblaasbare kastelen, eendenvissen en geweerschieten.\n21u/2u: Maaltijd gevolgd door een dansfeest."
    ],
    "es": [
      "10h: Torneo de petanca, demostración de los bomberos de Navailles-Angos.\n11.00 h: Catering y zona de restauración al aire libre.\n15.00 h: Castillos hinchables, pesca de patos y tiro con carabina.\n21.00/2.00 h: Comida seguida de baile."
    ]
  },
