  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/994b4e1d-6fa9-3f0a-ba13-c55b7536cb6e",
      "dc:description": {
        "de": [
          "Holzhaus (70 m²), 2 Schlafzimmer (1 Bett 140, 2 Betten 90), Schlafsofa, Badezimmer, ausgestattete Küche, Waschmaschine, Geschirrspüler, Heizung, Fernseher,Wifi, Pool 15 m², Terrasse,Garage, Garten, Grill."
        ],
        "en": [
          "2 bedrooms (2 beds 90, 1 bed 140), shower room, kitchenette (dishwasher, washing machine)/living room (sofa bed, TV, wood stove), wood-fired heating, not closed private terrace (25m²) and garden (1000m²), garden furniture, barbecue, garage, car park, sheets and bathroom linen for rent, tourist tax."
        ],
        "it": [
          "Casa in legno (70 m²), 2 camere (1 letto 140, 2 letti 90), divano letto, bagno con doccia, cucina attrezzata, lavatrice, lavastoviglie, riscaldamento, televisione, Wifi, piscina 15 m², terrazza, garage, giardino, barbecue."
        ],
        "fr": [
          "Maison en bois (70 m²), 2 chambres (1 lit en 140, 2 lits en 90), canapé convertible, salle d'eau, cuisine équipée, lave-linge, lave-vaisselle, chauffage, télévision,Wifi, piscine 15 m², terrasse,garage, jardin, barbecue."
        ],
        "nl": [
          "Houten huis (70 m²), 2 slaapkamers (1 bed van 140, 2 bedden van 90), slaapbank, badkamer met douche, ingerichte keuken, wasmachine, vaatwasser, verwarming, televisie, Wifi, zwembad van 15 m², terras, garage, tuin, barbecue."
        ],
        "es": [
          "Casa de madera (70 m²), 2 dormitorios (1 cama de 140, 2 camas de 90), sofá cama, cuarto de ducha, cocina equipada, lavadora, lavavajillas, calefacción, televisión, Wifi, piscina de 15 m², terraza, garaje, jardín, barbacoa."
        ]
      },
