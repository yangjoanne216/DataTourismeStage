  "rdfs:comment": {
    "de": [
      "Gästezimmer in Mouriès, einem autenthischen provenzalischen Dorf im Herzen der Alpilles. Charakteristisches Haus mit Garten, Pool, Jacuzzi, schattigem Bouleplatz."
    ],
    "en": [
      "Bed and breakfast located in Mouriès, an autentic Provencal village in the heart of the Alpilles. House of character with garden, swimming pool, jacuzzi, shaded ground of balls."
    ],
    "it": [
      "Bed and breakfast situato a Mouriès, un autentico villaggio provenzale nel cuore delle Alpilles. Casa di carattere con giardino, piscina, jacuzzi, campo da bocce ombreggiato."
    ],
    "fr": [
      "Chambre d'hôtes située à Mouriès, village provençal autenthique situé au coeur des Alpilles. Maison de caractère avec jardin, piscine, jacuzzi, terrain de boules ombragé."
    ],
    "es": [
      "Bed and breakfast situado en Mouriès, un auténtico pueblo provenzal en el corazón de los Alpilles. Casa de carácter con jardín, piscina, jacuzzi, bolera sombreada."
    ],
    "nl": [
