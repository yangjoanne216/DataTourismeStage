  "rdfs:comment": {
    "de": [
      "Carnivor, das ist eine Auswahl an frischen Produkten. Entdecken Sie unsere Metzgerei, Wurstwaren."
    ],
    "en": [
      "Carnivor, it's a selection of fresh products, come and discover our butchery, delicatessen."
    ],
    "it": [
      "Carnivor, è una selezione di prodotti freschi, venite a scoprire la nostra macelleria e salumeria."
    ],
    "fr": [
      "Carnivor, c'est une sélection de produits frais, venez découvrir  notre boucherie, charcuterie."
    ],
    "nl": [
      "Carnivor, het is een selectie van verse producten, kom en ontdek onze slagerij, delicatessen."
    ],
    "es": [
      "Carnivor, es una selección de productos frescos, venga a descubrir nuestra carnicería, charcutería."
