  "rdfs:comment": {
    "de": [
      "Besichtigung von drei Kirchen: Saint-Angel, Saint-Front-la-Rivière, Milhac-de-Nontron.\nWort des Bürgermeisters, historischer Rückblick durch einen lokalen Referenten, Musikgruppe.\nPreis: 7?/Person, kostenlos für Kinder unter 16 Jahren."
    ],
    "en": [
      "Visit of three churches: Saint-Angel, Saint-Front-la-Rivière, Milhac-de-Nontron.\nA word from the mayor, historical reminder by a local referent, musical group.\nPrice: 7€ per person, free for children under 16."
    ],
    "it": [
      "Visita di tre chiese: Saint-Angel, Saint-Front-la-Rivière, Milhac-de-Nontron.\nParola del sindaco, ricordo storico da parte di un referente locale, gruppo musicale.\nPrezzo: 7€/persona, gratis per i bambini sotto i 16 anni."
    ],
    "fr": [
      "Visite de trois églises : Saint-Angel, Saint-Front-la-Rivière, Milhac-de-Nontron.\nMot du maire, rappel historique par un référent local, groupe musical. \nTarif : 7€/personne, gratuit pour les moins de 16 ans."
    ],
    "es": [
      "Visita de tres iglesias: Saint-Angel, Saint-Front-la-Rivière, Milhac-de-Nontron.\nPalabras del alcalde, recordatorio histórico a cargo de un referente local, grupo musical.\nPrecio: 7€/persona, gratis para menores de 16 años."
    ],
    "nl": [
      "Bezoek aan drie kerken: Saint-Angel, Saint-Front-la-Rivière, Milhac-de-Nontron.\nEen woord van de burgemeester, historische herinnering door een lokale referent, muzikale groep.\nPrijs: 7€/persoon, gratis voor kinderen tot 16 jaar."
