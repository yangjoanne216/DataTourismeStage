  "rdfs:comment": {
    "de": [
      "Dieses kleine Landwirtschaftsmuseum zeigt einen kompletten Bauernhof auf dem Land in Lauragaise. Besichtigung nach Vereinbarung."
    ],
    "en": [
      "This small agricultural museum retraces an entire Laurentian country farm. Visit by appointment."
    ],
    "it": [
      "Questo piccolo museo agricolo ripercorre l'intera fattoria della campagna del Lauragais. Visita su appuntamento."
    ],
    "fr": [
      "Ce petit musée agricole retrace en intégralité une ferme de campagne lauragaise. Visite sur rendez vous."
    ],
    "es": [
      "Este pequeño museo agrícola recorre la totalidad de una granja en el campo de Lauragais. Visita con cita previa."
    ],
    "nl": [
      "Dit kleine landbouwmuseum reconstrueert het geheel van een boerderij op het platteland van Lauragais. Bezoek op afspraak."
    ]
  },
