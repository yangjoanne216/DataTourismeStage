  "rdfs:comment": {
    "de": [
      "Auf dieser etwa 20 km langen Strecke können Sie das Tal des Col Amic von Wuenheim bis zum Bauernhof Kohlschlag erkunden."
    ],
    "en": [
      "This 20 km long tour takes you through the valley of the Amic Pass from Wuenheim to the Kohlschlag farm."
    ],
    "it": [
      "Questo circuito di 20 km vi permetterà di scoprire la valle del Passo dell'Amic, da Wuenheim alla fattoria Kohlschlag."
    ],
    "fr": [
      "Ce circuit d'une vingtaine de km vous permettra de découvrir le vallon du col Amic, de Wuenheim jusqu’à la ferme du Kohlschlag."
    ],
    "es": [
      "Este circuito de 20 km le permitirá descubrir el valle del puerto de Amic, desde Wuenheim hasta la granja de Kohlschlag."
    ],
    "nl": [
      "Dit circuit van 20 km laat u de vallei van de Amicpas ontdekken, van Wuenheim tot de boerderij van Kohlschlag."
    ]
  },
