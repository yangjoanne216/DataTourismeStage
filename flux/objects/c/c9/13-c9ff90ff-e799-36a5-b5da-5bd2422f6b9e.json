  "rdfs:comment": {
    "de": [
      "Verkaufsausstellung : Magasin du Monastère (5 km von Aix entfernt auf der Route du Revard, neben der Kirche St Joseph): religiöse Statuen, Krippen, Tischgeschirr, natürliche Hygieneprodukte, Marmeladen, Süßwaren, Kerzen, Lederwaren, Karten ..."
    ],
    "en": [
      "Exhibition/sale: Monastery shop (5 km from Aix on the Revard road, next to St Joseph's Church): religious statuary, nativity scenes, tableware, natural hygiene products, jams, candies, candles, leather goods, greeting cards, etc."
    ],
    "it": [
      "Mostra-vendita: Negozio del Monastère (a 5 km da Aix dalla strada del Revard, accanto alla chiesa St Joseph): statuaria religiosa, asilo nido, servizi da tavola, prodotti per l'igiene, confetture, dolciumi, candele, pelletteria, cartoleria..."
    ],
    "fr": [
      "Exposition-vente : Magasin du  Monastère (à 5 km d’Aix par la route du Revard, à coté de l'église St Joseph) :  statuaire religieuse, crèches, vaisselle de table, produits naturels d'hygiène, confitures, confiseries, bougies, maroquinerie, carterie ..."
    ],
    "nl": [
      "Tentoonstelling-verkoop: Kloosterwinkel (5 km van Aix via de weg naar Revard, naast de Sint-Jozefkerk): religieuze beelden, kinderbedjes, serviesgoed, natuurlijke hygiëneproducten, jam, snoepgoed, kaarsen, lederwaren, kaarten, enz."
    ],
    "es": [
      "Exposición-venta : Tienda del monasterio (a 5 km de Aix por la carretera de Revard, junto a la iglesia de San José): estatuas religiosas, catres, vajilla, productos de higiene natural, mermeladas, dulces, velas, artículos de cuero, tarjetas, etc."
    ]
  },
