  "rdfs:comment": {
    "de": [
      "Das Centre International Eugène de Mazenod stellt mehrere Räume für Begegnungen und seine Kirche für Konzerte zur Verfügung."
    ],
    "en": [
      "The Eugene de Mazenod International Center makes available several rooms for meetings and its church for concerts."
    ],
    "it": [
      "Il Centre International Eugène de Mazenod offre diverse sale per riunioni e la sua chiesa per concerti."
    ],
    "fr": [
      "Le Centre International Eugène de Mazenod met à disposition plusieurs salles pour des rencontres et son église pour des concerts."
    ],
    "nl": [
      "Het Centre International Eugène de Mazenod biedt verschillende zalen voor vergaderingen en zijn kerk voor concerten."
    ],
    "es": [
      "El Centro Internacional Eugène de Mazenod ofrece varias salas para reuniones y su iglesia para conciertos."
    ]
