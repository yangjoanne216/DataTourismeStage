  "rdfs:comment": {
    "de": [
      "Haus in Holzrahmenbauweise auf geschlossenem und schattigem Gelände, bestehend aus 3 Schlafzimmern, Küche, Wohnzimmer, Duschbad mit WC. Terrasse mit Gartenmöbeln, Grill, Sonnenschirm."
    ],
    "en": [
      "Chalet located on a fenced garden with private parking for up to 6 people. 4 rooms: 2 rooms with 1 bed for 2 persons and 1 room with 2 beds for 1 person, kitchen-living room, bathroom. Quiet edge of the forest in a dead end. Pets accepted under certain conditions. Shops nearby."
    ],
    "it": [
      "Casa con struttura in legno su terreno recintato e ombreggiato con 3 camere da letto, cucina, soggiorno, bagno con doccia e WC. Terrazza con mobili da giardino, barbecue, ombrellone."
    ],
    "fr": [
      "Maison à ossature bois sur terrain clos et ombragé composée de 3 chambres, cuisine, salon, salle de douche avec wc. Terrasse avec salon de jardin, barbecue, parasol."
    ],
    "nl": [
      "Houten frame huis op omheind en schaduwrijk terrein met 3 slaapkamers, keuken, woonkamer, doucheruimte met toilet. Terras met tuinmeubelen, barbecue, parasol."
    ],
    "es": [
      "Casa de madera en terreno cerrado y sombreado con 3 dormitorios, cocina, sala de estar, cuarto de ducha con aseo. Terraza con muebles de jardín, barbacoa, sombrilla."
    ]
  },
