  "rdfs:comment": {
    "de": [
      "Mittagsgerichte zum Mitnehmen montags, dienstags, donnerstags, freitags und samstags (9 € Wochenkarte, Vorspeise, Hauptgericht, Dessert - 15,50 € Wochenendkarte). Telefonische Bestellung bis zum Morgen."
    ],
    "en": [
      "Takeaway lunch on Monday, Tuesday, Thursday, Friday and Saturday (9 € weekly formula, starter, main course, dessert - 15,50 € week-end formula). Order by phone until the morning."
    ],
    "it": [
      "Pasti da asporto il lunedì, martedì, giovedì, venerdì e sabato (9 ? formula feriale, antipasto, piatto principale, dessert - 15,50 ? formula weekend). Ordini telefonici fino al mattino."
    ],
    "fr": [
      "Plats à emporter le midi le lundi, mardi, jeudi, vendredi et samedi (9 € formule semaine, entrée, plat, dessert - 15,50 € formule week-end). Commande téléphone jusqu'au matin."
    ],
    "es": [
      "Comidas para llevar los lunes, martes, jueves, viernes y sábado (9€ fórmula entre semana, entrante, plato principal, postre - 15,50€ fórmula fin de semana). Pedidos por teléfono hasta la mañana."
    ],
    "nl": [
      "Afhaalmaaltijden op maandag, dinsdag, donderdag, vrijdag en zaterdag (9 € weekformule, voorgerecht, hoofdgerecht, dessert - 15,50 € weekendformule). Bestel per telefoon tot de ochtend."
    ]
  },
