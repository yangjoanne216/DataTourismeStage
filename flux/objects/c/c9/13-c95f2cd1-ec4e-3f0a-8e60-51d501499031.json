  "rdfs:comment": {
    "de": [
      "Fish and Chips Le Merluchon bietet Ihnen Seehecht aus Schleppnetzfischerei und hausgemachte Pommes frites aus Kartoffeln, die direkt vom Erzeuger in Saint-Denis kommen!"
    ],
    "en": [
      "Le Merluchon Fish and chips Le Merluchon offers trawl-caught hake and homemade chips made with potatoes that come straight from the Saint-Denis producer!"
    ],
    "it": [
      "Fish and chips Le Merluchon vi offre nasello pescato a strascico e patatine fritte fatte in casa con patate provenienti direttamente dal produttore di Saint-Denis!"
    ],
    "fr": [
      "Le Fish and chips Le Merluchon vous propose du merlu pêché au chalut et des frites maison faites avec des pommes de terre qui arrivent tout droit du producteur de Saint-Denis!"
    ],
    "es": [
      "Fish and chips Le Merluchon le ofrece merluza de arrastre y patatas fritas caseras hechas con patatas que vienen directamente del productor de Saint-Denis"
    ],
    "nl": [
      "Fish and chips Le Merluchon biedt u heek die met een sleepnet is gevangen en huisgemaakte frieten met aardappelen die rechtstreeks van de producent in Saint-Denis komen!"
    ]
  },
