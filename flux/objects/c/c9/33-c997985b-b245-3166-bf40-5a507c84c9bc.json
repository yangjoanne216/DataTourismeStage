  "rdfs:comment": {
    "de": [
      "Im Herzen des Dorfes Drudas gönnen Sie sich eine Picknickpause vor der Orangerie des Schlosses."
    ],
    "en": [
      "In the heart of the village of Drudas, treat yourself to a picnic break in front of the orangery of the Castle."
    ],
    "it": [
      "Nel cuore del villaggio di Drudas, godetevi un picnic davanti all'aranciera del castello."
    ],
    "fr": [
      "Au cœur du village de Drudas offrez-vous une pause pique-nique en face de l'orangerie du Château."
    ],
    "nl": [
      "In het hart van het dorp Drudas kunt u picknicken voor de orangerie van het kasteel."
    ],
    "es": [
      "En el corazón del pueblo de Drudas, disfrute de un picnic frente al invernadero del castillo."
    ]
  },
