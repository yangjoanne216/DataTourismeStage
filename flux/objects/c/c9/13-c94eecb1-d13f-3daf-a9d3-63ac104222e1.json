  "rdfs:comment": {
    "de": [
      "Kapelle mit quadratischem Grundriss aus Backstein aus dem Jahr 1923. Sie steht über der Straße und ist an ein großes Backsteingebäude angebaut und angelehnt."
    ],
    "en": [
      "Square brick chapel dating from 1923. It is located overhanging the road, next to a large brick building."
    ],
    "it": [
      "Cappella quadrata in mattoni del 1923. Si trova a strapiombo sulla strada, accanto e appoggiato a un grande edificio in mattoni."
    ],
    "fr": [
      "Chapelle de plan carré en brique datant de 1923. Elle se trouve en surplomb de la route, accolée et adossée à un grand bâtiment en brique."
    ],
    "nl": [
      "Vierkante bakstenen kapel uit 1923. Het ligt over de weg, naast en leunend tegen een groot bakstenen gebouw."
    ],
    "es": [
      "Capilla cuadrada de ladrillo que data de 1923. Está situado en la carretera, junto a un gran edificio de ladrillos y apoyado en él."
    ]
  },
