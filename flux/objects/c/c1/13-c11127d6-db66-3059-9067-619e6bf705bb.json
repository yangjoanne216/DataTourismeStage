  "rdfs:comment": {
    "de": [
      "Vertical Aventure ist ein Team von Monitoren und Führern, Bergprofis, die Sie bei Kletterausflügen, Canyoning, Klettersteig, Via Corda, Höhlenforschung, Bergsteigen, Gleitschirmfliegen und Deltasegeln in der Chartreuse betreuen"
    ],
    "en": [
      "Vertical Aventure is a team of instructors and guides, mountain professionals, who offer to supervise you in climbing, canyoning, via ferrata, via corda, caving, mountaineering, paragliding and delta in Chartreuse"
    ],
    "it": [
      "Vertical Aventure è un team di istruttori e guide, professionisti della montagna, che si offrono di condurvi in arrampicata, canyoning, via ferrata, via corda, speleologia, alpinismo, parapendio e deltaplano nella Chartreuse"
    ],
    "fr": [
      "Vertical Aventure est une équipe de moniteurs et guides, professionnels de la montagne, qui vous proposent de vous encadrer en sorties escalade, canyoning, via ferrata, via corda, spéléologie, alpinisme, parapente et delta en Chartreuse"
    ],
    "nl": [
      "Vertical Aventure is een team van instructeurs en gidsen, professionals uit de bergen, die u begeleiden bij het klimmen, canyoning, via ferrata, via corda, speleologie, alpinisme, paragliding en deltavliegen in de Chartreuse"
    ],
    "es": [
      "Vertical Aventure es un equipo de monitores y guías, profesionales de la montaña, que se ofrecen a guiarle en viajes de escalada, barranquismo, vía ferrata, vía corda, espeleología, alpinismo, parapente y ala delta en la Chartreuse"
    ]
  },
