  "rdfs:comment": {
    "de": [
      "Das Empfangsbüro in Simiane-la-Rotonde, eine Zweigstelle des interkommunalen Fremdenverkehrsamtes Haute Provence Pays de Banon, empfängt Sie zu den Öffnungszeiten des Schlosses."
    ],
    "en": [
      "The reception office of Simiane-la-Rotonde, branch of the Intercommunal Tourist Office of Haute Provence Pays de Banon welcomes you according to the opening hours of the castle."
    ],
    "it": [
      "L'ufficio di accoglienza di Simiane-la-Rotonde, filiale dell'Ufficio Intercomunale del Turismo dell'Alta Provenza Pays de Banon, vi accoglie durante gli orari di apertura del castello."
    ],
    "fr": [
      "Le bureau d'accueil de Simiane-la-Rotonde, antenne de l'Office de Tourisme Intercommunal Haute Provence Pays de Banon vous accueille selon les horaires d'ouverture du château."
    ],
    "es": [
      "La oficina de recepción de Simiane-la-Rotonde, dependiente de la Oficina Intermunicipal de Turismo de Haute Provence Pays de Banon, le recibe durante el horario de apertura del castillo."
    ],
    "nl": [
      "De receptie van Simiane-la-Rotonde, een filiaal van het Intercommunaal Bureau voor Toerisme van de Haute Provence Pays de Banon, ontvangt u tijdens de openingsuren van het kasteel."
    ]
  },
