  "rdfs:comment": {
    "de": [
      "Möglichkeit zum Verkauf von Baby- und Kinderartikeln, Kleidung von 0 bis 16 Jahren, Babyartikel, Spielzeug und Bücher. Vor Ort: heiße Würstchen, Kuchen und alkoholfreie Getränke."
    ],
    "en": [
      "Possibility to sell baby and children's items, clothing from 0 to 16 years old, childcare articles, toys and books. On site: hot sausages, cakes and soft drinks."
    ],
    "it": [
      "Possibilità di vendere articoli per neonati e bambini, vestiti da 0 a 16 anni, articoli per l'infanzia, giocattoli e libri. Sul posto: salsicce calde, torte e bibite."
    ],
    "fr": [
      "Possibilité de vendre des articles bébé et enfant, vêtements de 0 à 16 ans, articles de puériculture, jouets et livres. Sur place : saucisses chaudes, gâteaux et boisson sans alcool."
    ],
    "es": [
      "Posibilidad de vender artículos para bebés y niños, ropa de 0 a 16 años, artículos de puericultura, juguetes y libros. En el acto: salchichas calientes, pasteles y refrescos."
    ],
    "nl": [
      "Mogelijkheid tot verkoop van baby- en kinderartikelen, kleding van 0 tot 16 jaar, kinderverzorgingsartikelen, speelgoed en boeken. Ter plaatse: warme worstjes, gebak en frisdrank."
    ]
  },
