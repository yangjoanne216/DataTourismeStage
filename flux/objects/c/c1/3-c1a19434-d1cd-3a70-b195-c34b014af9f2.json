      "dc:description": {
        "de": [
          "Brasserie / bistronomisch.\nEssen nur mittags von Mittwoch bis Sonntag.\nBietet von Mittwoch bis Sonntag von 10:00 bis 18:00 Uhr Getränke / Eis an."
        ],
        "en": [
          "Brasserie / bistro.\nMeals only at lunchtime from Wednesday to Sunday.\nOffers drinks / ice cream from 10am to 6pm, Wednesday to Sunday."
        ],
        "it": [
          "Brasserie / bistrot.\nSolo pranzo da mercoledì a domenica.\nOffre bevande/gelati dalle 10.00 alle 18.00, da mercoledì a domenica."
        ],
        "fr": [
          "Brasserie / bistronomique.\nRepas uniquement le midi du mercredi au dimanche.\nPropose des boissons / glaces de 10h à 18h, du mercredi au dimanche."
        ],
        "es": [
          "Brasserie / bistro.\nSólo se puede comer de miércoles a domingo.\nOfrece bebidas y helados de 10 a 18 horas, de miércoles a domingo."
        ],
        "nl": [
          "Brasserie / bistro.\nMaaltijden alleen 's middags van woensdag tot zondag.\nBiedt drankjes/ijs aan van 10 tot 18 uur, woensdag tot zondag."
        ]
      },
