  "rdfs:comment": {
    "de": [
      "ÜBER DAS RESTAURANT\n\nDieses Restaurant liegt direkt am Meer, in der Nähe des Hafens und lädt Sie ein, sein Motto zu entdecken: Frische, Kreativität und Gastfreundschaft."
    ],
    "en": [
      "ABOUT THE RESTAURANT\n\nThis restaurant located in front of the sea, near the port, invites you to discover its motto: freshness, creativity and conviviality."
    ],
    "it": [
      "IL RISTORANTE\n\nQuesto ristorante, situato di fronte al mare, vicino al porto, vi invita a scoprire il suo motto: freschezza, creatività e convivialità."
    ],
    "fr": [
      "A PROPOS DU RESTAURANT\n\nCe restaurant situé face à la mer, proche du port,vous invite à découvrir sa devise: fraîcheur, créativité et convivialité."
    ],
    "es": [
      "SOBRE EL RESTAURANTE\n\nEste restaurante, situado frente al mar, cerca del puerto, le invita a descubrir su lema: frescura, creatividad y convivencia."
    ],
    "nl": [
      "OVER HET RESTAURANT\n\nDit restaurant, gelegen aan zee, vlakbij de haven, nodigt u uit om zijn motto te ontdekken: versheid, creativiteit en gezelligheid."
    ]
  },
