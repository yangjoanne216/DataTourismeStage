  "rdfs:comment": {
    "de": [
      "Ein Park mit aufblasbaren Strukturen, den man mit der ganzen Familie besuchen kann!\nPreise: 6.50 ?, 6 ? ab 3 Kindern, 4 ? für Kinder unter 4 Jahren.\nGeöffnet je nach Wetterlage."
    ],
    "en": [
      "Park of inflatable structures, to practice in family!\nPrices: 6.50 €, 6 € for 3 or more children, 4 € for children under 4.\nOpen according to weather conditions."
    ],
    "it": [
      "Parco giochi gonfiabile per tutta la famiglia!\nPrezzi: 6,50 €, 6 € per 3 o più bambini, 4 € per i minori di 4 anni.\nApertura in base alle condizioni meteorologiche."
    ],
    "fr": [
      "Parc de structures gonflables, à pratiquer en famille !\nTarifs : 6.50 €, 6 € à partir de 3 enfants, 4 € pour les - 4 ans.\nOuvert selon conditions météorologiques."
    ],
    "es": [
      "¡Parque infantil hinchable para toda la familia!\nPrecios: 6,50€, 6€ para 3 o más niños, 4€ para menores de 4 años.\nAbierto en función de las condiciones meteorológicas."
    ],
    "nl": [
      "Opblaasbare speeltuin voor het hele gezin!\nPrijzen: 6,50 euro, 6 euro voor 3 of meer kinderen, 4 euro voor kinderen onder de 4 jaar.\nGeopend afhankelijk van het weer."
    ]
  },
