  "rdfs:comment": {
    "de": [
      "Die Kirche wurde von Pascal Coste auf einem basilikalen Grundriss mit Kassettendecken errichtet. Die unvollendete Fassade sollte ein Portikus mit korinthischen Säulen sein."
    ],
    "en": [
      "Church built on a basilica plan with coffered ceilings, an architectural work by Pascal Coste."
    ],
    "it": [
      "Chiesa a pianta basilicale con soffitti a cassettoni, progettata da Pascal Coste. La facciata incompiuta doveva essere un portico con colonne corinzie."
    ],
    "fr": [
      "Eglise construite sur un plan basilical avec des plafonds à caissons, réalisée par Pascal Coste. La façade inachevée devait être un portique à colonnes corinthiennnes."
    ],
    "nl": [
      "Kerk gebouwd op basiliekplan met cassetteplafonds, ontworpen door Pascal Coste. De onvoltooide gevel moest een portiek worden met Korinthische zuilen."
    ],
    "es": [
      "Iglesia de planta basilical con artesonado, diseñada por Pascal Coste. La fachada inacabada debía ser un pórtico con columnas corintias."
    ]
  },
