  "rdfs:comment": {
    "de": [
      "Der Hafen von Mèze liegt am Rande der Lagune von Thau und empfängt Bootsfahrer für kurze und lange Aufenthalte."
    ],
    "en": [
      "The port of Mèze, located on the edge of the Thau lagoon, welcomes yachtsmen for short and long stays."
    ],
    "it": [
      "Il porto di Mèze, situato ai margini della laguna di Thau, accoglie i diportisti per soggiorni brevi e lunghi."
    ],
    "fr": [
      "Le port de Mèze, situé en bordure de la lagune de Thau, accueille les plaisanciers pour des courts et longs séjours."
    ],
    "es": [
      "El puerto de Mèze, situado al borde de la laguna de Thau, acoge a los navegantes para estancias cortas y largas."
    ],
    "nl": [
      "De haven van Mèze, gelegen aan de rand van de lagune van Thau, verwelkomt zeilers voor korte en lange verblijven."
    ]
  },
