  "rdfs:comment": {
    "de": [
      "L'Escapade Damazanaise lädt Sie während eines Zwischenstopps dazu ein, seine großzügige und schmackhafte Küche zu genießen, die ausschließlich aus frischen Produkten zubereitet wird."
    ],
    "en": [
      "L'Escapade Damazanaise invites you for a stopover to taste its generous and tasty cuisine prepared exclusively with fresh products."
    ],
    "it": [
      "L'Escapade Damazanaise vi invita a fermarvi e a gustare la sua generosa e gustosa cucina preparata esclusivamente con prodotti freschi."
    ],
    "fr": [
      "L'Escapade Damazanaise vous invite le temps d'une halte à déguster sa cuisine généreuse et savoureuse exclusivement préparée à base de produits frais."
    ],
    "nl": [
      "L'Escapade Damazanaise nodigt u uit om te stoppen en te genieten van zijn genereuze en smakelijke keuken, uitsluitend bereid met verse producten."
    ],
    "es": [
      "L'Escapade Damazanaise le invita a detenerse y disfrutar de su generosa y sabrosa cocina preparada exclusivamente con productos frescos."
    ]
  },
