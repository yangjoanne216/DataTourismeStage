  "COVID19SpecialMeasures": {
    "de": [
      "Vollständige Reinigung nach jedem Mieter. Desinfektionsgel und -tücher stehen zur Verfügung."
    ],
    "en": [
      "Complete cleaning after each tenant. Disinfecting gel and wipes available."
    ],
    "it": [
      "Pulizia completa dopo ogni inquilino. Sono disponibili gel disinfettante e salviette."
    ],
    "fr": [
      "Ménage complet après chaque locataire. Gel et lingettes désinfectantes à disposition."
    ],
    "nl": [
      "Volledige schoonmaak na elke huurder. Desinfecterende gel en doekjes beschikbaar."
    ],
    "es": [
      "Limpieza completa después de cada inquilino. Gel desinfectante y toallitas disponibles."
    ]
  },
