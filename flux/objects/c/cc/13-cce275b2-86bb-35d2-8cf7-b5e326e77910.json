  "rdfs:comment": {
    "de": [
      "Willkommen auf dem Camping de la CLAIRIÈRE in Aydat - im Herzen des UNESCO-Weltkulturerbes Parc des Volcans - in der Nähe des Lac d'Aydat, dem größten natürlichen See der Auvergne gelegen. L'Esprit de famille in 19 Stellplätzen und 26 Mietunterkünften."
    ],
    "en": [
      "Welcome to Camping de la CLAIRIÈRE in Aydat - in the heart of the UNESCO-listed Parc des Volcans - located near Lake Aydat, the largest natural lake in Auvergne. L'Esprit de famille in 19 pitches and 26 rentals."
    ],
    "it": [
      "Benvenuti al Camping de la CLAIRIÈRE di Aydat - nel cuore del Parc des Volcans, dichiarato Patrimonio dell'Umanità dall'UNESCO - situato vicino al Lago di Aydat, il più grande lago naturale dell'Alvernia. L'Esprit de famille in 19 piazzole e 26 affitti."
    ],
    "fr": [
      "Bienvenue au Camping de la CLAIRIÈRE à Aydat - au cœur du Parc des Volcans, inscrit à l'UNESCO - situé à proximité du lac d'Aydat, le lac naturel le plus grand d'Auvergne. L’Esprit de famille en 19 emplacements et 26 locatifs."
    ],
    "nl": [
      "Welkom op Camping de la CLAIRIÈRE in Aydat - in het hart van het Parc des Volcans, dat op de Werelderfgoedlijst van de UNESCO staat - gelegen bij het meer van Aydat, het grootste natuurlijke meer van de Auvergne. L'Esprit de famille in 19 plaatsen en 26 huuraccommodaties."
    ],
    "es": [
      "Bienvenido al Camping de la CLAIRIÈRE en Aydat - en el corazón del Parc des Volcans, declarado Patrimonio de la Humanidad por la UNESCO - situado cerca del lago Aydat, el lago natural más grande de Auvernia. L'Esprit de famille en 19 parcelas y 26 alquileres."
    ]
  },
