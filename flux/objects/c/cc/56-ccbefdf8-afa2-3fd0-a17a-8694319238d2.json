  "rdfs:comment": {
    "de": [
      "Tapas, Fisch, Meeresfrüchte, vegetarische Gerichte, frisch und saisonal.\nGarten mit Bäumen, Parkplatz, gegenüber dem Hafen.\nWeinkeller des Weinguts zur Verkostung."
    ],
    "en": [
      "Tapas, fish, seafood, vegetarian dishes, fresh and seasonal.\nGarden with trees, parking, facing the port.\nCellar of tasting of the domain."
    ],
    "it": [
      "Tapas, pesce, frutti di mare, piatti vegetariani, freschi e di stagione.\nGiardino alberato, parcheggio, di fronte al porto.\nCantina di degustazione del dominio."
    ],
    "fr": [
      "Tapas, poissons, fruits de mer, plats végétariens, frais et de saison.\nJardin arboré, parking, face au port.\nCaveau de dégustation du domaine."
    ],
    "nl": [
      "Tapas, vis, zeevruchten, vegetarische gerechten, vers en seizoensgebonden.\nTuin met bomen, parkeerplaats, tegenover de haven.\nProefkelder van het domein."
    ],
    "es": [
      "Tapas, pescados, mariscos, platos vegetarianos, frescos y de temporada.\nJardín con árboles, aparcamiento, frente al puerto.\nBodega de degustación del dominio."
    ]
  },
