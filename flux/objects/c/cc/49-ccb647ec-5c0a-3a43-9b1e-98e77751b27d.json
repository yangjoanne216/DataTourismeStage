  "rdfs:comment": {
    "de": [
      "Genießen Sie eine einzigartige Nacht am Ufer der Loire auf einer Kabanentrommel zwischen Angers und Nantes in Montjean-sur-Loire"
    ],
    "en": [
      "Enjoy a unique night on the banks of the Loire River on a rowing boat between Angers and Nantes in Montjean-sur-Loire"
    ],
    "it": [
      "Venite a vivere una notte unica sulle rive della Loira su una barca a remi tra Angers e Nantes a Montjean-sur-Loire"
    ],
    "fr": [
      "Venez profiter d'une nuit unique en bord de Loire sur une toue cabanée entre Angers et Nantes à Montjean-sur-Loire"
    ],
    "es": [
      "Venga a disfrutar de una noche única a orillas del Loira en un barco de remos entre Angers y Nantes en Montjean-sur-Loire"
    ],
    "nl": [
      "Kom genieten van een unieke nacht aan de oevers van de Loire op een roeiboot tussen Angers en Nantes in Montjean-sur-Loire"
    ]
