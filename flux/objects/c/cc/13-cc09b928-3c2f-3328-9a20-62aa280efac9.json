  "rdfs:comment": {
    "de": [
      "Spielen Sie in ruhiger Umgebung am Ufer des Brevon. Entdecken Sie einen klassischen Tennisplatz sowie eine Mauer, um allein oder mit mehreren Spielern zu spielen."
    ],
    "en": [
      "Play on a traditional court or against the wall (solo or against several players) in a peaceful setting by the Brevon."
    ],
    "it": [
      "Giocate nella pace e nella tranquillità del fiume Brevon. Scoprite un campo classico e un muro per giocare da soli o in compagnia."
    ],
    "fr": [
      "Jouez au calme, au bord du Brevon. Découvrez un court classique ainsi qu'un mur pour jouer seul ou à plusieurs."
    ],
    "nl": [
      "Speel in de rust van de Brevon rivier. Ontdek een klassieke baan en een muur om alleen of met anderen te spelen."
    ],
    "es": [
      "Juega en la tranquilidad del río Brevon. Descubre una pista clásica y un muro para jugar solo o en compañía."
    ]
  },
