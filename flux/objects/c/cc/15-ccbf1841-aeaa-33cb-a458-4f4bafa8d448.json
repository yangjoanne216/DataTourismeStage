  "rdfs:comment": {
    "de": [
      "Entdecken Sie eine Vielzahl von Vögeln, die die Brenne inmitten einer harmonischen und noch unberührten Landschaft bevölkern."
    ],
    "en": [
      "Discovery of a multitude of birds populating the Brenne in the middle of a harmonious and still preserved landscape."
    ],
    "it": [
      "Scoprite la moltitudine di uccelli che popolano la Brenne nel mezzo di un paesaggio armonioso e ancora preservato."
    ],
    "fr": [
      "Découverte d'une multitude d'oiseaux peuplant la Brenne au beau milieu d'un paysage harmonieux et encore préservé."
    ],
    "nl": [
      "Ontdek de veelheid aan vogels die de Brenne bewonen te midden van een harmonieus en nog bewaard gebleven landschap."
    ],
    "es": [
      "Descubra la multitud de aves que habitan en el Brenne en medio de un paisaje armonioso y aún conservado."
    ]
  },
