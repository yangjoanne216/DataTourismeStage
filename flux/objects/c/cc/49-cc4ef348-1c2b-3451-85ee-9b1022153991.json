  "rdfs:comment": {
    "de": [
      "Die Crêperie befindet sich in Beaupréau, im Herzen der Mauges, nur zwei Schritte vom Stadtzentrum entfernt."
    ],
    "en": [
      "The crêperie is located in Beaupréau, in the heart of the Mauges, a stone's throw from downtown."
    ],
    "it": [
      "La crêperie si trova a Beaupréau, nel cuore delle Mauges, a pochi passi dal centro della città."
    ],
    "fr": [
      "La crêperie est située à Beaupréau, au cœur des Mauges, à 2 pas de son centre-ville."
    ],
    "es": [
      "La crepería está situada en Beaupréau, en el corazón de Mauges, a dos pasos del centro de la ciudad."
    ],
