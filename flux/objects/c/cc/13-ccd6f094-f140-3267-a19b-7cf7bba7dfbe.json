  "rdfs:comment": {
    "de": [
      "Da der Hafen von Sciez , Surfen Genf nach Yvoire das mittelalterliche Dorf zu besuchen."
    ],
    "en": [
      "Sail from Sciez port and put down anchor in Yvoire to visit this medieval village in your own time."
    ],
    "it": [
      "Dal porto di Sciez, navigherete sul Lago di Ginevra e farete rotta verso Yvoire per visitare, a vostro piacimento, questo villaggio medievale definito \"Il più bel villaggio di Francia\"."
    ],
    "fr": [
      "Depuis le Port de Sciez, vous naviguerez sur le Léman et larguerez les amarres à Yvoire pour visiter, à votre rythme, ce village médiéval labellisé \" Plus beau village de France\"."
    ],
    "nl": [
      "Vanuit de haven van Sciez vaart u het meer van Genève op en gooit u de trossen los in Yvoire om op uw eigen tempo dit middeleeuwse dorp te bezoeken dat het label \"Mooiste dorp van Frankrijk\" draagt."
    ],
    "es": [
      "Desde el puerto de Sciez, navegará por el lago Lemán y soltará amarras en Yvoire para visitar, a su ritmo, este pueblo medieval calificado como \"el más bello de Francia\"."
    ]
  },
  "rdfs:label": {
    "de": [
      "Medieval Outing on the lake"
    ],
    "en": [
      "Medieval Outing, Cruise on the Lake Geneva"
    ],
    "fr": [
      "Croisière Médiévale en catamaran moteur"
    ]
  },
