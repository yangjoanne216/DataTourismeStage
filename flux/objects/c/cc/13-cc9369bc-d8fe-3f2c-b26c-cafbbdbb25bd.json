  "rdfs:comment": {
    "de": [
      "Restaurant und Weinbar. Speisesaal im Erdgeschoss, große Terrasse unter den Platanen des Dorfplatzes. Vollständig hausgemachte Küche aus frischen, möglichst lokalen, biologischen oder nachhaltigen Produkten."
    ],
    "en": [
      "Restaurant and wine bar. Dining room on the ground floor, large terrace under the plane trees of the village square. Entirely homemade cooking from fresh products, as much as possible local, organic or reasoned."
    ],
    "it": [
      "Ristorante e wine bar. Sala da pranzo al piano terra, ampia terrazza sotto i platani della piazza del paese. Cucina interamente fatta in casa con prodotti freschi, locali, biologici o sostenibili."
    ],
    "fr": [
      "Restaurant et bar à vins. Salle de restauration au RDC, grande terrasse sous les platanes de la place du village. Cuisine entièrement faite maison à partir de produits frais, le plus possible locaux, bios ou raisonnés."
    ],
    "es": [
      "Restaurante y bar de vinos. Comedor en la planta baja, gran terraza bajo los plátanos de la plaza del pueblo. Cocina totalmente casera con productos frescos, locales, ecológicos o sostenibles."
    ],
    "nl": [
      "Restaurant en wijnbar. Eetkamer op de begane grond, groot terras onder de platanen op het dorpsplein. Volledig huisgemaakte gerechten met verse, lokale, biologische of duurzame producten."
    ]
  },
