  "rdfs:comment": {
    "de": [
      "Ziegenzuchtbetrieb in Saint-Cézaire-sur-Siagne mit zahlreichen Aktivitäten: Ausritte zu Pferd oder Pony, Entdeckungstage, pädagogischer Bauernhof, nicht zu vergessen die Käserei!"
    ],
    "en": [
      "Goat farm, located in Saint-Cézaire-sur-Siagne offering many activities: horse or pony rides, discovery days, educational farm, not forgetting the cheese factory!"
    ],
    "it": [
      "La fattoria delle capre, situata a Saint-Cézaire-sur-Siagne, offre numerose attività: passeggiate a cavallo o con i pony, giornate di scoperta, fattoria didattica, senza dimenticare il caseificio!"
    ],
    "fr": [
      "Exploitation caprine, située à Saint-Cézaire-sur-Siagne proposant de nombreuses activités : balades à cheval ou poney, journées découverte, ferme pédagogique, sans oublier la fromagerie !"
    ],
    "es": [
      "Granja de cabras, situada en Saint-Cézaire-sur-Siagne, que ofrece numerosas actividades: paseos a caballo o en poni, jornadas de descubrimiento, granja educativa, ¡sin olvidar la quesería!"
    ],
    "nl": [
      "Geitenboerderij, gelegen in Saint-Cézaire-sur-Siagne, met tal van activiteiten: paardrijden of ponyrijden, ontdekkingsdagen, educatieve boerderij en niet te vergeten de kaasmakerij!"
    ]
  },
