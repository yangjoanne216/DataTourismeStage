  "COVID19SpecialMeasures": {
    "de": [
      "Systematische Desinfektion der betroffenen Stellen, Kopfhörerabdeckungen und Mikrofonschaum werden bei jedem Flug gewechselt."
    ],
    "en": [
      "Systematic disinfection of the affected parts, earphone cover and microphone foam changed at each flight."
    ],
    "it": [
      "Disinfezione sistematica delle aree interessate, sostituzione delle coperture delle cuffie e della schiuma del microfono per ogni volo."
    ],
    "fr": [
      "Désinfection systématique des parties touchées, couvre écouteurs et mousse de micro changés à chaque vol."
    ],
    "nl": [
      "Systematische ontsmetting van de getroffen gebieden, koptelefoonhoesjes en microfoonschuim vervangen voor elke vlucht."
    ],
    "es": [
      "Desinfección sistemática de las zonas afectadas, cambio de las fundas de los auriculares y de la espuma de los micrófonos en cada vuelo."
    ]
  },
