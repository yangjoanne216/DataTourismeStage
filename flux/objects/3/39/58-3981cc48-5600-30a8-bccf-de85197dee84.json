  "rdfs:comment": {
    "de": [
      "Christophe, Magali und ihr Sohn Léo Mallesson sind Biobauern in Vauvenargues und produzieren - destillieren nach alter Tradition - Parfüm-, Aroma- und Medizinalpflanzen."
    ],
    "en": [
      "Magali and Christophe Mallesson are organic farmers based in Vauvenargues, traditional producers-distillers of aromatic and medicinal plants (common lavender, lavandula hybrida, spike lavender, thyme, rosemary, helichrysum italicum, savory)."
    ],
    "it": [
      "Christophe, Magali e il figlio Léo Mallesson sono agricoltori biologici a Vauvenargues, produttori e distillatori di profumi, piante aromatiche e medicinali."
    ],
    "fr": [
      "Christophe, Magali et leur fils Léo Mallesson sont agriculteurs en bio à Vauvenargues, producteurs - distillateurs à l'ancienne de plantes à parfum, aromatiques et médicinales."
    ],
    "es": [
      "Christophe, Magali y su hijo Léo Mallesson son agricultores ecológicos en Vauvenargues, productores y destiladores de perfumes, plantas aromáticas y medicinales."
    ],
    "nl": [
      "Christophe, Magali en hun zoon Léo Mallesson zijn biologische landbouwers in Vauvenargues, producenten en distilleerders van parfum, aromatische en medicinale planten."
    ]
  },
