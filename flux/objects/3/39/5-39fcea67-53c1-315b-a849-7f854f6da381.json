  "rdfs:comment": {
    "de": [
      "Ehemaliger Bauernhof aus dem Jahr 1832, der vollständig renoviert wurde. Hotel mit Panoramablick am Waldrand und am Anfang vieler Wanderwege gelegen. Zimmer mit Mezzaninen."
    ],
    "en": [
      "Old farmhouse from 1832 completely renovated. Hotel with a panoramic view located on the edge of the forest and at the start of many hiking trails. Rooms on mezzanines."
    ],
    "it": [
      "Antico casale del 1832, completamente ristrutturato. Hotel con vista panoramica situato ai margini del bosco e all'inizio di numerosi sentieri escursionistici. Camere con soppalco."
    ],
    "fr": [
      "Ancienne ferme de 1832 entièrement rénovée. Hôtel avec une vue panoramique situé en lisière de forêt et au départ de nombreux sentiers de randonnées. Chambres en mezzanines."
    ],
    "nl": [
      "Oude boerderij uit 1832, volledig gerenoveerd. Hotel met panoramisch uitzicht gelegen aan de rand van het bos en aan het begin van vele wandelroutes. Kamers met tussenverdiepingen."
    ],
    "es": [
      "Antigua granja de 1832, completamente renovada. Hotel con vistas panorámicas situado al borde del bosque y al inicio de numerosas rutas de senderismo. Habitaciones con altillo."
    ]
  },
