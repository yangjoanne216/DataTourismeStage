  "rdfs:comment": {
    "de": [
      "Sanfte Gymnastik Montag 18h/19h und 19h/20h, Traditionelles Yoga Dienstag 19h30/21h,Tai Chi Chen Mittwoch 17h-18h15, Kung Fu Mittwoch 19h30/22h, Chor Mittwoch 19h/21h, Französisches Boxen Donnerstag ab 18h, Bildende Kunst 1 Samstag/Monat 10h/11h30, Annäherung an Yoga Dienstag 18h/19h30."
    ],
    "en": [
      "Soft Gym Monday 6pm/7pm and 7pm/20pm, Traditional Yoga Tuesday 7.30pm/21pm, Taï chi chen sea 5pm-6.15pm, Kung Fu mercr 7.30pm/22pm, Choir Wednesday 7pm/21pm, French Boxing Thursday from 6pm, Arts 1 Saturday/month 10am/11.30am, Approach to Yoga Tuesday 6pm/ 7.30pm."
    ],
    "it": [
      "Ginnastica dolce lunedì 18.00/7.00 e 19.00/8.00, Yoga tradizionale martedì 19.30/9.00, Tai chi chen mercoledì 17.00/18.15, Kung Fu mercoledì 19.30/10.00, Coro mercoledì 19.00/9.00, Boxe francese giovedì dalle 18.00, Arte 1 sabato/mese 10.00/11.30, Approccio allo yoga martedì 18.00/17.30."
    ],
    "fr": [
      "Gym douce lundi 18h/19h et 19h/20h, Yoga traditionnel mardi 19h30/21h,Taï chi chen mer 17h-18h15, Kung Fu mercr 19h30/22h, Chorale mercredi 19h/21h, Boxe française jeudi dès 18h, arts plastiques 1 samedi/mois 10h/11h30, Approche du Yoga mardi 18h/19h30."
    ],
    "nl": [
      "Zachte gymnastiek maandag 18.00/7.00 uur en 19.00/8.00 uur, Traditionele yoga dinsdag 19.30/9.00 uur, Tai chi chen woensdag 17.00-6.15 uur, Kung Fu woensdag 19.30/10.00 uur, Koor woensdag 19.00/9.00 uur, Frans boksen donderdag vanaf 18.00 uur, Kunst 1 zaterdag/maand 10.00/11.30 uur, Aanpak yoga dinsdag 18.00/7.30 uur."
    ],
    "es": [
      "Gimnasia suave lunes 18:00/7:00 y 19:00/8:00, yoga tradicional martes 19:30/9:00, tai chi chen miércoles 17:00-6:15, kung fu miércoles 19:30/10:00, coro miércoles 19:00/9:00, boxeo francés jueves a partir de las 18:00, arte 1 sábado/mes 10:00/11:30, acercamiento al yoga martes 18:00/7:30."
