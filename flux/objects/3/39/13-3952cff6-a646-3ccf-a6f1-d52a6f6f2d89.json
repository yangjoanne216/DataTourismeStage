  "rdfs:comment": {
    "de": [
      "Entdeckung, Erlernen und Perfektionieren von Bergsportarten: Bergsteigen, Klettern, Gletscher- und Wandertouren, Klettersteige, Eisklettern, Skifahren und Snowboarden abseits der Pisten, Schneeschuhwandern."
    ],
    "en": [
      "Discover, learn mountain sports: mountaineering, climbing, glacier trekking and hiking, via ferrata, canyoning, ice climbing, backcountry skiing and snowboarding, snowshoeing."
    ],
    "it": [
      "Scoperta, apprendimento e perfezionamento degli sport di montagna: alpinismo, arrampicata, ghiacciaio ed escursionismo, via ferrata, arrampicata su ghiaccio, sci e snowboard fuori pista, racchette da neve."
    ],
    "fr": [
      "Découverte, apprentissage et perfectionnement des sports de montagne : alpinisme, escalade, randonnée glaciaire et pédestre, via-ferrata, cascade de glace, ski et surf hors piste, raquettes à neige."
    ],
    "nl": [
      "Ontdekking, opleiding en perfectionering van de bergsporten : alpinisme, klimmen, gletsjertochten en wandelingen, via-ferrata, ijsklimmen, off piste ski en surf, sneeuwschoenwandelingen."
    ],
    "es": [
      "Descubrimiento, aprendizaje y perfeccionamiento de los deportes de montaña: alpinismo, escalada, glaciar y senderismo, vía ferrata, escalada en hielo, esquí y snowboard fuera de pista, raquetas de nieve."
    ]
  },
