  "rdfs:comment": {
    "de": [
      "Direkter Zugang zu Les Arcs von den Bahnsteigen des SNCF-Bahnhofs in Bourg-Saint-Maurice. Mit der Standseilbahn erreichen Sie die Station Arc 1600 in 7 Minuten. Haltestellen auf Anfrage in Les Granges und Montrigon."
    ],
    "en": [
      "Direct Access to Les Arcs, from the SNCF trainstation of Bourg-Saint-Maurice.\r\nIt allows to join Arc 1600 within 7 mn, and you can ask for different stops during transport such as Granges or Montrignon."
    ],
    "it": [
      "Accesso diretto a Les Arcs dai binari della stazione SNCF di Bourg-Saint-Maurice. La funicolare vi porta al resort Arc 1600 in 7 minuti. Fermate su richiesta a Les Granges e Montrigon."
    ],
    "fr": [
      "Accès direct aux Arcs depuis les quais de la gare SNCF de Bourg-Saint-Maurice. Le funiculaire permet de rejoindre la station d'Arc 1600 en 7 mn. Arrêt sur demande aux Granges et à Montrigon."
    ],
    "nl": [
      "Directe toegang tot Les Arcs vanaf de perrons van het SNCF-station Bourg-Saint-Maurice. De kabelbaan brengt u in 7 minuten naar het resort Arc 1600. Stopt op verzoek in Les Granges en Montrigon."
    ],
    "es": [
      "Acceso directo a Les Arcs desde los andenes de la estación SNCF de Bourg-Saint-Maurice. El funicular le lleva a la estación de Arc 1600 en 7 minutos. Paradas a petición en Les Granges y Montrigon."
    ]
  },
