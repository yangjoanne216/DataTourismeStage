  "rdfs:comment": {
    "de": [
      "Das Maison des associations, ein ehemaliger Genossenschaftskeller, beherbergt die Vereine von Signois (Sport, Kultur, Bildung,....)\r\nDas Forum der Vereine findet hier jeden ersten Samstag im September statt."
    ],
    "en": [
      "Formerly a cooperative cellar, the Maison des associations welcomes the Signois associations (sports, cultural, educational, ....)\r\nThe forum of associations is present there every 1st Saturday of September."
    ],
    "it": [
      "Ex cantina cooperativa, la Maison des associations accoglie le associazioni locali (sportive, culturali, educative, ....)\r\nIl forum delle associazioni si tiene ogni primo sabato di settembre."
    ],
    "fr": [
      "Ancienne cave coopérative, la Maison des associations accueillent les associations signoises (sportives, culturelle, éducatives,....)\r\nLe forum des associations y est présent tous les 1er samedi du mois de septembre."
    ],
    "nl": [
      "Het Maison des associations, vroeger een coöperatieve kelder, ontvangt de lokale verenigingen (sport, cultuur, onderwijs, ....)\r\nHet verenigingsforum wordt daar elke eerste zaterdag van september gehouden."
    ],
    "es": [
      "Antigua bodega cooperativa, la Maison des associations acoge a las asociaciones locales (deportivas, culturales, educativas, ....)\r\nEl foro de asociaciones se celebra allí cada primer sábado de septiembre."
    ]
  },
