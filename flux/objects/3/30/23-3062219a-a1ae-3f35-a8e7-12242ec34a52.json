  "rdfs:comment": {
    "de": [
      "Restaurant mit Terrasse mit Blick auf die Vézère. Der Chefkoch bietet Ihnen eine köstliche traditionelle hausgemachte Küche, die aus frischen Produkten zubereitet wird."
    ],
    "en": [
      "Francis and his team welcome you to their restaurant with a terrace overlooking the Vézère and offer you delicious traditional home cooking made from fresh produce."
    ],
    "it": [
      "Ristorante con terrazza affacciata sulla Vézère, dove lo chef propone una deliziosa cucina tradizionale casalinga a base di prodotti freschi."
    ],
    "fr": [
      "Restaurant avec terrasse dominant la Vézère, le chef vous  propose une délicieuse cuisine traditionnelle maison élaborée à base de produits frais."
    ],
    "es": [
      "Restaurante con terraza con vistas al Vézère, donde el chef sirve deliciosa cocina casera tradicional con productos frescos."
    ],
    "nl": [
      "Restaurant met terras dat uitkijkt over de Vézère, waar de chef heerlijke traditionele huisgemaakte gerechten serveert met verse producten."
    ]
  },
