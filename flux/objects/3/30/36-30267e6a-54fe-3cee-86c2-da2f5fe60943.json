      "dc:description": {
        "de": [
          "28 m² großes Studio im 3. Stock mit Aufzug. Stock. Fußbodenheizung. Abstellraum für Skier. Supermarkt und Sportgeschäft im Erdgeschoss der Residenz.",
          "Studio für 6 Personen mit Blick auf die Pisten."
        ],
        "en": [
          "Studio 6 persons with view on the slopes.",
          "This apartment of 28 m² (about 301 ft²) is located on the 3rd floor with a lift. Floor heating. Ski room. Grocery and sports shop on the ground floor of the residence."
        ],
        "it": [
          "Monolocale di 28 m² al 3° piano con ascensore. Riscaldamento a pavimento. Sala sci. Supermercato e negozio di articoli sportivi al piano terra del residence.",
          "Studio per 6 persone con vista sulle piste."
        ],
        "fr": [
          "Studio 6 personnes avec vue sur pistes.",
          "Studio de 28 m² au 3ème étage avec ascenseur. Chauffage au sol. Local à ski. Supérette et magasin de sport au rez de chaussée de la résidence."
        ],
        "nl": [
          "Studio van 28 m² op de 3e verdieping met lift. Vloerverwarming. Skikamer. Supermarkt en sportwinkel op de begane grond van de residentie.",
          "Studio voor 6 personen met uitzicht op de piste."
        ],
        "es": [
          "Estudio de 28 m² en la 3ª planta con ascensor. Calefacción por suelo radiante. Sala de esquí. Supermercado y tienda de deportes en la planta baja de la residencia.",
          "Estudio para 6 personas con vistas a las pistas."
        ]
      },
