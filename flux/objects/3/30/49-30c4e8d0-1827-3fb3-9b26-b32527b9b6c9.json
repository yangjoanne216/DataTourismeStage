  "rdfs:comment": {
    "de": [
      "Renaissance-Herrenhaus aus dem 15. und 16. Jahrhundert, das von der ab 1988 gegründeten Association pour la Protection du Patrimoine (Vereinigung zum Schutz des Kulturerbes) restauriert wurde. Privates Gelände nach Vereinbarung für die Öffentlichkeit zugänglich."
    ],
    "en": [
      "Renaissance manor house from the 15th and 16th centuries, restored by the Association for the Protection of Heritage created in 1988. Private site open to the public by appointment."
    ],
    "it": [
      "Casa padronale rinascimentale del XV e XVI secolo, restaurata dall'Associazione per la tutela del patrimonio creata nel 1988. Sito privato aperto al pubblico su appuntamento."
    ],
    "fr": [
      "Manoir Renaissance des XVe et XVIe siècles, restauré par l'Association pour la Protection du Patrimoine créée à partir de 1988. Site privé ouvert au public sur rendez-vous."
    ],
    "nl": [
      "Renaissance herenhuis uit de 15e en 16e eeuw, gerestaureerd door de in 1988 opgerichte Vereniging ter Bescherming van het Erfgoed. Privéterrein open voor publiek op afspraak."
    ],
    "es": [
      "Casa señorial renacentista de los siglos XV y XVI, restaurada por la Asociación para la Protección del Patrimonio creada en 1988. Sitio privado abierto al público con cita previa."
    ]
  },
