  "rdfs:comment": {
    "de": [
      "Ombeline Fichant, Doktorandin an der Ecole Pratique des Hautes Etudes in Paris, wird Imaginationen rund um den Drachen im Mittelalter und seine verschiedenen Darstellungen präsentieren"
    ],
    "en": [
      "Ombeline Fichant, doctoral student at the Ecole Pratique des Hautes Etudes de Paris, will present the imaginary world of the dragon in the Middle Ages and its various representations"
    ],
    "it": [
      "Ombeline Fichant, dottoranda presso l'Ecole Pratique des Hautes Etudes di Parigi, presenterà il mondo immaginario del drago nel Medioevo e le sue diverse rappresentazioni"
    ],
    "fr": [
      "Ombeline Fichant, doctorante à l'Ecole Pratique des Hautes Etudes de Paris, présentera imaginaires autour du dragon au Moyen Âge et ses différentes représentations"
    ],
    "nl": [
      "Ombeline Fichant, doctorandus aan de Ecole Pratique des Hautes Etudes in Parijs, zal de imaginaire wereld van de draak in de Middeleeuwen en de verschillende voorstellingen ervan toelichten"
    ],
    "es": [
      "Ombeline Fichant, doctoranda en la Escuela Práctica de Altos Estudios de París, presentará el mundo imaginario del dragón en la Edad Media y sus diversas representaciones"
    ]
