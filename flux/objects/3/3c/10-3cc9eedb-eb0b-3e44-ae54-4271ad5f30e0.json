  "rdfs:comment": {
    "de": [
      "Befindet sich am Eingang des städtischen Campingplatzes. Kostenloses Parken und kostenlose Dienstleistungen für max. 48 Stunden, danach 20? Kein Wasser bei starkem Frost."
    ],
    "en": [
      "Located at the entrance of the municipal camping. Free parking and services for up to 48 hours, then 20 ? No water in case of heavy frost."
    ],
    "it": [
      "Situato all'ingresso del campeggio comunale. Parcheggio e servizi gratuiti fino a 48 ore, poi 20 ? Niente acqua in caso di forte gelo."
    ],
    "fr": [
      "Située à l'entrée du camping municipal. Stationnement et services gratuits pendant 48 heures maxi, puis 20 €. Pas d'eau en cas de forte gelée."
    ],
    "es": [
      "Situado a la entrada del camping municipal. Aparcamiento y servicios gratuitos hasta 48 horas, después 20€. Sin agua en caso de heladas severas."
    ],
    "nl": [
      "Gelegen aan de ingang van de gemeentelijke camping. Gratis parkeren en diensten tot 48 uur, daarna 20 ? Geen water bij strenge vorst."
    ]
  },
