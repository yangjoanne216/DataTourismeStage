  "rdfs:comment": {
    "de": [
      "Payapa bietet Kerzen und Fondants an, die mit einem Wachs aus 100% gentechnikfreiem Rapsöl parfümiert sind, ökologisch verantwortlich mit hochwertigen Düften aus der Welthauptstadt des Parfums, Grasse."
    ],
    "en": [
      "Payapa offers candles and fluxes scented with 100% colza wax without GMO, eco-responsible with quality perfumes from the world capital of perfume, Grasse."
    ],
    "it": [
      "Payapa offre candele profumate e fusioni con cera di colza ecologica e 100% priva di OGM e fragranze di qualità provenienti dalla capitale mondiale del profumo, Grasse."
    ],
    "fr": [
      "Payapa propose des bougies et des fondants parfumés avec une cire 100% colza sans OGM, éco responsable avec des parfums de qualité provenant de la capitale mondiale du parfum, Grasse."
    ],
    "es": [
      "Payapa ofrece velas perfumadas y fundentes con cera de colza 100% ecológica y sin OMG y fragancias de calidad procedentes de la capital mundial del perfume, Grasse."
    ],
    "nl": [
      "Payapa biedt geurkaarsen en melts met 100% GMO-vrije, milieuvriendelijke koolzaadwas en kwaliteitsgeuren uit de parfumhoofdstad van de wereld, Grasse."
    ]
  },
