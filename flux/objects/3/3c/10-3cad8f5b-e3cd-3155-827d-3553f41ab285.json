  "rdfs:comment": {
    "de": [
      "La Bonne Auberge, ein Gourmetrestaurant mit 1 Stern im Guide Michelin, bietet edle Produkte und traditionelle Küche und entführt Sie in einen Strudel aus Geschmacks-, Geruchs- und Sehfreuden"
    ],
    "en": [
      "La Bonne Auberge, a gourmet restaurant with 1 star in the Michelin Guide, offers noble products, traditional cuisine, and takes you into a whirlwind of gustatory, olfactory and visual delights"
    ],
    "it": [
      "La Bonne Auberge, ristorante gastronomico con 1 stella Michelin, offre prodotti nobili, cucina tradizionale e vi porta in un turbinio di delizie gustative, olfattive e visive"
    ],
    "fr": [
      "La Bonne Auberge, restaurant gastronomique avec 1 étoile au Guide Michelin, propose des produits nobles, une cuisine traditionnelle, et vous entraîne dans un tourbillon de délices gustatifs, olfactifs et visuels"
    ],
    "es": [
      "La Bonne Auberge, restaurante gastronómico con 1 estrella Michelin, ofrece productos nobles, cocina tradicional, y le lleva a un torbellino de delicias gustativas, olfativas y visuales"
    ],
    "nl": [
      "La Bonne Auberge, een gastronomisch restaurant met 1 Michelin-ster, biedt edele producten en een traditionele keuken en neemt u mee op een wervelende tocht van smaak-, geur- en visuele hoogstandjes"
    ]
  },
  "rdfs:label": {
    "fr": [
      "RESTAURANT LA BONNE AUBERGE"
    ]
  },
