  "rdfs:comment": {
    "de": [
      "Spiel mit der Fort-Kugel im Freien. Vorführung und Einführung, nach Vereinbarung. Abends ab 18:30 Uhr und sonntags ab 11:30 Uhr geöffnet. Kostenlose Vorführung, 0,50€ pro Spieler für eine Partie."
    ],
    "en": [
      "Outdoor fort ball game. Demonstration and initiation, by appointment. Open in the evening from 6.30 pm and on Sundays from 11.30 am. Free demonstration, 0,50€ per player for one game."
    ],
    "it": [
      "Gioco della boule de fort all'aperto. Dimostrazione e iniziazione, su appuntamento. Aperto la sera dalle 18.30 e la domenica dalle 11.30. Dimostrazione gratuita, 0,50? per giocatore per una partita."
    ],
    "fr": [
      "Jeu de boule de fort en plein air. Démonstration et initiation, sur rendez-vous. Ouvert le soir à partir de 18h30 et le dimanche à partir de 11h30. Démonstration gratuite, 0,50€ par joueur pour une partie."
    ],
    "nl": [
      "Openlucht boule de fort spel. Demonstratie en inwijding, op afspraak. S Avonds geopend vanaf 18.30 uur en op zondag vanaf 11.30 uur. Gratis demonstratie, € 0,50 per speler voor een spel."
    ],
    "es": [
      "Juego de la boule de fort al aire libre. Demostración e iniciación, con cita previa. Abierto por la tarde a partir de las 18.30 horas y el domingo a partir de las 11.30 horas. Demostración gratuita, 0,50? por jugador para una partida."
    ]
  },
