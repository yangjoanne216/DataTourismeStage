  "rdfs:comment": {
    "de": [
      "Das Gîte de l'oiselière liegt 10 km von Le Mans entfernt und empfängt Sie für Ihre Empfänge, Feiern, Hochzeiten, Geburtstage und andere Familienereignisse."
    ],
    "en": [
      "The Gîte de l'oiselière located 10 km from Le Mans welcomes you for your receptions, parties, weddings, birthdays and other family events."
    ],
    "it": [
      "Il Gîte de l'Oiselière, situato a 10 km da Le Mans, vi accoglie per ricevimenti, feste, matrimoni, compleanni e altri eventi familiari."
    ],
    "fr": [
      "Le Gîte de l'oiselière situé à 10 km du Mans vous accueille pour vos réceptions, fêtes, mariages,  anniversaires et autres évènements familiaux."
    ],
    "es": [
      "El Gîte de l'Oiselière, situado a 10 km de Le Mans, le acoge para sus recepciones, fiestas, bodas, cumpleaños y otros eventos familiares."
    ],
    "nl": [
      "De Gîte de l'Oiselière, gelegen op 10 km van Le Mans, verwelkomt u voor uw recepties, feesten, bruiloften, verjaardagen en andere familiefeesten."
    ]
