      "dc:description": {
        "de": [
          "20 Strecken zum Forellenangeln!\npreis: 16 € für einen halben Tag\n \nunterhaltung: Tretboote und zahlreiche Spiele\nunterkunft: Campingplatz mit 150 Stellplätzen\nempfang: Saal für 150 bis 200 Personen\nsonstiges: Grillmöglichkeiten im Freien"
        ],
        "en": [
          "20 trout fishing courses!\nprice: 16 € per half day\n \nentertainment: pedal boats and many games\naccommodation: campsite with 150 pitches\nreception: room for 150 to 200 people\nmiscellaneous: possibility of outdoor barbecue"
        ],
        "it": [
          "20 uscite di pesca alla trota!\nprezzo: 16 ? per mezza giornata\n \nintrattenimento: pedalò e tanti giochi\nalloggio: campeggio con 150 piazzole\nricevimento: sala per 150-200 persone\nvarie: possibilità di barbecue all'aperto"
        ],
        "fr": [
          "20 parcours de pêche à la truite! \ntarif: 16 € la demi-journée \n \ndivertissements: pédalos et de nombreux jeux \nhébergement: camping de 150 emplacements \naccueil: salle pouvant accueillir 150 à 200 personnes \ndivers: possibilité de barbecue en plein air"
        ],
        "es": [
          "¡20 viajes de pesca de trucha!\nprecio: 16€ por medio día\n \nentretenimiento: botes de pedal y muchos juegos\nalojamiento: camping con 150 parcelas\nrecepción: sala para 150 a 200 personas\nvarios: posibilidad de barbacoa al aire libre"
        ],
        "nl": [
          "20 forelvisreizen!\nprijs: 16 € per halve dag\n \nentertainment: waterfietsen en vele spelletjes\naccommodatie: camping met 150 plaatsen\nreceptie: ruimte voor 150 tot 200 personen\ndiversen: mogelijkheid tot barbecue in de open lucht"
        ]
      },
