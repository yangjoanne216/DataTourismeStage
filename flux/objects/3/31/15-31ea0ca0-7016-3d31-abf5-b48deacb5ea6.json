  "rdfs:comment": {
    "de": [
      "Entdecken Sie die Geschichte der Eisenbahnlinie, Argenton sur Creuse/Le Blanc, die durch Saint Benoit du Sault führte, mit einem ehemaligen Zugführer."
    ],
    "en": [
      "Discover the history of the Argenton sur Creuse/Le Blanc railway line, which passed through Saint Benoit du Sault, with a former train driver."
    ],
    "it": [
      "Scoprite la storia della linea ferroviaria Argenton sur Creuse/Le Blanc, che passava per Saint Benoit du Sault, con un ex macchinista."
    ],
    "fr": [
      "Découvrez l'histoire de la ligne de chemin de fer, Argenton sur Creuse/Le Blanc qui passait par Saint Benoit du Sault avec un ancien conducteur de train."
    ],
    "nl": [
      "Ontdek met een voormalige machinist de geschiedenis van de spoorlijn Argenton sur Creuse/Le Blanc, die door Saint Benoit du Sault liep."
    ],
    "es": [
      "Descubra la historia de la línea de ferrocarril Argenton sur Creuse/Le Blanc, que pasaba por Saint Benoit du Sault, de la mano de un antiguo maquinista."
    ]
  },
