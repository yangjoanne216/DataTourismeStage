  "rdfs:comment": {
    "de": [
      "Kaffee, Bistro, Shop, Restaurant, die Conserverie bietet feine Teller aus lokalen und saisonalen Produkten sowie Produktionen."
    ],
    "en": [
      "Café, bistro, boutique, restaurant, la Conserverie offers you delicious plates composed of local and seasonal products as well as its productions."
    ],
    "it": [
      "Ristorante, caffetteria, bistrot, negozio, brunch domenicale, la Conserverie offre piatti deliziosi a base di prodotti locali e stagionali e di produzione propria.  Ristorazione tradizionale, pause dolci e salate, sala da tè."
    ],
    "fr": [
      "Restaurant, Café, bistrot, boutique, le Brunch du dimanche, la Conserverie vous propose de délicieuses assiettes composées de produits locaux et de saison ainsi que ses productions.  Restauration traditionnelle , pauses salées et sucrées , salon de thé."
    ],
    "es": [
      "Restaurante, cafetería, bistró, tienda, brunch dominical, la Conserverie ofrece deliciosos platos elaborados con productos locales y de temporada, así como con sus propias producciones.  Restauración tradicional, pausas dulces y saladas, salón de té."
    ],
    "nl": [
      "Café, bistro, boetiek, restaurant, la Conserverie biedt u heerlijke gerechten samengesteld uit lokale en seizoensgebonden producten, evenals zijn producties."
    ]
  },
