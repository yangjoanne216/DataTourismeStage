  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/f65c2b18-061c-37af-9f7a-fe6af025f9c7",
      "dc:description": {
        "de": [
          "Geflügelhof im Soultzbachtal.\n\n\nGeflügel: Hühner, Blässhühner, Truthähne, Kaninchen.\n\n\nWurstwaren aus Geflügel, Braten."
        ],
        "en": [
          "Poultry farm located in the valley of Soultzbach.\n\n\nPoultry : chickens, cockerels, turkeys, rabbits.\n\n\nPoultry delicatessen, rotisserie."
        ],
        "it": [
          "Azienda avicola situata nella valle del Soultzbach.\n\n\nPollame: polli, galli, tacchini, conigli.\n\n\nSalumeria di pollame, arrosto."
        ],
        "fr": [
          "Ferme avicole située dans le vallon du Soultzbach.\n\n\nVolailles : poulets, coquelets, dindes, lapins.\n\n\nCharcuteries de volailles, rôtisserie."
        ],
        "nl": [
          "Pluimveebedrijf gelegen in het dal van de Soultzbach.\n\n\nPluimvee: kippen, hanen, kalkoenen, konijnen.\n\n\nGevogelte delicatessen, roosteren."
        ],
        "es": [
          "Granja avícola situada en el valle de Soultzbach.\n\n\nAves de corral: pollos, gallos, pavos, conejos.\n\n\nCharcutería de aves de corral, asado."
        ]
      },
