  "rdfs:comment": {
    "de": [
      "Einladendes Hotel, gegenüber dem SNCF-, TGV- und Tramway-Bahnhof, Nordzugang. 5 Gehminuten entfernt: Jardin des Plantes, Musée des Beaux-Arts, Fußgängerzone im historischen Zentrum, Château des Ducs de Bretagne, Kathedrale, Cité des Congrès, Lieu unique."
    ],
    "en": [
      "Welcoming hotel, facing the SNCF train station, TGV, Tramway, north access. 5mn on foot: Jardin des Plantes, Museum of Fine Arts, pedestrian historical centre, Castle of the Dukes of Brittany, Cathedral, Cité des Congrès, Unique place."
    ],
    "it": [
      "Hotel accogliente, di fronte alla stazione SNCF, TGV, Tram, accesso nord. A 5 minuti a piedi: Jardin des Plantes, Musée des Beaux-Arts, centro storico pedonale, Château des Ducs de Bretagne, Cattedrale, Cité des Congrès, Lieu unique."
    ],
    "fr": [
      "Accueillant hôtel, face gare SNCF, TGV, Tramway, accès nord. A 5mn à pied : Jardin des Plantes, Musée des Beaux-Arts, centre historique piétonnier, Château des Ducs de Bretagne, Cathédrale, Cité des Congrès, Lieu unique."
    ],
    "es": [
      "Hotel acogedor, frente a la SNCF, TGV, estación de tranvía, acceso norte. A 5 minutos a pie: Jardin des Plantes, Musée des Beaux-Arts, centro histórico peatonal, Château des Ducs de Bretagne, Catedral, Cité des Congrès, Lieu unique."
    ],
    "nl": [
      "Gastvrij hotel, tegenover SNCF, TGV, Tramstation, toegang noord. Op 5 minuten loopafstand: Jardin des Plantes, Musée des Beaux-Arts, autovrij historisch centrum, Château des Ducs de Bretagne, Kathedraal, Cité des Congrès, Lieu unique."
    ]
  },
