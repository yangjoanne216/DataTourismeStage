  "rdfs:comment": {
    "de": [
      "Authentischer Bauernhof im Tourstil aus dem 15. und 18. Jahrhundert.\nThemenzimmer mit eigenem Bad oder Dusche und WC. Wohnzimmer und Küche stehen den Gästen zur Verfügung."
    ],
    "en": [
      "Authentic 15th and 18th century Touraine farmhouse.\nTheme rooms with bathroom or shower room, private wc. Living room and kitchen at the disposal of the guests."
    ],
    "it": [
      "Autentica casa colonica della Touraine del XV e XVIII secolo.\nCamere a tema con bagno o doccia, servizi igienici privati. Soggiorno e cucina a disposizione degli ospiti."
    ],
    "fr": [
      "Authentique ferme tourangelle des XVe et XVIIIe siècles.\nChambres à thèmes avec salle de bain ou salle d'eau, wc privatifs. Salon et cuisine à disposition des hôtes."
    ],
    "nl": [
      "Authentieke Touraine boerderij uit de 15e en 18e eeuw.\nThemakamers met badkamer of doucheruimte, eigen toiletten. Woonkamer en keuken beschikbaar voor gasten."
    ],
    "es": [
      "Auténtica granja de Touraine de los siglos XV y XVIII.\nHabitaciones temáticas con baño o ducha, aseos privados. Salón y cocina a disposición de los huéspedes."
    ]
  },
