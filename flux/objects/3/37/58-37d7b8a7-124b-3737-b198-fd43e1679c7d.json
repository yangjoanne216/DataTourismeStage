  "rdfs:comment": {
    "de": [
      "Dieses 1873 errichtete Waschhaus wurde durch die Ableitung von Wasser aus dem von der Minengesellschaft gegrabenen Stollen gespeist."
    ],
    "en": [
      "This washhouse was built in 1873 and was fed by water from the gallery dug by the Société des Mines."
    ],
    "it": [
      "Questo lavatoio, costruito nel 1873, era alimentato dall'acqua della galleria scavata dalla Société des Mines."
    ],
    "fr": [
      "Ce lavoir construit en 1873 fût alimenté par  l'évacuation des eaux provenants de la galerie creusée par la Société des Mines."
    ],
    "es": [
      "Este lavadero, construido en 1873, se alimentaba del agua de la galería excavada por la Société des Mines."
    ],
    "nl": [
      "Dit washuis, gebouwd in 1873, werd gevoed door water uit de galerij gegraven door de Société des Mines."
    ]
  },
