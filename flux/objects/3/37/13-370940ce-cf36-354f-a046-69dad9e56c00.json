  "rdfs:comment": {
    "de": [
      "Therapeutische Hypnose nach Erickson.\r\nEssstörungen, stressbedingte Hautprobleme, Raucherentwöhnung, Kopfschmerzen, Phobien...\r\nSüchte, Besessenheit, psychosomatische Störungen, Traumata, Schlaflosigkeit, Trauer, Prüfungsvorbereitung..."
    ],
    "en": [
      "Ericksonian therapeutic hypnosis.\r\nEating disorders, stress-related skin problems, smoking cessation, headaches, phobias...\r\nAddictions, obsessions, psychosomatic disorders, trauma, insomnia, grief, exam preparation..."
    ],
    "it": [
      "Ipnosi terapeutica Ericksoniana.\r\nDisturbi alimentari, problemi cutanei legati allo stress, disassuefazione dal fumo, mal di testa, fobie...\r\nDipendenze, ossessioni, disturbi psicosomatici, traumi, insonnia, lutto, preparazione agli esami..."
    ],
    "fr": [
      "Hypnose thérapeutique Ericksonienne.\r\nTroubles alimentaires, problèmes de peau liés au stress, arrêt du tabac, maux de tête, phobies...\r\nAddictions, obsessions, troubles psychosomatiques, traumatismes, insomnies, deuils, préparation aux examens..."
    ],
    "es": [
      "Hipnosis terapéutica ericksoniana.\r\nTrastornos alimentarios, problemas cutáneos relacionados con el estrés, dejar de fumar, dolores de cabeza, fobias...\r\nAdicciones, obsesiones, trastornos psicosomáticos, traumas, insomnio, duelo, preparación de exámenes..."
    ],
    "nl": [
      "Ericksoniaanse therapeutische hypnose.\r\nEetstoornissen, stressgerelateerde huidproblemen, stoppen met roken, hoofdpijn, fobieën...\r\nVerslavingen, obsessies, psychosomatische stoornissen, trauma, slapeloosheid, verdriet, examenvoorbereiding..."
    ]
  },
