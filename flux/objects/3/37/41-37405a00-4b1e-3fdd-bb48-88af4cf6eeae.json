  "rdfs:comment": {
    "de": [
      "Spaziergänge und Ausritte zu Pferd. Vermietung von Ponys. Empfang von Wanderreitern. Animation von Ponys. Reitunterricht (Baby-Pony, Steinbruch, PTV, TREC). 5-Tages-Kurse mit Unterkunft."
    ],
    "en": [
      "Walks and horseback riding. Rental of ponies. Reception of horseback riders and hikers. Pony animation. Riding lessons (baby-pony, quarry, PTV, TREC). 5-day courses with accommodation."
    ],
    "it": [
      "Equitazione e trekking. Noleggio di pony. Accoglienza dei cavalieri. Pony d'animazione. Lezioni di equitazione (baby pony, carriera, PTV, TREC). Corsi di 5 giorni con alloggio."
    ],
    "fr": [
      "Promenades et randonnées à cheval. Location de poneys. Accueil de cavaliers randonneurs. Animation poneys. Leçons d'équitation (baby-poney, carrière, PTV, TREC). Stages 5 jours avec hébergement."
    ],
    "es": [
      "Paseos a caballo y senderismo. Alquiler de ponis. Recepción de jinetes. Ponis de animación. Clases de equitación (baby pony, carrera, PTV, TREC). Cursos de 5 días con alojamiento."
    ],
    "nl": [
      "Paardrijden en trekking. Pony verhuur. Ontvangst van ruiters te paard. Animatie pony's. Paardrijlessen (baby pony, carrière, PTV, TREC). 5-daagse cursussen met accommodatie."
    ]
  },
