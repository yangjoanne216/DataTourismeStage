  "rdfs:comment": {
    "de": [
      "Restaurant - Bar - Neu: Weinkeller (Beratung und Verkostung)\r\nGerichte und Tagesmenüs unter der Woche am Mittag, Abend- und Wochenendmenüs, Burger am Mittwochabend zum Genießen vor Ort oder zum Mitnehmen. Brunch auf Reservierung am Wochenende."
    ],
    "en": [
      "Restaurant - Bar - New : Wine cellar (advice and tasting)\nDishes and menus of the day during the week at lunchtime, evening and weekend menus, burgers on Wednesday evenings to eat in or take away. Brunch on reservation on weekends."
    ],
    "it": [
      "Ristorante - Bar - Novità: cantina (consulenza e degustazione)\r\nPiatti e menu del giorno durante la settimana a pranzo, menu serali e del fine settimana, hamburger il mercoledì sera da mangiare o da portare via. Brunch su prenotazione nel fine settimana."
    ],
    "fr": [
      "Restaurant - Bar - Nouveau : Cave à vin (conseil et dégustation)\r\nPlats et menus du jour en semaine le midi, menus du soir et week-end, burgers le mercredi soir à déguster sur place ou à emporter. Brunch sur réservation en week-end."
    ],
    "es": [
      "Restaurante - Bar - Novedad: Bodega (asesoramiento y degustación)\r\nPlatos y menús del día entre semana al mediodía, menús de noche y fin de semana, hamburguesas los miércoles por la noche para comer o llevar. Brunch con reserva los fines de semana."
    ],
    "nl": [
      "Restaurant - Bar - Nieuw: Wijnkelder (advies en proeverij)\r\nGerechten en menu's van de dag tijdens de week tijdens de lunch, avond- en weekendmenu's, hamburgers op woensdagavond om te eten of mee te nemen. Brunch op reservering in het weekend."
    ]
  },
  "rdfs:label": {
    "en": [
      "The Auberge du Cheval Blanc"
    ],
    "fr": [
      "L'Auberge du Cheval Blanc"
    ]
  },
