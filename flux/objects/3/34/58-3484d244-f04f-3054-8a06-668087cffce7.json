  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/84c37b9f-93fa-3695-822d-3233daf3614d",
      "dc:description": {
        "de": [
          "Balkon, Küchenzeile, Backofen, Mikrowelle, Geschirrspüler, Waschmaschine, Internetzugang, WLAN-Zugang, Fernseher, Wohnzimmer, Zentralheizung, Dusche, 1 Schlafzimmer, 1 Doppelbett, 1 Klappbank, Handtücher und Bettwäsche vorhanden. Garten. Privater Parkplatz. Kurtaxe und Mietkosten inbegriffen."
        ],
        "en": [
          "Balcony, kitchenette, oven, microwave, dishwasher, washing machine, internet access, wifi access, television, living room, central heating, shower, 1 bedroom, 1 double bed, 1 sofa bed, towels and household linen provided. Garden. Private parking. Tourist tax and rental charges included."
        ],
        "it": [
          "Balcone, angolo cottura, forno, microonde, lavastoviglie, lavatrice, accesso internet, accesso wifi, televisione, soggiorno, riscaldamento centralizzato, doccia, 1 camera da letto, 1 letto matrimoniale, 1 divano letto, asciugamani e biancheria per la casa forniti. Giardino. Parcheggio privato. Tassa di soggiorno e spese di affitto incluse."
        ],
        "fr": [
          "Balcon, coin cuisine, four, micro-ondes, lave-vaisselle, lave-linge, accès internet, accès wifi, télévision, séjour, chauffage central, douche, 1 chambre, 1 lit/2 pers., 1 banquette clic-clac, linge de toilette et de maison fournis. Jardin. Parking privé. Taxe de séjour et charges locatives comprises."
        ],
        "nl": [
          "Balkon, kitchenette, oven, magnetron, vaatwasser, wasmachine, internettoegang, wifi, televisie, woonkamer, centrale verwarming, douche, 1 slaapkamer, 1 tweepersoonsbed, 1 slaapbank, handdoeken en huishoudlinnen aanwezig. Tuin. Privé parkeerplaats. Toeristenbelasting en huurkosten inbegrepen."
        ],
        "es": [
          "Balcón, cocina americana, horno, microondas, lavavajillas, lavadora, acceso a internet, acceso wifi, televisión, salón, calefacción central, ducha, 1 dormitorio, 1 cama doble, 1 sofá cama, toallas y ropa de hogar incluidas. Jardín. Aparcamiento privado. Tasa turística y gastos de alquiler incluidos."
        ]
      },
