  "COVID19SpecialMeasures": {
    "de": [
      "Maske Obligatorisch im Laden.\nNicht mehr als 25 Personen gleichzeitig im Laden.\nHydroalkoholisches Gel steht Ihnen am Eingang des Geschäfts zur Verfügung."
    ],
    "en": [
      "Mask Mandatory in the store.\nNo more than 25 people at the same time in the store.\nHydroalcoholic gel at your disposal at the entrance of the store."
    ],
    "it": [
      "Maschera Obbligatoria in negozio.\nNon più di 25 persone alla volta nel negozio.\nGel idroalcolico disponibile all'ingresso del negozio."
    ],
    "fr": [
      "Masque Obligatoire dans le magasin.\nPas plus de 25 personnes en même temps dans le magasin.\nGel hydroalcoolique à votre disposition à l'entrée du magasin."
    ],
    "nl": [
      "Masker verplicht in de winkel.\nNiet meer dan 25 mensen tegelijk in de winkel.\nHydro-alcoholische gel verkrijgbaar bij de ingang van de winkel."
    ],
    "es": [
      "Máscara obligatoria en la tienda.\nNo más de 25 personas a la vez en la tienda.\nGel hidroalcohólico disponible en la entrada de la tienda."
    ]
  },
