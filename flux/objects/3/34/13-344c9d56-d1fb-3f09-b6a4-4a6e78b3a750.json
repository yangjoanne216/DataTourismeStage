  "rdfs:comment": {
    "de": [
      "Télé Bleue, l'Escape Game des reporters bietet 4 Escape Rooms in Garons, Ermittlungsspiele in Nîmes und Escape Games in den Arenen, dem Théâtre Antique d'Orange, der Festung von Beaucaire und dem Schloss von Baux-de-Provence."
    ],
    "en": [
      "Télé Bleue, l'Escape Game des reporters offers 4 Escape Rooms in Garons, investigation games in Nîmes and escapes in the Arènes, the Théâtre Antique d'Orange, the Fortress of Beaucaire and the Château des Baux-de-Provence."
    ],
    "it": [
      "Télé Bleue, l'Escape Game des reporters propone 4 Escape Room a Garons, giochi di investigazione a Nîmes e giochi di fuga nelle Arene, nel Teatro Antico di Orange, nella Fortezza di Beaucaire e nel Castello di Baux-de-Provence."
    ],
    "fr": [
      "Télé Bleue, l'Escape Game des reporters propose 4 Escape Room à Garons, des jeux d'enquête dans Nîmes et des escapes games dans les Arènes, le Théâtre Antique d'Orange, la Forteresse de Beaucaire et le Château des Baux-de-Provence."
    ],
    "es": [
      "Télé Bleue, l'Escape Game des reporters ofrece 4 Escape Rooms en Garons, juegos de investigación en Nîmes y juegos de escape en las Arenas, el Antiguo Teatro de Orange, la Fortaleza de Beaucaire y el Castillo de Baux-de-Provence."
    ],
    "nl": [
      "Télé Bleue, l'Escape Game des reporters biedt 4 Escape Rooms in Garons, opsporingsspelen in Nîmes en ontsnappingsspelen in de Arena's, het Antieke Theater van Orange, het Fort van Beaucaire en het Kasteel van Baux-de-Provence."
    ]
  },
