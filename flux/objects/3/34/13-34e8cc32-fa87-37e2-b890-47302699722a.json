  "rdfs:comment": {
    "de": [
      "Ausstellungs- und Kongresszentrum von GrandAngoulême.\r\nIm Espace Carat finden Kongresse, Seminare, öffentliche oder Fachmessen, Firmenveranstaltungen, Geschäftstourismus, Konzerte und Aufführungen statt."
    ],
    "en": [
      "Exhibition and congress centre of GrandAngoulême.\r\nThe Espace Carat hosts congresses, seminars, public or trade shows, corporate events, business tourism, concerts and shows."
    ],
    "it": [
      "Centro esposizioni e congressi Grand Angoulême.\r\nL'Espace Carat ospita congressi, seminari, fiere pubbliche o professionali, eventi aziendali, turismo d'affari, concerti e spettacoli."
    ],
    "fr": [
      "Parc des expositions et des congrès de GrandAngoulême.\r\nL'Espace Carat accueille congrès, séminaires, salons publics ou professionnels, événements d'entreprise, tourisme d'affaires, concerts et spectacles."
    ],
    "nl": [
      "Grand Angoulême Exhibition and Convention Centre.\r\nHet Espace Carat biedt onderdak aan congressen, seminars, openbare of professionele beurzen, bedrijfsevenementen, zakentoerisme, concerten en shows."
    ],
    "es": [
      "Centro de Exposiciones y Congresos de Grand Angoulême.\r\nEl Espace Carat acoge congresos, seminarios, ferias públicas o profesionales, eventos corporativos, turismo de negocios, conciertos y espectáculos."
    ]
  },
