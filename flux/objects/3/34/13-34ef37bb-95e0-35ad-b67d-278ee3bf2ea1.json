  "rdfs:comment": {
    "de": [
      "Das Awa Café am Hafen von Hyères empfängt Sie, um Sie in seine kalifornische Welt zu entführen. Eine ausgewogene, leckere, aber vor allem schmackhafte Küche."
    ],
    "en": [
      "The Awa Café, on the port of Hyères, welcomes you to discover its Californian universe. A well-balanced, gourmet but above all tasty cuisine."
    ],
    "it": [
      "L'Awa Café, sul porto di Hyères, vi accoglie per farvi scoprire il suo universo californiano. Una cucina equilibrata, gourmet ma soprattutto gustosa."
    ],
    "fr": [
      "L'Awa Café, sur le port d'Hyères, vous reçoit pour vous faire découvrir son univers californien. Une cuisine équilibrée, gourmande mais avant tout savoureuse."
    ],
    "nl": [
      "Het Awa Café, aan de haven van Hyères, verwelkomt u om zijn Californische universum te ontdekken. Een uitgebalanceerde, gastronomische maar vooral smakelijke keuken."
    ],
    "es": [
      "El Awa Café, en el puerto de Hyères, le invita a descubrir su universo californiano. Una cocina equilibrada, gourmet pero sobre todo sabrosa."
    ]
  },
