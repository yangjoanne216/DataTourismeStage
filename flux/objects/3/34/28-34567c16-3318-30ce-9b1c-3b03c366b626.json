  "rdfs:comment": {
    "de": [
      "Klassische Fahrräder können beim Bureau d'Information Touristique du Pays du Coquelicot mit Sitz im Flusshafen von Cappy gemietet werden!"
    ],
    "en": [
      "Classic bicycles for rent at the Tourist Information Office of the Poppy Country based at the river marina of Cappy !"
    ],
    "it": [
      "Noleggio di biciclette d'epoca presso l'Ufficio Informazioni Turistiche del Paese dei Papaveri con sede presso il porticciolo fluviale di Cappy!"
    ],
    "fr": [
      "Vélos classiques en location au Bureau d'Information Touristique du Pays du Coquelicot basé au port de plaisance fluvial de Cappy !"
    ],
    "nl": [
      "Klassieke fietsen te huur bij het VVV-kantoor van het Land van de Klaproos, gevestigd aan de rivierhaven in Cappy!"
    ],
    "es": [
      "Alquiler de bicicletas clásicas en la Oficina de Turismo del País de la Amapola, situada en el puerto deportivo fluvial de Cappy"
    ]
  },
