  "rdfs:comment": {
    "de": [
      "Elsässische Folkloreveranstaltung mit Tänzern und traditioneller Musik auf dem Dorfplatz. Vorführung durch die Jugendfeuerwehr. Kleiner Imbiss, Flammkuchen, Gebäck, elsässische Weine. 15 Uhr: Geführte Besichtigung des Weinlehrpfads."
    ],
    "en": [
      "Alsatian folklore animation with dancers and traditional music in the village square. Demonstration by the young firemen. Snacks, tarts flambées, pastries, Alsatian wines. 3pm: guided tour of the wine trail."
    ],
    "it": [
      "Intrattenimento folkloristico alsaziano con danzatori e musica tradizionale nella piazza del villaggio. Dimostrazione dei giovani pompieri. Snack, crostate, dolci, vini alsaziani. ore 15.00: visita guidata alla strada del vino."
    ],
    "fr": [
      "Animation folklorique alsacienne avec danseurs et musiques traditionnelles sur la place du village. Démonstration par les jeunes sapeurs pompiers. Petite restauration, tartes flambées, pâtisseries, vins d'Alsace. 15h : visite guidée du sentier viticole."
    ],
    "nl": [
      "Elzasser volksvermaak met dansers en traditionele muziek op het dorpsplein. Demonstratie van de jonge brandweermannen. Snacks, taarten, gebak, Elzasser wijnen. 15.00 uur: rondleiding over de wijnroute."
    ],
    "es": [
      "Animación folclórica alsaciana con bailarines y música tradicional en la plaza del pueblo. Demostración de los jóvenes bomberos. Aperitivos, tartas, pasteles, vinos alsacianos. 15:00: visita guiada por la ruta del vino."
    ]
  },
