  "rdfs:comment": {
    "de": [
      "In Châtillon-sur-Loire können Sie den Picknickplatz mit herrlichem Blick auf das Becken von Mantelot nutzen."
    ],
    "en": [
      "In Châtillon-sur-Loire, enjoy the picnic area with a breathtaking view of the Mantelot basin."
    ],
    "it": [
      "A Châtillon-sur-Loire, approfittate dell'area picnic con vista mozzafiato sul bacino del Mantelot."
    ],
    "fr": [
      "A Châtillon-sur-Loire, profitez de l'aire de pique-nique avec vue imprenable sur le bassin de Mantelot."
    ],
    "nl": [
      "Profiteer in Châtillon-sur-Loire van de picknickplaats met een adembenemend uitzicht op het bekken van Mantelot."
    ],
    "es": [
      "En Châtillon-sur-Loire, aproveche la zona de picnic con impresionantes vistas de la cuenca del Mantelot."
