  "rdfs:comment": {
    "de": [
      "Ein Landgut wiederzubeleben und gleichzeitig ihre Leidenschaft zu entfalten, war das Ziel von Maxime und Audrey Touzet."
    ],
    "en": [
      "Maxime and Audrey Touzet's objective was to revive a domain while developing their passion."
    ],
    "it": [
      "L'obiettivo di Maxime e Audrey Touzet era quello di far rivivere una tenuta sviluppando al contempo la loro passione per essa."
    ],
    "fr": [
      "Faire renaître un domaine tout en y développant leur passion, tel a été l'objectif de Maxime et Audrey Touzet."
    ],
    "nl": [
      "Het doel van Maxime en Audrey Touzet was om een landgoed nieuw leven in te blazen en tegelijkertijd hun passie ervoor te ontwikkelen."
    ],
    "es": [
      "El objetivo de Maxime y Audrey Touzet era revivir una finca al tiempo que desarrollaban su pasión por ella."
    ]
  },
