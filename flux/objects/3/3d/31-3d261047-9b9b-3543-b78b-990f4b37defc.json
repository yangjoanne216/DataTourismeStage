  "rdfs:comment": {
    "de": [
      "60 m² große Unterkunft in einem Zentrum für Reittourismus, 500 m vom Dorf Montlaur entfernt. Möglichkeit, mit Ihren Pferden, Ponys und Haustieren zu kommen. Reitsportliche Aktivitäten, Tiere vom Bauernhof."
    ],
    "en": [
      "Gite of 60 m² located in an equestrian tourism center at 500 m from the village of Montlaur. Possibility to come with your horses, ponies and pets. Equestrian activities, farm animals."
    ],
    "it": [
      "Gîte di 60 m² situato in un centro di turismo equestre a 500 m dal villaggio di Montlaur. Possibilità di venire con cavalli, pony e animali domestici. Attività equestri, animali da fattoria."
    ],
    "fr": [
      "Gîte de 60 m² situé dans un centre de tourisme équestre à 500 m du village de Montlaur. Possibilité de venir avec vos chevaux, poneys et animaux domestiques. Activités équestres, animaux de la ferme."
    ],
    "nl": [
      "Gîte van 60 m² gelegen in een centrum voor paardentoerisme, op 500 m van het dorp Montlaur. Mogelijkheid om met uw paarden, pony's en huisdieren te komen. Paardensportactiviteiten, boerderijdieren."
    ],
    "es": [
      "Casa rural de 60 m² situada en un centro de turismo ecuestre a 500 m del pueblo de Montlaur. Posibilidad de venir con sus caballos, ponis y mascotas. Actividades ecuestres, animales de granja."
    ]
  },
