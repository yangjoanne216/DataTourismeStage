  "rdfs:comment": {
    "de": [
      "Angenehmer Yachthafen in der Nähe des Stadtzentrums.\nStellplatz mit Flüssigkeiten: 10 € / Boot / Tag. Kurtaxe: 0,20 € / Tag / Person.\nGeöffnet von Mai bis September."
    ],
    "en": [
      "Pleasant marina located near the city centre.\nPitch with fluids: 10 € / boat / day. Tourist tax : 0,20 € / day / person.\nOpen from May to September."
    ],
    "it": [
      "Piacevole porto turistico situato vicino al centro città.\nPiazzola con liquidi: 10 ? / barca / giorno. Tassa di soggiorno: 0,20 ? / giorno / persona.\nAperto da maggio a settembre."
    ],
    "fr": [
      "Agréable port de plaisance situé à proximité du centre-ville.\nEmplacement avec fluides : 10 € / bateau / jour. Taxe de séjour : 0,20 € / jour / personne.\nOuvert de mai à septembre."
    ],
    "es": [
      "Agradable puerto deportivo situado cerca del centro de la ciudad.\nParcela con líquidos: 10€/barco/día. Tasa turística: 0,20 €/día/persona.\nAbierto de mayo a septiembre."
    ],
    "nl": [
      "Aangename jachthaven gelegen nabij het stadscentrum.\nStandplaats met vloeistoffen : 10 € / boot / dag. Toeristenbelasting: 0,20 € / dag / persoon.\nGeopend van mei tot september."
    ]
  },
  "rdfs:label": {
