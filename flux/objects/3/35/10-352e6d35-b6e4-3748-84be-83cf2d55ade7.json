  "rdfs:comment": {
    "de": [
      "MÉGAPIZZ' TRESSANGE\nRESTAURANT 40 SITZPLÄTZE + TERRASSE\nALLE UNSERE SPEZIALITÄTEN SIND HAUSGEMACHT.\nHOLEN SIE SICH IHRE PIZZEN UND IHRE TREUEKARTE!\nLIEFERUNG NACH HAUSE (KOSTENLOSE LIEFERUNG AB 2 PIZZEN)\nFÜR EINE PIZZA 2€\nLIEFERGEBIET; BOULANGE, AUMETZ, CRUSNES, HAVANGE, FONTOY, ANGEVILLERS, ROCHONVILLERS, TRESSANGE, BURE, NONDKEIL, LUDELANGE, OTTANGE"
    ],
    "en": [
      "MEGAPIZZ' TRESSANGE\nRESTAURANT 40 SEATS + TERRACE\nALL OUR SPECIALTIES ARE HOMEMADE.\nCOME AND GET YOUR PIZZAS AND YOUR LOYALTY CARD!\nHOME DELIVERY (FREE DELIVERY FROM 2 PIZZAS)\nFOR A 2€ PIZZA\nDELIVERY ZONE; BAKERY, AUMETZ, CRUSNES, HAVANGE, FONTOY, ANGEVILLERS, ROCHONVILLERS, TRESSANGE, BURE, NONDKEIL, LUDELANGE, OTTANGE"
    ],
    "it": [
      "MEGAPIZZ' TRESSANGE\nRISTORANTE 40 POSTI + TERRAZZA\nTUTTE LE NOSTRE SPECIALITÀ SONO FATTE IN CASA.\nVENITE A PRENDERE LE VOSTRE PIZZE E LA VOSTRA CARTA FEDELTÀ!\nCONSEGNA A DOMICILIO (CONSEGNA GRATUITA A PARTIRE DA 2 PIZZE)\nPER UNA PIZZA 2?\nZONA DI CONSEGNA ; BOULANGE, AUMETZ, CRUSNES, HAVANGE, FONTOY, ANGEVILLERS, ROCHONVILLERS, TRESSANGE, BURE, NONDKEIL, LUDELANGE, OTTANGE"
    ],
    "fr": [
      "MÉGAPIZZ' TRESSANGE\nRESTAURANT 40 PLACES ASSISES + TERRASSE\nTOUTES NOS SPÉCIALITÉS SONT FAITES MAISON.\nVENEZ CHERCHER VOS PIZZAS ET VOTRE CARTE DE FIDÉLITÉ!\nLIVRAISON À DOMICILE (LIVRAISON GRATUITE À PARTIR DE 2 PIZZAS)\nPOUR UNE PIZZA 2€\nZONE DE LIVRAISON ; BOULANGE,  AUMETZ, CRUSNES, HAVANGE, FONTOY, ANGEVILLERS, ROCHONVILLERS, TRESSANGE, BURE, NONDKEIL, LUDELANGE, OTTANGE"
    ],
    "es": [
      "MEGAPIZZ' TRESSANGE\nRESTAURANTE 40 PLAZAS + TERRAZA\nTODAS NUESTRAS ESPECIALIDADES SON CASERAS.\n¡VEN A POR TUS PIZZAS Y TU TARJETA DE FIDELIDAD!\nENTREGA A DOMICILIO (ENTREGA GRATUITA A PARTIR DE 2 PIZZAS)\nPARA UNA PIZZA 2?\nZONA DE ENTREGA ; BOULANGE, AUMETZ, CRUSNES, HAVANGE, FONTOY, ANGEVILLERS, ROCHONVILLERS, TRESSANGE, BURE, NONDKEIL, LUDELANGE, OTTANGE"
    ],
    "nl": [
      "MEGAPIZZ' TRESSANGE\nRESTAURANT 40 PLAATSEN + TERRAS\nAL ONZE SPECIALITEITEN ZIJN HUISGEMAAKT.\nKOM JE PIZZA'S EN JE KLANTENKAART HALEN!\nTHUISBEZORGING (GRATIS LEVERING VANAF 2 PIZZA'S)\nVOOR EEN PIZZA 2€\nLEVERINGSGEBIED ; BOULANGE, AUMETZ, CRUSNES, HAVANGE, FONTOY, ANGEVILLERS, ROCHONVILLERS, TRESSANGE, BURE, NONDKEIL, LUDELANGE, OTTANGE"
