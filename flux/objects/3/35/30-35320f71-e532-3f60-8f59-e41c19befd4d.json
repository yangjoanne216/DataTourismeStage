  "rdfs:comment": {
    "de": [
      "2,5 km vom Meer entfernt, in Diélette Abfahrt zu den Kanalinseln. Geräumiges und angenehmes Haus, geschlossener Hof, kleiner Garten mit Südlage."
    ],
    "en": [
      "At 2,5 km from the sea, in Diélette departure for the Channel Islands. Spacious and pleasant house, closed courtyard, small garden facing south."
    ],
    "it": [
      "A 2,5 km dal mare, a Diélette, partenza per le Isole del Canale. Casa spaziosa e piacevole, cortile chiuso, piccolo giardino esposto a sud."
    ],
    "fr": [
      "A 2,5 km de la mer, à Diélette départ pour les îles anglo-normandes. Maison spacieuse et agréable, cour fermée, petit jardin exposé plein sud."
    ],
    "es": [
      "A 2,5 km del mar, en Diélette, salida hacia las Islas del Canal. Casa amplia y agradable, patio cerrado, pequeño jardín orientado al sur."
    ],
    "nl": [
      "2.5 km van de zee, in Diélette, vertrekpunt voor de Kanaaleilanden. Ruim en aangenaam huis, afgesloten binnenplaats, kleine tuin op het zuiden."
    ]
  },
