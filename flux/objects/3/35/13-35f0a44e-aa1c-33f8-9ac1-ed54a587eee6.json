  "rdfs:comment": {
    "de": [
      "Sommer/Winter-Spielschwimmbad, Rutsche, Wasserpilz, Blasenbank, Pentagliss, Splashpad (Wasserspielplatz und interaktiver Spielplatz). Bereich für sportliche Aktivitäten. Geschlossen vom 18.12.22 bis 01.01.23. Entspannungsbereich bis auf weiteres geschlossen."
    ],
    "en": [
      "Indoor facilities consist of 25 metre heated pool, paddling-pool, playpool and toboggan, and outdoors there is large amusement pool, paddling-pool , multislide and splashpad . There is also an exercise room with 16 sets of cardio training apparatus and a relaxation area."
    ],
    "it": [
      "Piscina per il divertimento estivo/invernale, scivolo, fungo d'acqua, panchina per le bolle, pentagliss, splashpad (area interattiva per i giochi d'acqua). Area sportiva. Chiuso dal 18/12/22 al 01/01/23. Area relax chiusa fino a nuovo avviso."
    ],
    "fr": [
      "Piscine ludique été/hiver, tobbogan, champignons d'eau, banquette à bulle, pentagliss, splashpad (aire de jeux aquatiques et interactifs). Espace sportif. Fermée du 18/12/22 au 01/01/23. Espace détente fermé jusqu'à nouvel ordre."
    ],
    "nl": [
      "Zomer-/winterpretbad, glijbaan, waterpaddenstoel, bellenbank, pentagliss, splashpad (interactieve waterspeelplaats). Sportruimte. Gesloten van 18/12/22 tot 01/01/23. Ontspanningsruimte gesloten tot nader order."
    ],
    "es": [
      "Piscina de verano/invierno, tobogán, seta de agua, banco de burbujas, pentagliss, splashpad (zona de juegos acuáticos interactivos). Zona deportiva. Cerrado del 18/12/22 al 01/01/23. Zona de descanso cerrada hasta nuevo aviso."
    ]
  },
