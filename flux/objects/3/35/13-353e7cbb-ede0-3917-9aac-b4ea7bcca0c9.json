  "rdfs:comment": {
    "de": [
      "Ideal gelegen in Pontaillac, in der Nähe des Casinos, empfängt Sie das Le Calumet auf seiner Terrasse mit Blick auf das Meer und bietet Ihnen traditionelle und regionale Küche."
    ],
    "en": [
      "Ideally located in Pontaillac, near the Casino, the Calumet welcomes you on its terrace facing the sea and offers you a traditional and regional cuisine."
    ],
    "it": [
      "Situato in posizione ideale a Pontaillac, vicino al Casinò, il Calumet vi accoglie sulla sua terrazza di fronte al mare e offre una cucina tradizionale e regionale."
    ],
    "fr": [
      "Idéalement situé à Pontaillac, près du Casino, le Calumet vous accueille sur sa terrasse face à la mer et vous propose une cuisine traditionnelle et régionale."
    ],
    "es": [
      "Idealmente situado en Pontaillac, cerca del Casino, el Calumet le acoge en su terraza frente al mar y le ofrece una cocina tradicional y regional."
    ],
    "nl": [
      "Ideaal gelegen in Pontaillac, vlakbij het Casino, verwelkomt de Calumet u op zijn terras met uitzicht op zee en biedt traditionele en regionale gerechten."
    ]
  },
