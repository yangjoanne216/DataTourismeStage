  "rdfs:comment": {
    "de": [
      "Brasserie, Café, Restaurant an einem legendären Ort in Saint-Avold. Ausgewogene Küche mit frischen Produkten und einem guten Preis-Leistungs-Verhältnis. Speisesaal im 1. Stock, schattige Terrasse im Sommer. Kleine Snacks zu jeder Tageszeit."
    ],
    "en": [
      "Brasserie, café, restaurant in a mythical place in Saint-Avold. Balanced cuisine with fresh products and a good quality/price ratio. Restaurant room on the 1st floor, shaded terrace in summer. Small catering at any time."
    ],
    "it": [
      "Brasserie, caffè, ristorante in un luogo mitico di Saint-Avold. Cucina equilibrata con prodotti freschi e buon rapporto qualità-prezzo. Ristorante al 1° piano, terrazza ombreggiata in estate. Gli spuntini sono disponibili in qualsiasi momento."
    ],
    "fr": [
      "Brasserie, café, restaurant dans un lieu mythique à Saint-Avold. Cuisine équilibrée avec des produits frais et un bon rapport qualité/prix. Salle de restaurant au 1er étage, terrasse ombragée en été. Petite restauration à toute heure."
    ],
    "es": [
      "Brasserie, café, restaurante en un lugar mítico de Saint-Avold. Cocina equilibrada con productos frescos y buena relación calidad-precio. Restaurante en la 1ª planta, terraza sombreada en verano. Merienda disponible en cualquier momento."
    ],
    "nl": [
      "Brasserie, café, restaurant op een mythische plek in Saint-Avold. Uitgebalanceerde keuken met verse producten en een goede prijs-kwaliteitverhouding. Restaurant op de 1e verdieping, schaduwrijk terras in de zomer. Snacks zijn altijd beschikbaar."
    ]
  },
