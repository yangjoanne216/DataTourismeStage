  "rdfs:comment": {
    "de": [
      "Bauernhof mit Aufenthalt, Gästezimmer und -tisch.\nIn der Ortschaft Varaignes, 3. Haus unterhalb des Schlosses. 2 Gästezimmer mit eigenem Sanitärbereich. Möglichkeit eines Zusatzbetts gegen Aufpreis.\nDas ganze Jahr über geöffnet."
    ],
    "en": [
      "Ferme de séjour, chambre et table d'hôtes.\nIn the village of Varaignes, 3rd house below the château. 2 guest rooms with private bathroom. Extra bed available at extra cost.\nOpen all year round."
    ],
    "it": [
      "Agriturismo, bed and breakfast e table d'hôtes.\nNel villaggio di Varaignes, terza casa sotto il castello. 2 camere per gli ospiti con bagno privato. Letto supplementare disponibile a pagamento.\nAperto tutto l'anno."
    ],
    "fr": [
      "Ferme de séjour, chambre et table d'hôtes.\nDans le bourg de Varaignes, 3ème maison en dessous du château. 2 chambres d'hôtes avec sanitaires privés. Possibilité lit d'appoint avec supplément.\nOuverture toute l'année."
    ],
    "es": [
      "Casa rural, bed and breakfast y table d'hôtes.\nEn el pueblo de Varaignes, 3ª casa debajo del castillo. 2 habitaciones con baño privado. Cama supletoria disponible con suplemento.\nAbierto todo el año."
    ],
    "nl": [
      "Boerderij, bed & breakfast en table d'hôtes.\nIn het dorp Varaignes, 3e huis onder het kasteel. 2 gastenkamers met eigen badkamer. Extra bed beschikbaar tegen extra kosten.\nHet hele jaar geopend."
    ]
  },
