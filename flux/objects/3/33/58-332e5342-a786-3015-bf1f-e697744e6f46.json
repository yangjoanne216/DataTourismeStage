  "rdfs:comment": {
    "de": [
      "Das Haus Riederer (seit 1780) ist eine renommierte Konditorei, Schokoladenfabrik und Confiserie. Finden Sie seine Boutiquen in Gardanne, Aix-en-Provence und Marseille."
    ],
    "en": [
      "The Riederer house (since 1780) is a renowned pastry, chocolate and confectionery shop. Find its shops in Gardanne, Aix-en-Provence, and Marseille."
    ],
    "it": [
      "La casa Riederer (dal 1780) è una rinomata pasticceria, cioccolateria e confetteria. I suoi negozi si trovano a Gardanne, Aix-en-Provence e Marsiglia."
    ],
    "fr": [
      "La maison Riederer (depuis 1780) est une pâtisserie, chocolaterie et confiserie de renom. Retrouvez ses boutiques à Gardanne, Aix-en-Provence, et Marseille."
    ],
    "es": [
      "La casa Riederer (desde 1780) es una reconocida pastelería, chocolatería y confitería. Encuentra sus tiendas en Gardanne, Aix-en-Provence y Marsella."
    ],
    "nl": [
      "Het Riedererhuis (sinds 1780) is een gerenommeerde banketbakkerij, chocolaterie en banketbakkerij. Vind zijn winkels in Gardanne, Aix-en-Provence en Marseille."
    ]
  },
