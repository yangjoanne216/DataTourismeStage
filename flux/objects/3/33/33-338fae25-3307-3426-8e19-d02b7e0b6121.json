  "rdfs:comment": {
    "de": [
      "Eine Oase der Ruhe im Herzen des Arbas-Tals und der Zentralpyrenäen, die bis zu 5 Personen beherbergen kann."
    ],
    "en": [
      "A haven of peace in the heart of the Arbas valley and the central Pyrenees that can accommodate up to 5 people."
    ],
    "it": [
      "Un'oasi di pace nel cuore della valle dell'Arbas e dei Pirenei centrali che può ospitare fino a 5 persone."
    ],
    "fr": [
      "Un havre de paix au cœur de la vallée de l'Arbas et des Pyrénées centrales pouvant accueillir jusqu'à 5 personnes."
    ],
    "es": [
      "Un remanso de paz en el corazón del valle de Arbas y de los Pirineos centrales con capacidad para 5 personas."
    ],
    "nl": [
      "Een oase van rust in het hart van de Arbas-vallei en de centrale Pyreneeën die plaats biedt aan maximaal 5 personen."
    ]
  },
