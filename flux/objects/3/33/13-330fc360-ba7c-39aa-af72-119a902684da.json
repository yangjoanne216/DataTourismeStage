  "rdfs:comment": {
    "de": [
      "Ferme la Grande Béconne: Ab-Hof-Verkauf von küchenfertigem Geflügel und Kaninchen, ?uf, Erdbeeren, Spargel, Kartoffeln, ..\r\nÄtherisches Öl aus Lavandin\r\nVom Erzeuger zum Verbraucher."
    ],
    "en": [
      "Ferme la Grande Béconne : farm sales of poultry and rabbits ready to cook, eggs, strawberries, asparagus, potatoes, ..\r\nEssential oil of lavandin\r\nFrom producer to consumer."
    ],
    "it": [
      "Ferme la Grande Béconne : vendita di pollame e conigli pronti per la cottura, uova, fragole, asparagi, patate, ..\r\nOlio essenziale di lavanda\r\nDal produttore al consumatore."
    ],
    "fr": [
      "Ferme la Grande Béconne : vente à la ferme de volailles et lapins prêts à cuire, œuf, fraises, asperges, pommes de terre, ... \r\nHuile essentielle de lavandin\r\nDu producteur au consommateur."
    ],
    "es": [
      "Ferme la Grande Béconne : venta en granja de aves y conejos listos para cocinar, huevos, fresas, espárragos, patatas, ..\r\nAceite esencial de lavanda\r\nDel productor al consumidor."
    ],
    "nl": [
      "Ferme la Grande Béconne : boerderij verkoop van pluimvee en konijnen klaar om te koken, eieren, aardbeien, asperges, aardappelen, ..\r\nLavendel etherische olie\r\nVan producent tot consument."
    ]
  },
