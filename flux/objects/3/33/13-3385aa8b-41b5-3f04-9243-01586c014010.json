  "rdfs:comment": {
    "de": [
      "Privatisierung eines Bereichs mit Spa und Sauna - atemberaubende Aussicht auf den Laouzas-See, der tagsüber glitzert, manchmal mit seinen Nebeln geheimnisvoll wirkt und bei Sonnenuntergang wunderschön ist."
    ],
    "en": [
      "Privatization of a space with spa and sauna - breathtaking view of the lake of Laouzas glittering during the day, sometimes mysterious with its mists and magnificent at sunset."
    ],
    "it": [
      "Spazio privato con spa e sauna - vista mozzafiato sul lago Laouzas, scintillante di giorno, a volte misterioso con le sue nebbie e magnifico al tramonto."
    ],
    "fr": [
      "Privatisation d’un espace avec spa et sauna - vue imprenable sur le lac du Laouzas scintillant en journée, parfois mystérieux avec ses brumes et magnifique au coucher du soleil."
    ],
    "nl": [
      "Privéruimte met spa en sauna - prachtig uitzicht op het meer van Laouzas, overdag sprankelend, soms mysterieus met zijn nevels en schitterend bij zonsondergang."
    ],
    "es": [
      "Espacio privado con spa y sauna. Impresionante vista del lago Laouzas, centelleante durante el día, a veces misterioso con sus brumas y magnífico al atardecer."
    ]
  },
  "rdfs:label": {
    "fr": [
