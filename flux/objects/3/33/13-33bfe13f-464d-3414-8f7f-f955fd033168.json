  "rdfs:comment": {
    "de": [
      "Das Musée des Arts et Traditions Populaires befindet sich in Saint-Arnoult-en-Yvelines. Dieses Museum umfasst das spirituelle, künstlerische und materielle Erbe dieser kleinen Gemeinde im Süden des Departements Yvelines."
    ],
    "en": [
      "The Museum of Popular Arts and Traditions is located in Saint-Arnoult-en-Yvelines. This museum brings together the spiritual, artistic and material heritage of this small town in the south of the Yvelines."
    ],
    "it": [
      "Il Museo delle Arti e Tradizioni Popolari si trova a Saint-Arnoult-en-Yvelines. Questo museo riunisce il patrimonio spirituale, artistico e materiale di questa piccola città nel sud delle Yvelines."
    ],
    "fr": [
      "Le Musée des Arts et Traditions Populaires est situé à Saint-Arnoult-en-Yvelines. Ce musée regroupe le patrimoine spirituel, artistique et matériel de cette petite commune du sud des Yvelines."
    ],
    "nl": [
      "Het Museum voor Volkskunst en -tradities bevindt zich in Saint-Arnoult-en-Yvelines. Dit museum brengt het spirituele, artistieke en materiële erfgoed van deze kleine stad in het zuiden van de Yvelines samen."
    ],
    "es": [
      "El Museo de Artes y Tradiciones Populares se encuentra en Saint-Arnoult-en-Yvelines. Este museo reúne el patrimonio espiritual, artístico y material de esta pequeña ciudad del sur de los Yvelines."
    ]
  },
