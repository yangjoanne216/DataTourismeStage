  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/afbaafcd-a69e-3252-9522-461fdf41a9d6",
      "dc:description": {
        "de": [
          "Hotel-Restaurant, im Stadtzentrum von Autun, 10 Gehminuten von der Kathedrale und dem römischen Theater entfernt. Parkplatz in der Nähe. Angenehmes, gemütliches Zimmer mit Aufzug. Frühstücksbuffet."
        ],
        "en": [
          "Hotel-restaurant, in the centre of Autun, 10 minutes walk from the cathedral and the Roman theatre. Parking nearby. Cosy cosy room with lift. Buffet breakfast."
        ],
        "it": [
          "Hotel-ristorante nel centro di Autun, a 10 minuti a piedi dalla cattedrale e dal teatro romano. Parcheggio nelle vicinanze. Camera accogliente con ascensore. Colazione a buffet."
        ],
        "fr": [
          "Hôtel-restaurant, au centre ville d'Autun, à 10 minutes à pied de la cathédrale et du théâtre romain. Parking à proximité. Chambre douillette agréable avec ascenseur. Petit-déjeuner buffet."
        ],
        "es": [
          "Hotel-restaurante, en el centro de Autun, a 10 minutos a pie de la catedral y del teatro romano. Aparcamiento en las inmediaciones. Acogedora habitación con ascensor. Desayuno buffet."
        ],
        "nl": [
          "Hotel-restaurant, in het centrum van Autun, op 10 minuten lopen van de kathedraal en het Romeinse theater. Parkeren in de buurt. Gezellige kamer met lift. Ontbijtbuffet."
        ]
      },
