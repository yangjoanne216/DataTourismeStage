  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/e6f73cf3-550e-3b54-8d56-fe58c6f6af35",
      "dc:description": {
        "de": [
          "Herstellung von Bauernkäse, reinen Ziegencabécous und Ziegen-/Kuhkäse. Regionale Produkte, Ziegen auf dem Laufhof. Verkauf ab Hof und in lokalen Geschäften."
        ],
        "en": [
          "Fabrication of farm produced cheese, pure goat's milk cabécous and half goat/half cow's milk cheese. Traditional local products, goats grazing. Sales on the farm and in local shops."
        ],
        "it": [
          "Produzione di formaggi di fattoria, di puro formaggio di capra e di formaggio metà capra/metà mucca. Prodotti locali, capra sul sentiero. Vendite in fattoria e nei negozi locali."
        ],
        "fr": [
          "Fabrication de fromage fermier, cabécous purs chèvre et fromage mi-chèvre/ mi-vache. Produits du terroir, chèvre en parcours. Vente à la ferme et commerces locaux."
        ],
        "nl": [
          "Productie van boerenkaas, zuivere geitenkaas en half geit/half koe kaas. Lokale producten, geit op het spoor. Verkoop op de boerderij en plaatselijke winkels."
        ],
        "es": [
          "Producción de queso de granja, queso puro de cabra y queso mitad cabra/mitad vaca. Productos locales, cabra en el camino. Venta en la granja y en tiendas locales."
        ]
      },
