  "rdfs:comment": {
    "de": [
      "Familien- und Spielbereich mit Picknicktischen, Grills, Großspielgeräten, Beachvolleyballplatz, Klettergerüsten mit Schaukeln, Kletterwand, Tischtennisplatten und Fußballplatz."
    ],
    "en": [
      "Play area equipped with picnic  tables, beach volley area, swing set, climbing wall, ping pong tables, football pitch."
    ],
    "it": [
      "Area familiare e ricreativa attrezzata con tavoli da picnic, barbecue, grandi strutture da gioco, area per il beach volley, altalene, parete da arrampicata, tavoli da ping pong e campo da calcio."
    ],
    "fr": [
      "Espace familial et ludique équipé de tables de pique-nique, barbecues, structures de grands jeux, aire de beach-volley, portiques avec balançoires, mur d'escalade, tables de ping-pong et terrain de foot."
    ],
    "nl": [
      "Familie- en recreatieterrein uitgerust met picknicktafels, barbecues, grote speelstructuren, beachvolleybalterrein, schommels, klimmuur, tafeltennistafels en voetbalveld."
    ],
    "es": [
      "Espacio familiar y lúdico equipado con mesas de picnic, barbacoas, estructuras de juegos de gran tamaño, área de voley playa, columpios, muro de escalada, mesas de ping pong y campo de fútbol."
    ]
  },
