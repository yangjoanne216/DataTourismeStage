  "rdfs:comment": {
    "de": [
      "Vocable Saint-Pierre Saint-Paul; zerstörte Kirche aus dem 14. Jahrhundert, von der nur noch zwei Okuli und der Eucharistische Schrank in der linken Sakristei erhalten sind; 1897-1898 (Datum trägt 1898) von Louis Lanternier Architekt in Gorze wiederaufgebaut"
    ],
    "en": [
      "Vocable Saint-Pierre Saint-Paul; 14th century church destroyed, of which only two oculi and the eucharistic cupboard in the left sacristy remain; rebuilt from 1897 to 1898 (date given 1898) by Louis Lanternier architect in Gorze"
    ],
    "it": [
      "Vocable Saint-Pierre Saint-Paul; chiesa del XIV secolo distrutta, di cui restano solo due oculi e l'armadio eucaristico nella sacrestia di sinistra; ricostruita dal 1897 al 1898 (data indicata 1898) dall'architetto Louis Lanternier a Gorze"
    ],
    "fr": [
      "Vocable Saint-Pierre Saint-Paul ; église 14e siècle détruite, dont il ne subsiste que deux oculi et l'armoire eucharistique dans la sacristie gauche ; reconstruite de 1897 à 1898 (date portée 1898) par Louis Lanternier architecte à Gorze"
    ],
    "nl": [
      "Vocable Saint-Pierre Saint-Paul; kerk uit de 14e eeuw verwoest, waarvan alleen twee oculi en de eucharistische kast in de linker sacristie zijn overgebleven; herbouwd van 1897 tot 1898 (datum opgegeven 1898) door architect Louis Lanternier in Gorze"
    ],
    "es": [
      "Vocable Saint-Pierre Saint-Paul; iglesia del siglo XIV destruida, de la que sólo quedan dos óculos y el armario eucarístico de la sacristía izquierda; reconstruida de 1897 a 1898 (fecha dada 1898) por Louis Lanternier arquitecto en Gorze"
    ]
  },
  "rdfs:label": {
    "fr": [
      "EGLISE SAINT PIERRE SAINT PAUL"
    ]
  },
