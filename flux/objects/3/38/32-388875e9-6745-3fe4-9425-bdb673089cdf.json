  "rdfs:comment": {
    "de": [
      "Château Fonsalade ist ein Weingut, das seit fast 130 Jahren im Herzen des Languedoc angesiedelt ist und 32 Hektar Weinberge in den Appellationen Saint Chinian und Roquebrun bewirtschaftet."
    ],
    "en": [
      "Château Fonsalade is a wine estate located in the heart of the Languedoc region for almost 130 years with its 32 hectares of vines in the Saint Chinian and Roquebrun appellations."
    ],
    "it": [
      "Château Fonsalade è un'azienda vinicola situata nel cuore della Linguadoca da quasi 130 anni con 32 ettari di vigneti nelle denominazioni Saint Chinian e Roquebrun."
    ],
    "fr": [
      "Château Fonsalade est un domaine viticole installé au cœur du Languedoc depuis bientôt 130 ans avec ses 32 hectares de vignes en appellation Saint Chinian et Roquebrun."
    ],
    "nl": [
      "Château Fonsalade is al bijna 130 jaar een wijndomein in het hart van de Languedoc met 32 hectare wijngaarden in de appellations Saint Chinian en Roquebrun."
    ],
    "es": [
      "Château Fonsalade es una finca vinícola situada en el corazón del Languedoc desde hace casi 130 años con 32 hectáreas de viñedos en las denominaciones de Saint Chinian y Roquebrun."
    ]
  },
