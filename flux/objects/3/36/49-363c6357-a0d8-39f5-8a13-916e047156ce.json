  "rdfs:comment": {
    "de": [
      "Anjou Sport Nature, Spezialist für Naturaktivitäten im blauen Anjou, bietet Ihnen während des Sommers den Verleih von Kajaks und Tretbooten auf dem Fluss Oudon ab Segré an."
    ],
    "en": [
      "Anjou Sport Nature, specialist in outdoor activities in the Anjou Bleu region, offers kayak and pedal boat rentals on the Oudon River from Segré during the summer."
    ],
    "it": [
      "Anjou Sport Nature, specialista delle attività all'aria aperta nella regione dell'Anjou Bleu, offre il noleggio di kayak e pedalò sul fiume Oudon da Segré durante l'estate."
    ],
    "fr": [
      "Anjou Sport Nature, spécialiste des activités de pleine nature en Anjou bleu, vous propose durant l'été, des locations de kayaks et pédalos sur l'Oudon au départ de Segré."
    ],
    "nl": [
      "Anjou Sport Nature, specialist in buitenactiviteiten in de Anjou Bleu, verhuurt in de zomer kajaks en waterfietsen op de rivier de Oudon vanuit Segré."
    ],
    "es": [
      "Anjou Sport Nature, especialista en actividades al aire libre en la región de Anjou Bleu, ofrece alquiler de kayaks y barcas a pedales en el río Oudon desde Segré durante el verano."
    ]
  },
  "rdfs:label": {
    "fr": [
