  "rdfs:comment": {
    "de": [
      "Der GAEC \"Le Bouquet Savoyard\", in der Nähe des Sees von Annecy, bietet zweimal wöchentlich Produkte aus dem LKW-Anbau und eine große Auswahl an Schnittblumen an."
    ],
    "en": [
      "The GAEC \"Le Bouquet Savoyard\", close to lake Annecy, proposes twice a week, products stemming from the truck farming and a wide range of cut flowers."
    ],
    "it": [
      "Produttore di ortaggi (agricoltura biologica) e fiori recisi. Vendita diretta in azienda. Si possono trovare anche carni, formaggi, salumi... di produttori locali."
    ],
    "fr": [
      "Producteur de légumes (Agriculture Bio) et fleurs coupées. Vente directe sur l'exploitation. Retrouvez également des viandes, fromages, salaisons... de producteurs locaux."
    ],
    "es": [
      "Productor de hortalizas (agricultura ecológica) y flores cortadas. Venta directa en la granja. También puede encontrar carnes, quesos, embutidos... de productores locales."
    ],
    "nl": [
      "Producent van groenten (biologische landbouw) en snijbloemen. Directe verkoop op de boerderij. U vindt er ook vlees, kaas, vleeswaren... van lokale producenten."
    ]
  },
