  "rdfs:comment": {
    "de": [
      "AKTIVITÄTEN\r\n- Jet-Wanderung (1h,1h30,4h mit Frühstück inbegriffen),\r\n- einführung in den Jetski (20 oder 30 Min.),\r\n- vermietung von Jetskis, mit oder ohne Führerschein,\r\n- gezogene Bojen, Banane, Flyfish,\r\n- bootsverleih, Wasserski,\r\n- erwachsene, Kinder ab 4 Jahren"
    ],
    "en": [
      "ACTIVITIES\r\n- Rando jet (1h,1h30,4h with breakfast included),\r\n- jet ski initiation (20 or 30 mn),\r\n- jet-ski rental, with or without permit,\r\n- towed buoys, banana, flyfish,\r\n- boat rental, water skiing,\r\n- adults, children from 4 years old"
    ],
    "it": [
      "ATTIVITÀ \r\n- Jet ski (1h, 1h30, 4h con colazione inclusa),\r\n- iniziazione alla moto d'acqua (20 o 30 mn),\r\n- noleggio di moto d'acqua, con o senza licenza,\r\n- boe trainate, banana, pesce mosca,\r\n- noleggio barche, sci nautico,\r\n- adulti, bambini a partire da 4 anni"
    ],
    "fr": [
      "ACTIVITES : \r\n- Rando jet (1h,1h30,4h avec petit déjeuner inclus),  \r\n- initiation jet ski (20 ou 30 mn),\r\n- location de jet-ski, avec ou sans permis, \r\n- bouées tractées, banane, flyfish, \r\n- location bateau, ski nautique,\r\n- adultes, enfants à partir de 4 ans"
    ],
    "nl": [
      "ACTIVITEITEN \r\n- Jetskiën (1u, 1u30, 4u met ontbijt inbegrepen),\r\n- jetski initiatie (20 of 30 mn),\r\n- jet-ski verhuur, met of zonder vergunning,\r\n- gesleepte boeien, banaan, vliegvis,\r\n- bootverhuur, waterskiën,\r\n- volwassenen, kinderen vanaf 4 jaar"
    ],
    "es": [
      "ACTIVIDADES \r\n- Motos de agua (1h, 1h30, 4h con desayuno incluido),\r\n- iniciación a la moto de agua (20 o 30 mn),\r\n- alquiler de motos acuáticas, con o sin licencia,\r\n- boyas remolcadas, plátano, pez mosca,\r\n- alquiler de barcos, esquí acuático,\r\n- adultos, niños a partir de 4 años"
    ]
  },
