  "rdfs:comment": {
    "de": [
      "Bahnhof auf halbem Weg zwischen Bar-le-Duc (30 km) und Verdun (25 km).\nDirekte Verbindung nach Paris, Lille, Nancy, Metz, Straßburg, Bordeaux, Brüssel, Disneyland Paris, Roissy Charles de Gaulle.\nPendelbusse nach und von Commercy, Bar-le-Duc und Verdun bedienen sie."
    ],
    "en": [
      "Station located halfway between Bar-le-Duc (30 km) and Verdun (25 km).\nDirect links with Paris, Lille, Nancy, Metz, Strasbourg, Bordeaux, Brussels, Disneyland Paris, Roissy Charles de Gaulle.\nShuttle buses to and from Commercy, Bar-le-Duc and Verdun."
    ],
    "it": [
      "Stazione situata a metà strada tra Bar-le-Duc (30 km) e Verdun (25 km).\nCollegamenti diretti con Parigi, Lille, Nancy, Metz, Strasburgo, Bordeaux, Bruxelles, Disneyland Paris, Roissy Charles de Gaulle.\nBus navetta da e per Commercy, Bar-le-Duc e Verdun."
    ],
    "fr": [
      "Gare située à mi-chemin entre Bar-le-Duc (30 km) et Verdun (25 km).\nLiaison directe avec Paris, Lille, Nancy, Metz, Strasbourg, Bordeaux, Bruxelles, Disneyland Paris, Roissy Charles de Gaulle.\nDes navettes de bus vers et depuis Commercy, Bar-le-Duc et Verdun la desservent."
    ],
    "nl": [
      "Station gelegen halverwege tussen Bar-le-Duc (30 km) en Verdun (25 km).\nDirecte verbindingen met Parijs, Lille, Nancy, Metz, Straatsburg, Bordeaux, Brussel, Disneyland Parijs, Roissy Charles de Gaulle.\nPendelbussen van en naar Commercy, Bar-le-Duc en Verdun."
    ],
    "es": [
      "Estación situada a medio camino entre Bar-le-Duc (30 km) y Verdún (25 km).\nEnlaces directos a París, Lille, Nancy, Metz, Estrasburgo, Burdeos, Bruselas, Disneyland París, Roissy Charles de Gaulle.\nAutobuses de ida y vuelta a Commercy, Bar-le-Duc y Verdun."
    ]
  },
