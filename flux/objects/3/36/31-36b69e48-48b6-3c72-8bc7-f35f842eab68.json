  "rdfs:comment": {
    "de": [
      "Gehen Sie durch die Straßen dieses Dorfes, das im 12. Jahrhundert auf dem Plateau von Ségala angesiedelt wurde!"
    ],
    "en": [
      "Walk the streets of this village established in the Ségala plateau during the12th century !"
    ],
    "it": [
      "Passeggiate per le strade di questo villaggio, sorto sull'altopiano di Ségala nel XII secolo!"
    ],
    "fr": [
      "Parcourez les rues de ce village implanté sur le plateau du Ségala au XIIe siècle !"
    ],
    "nl": [
      "Wandel door de straten van dit dorp, dat in de 12e eeuw op het plateau van Ségala werd gesticht!"
    ],
    "es": [
      "Pasee por las calles de este pueblo, establecido en la meseta de Ségala en el siglo XII"
    ]
  },
