  "rdfs:comment": {
    "de": [
      "Das Dorf Grez-Neuville liegt 15 Minuten von Angers und 5 Minuten von Lion-d'Angers entfernt. Es ist ein Dorf mit Charme, durch das der Fluss Mayenne fließt."
    ],
    "en": [
      "Located 15 minutes from Angers and 5 minutes from Le Lion-d'Angers, the village of Grez-Neuville is a charming village crossed by the river Mayenne."
    ],
    "it": [
      "Situato a 15 minuti da Angers e a 5 minuti da Le Lion-d'Angers, il villaggio di Grez-Neuville è un incantevole borgo sulle rive del fiume Mayenne."
    ],
    "fr": [
      "Situé à 15 minutes d'Angers et 5 minutes du Lion-d'Angers, le village de Grez-Neuville est un Village de Charme traversé par la rivière Mayenne."
    ],
    "es": [
      "Situado a 15 minutos de Angers y a 5 minutos de Le Lion-d'Angers, el pueblo de Grez-Neuville es un encantador pueblo a orillas del río Mayenne."
    ],
    "nl": [
      "Op 15 minuten van Angers en 5 minuten van Le Lion-d'Angers ligt Grez-Neuville, een charmant dorpje aan de oevers van de rivier de Mayenne."
    ]
  },
