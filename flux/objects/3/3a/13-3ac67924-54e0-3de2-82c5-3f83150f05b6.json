  "rdfs:comment": {
    "de": [
      "\"Les Jeunes amours du roi Louis XIV\" - Le Poème Harmonique ;\r\nOpern, höfische Arien und Lieder im Königspalast, mit Eva Zaïcik, Mezzosopran, ein lebhafter und berauschender musikalischer Parcours."
    ],
    "en": [
      "\"Les Jeunes amours du roi Louis XIV\" - Le Poème Harmonique ;\r\nOperas, court arias and songs at the Royal Palace, with Eva Zaïcik, mezzo-soprano, a lively and intoxicating musical journey."
    ],
    "it": [
      "\"Les Jeunes amours du roi Louis XIV\" - Le Poème Harmonique ;\r\nOpere, arie di corte e canzoni al Palais Royal, con Eva Zaïcik, mezzosoprano, un viaggio musicale vivace e inebriante."
    ],
    "fr": [
      "«Les Jeunes amours du roi Louis XIV» - Le Poème Harmonique ;\r\nOpéras, airs de cour et chansons au Palais royal, avec Eva Zaïcik, mezzo-soprano, un parcours musical vif et enivrant."
    ],
    "es": [
      "\"Les Jeunes amours du roi Louis XIV\" - Le Poème Harmonique ;\r\nÓperas, arias de corte y canciones del Palacio Real, con Eva Zaïcik, mezzosoprano, un viaje musical animado y embriagador."
    ],
    "nl": [
      "\"Les Jeunes amours du roi Louis XIV\" - Le Poème Harmonique ;\r\nOpera's, hofaria's en liederen in het Palais Royal, met Eva Zaïcik, mezzosopraan, een levendige en bedwelmende muzikale reis."
    ]
  },
