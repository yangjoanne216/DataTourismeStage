  "rdfs:comment": {
    "de": [
      "Vérone, Praktikerin in Ayurveda-Massagen, Thai-Fußreflexzonenmassage, Amma-Sitzmassage... Bachblüten- und Aromatherapie-Beraterin, ich kann Ihnen eine Wohlfühlpause verschaffen."
    ],
    "en": [
      "Verona, practitioner of Ayurvedic massage, Thai foot reflexology, Amma assis... Bach flower and aromatherapy advisor, I will bring you a well-being break."
    ],
    "it": [
      "Vérone, praticante di massaggio ayurvedico, di riflessologia plantare thailandese, di Amma assis... consulente di fiori di Bach e di aromaterapia, vi porterà una pausa di benessere."
    ],
    "fr": [
      "Vérone, praticienne en massages Ayurvédique, réflexologie plantaire Thaïe, Amma assis... conseillère en fleurs de Bach et aromathérapie,  je saurais vous apporter une pause bien-être."
    ],
    "es": [
      "Vérone, practicante de masaje ayurvédico, reflexología podal tailandesa, Amma assis... asesora de flores de Bach y aromaterapia, le traeré una pausa de bienestar."
    ],
    "nl": [
      "Vérone, beoefenaar van Ayurvedische massage, Thaise voetreflexologie, Amma assis... Bach bloem en aromatherapie adviseur, ik breng u een welzijnspauze."
    ]
  },
