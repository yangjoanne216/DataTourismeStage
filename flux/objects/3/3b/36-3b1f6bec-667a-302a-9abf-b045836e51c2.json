  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/d97ddae7-db3b-31f5-9aa3-c152e5d1ec48",
      "dc:description": {
        "de": [
          "Im Kupferkessel gekochte, handwerklich hergestellte Konfitüren",
          "Konfitüren aus im Louron-Tal geernteten Früchten: wilde Heidelbeeren, Brombeeren, Äpfel, Holunder, Himbeeren aus dem Garten, Löwenzahn, Quitten..."
        ],
        "en": [
          "Homemade jams cooked in copper cauldron",
          "Jams made from fruit harvested in the Louron Valley: wild blueberries, blackberries, apples, elderberries, garden raspberries, dandelions, quinces..."
        ],
        "it": [
          "Confetture prodotte con la frutta raccolta nella Valle di Louron: mirtilli selvatici, more, mele, bacche di sambuco, lamponi da giardino, dente di leone, mele cotogne...",
          "Marmellate artigianali cotte in un calderone di rame"
        ],
        "fr": [
          "Confitures artisanales cuites au chaudron de cuivre",
          "Confitures de fruits récoltés dans la Vallée du Louron : myrtilles sauvages, mûres, pommes, sureau, framboises du jardin, pissenlits, coings..."
        ],
        "es": [
          "Mermeladas artesanales cocinadas en un caldero de cobre",
          "Mermeladas elaboradas a partir de frutas cosechadas en el Valle de Louron: arándanos silvestres, moras, manzanas, bayas de saúco, frambuesas de jardín, dientes de león, membrillos..."
        ],
        "nl": [
          "Artisanale jam gekookt in een koperen ketel",
          "Jams gemaakt van fruit geoogst in de Louron Valley: wilde bosbessen, bramen, appels, vlierbessen, tuinframbozen, paardenbloemen, kweeperen..."
        ]
      },
