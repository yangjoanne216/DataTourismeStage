  "rdfs:comment": {
    "de": [
      "Michel Lorblanchet, Forschungsdirektor am CNRS und Spezialist für prähistorische Kunst, stellt seine Methoden zur Untersuchung von verzierten Höhlen vor.\nFür jedes Publikum zugänglich. Nach vorheriger Anmeldung."
    ],
    "en": [
      "Michel Lorblanchet, Director of Research at the CNRS and specialist in prehistoric art, will present his methods for studying decorated caves.\nAll public. On reservation."
    ],
    "it": [
      "Michel Lorblanchet, direttore di ricerca del CNRS e specialista di arte preistorica, presenterà i suoi metodi di studio delle grotte decorate.\nAperto al pubblico. Su prenotazione."
    ],
    "fr": [
      "Michel Lorblanchet, Directeur de recherches au CNRS et spécialiste de l’art préhistorique, vient présenter ses méthodes d'étude des grottes ornées.\nTout public. Sur réservation."
    ],
    "es": [
      "Michel Lorblanchet, director de investigación del CNRS y especialista en arte prehistórico, presentará sus métodos de estudio de las cuevas decoradas.\nAbierto al público. En la reserva."
    ],
    "nl": [
      "Michel Lorblanchet, directeur onderzoek van het CNRS en specialist in prehistorische kunst, zal zijn methoden voor het bestuderen van versierde grotten presenteren.\nOpen voor het publiek. Op reservering."
    ]
  },
  "rdfs:label": {
    "fr": [
