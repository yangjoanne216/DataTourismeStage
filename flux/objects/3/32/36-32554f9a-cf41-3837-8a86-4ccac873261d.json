  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/40d51693-7d37-30cb-93c1-c0e0ef322eed",
      "dc:description": {
        "de": [
          "Wohnung für 6 Personen, Blick auf die Pisten",
          "Wohnzimmer 1 clic-clac, Schlafzimmer 4 Schlafplätze, Kochnische, Garage im Untergeschoss für 2 Autos\nEinkaufsgalerie, Ticketverkauf und Skischule im Gebäude."
        ],
        "en": [
          "Apartment for 6 persons, view on the slopes",
          "Living room 1 sofa bed, bedroom 4 beds, kitchenette, garage in basement for 2 cars\nShopping mall, ticket office and ski school in the building."
        ],
        "it": [
          "Appartamento per 6 persone, vista sulle piste",
          "Soggiorno 1 divano letto, camera da letto 4 posti, angolo cottura, garage nel seminterrato per 2 auto\nCentro commerciale, biglietteria e scuola di sci nell'edificio."
        ],
        "fr": [
          "Appartement pour 6 personne, vue sur pistes",
          "Séjour 1 clic-clac, chambre 4 couchages, kitchenette, garage en sous-sol pour 2 voitures. \nGalerie commerciale, billetterie et école de ski dans l'immeuble."
        ],
        "es": [
          "Piso para 6 personas, vista a las pistas",
          "Salón 1 sofá cama, dormitorio 4 camas, cocina americana, garaje en el sótano para 2 coches\nCentro comercial, taquilla y escuela de esquí en el edificio."
        ],
        "nl": [
          "Appartement voor 6 personen, uitzicht op de piste",
          "Woonkamer 1 slaapbank, slaapkamer 4 bedden, kitchenette, garage in de kelder voor 2 auto's\nWinkelcentrum, loket en skischool in het gebouw."
        ]
      },
