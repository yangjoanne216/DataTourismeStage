  "rdfs:comment": {
    "de": [
      "100 m vom feinen Sandstrand Grande Plage von Royan entfernt, in der Ruhe des bewaldeten Vallières-Viertels, 2,5 km von der Stadt Saint-Georges-de-Didonne und 3 km vom Stadtzentrum von Royan entfernt."
    ],
    "en": [
      "At 100 m from the Great Beach of fine sand of Royan, in the calm of the wooded district of Vallières, at 2,5 km from the town of Saint-Georges-de-Didonne and 3 km from the center of Royan."
    ],
    "it": [
      "A 100 m dalla grande spiaggia di sabbia di Royan, nella tranquilla zona boschiva di Vallières, a 2,5 km dalla città di Saint-Georges-de-Didonne e a 3 km dal centro di Royan."
    ],
    "fr": [
      "À 100 m de la Grande Plage de sable fin de Royan, dans le calme du quartier boisé de Vallières, à 2,5 km du bourg de Saint-Georges-de-Didonne et 3 km du centre-ville de Royan."
    ],
    "nl": [
      "Op 100 m van het grote zandstrand van Royan, in de rustige bosrijke omgeving van Vallières, op 2,5 km van de stad Saint-Georges-de-Didonne en op 3 km van het centrum van Royan."
    ],
    "es": [
      "A 100 m de la gran playa de arena de Royan, en la tranquila zona boscosa de Vallières, a 2,5 km de la ciudad de Saint-Georges-de-Didonne y a 3 km del centro de Royan."
    ]
  },
