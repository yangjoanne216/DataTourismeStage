  "rdfs:comment": {
    "de": [
      "Au Son des Clarines :\r\nEin gemütliches Familienrestaurant, in dem Sie frühstücken, sich mit Brot und Gebäck versorgen und gute savoyische Spezialitäten, Burger und Galettes genießen können. Restauration den ganzen Tag über"
    ],
    "en": [
      "Au Son des Clarines :\r\nA friendly family restaurant where you can have your breakfast, get bread and pastries, taste good Savoyard specialities, burgers and galettes. Catering all day long."
    ],
    "it": [
      "Au Son des Clarines :\r\nUn accogliente ristorante a conduzione familiare dove fare colazione, prendere pane e dolci, gustare buone specialità savoiarde, hamburger e galettes. Catering tutto il giorno"
    ],
    "fr": [
      "Au Son des Clarines :\r\nUn restaurant familial convivial où vous pourrez prendre votre petit déjeuner, vous procurer pains et viennoiseries, déguster de bonnes spécialités savoyardes, burgers et galettes. Restauration toute la journée"
    ],
    "nl": [
      "Au Son des Clarines:\r\nEen gezellig familierestaurant waar u kunt ontbijten, brood en gebak kunt krijgen, goede specialiteiten uit de Savoie kunt proeven, hamburgers en galettes. Catering de hele dag"
    ],
    "es": [
      "Au Son des Clarines :\r\nUn restaurante familiar y acogedor donde podrá desayunar, tomar pan y bollería, degustar buenas especialidades saboyanas, hamburguesas y galettes. Catering durante todo el día"
    ]
  },
