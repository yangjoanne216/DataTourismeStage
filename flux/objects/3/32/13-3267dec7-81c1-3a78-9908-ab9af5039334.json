  "rdfs:comment": {
    "de": [
      "Restaurant mit Spezialitäten aus Nordafrika: Mechouia-Salat, tunesisches Omelett, gegrillte Gambas, eine köstliche tunesische Chorba-Suppe oder eine überraschende Tschakchouka mit Ei."
    ],
    "en": [
      "Restaurant with Maghreb specialties: a mechouia salad, a Tunisian omelette, grilled prawns, a delicious Tunisian chorba soup or a surprising egg tchakchouka."
    ],
    "it": [
      "Ristorante con specialità magrebine: un'insalata méchouia, un'omelette tunisina, gamberi alla griglia, una deliziosa zuppa chorba tunisina o una sorprendente tchakchouka all'uovo."
    ],
    "fr": [
      "Restaurant avec les spécialités du Maghreb: une salade méchouia, une omelette tunisienne, des gambas grillées, une délicieuse soupe tunisienne chorba ou une surprenante tchakchouka à l'oeuf."
    ],
    "es": [
      "Restaurante con especialidades magrebíes: una ensalada méchouia, una tortilla tunecina, gambas a la plancha, una deliciosa sopa chorba tunecina o una sorprendente tchakchouka de huevo."
    ],
    "nl": [
      "Restaurant met Maghrebijnse specialiteiten: een méchouia salade, een Tunesische omelet, gegrilde gamba's, een heerlijke Tunesische chorba soep of een verrassende ei tchakchouka."
    ]
  },
