  "rdfs:comment": {
    "de": [
      "Charmantes kleines unabhängiges Haus mit Terrasse und kleinem Garten, nur wenige Gehminuten vom Stadtzentrum und den Thermen entfernt. Grillecke."
    ],
    "en": [
      "Charming little detached house with terrace and small garden, a few minutes walk from the town centre and the thermal baths. Barbecue area."
    ],
    "it": [
      "Graziosa casetta indipendente con terrazza e piccolo giardino, a pochi minuti a piedi dal centro città e dalle terme. Area barbecue."
    ],
    "fr": [
      "Charmante petite maison indépendante avec terrasse et jardinet, à quelques minutes à pied du centre-ville et des Thermes. Coin barbecue."
    ],
    "es": [
      "Encantadora casita independiente con terraza y pequeño jardín, a pocos minutos a pie del centro de la ciudad y de los baños termales. Zona de barbacoa."
    ],
    "nl": [
      "Charmant vrijstaand huisje met terras en kleine tuin, op een paar minuten lopen van het centrum en de thermale baden. Barbecuegebied."
