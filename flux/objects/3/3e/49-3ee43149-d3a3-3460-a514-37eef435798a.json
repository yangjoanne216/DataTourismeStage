  "rdfs:comment": {
    "de": [
      "4 Kilometer vom Stadtzentrum von Saumur entfernt, inmitten eines grünen Parks, öffnet das Gästehaus La Croix de la Voulte seine Türen für einen Aufenthalt außerhalb der Zeit!"
    ],
    "en": [
      "4 kilometers from the center of Saumur, in the heart of a green park, the guest house La Croix de la Voulte opens its doors for a stay out of time!"
    ],
    "it": [
      "A 4 chilometri dal centro di Saumur, nel cuore di un parco verde, la pensione La Croix de la Voulte vi apre le porte per un soggiorno senza tempo!"
    ],
    "fr": [
      "A 4 kilomètres du centre ville de Saumur, au cœur d’un parc verdoyant, la maison d'hôtes La Croix de la Voulte vous ouvre ses portes pour un séjour hors du temps !"
    ],
    "nl": [
      "Op 4 kilometer van het centrum van Saumur, in het hart van een groen park, opent het gastenverblijf La Croix de la Voulte haar deuren voor een tijdloos verblijf!"
    ],
    "es": [
      "A 4 kilómetros del centro de Saumur, en el corazón de un parque verde, la casa de huéspedes La Croix de la Voulte le abre sus puertas para una estancia intemporal"
    ]
  },
