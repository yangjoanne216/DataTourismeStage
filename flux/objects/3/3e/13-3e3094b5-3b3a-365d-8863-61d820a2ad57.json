  "rdfs:comment": {
    "de": [
      "Traditioneller BIO-Betrieb, Rinderzucht.\r\nHerstellung von Cantal-Fermier-Käse, Landkäse, Joghurt, Faisselle, Milchkonfitüre, Wurstwaren von Schweinen und Salers-Rindern, blonde Linsen."
    ],
    "en": [
      "Traditional farm in BIO, cattle breeding.\r\nProduction of cantal cheese, local cheeses, yoghurts, faisselle, milk jam, pork and beef salers, lentil blond."
    ],
    "it": [
      "Azienda agricola biologica tradizionale, allevamento di bovini.\r\nProduzione di formaggio di fattoria del Cantal, formaggi locali, yogurt, faisselle, marmellata di latte, salsicce di maiale e di manzo di Salers, lenticchie."
    ],
    "fr": [
      "Exploitation traditionnelle en BIO, élevage de bovins.\r\nFabrication de fromage type cantal fermier, fromages de pays, yaourts, faisselle, confiture de lait, charcuterie de porc et bovins Salers, lentille blonde."
    ],
    "nl": [
      "Traditionele biologische boerderij, veeteelt.\r\nVervaardiging van boerenkaas uit de Cantal, lokale kazen, yoghurt, faisselle, melkjam, varkens- en Salers runderworst, linzen."
    ],
    "es": [
      "Granja ecológica tradicional, cría de ganado.\r\nFabricación de queso de granja de Cantal, quesos locales, yogures, faisselle, mermelada de leche, embutidos de cerdo y de ternera de Salers, lentejas."
    ]
  },
