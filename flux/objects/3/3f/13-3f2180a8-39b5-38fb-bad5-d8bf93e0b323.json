  "rdfs:comment": {
    "de": [
      "Best Western Hotel le Sud***\r\nRestaurant vor Ort, gepflegte Küche, Terrasse, Garten, Bar, Lounge, Dauerausstellung zeitgenössischer Maler. WIFI kostenlos. Sehr nah am historischen Zentrum der Stadt."
    ],
    "en": [
      "Hotel Best Western le Sud*** (South)\r\nRestaurant on site, fine cuisine, terrace, garden, bar, lounge, permanent exhibition of contemporary painters. Free WIFI. Very close to the historical center of the city."
    ],
    "it": [
      "Hotel Best Western le Sud*** (solo in francese)\r\nRistorante in loco, cucina raffinata, terrazza, giardino, bar, lounge, mostra permanente di pittori contemporanei. WIFI gratuito. Molto vicino al centro storico della città."
    ],
    "fr": [
      "Hôtel Best Western le Sud***\r\nRestaurant sur place, cuisine soignée, terrasse, jardin, bar, salon, exposition permanente de peintres contemporains. WIFI gratuit. Très proche du centre historique de la ville."
    ],
    "es": [
      "Best Western le Sud Hotel*** (sólo en francés)\r\nRestaurante in situ, alta cocina, terraza, jardín, bar, salón, exposición permanente de pintores contemporáneos. WIFI gratuito. Muy cerca del centro histórico de la ciudad."
    ],
    "nl": [
      "Best Western Hotel le Sud*** (Zuid)\r\nRestaurant ter plaatse, fijne keuken, terras, tuin, bar, salon, permanente tentoonstelling van hedendaagse schilders. Gratis WIFI. Zeer dicht bij het historische centrum van de stad."
    ]
  },
