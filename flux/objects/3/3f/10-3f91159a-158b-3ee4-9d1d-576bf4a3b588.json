  "rdfs:comment": {
    "de": [
      "Bioproduzent von Eiern, Eiernudeln aus Hartweizen-, Weichweizen-, Dinkel- und Einkornmehl, Geflügel (Huhn, Perlhuhn, Truthahn), Obst und Gemüse der Saison."
    ],
    "en": [
      "Organic producer of eggs, egg pasta (hard wheat flour, soft wheat, spelt and baby spelt), meat poultry (chicken, guinea fowl, turkey), seasonal fruit and vegetables."
    ],
    "it": [
      "Produttore biologico di uova, pasta all'uovo (farina di grano duro, grano tenero, farro e piccolo farro), pollame (pollo, faraona, tacchino), frutta e verdura di stagione."
    ],
    "fr": [
      "Producteur bio d'oeufs, pâtes aux œufs farine de BLE dur, Blé tendre, épeautre et petit épeautre), volailles de chair (poulet, pintade, dinde), fruits et légumes de saison."
    ],
    "es": [
      "Productor ecológico de huevos, pasta al huevo (trigo duro, trigo blando, harina de espelta y espelta pequeña), aves de corral (pollo, pintada, pavo), frutas y verduras de temporada."
    ],
    "nl": [
      "Biologische producent van eieren, eierpasta (harde tarwe, zachte tarwe, spelt en klein speltmeel), gevogelte (kip, parelhoen, kalkoen), seizoensfruit en -groenten."
    ]
  },
