  "rdfs:comment": {
    "de": [
      "Geführte Touren in der Vaucluse - Provence. Lassen Sie sich von dieser Gegend mit ihren reichen Traditionen und ihrem reichen Kulturerbe erzählen. Entschlüsseln Sie seine Geheimnisse anhand von Landschaften, Dörfern, Traditionen und Geschichte."
    ],
    "en": [
      "Guided tours in Vaucluse - Provence. Hear the fascinating stories behind this region's wealth of traditions and heritage. Learn the mysteries of its landscapes, villages, traditions and history."
    ],
    "it": [
      "Visite guidate in Vaucluse - Provenza. Lasciatevi raccontare questo territorio ricco di tradizioni e patrimonio. Scoprite i suoi misteri attraverso paesaggi, villaggi, tradizioni e storia."
    ],
    "fr": [
      "Visites guidées en Vaucluse - Provence. Laissez-vous conter ce territoire aux traditions et au patrimoine si riches. Percez ses mystères au fil des paysages, des villages, des traditions et de l'histoire."
    ],
    "es": [
      "Visitas guiadas en Vaucluse - Provenza. Déjese llevar por este territorio de ricas tradiciones y patrimonio. Descubra sus misterios a través de paisajes, pueblos, tradiciones e historia."
    ],
    "nl": [
      "Rondleidingen in de Vaucluse - Provence. Laat u vertellen over dit gebied met zijn rijke tradities en erfgoed. Ontdek zijn mysteries via landschappen, dorpen, tradities en geschiedenis."
    ]
  },
  "rdfs:label": {
