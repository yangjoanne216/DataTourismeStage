  "rdfs:comment": {
    "de": [
      "Restaurant und Lounge mit außergewöhnlichen Fleischspezialitäten vom Grill im historischen Zentrum von Aix-en-Provence. Eine schöne Karte mit Spirituosen und Weinen wird Ihnen angeboten, um Ihr Geschmackserlebnis zu begleiten."
    ],
    "en": [
      "Restaurant and lounge specializing in exceptional barbecued meats in the historic center of Aix-en-Provence. A fine wine and spirits list accompanies your dining experience."
    ],
    "it": [
      "Ristorante e lounge specializzato in eccezionali carni alla brace nel centro storico di Aix-en-Provence. Una ricca carta di vini e liquori accompagna la vostra esperienza culinaria."
    ],
    "fr": [
      "Restaurant et lounge de spécialité de viandes d’exception cuites au barbecue dans le centre historique d'Aix-en-Provence. Une belle carte avec alcools et vins vous est proposée pour accompagner votre expérience gustative."
    ],
    "es": [
      "Restaurante y salón especializado en carnes a la brasa excepcionales en el centro histórico de Aix-en-Provence. Una amplia carta de vinos y licores acompañará su experiencia gastronómica."
    ],
    "nl": [
      "Restaurant en lounge gespecialiseerd in uitzonderlijk gegrild vlees in het historische centrum van Aix-en-Provence. Een uitgebreide wijn- en drankkaart is beschikbaar om uw eetervaring te begeleiden."
    ]
  },
