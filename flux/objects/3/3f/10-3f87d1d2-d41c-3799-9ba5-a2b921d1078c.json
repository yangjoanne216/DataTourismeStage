  "rdfs:comment": {
    "de": [
      "Freistehendes Häuschen, das an das Haus des Eigentümers angrenzt. Esszimmer mit ausziehbarem Bett, Küchenecke, TV-Anschluss. Badezimmer mit Dusche. Waschmaschine. Elektrische Heizung. Kleine Veranda. Terrasse. Gemeinsames Grundstück. Nebenkosten inbegriffen."
    ],
    "en": [
      "Dining room with pull-out bed, kitchenette, TV socket. Shower room. Washing machine. Electric heating. Small veranda. Terrace. Common ground. Charges included."
    ],
    "it": [
      "Sala da pranzo con letto a scomparsa, angolo cottura, presa TV. Bagno con doccia. Lavatrice. Riscaldamento elettrico. Piccola veranda. Terrazza. Terreno comune. Spese incluse."
    ],
    "fr": [
      "Maisonnette individuelle mitoyenne à la maison du propriétaire.Salle à manger avec lit escamotable, coin-cuisine, prise TV. Salle d’eau. Lave linge. Chauffage électrique. Petite véranda. Terrasse. Terrain commun. Charges comprise."
    ],
    "nl": [
      "Eetkamer met opklapbed, kitchenette, TV-aansluiting. Doucheruimte. Wasmachine. Elektrische verwarming. Kleine veranda. Terras. Gemeenschappelijke grond. Kosten inbegrepen."
    ],
    "es": [
      "Comedor con cama plegable, cocina americana, toma de TV. Ducha. Lavadora. Calefacción eléctrica. Pequeña veranda. Terraza. Terreno común. Cargos incluidos."
    ]
  },
