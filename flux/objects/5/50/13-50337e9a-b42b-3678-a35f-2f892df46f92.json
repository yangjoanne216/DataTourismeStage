  "rdfs:comment": {
    "de": [
      "Weinbar und Weinhandlung. Über 300 Referenzen an natürlichen Weinen, vor Ort oder zum Mitnehmen. Spirituosen, Wurst- und Käseplatten, Tapas, Aufstriche aus dem Meer und vegetarische Aufstriche. Handwerklich hergestellte Biere und Softdrinks."
    ],
    "en": [
      "Wine bar and wine shop. More than 300 references of natural wines, on the spot or to take away. Spirits, charcuterie and cheese platters, tapas, seafood and vegetarian spreads. Artisanal beers and soft drinks."
    ],
    "it": [
      "Enoteca e wine bar. Oltre 300 referenze di vini naturali, sul posto o da asporto. Liquori, taglieri di salumi e formaggi, tapas, frutti di mare e piatti vegetariani. Birre artigianali e bevande analcoliche."
    ],
    "fr": [
      "Bar à vins et caviste. Plus de 300 références de vins naturels, sur place ou à emporter. Spiritueux, assiettes de charcuterie et fromage, tapas, tartinades de la mer et végétariennes. Bières et soft artisanaux."
    ],
    "nl": [
      "Wijnbar en wijnwinkel. Meer dan 300 referenties van natuurlijke wijnen, ter plaatse of om mee te nemen. Sterke dranken, vleeswaren en kaasplateaus, tapas, zeevruchten en vegetarisch beleg. Ambachtelijke bieren en frisdranken."
    ],
    "es": [
      "Vinoteca y tienda de vinos. Más de 300 referencias de vinos naturales, in situ o para llevar. Licores, tablas de embutidos y quesos, tapas, mariscos y cremas vegetarianas. Cervezas artesanales y refrescos."
    ]
  },
