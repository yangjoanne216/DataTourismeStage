      "dc:description": {
        "de": [
          "Programm: 16:30 bis 17:30 Uhr: Gospelgesang von José Geraldo de Lima, prmeier Tenor der Straßburger Oper, und Tania. 17:30 bis 18:15 Uhr: Folkloretänze von Kindern aus Truchtersheim. Ab 18.30 Uhr: Flammkuchen."
        ],
        "en": [
          "Program: 4:30pm to 5:30pm: Gospel singing by José Geraldo de Lima, principal tenor of the Strasbourg Opera, and Tania. 5:30pm to 6:15pm: Folk dances by Truchtersheim children. From 6.30pm: Tart flambées."
        ],
        "it": [
          "Programma: dalle 16.30 alle 17.30: canti gospel di José Geraldo de Lima, tenore principale dell'Opera di Strasburgo, e Tania. Dalle 17.30 alle 18.15: danze popolari dei bambini di Truchtersheim. Dalle 18.30: Crostata flambée."
        ],
        "fr": [
          "Au programme : 16h30 à 17h30 : Chants Gospel par José Geraldo de Lima, prmeier ténor de l'Opéra de Strasbourg, et Tania. 17h30 à 18h15 : Danses Folkloriques par les enfants de Truchtersheim. A partir de 18h30 : Tartes flambées."
        ],
        "nl": [
          "Programma: 16.30-17.30 uur: Gospelzang door José Geraldo de Lima, eerste tenor van de Opera van Straatsburg, en Tania. 17.30-18.15 uur: Volksdansen door de kinderen van Truchtersheim. Vanaf 18.30 uur: Flammkuchen."
        ],
        "es": [
          "Programa: De 16.30 a 17.30 h: Canto gospel a cargo de José Geraldo de Lima, tenor principal de la Ópera de Estrasburgo, y Tania. De 17.30 a 18.15 h: Danzas folclóricas a cargo de los niños de Truchtersheim. A partir de las 18.30 h: Tarta flambeada."
        ]
