  "rdfs:comment": {
    "de": [
      "Auf Ihrem Spaziergang entdecken Sie die Ufer des Flusses Charente, um eine Bootstour zur Ile Madame zu machen."
    ],
    "en": [
      "During your walk you will discover the banks of the river Charente and take a boat trip around the Ile Madame."
    ],
    "it": [
      "Durante la passeggiata scoprirete le rive del fiume Charente e farete un tour in barca dell'Ile Madame."
    ],
    "fr": [
      "Lors de votre balade vous découvrirez les rivages du fleuve Charente pour faire le tour en bateau de l'ile Madame."
    ],
    "nl": [
      "Tijdens uw wandeling ontdekt u de oevers van de Charente en maakt u een boottocht op het Ile Madame."
    ],
    "es": [
      "Durante su paseo descubrirá las orillas del río Charente y dará un paseo en barco por la Ile Madame."
    ]
  },
