  "rdfs:comment": {
    "de": [
      "Angenehmes Hotel-Restaurant mit geschlossenem Parkplatz, traditioneller Küche, 24-Stunden-Automat für Zimmerschlüssel, kostenlosem WLAN. Unter Videoüberwachung\nKontinentales Frühstück in Buffetform\nBarbereich, Lounge."
    ],
    "en": [
      "Pleasant hotel-restaurant with closed parking, traditional cuisine, 24-hour room key machine, free wifi. Under video surveillance\nContinental breakfast buffet\nBar and lounge area."
    ],
    "it": [
      "Piacevole hotel-ristorante con parcheggio chiuso, cucina tradizionale, distributore automatico di chiavi in camera 24 ore su 24, wifi gratuito. Sotto sorveglianza video\nColazione continentale a buffet\nArea bar, salone."
    ],
    "fr": [
      "Hôtel-restaurant agréable avec parking fermé, cuisine traditionnelle, distributeur automatique de clés de chambres 24h/24, wifi gratuit. Sous vidéo surveillance. \nPetit déjeuner continental sous forme de buffet. \nEspace bar, salon."
    ],
    "es": [
      "Agradable hotel-restaurante con aparcamiento cerrado, cocina tradicional, máquina de llaves de habitaciones 24 horas, wifi gratuito. Bajo video vigilancia\nDesayuno buffet continental\nZona de bar, salón."
    ],
    "nl": [
      "Aangenaam hotel-restaurant met afgesloten parkeerplaats, traditionele keuken, 24-uurs kamersleutelmachine, gratis wifi. Onder videobewaking\nContinentaal ontbijtbuffet\nBar, lounge."
    ]
  },
