  "rdfs:comment": {
    "de": [
      "Mademoiselle Hortense ist eine kleine Boutique im Herzen von Saint-Rémy-de-Provence, die Prêt-à-porter-Mode, Schmuck und Accessoires für Frauen zu günstigen Preisen anbietet."
    ],
    "en": [
      "Mademoiselle Hortense is a small boutique of ready-to-wear, jewelry and accessories for women at low prices in the heart of Saint-Rémy-de-Provence."
    ],
    "it": [
      "Mademoiselle Hortense è una piccola boutique di prêt-à-porter femminile, gioielli e accessori a prezzi bassi nel cuore di Saint-Rémy-de-Provence."
    ],
    "fr": [
      "Mademoiselle Hortense est une petite boutique de prêt-à-porter, bijoux et accessoires pour femmes à des prix doux au cœur de Saint-Rémy-de-Provence."
    ],
    "nl": [
      "Mademoiselle Hortense is een kleine boetiek met prêt-à-porter, juwelen en accessoires voor vrouwen tegen lage prijzen in het hart van Saint-Rémy-de-Provence."
    ],
    "es": [
      "Mademoiselle Hortense es una pequeña boutique que vende prêt-à-porter femenino, joyas y accesorios a precios bajos en el corazón de Saint-Rémy-de-Provence."
    ]
  },
