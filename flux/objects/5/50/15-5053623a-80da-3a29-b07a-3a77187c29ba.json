      "dc:description": {
        "de": [
          "Badminton, Basketball, CDOS, Radsport, Fußball, Golf, Handball, innovative Spiele, Krafttraining, Rugby, Erste Hilfe Rettungsdienst, Tennis, Tischtennis - testen Sie mit der Tour de l'Indre des Sports kostenlos alle diese Sportarten."
        ],
        "en": [
          "Badminton, basketball, CDOS, cycling, soccer, golf, handball, innovative games, bodybuilding, rugby, first aid, tennis, table tennis, come and test all these sports disciplines for free with the Tour de l'Indre des Sports."
        ],
        "it": [
          "Badminton, pallacanestro, CDOS, ciclismo, calcio, golf, pallamano, giochi innovativi, bodybuilding, rugby, primo soccorso, salvataggio, tennis, tennis da tavolo, venite a provare gratuitamente tutte queste discipline sportive con il Tour de l'Indre des Sports."
        ],
        "fr": [
          "Badminton, basket, CDOS, cyclisme, football, golf, handball, jeux innovants, musculation, rugby, secourisme sauvetage, tennis, tennis de table,  venez tester avec le Tour de l'Indre des Sports toutes ces disciplines sportives  gratuitement."
        ],
        "nl": [
          "Badminton, basketbal, CDOS, wielrennen, voetbal, golf, handbal, innovatieve spelletjes, bodybuilding, rugby, EHBO, redding, tennis, tafeltennis, kom al deze sportdisciplines gratis uitproberen met de Tour de l'Indre des Sports."
        ],
        "es": [
          "Bádminton, baloncesto, CDOS, ciclismo, fútbol, golf, balonmano, juegos innovadores, musculación, rugby, primeros auxilios, salvamento, tenis, tenis de mesa, ven a probar gratis todas estas disciplinas deportivas con el Tour de l'Indre des Sports."
        ]
      },
