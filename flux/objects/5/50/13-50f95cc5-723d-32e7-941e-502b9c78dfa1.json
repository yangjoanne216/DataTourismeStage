  "rdfs:comment": {
    "de": [
      "Bar-Restaurant auf der Ostseite des Grand Colombier-Massivs, auf 1500 m Höhe: atemberaubende Aussicht auf die Alpenkette und den Mont-Blanc. Inneneinrichtung aus Holz, rustikale Einrichtung. Spezialitäten: Fondues, Brasérades, Raclettes."
    ],
    "en": [
      "Altitude bar restaurant located on the eastern slope of the Grand Colombier Massif, at an altitude of 1500 meters: breathtaking view of the Alps and the Mont-Blanc. Wooden interior, rustic furnishings. Specialities: fondues, brasérades, raclettes."
    ],
    "it": [
      "Bar-ristorante d'alta quota situato sul versante orientale del Massiccio del Grand Colombier, a 1500 metri di altitudine: vista mozzafiato sulle Alpi e sul Monte Bianco. Interni in legno, arredamento rustico. Specialità: fondute, brasérades, raclettes."
    ],
    "fr": [
      "Bar restaurant d'altitude situé sur le versant est du Massif du Grand Colombier, à 1500 mètres d'altitude : vue imprenable sur la Chaîne des Alpes et le Mont-Blanc. Intérieur en bois, aménagement rustique. Spécialités : fondues, brasérades, raclettes."
    ],
    "nl": [
      "Hooggelegen bar-restaurant op de oostelijke helling van het Grand Colombier-massief, op 1500 meter hoogte: adembenemend uitzicht op de Alpen en de Mont Blanc. Houten interieur, rustieke inrichting. Specialiteiten: fondues, brasérades, raclettes."
    ],
    "es": [
      "Bar-restaurante de altura situado en la vertiente oriental del macizo del Grand Colombier, a 1.500 metros de altitud: impresionante vista de los Alpes y del Mont Blanc. Interior de madera, mobiliario rústico. Especialidades: fondues, brasérades, raclettes."
    ]
  },
