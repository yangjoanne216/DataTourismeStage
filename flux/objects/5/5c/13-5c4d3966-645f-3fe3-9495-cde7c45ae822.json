  "rdfs:comment": {
    "de": [
      "Enjoy a traditional meal in this former grocer's shop !"
    ],
    "en": [
      "Enjoy a traditional meal in this former grocer's shop !"
    ],
    "it": [
      "Ex negozio di alimentari del villaggio, trasformato in un piccolo ristorante dove saremo lieti di accogliervi, in tutta semplicità, intorno ai buoni piatti della terra dell'Ardéchois."
    ],
    "fr": [
      "Ancienne épicerie du village, transformée en petit restaurant où nous aurons plaisir à vous recevoir, en toute simplicité, autour de bons plats du terroir Ardéchois."
    ],
    "nl": [
      "Voormalige dorpskruidenier, omgebouwd tot een klein restaurant waar wij u graag ontvangen, in alle eenvoud, rond goede gerechten van Ardéchois-grond."
    ],
    "es": [
      "Antigua tienda de comestibles del pueblo, transformada en un pequeño restaurante donde estaremos encantados de acogerle, con toda sencillez, en torno a los buenos platos de la tierra de Ardéchois."
    ]
  },
