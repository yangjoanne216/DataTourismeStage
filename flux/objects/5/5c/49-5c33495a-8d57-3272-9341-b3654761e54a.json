  "rdfs:comment": {
    "de": [
      "Wir befinden uns 10 Minuten von der Rennstrecke des 24-Stunden-Rennens von Le Mans entfernt und begrüßen Sie gerne in einer Umgebung inmitten der Natur."
    ],
    "en": [
      "Situated 10 minutes from the 24 heures du mans circuit, we welcome you with pleasure in a natural setting."
    ],
    "it": [
      "A 10 minuti dalla 24 ore di Le Mans, vi accogliamo con piacere in un ambiente naturale."
    ],
    "fr": [
      "Situés à 10mn du circuit des 24 heures du mans, nous vous accueillons avec plaisir dans un cadre de pleine nature."
    ],
    "es": [
      "Situado a 10 minutos del circuito de las 24 horas de Le Mans, le damos la bienvenida con gusto en un entorno natural."
    ],
    "nl": [
      "Op 10 minuten van de 24 uur van Le Mans, verwelkomen wij u met plezier in een natuurlijke omgeving."
    ]
