  "rdfs:comment": {
    "de": [
      "Lust auf Regionalität und Authentizität? Begeben Sie sich auf den Albinque-Markt am Place Pierre Fabre, um einen kleinen Rundgang durch die Produkte unserer Heimat zu machen."
    ],
    "en": [
      "Do you want terroir and authenticity? Go to the Albinque market, place Pierre Fabre, for a small overview of local products."
    ],
    "it": [
      "Cercate prodotti locali e autenticità? Visita al mercato di Albinque, place Pierre Fabre, per un piccolo tour dei prodotti locali."
    ],
    "fr": [
      "Envie de terroir et d'authenticité ? Rendez-vous sur le marché de l'Albinque, place Pierre Fabre, pour un petit tour d'horizon des produits de chez nous."
    ],
    "es": [
      "¿Busca productos locales y autenticidad? Visite el mercado de Albinque, en la plaza Pierre Fabre, para hacer un pequeño recorrido por los productos locales."
    ],
    "nl": [
      "Op zoek naar lokale producten en authenticiteit? Bezoek de markt van Albinque, place Pierre Fabre, voor een kleine rondleiding langs lokale producten."
    ]
