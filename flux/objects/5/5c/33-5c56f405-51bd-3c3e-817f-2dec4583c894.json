  "rdfs:comment": {
    "de": [
      "Das ganze Paragliding! Tandemtaufen und Gleitschirmkurse im Herzen der Zentralpyrenäen und des Arbas-Tals, 1 Stunde von Toulouse entfernt."
    ],
    "en": [
      "All the paragliding ! First tandem flight and paragliding course in the heart of the central Pyrenees and the Arbas valley, 1 hour from Toulouse."
    ],
    "it": [
      "Tutto sul parapendio! Lezioni e corsi di parapendio in tandem nel cuore dei Pirenei centrali e nella valle di Arbas, a 1 ora da Tolosa."
    ],
    "fr": [
      "Tout le parapente ! Baptême biplace et stage de parapente au cœur des Pyrénées centrales et de la vallée de l'Arbas, à 1h de Toulouse."
    ],
    "es": [
      "¡Todo sobre el parapente! Clases y cursos de parapente biplaza en el corazón de los Pirineos centrales y el valle de Arbas, a 1 hora de Toulouse."
    ],
    "nl": [
      "Alles over paragliding! Tandem paragliding lessen en cursussen in het hart van de centrale Pyreneeën en de Arbas vallei, 1 uur van Toulouse."
    ]
  },
