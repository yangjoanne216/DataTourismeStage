  "rdfs:comment": {
    "de": [
      "2*NN-Hotel. LOGIS-Kette gemütliches Hotel und Gourmet-Restaurant . Ehemalige Poststation. Geräumige, voll ausgestattete Zimmer. Traditionelles Restaurant mit Terrasse im privaten Innenhof. Salon für Versammlungen oder Gruppenmahlzeiten"
    ],
    "en": [
      "Hotel 2*NN. chain LOGIS cosy hotel and gourmet restaurant. Former post office. Spacious rooms all equipped. Traditional restaurant with terrace in private court. Lounge for meetings or group meals"
    ],
    "it": [
      "Hotel 2*NN. Hotel accogliente della catena LOGIS e ristorante gourmet. Ex ufficio postale. Camere spaziose tutte attrezzate. Ristorante tradizionale con terrazza nel cortile privato. Salone per riunioni o pasti di gruppo"
    ],
    "fr": [
      "Hôtel 2*NN. chaîne LOGIS hôtel cosy et restaurant gourmand . Ancien relais postal. Chambres spacieuses toutes équipées. Restaurant traditionnel avec terrasse en cour privative. Salon pour accueil des réunions ou repas de groupes"
    ],
    "nl": [
      "Hotel 2*NN. LOGIS-keten gezellig hotel en gastronomisch restaurant. Voormalig postkantoor. Ruime kamers van alle gemakken voorzien. Traditioneel restaurant met terras op eigen binnenplaats. Lounge voor vergaderingen of groepsmaaltijden"
    ],
    "es": [
      "Hotel 2*NN. Hotel acogedor de la cadena LOGIS y restaurante gastronómico. Antigua oficina de correos. Habitaciones amplias y equipadas. Restaurante tradicional con terraza en patio privado. Salón para reuniones o comidas de grupo"
    ]
  },
