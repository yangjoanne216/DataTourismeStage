  "rdfs:comment": {
    "de": [
      "Das leicht zugängliche Hotel Ibis Angoulême Nord ist 5 Minuten vom Bahnhof Angoulême entfernt und liegt in der Nähe der Verkehrsachsen Paris/Bordeaux und Limoges/Cognac. Das Hotel empfängt Sie rund um die Uhr."
    ],
    "en": [
      "Easy access hotel, 5mn from Angoulême train station, close to Paris/Bordeaux and Limoges/Cognac axis, the Ibis Angoulême Nord hotel welcomes you 24 hours a day."
    ],
    "it": [
      "Hotel di facile accesso, a 5 minuti dalla stazione ferroviaria di Angoulême, vicino all'asse Parigi/Bordeaux e Limoges/Cognac, l'hotel Ibis Angoulême Nord vi accoglie 24 ore al giorno."
    ],
    "fr": [
      "Hôtel facile d'accès, à 5mn de la gare d'Angoulême, proche de l'axe Paris/Bordeaux et Limoges/Cognac, l'hôtel Ibis Angoulême Nord vous accueille 24h/24."
    ],
    "es": [
      "Hotel de fácil acceso, a 5mn de la estación de tren de Angoulême, cerca de los ejes París/Burdeos y Limoges/Cognac, el hotel Ibis Angoulême Nord le da la bienvenida las 24 horas del día."
    ],
    "nl": [
      "Het Ibis-hotel Angoulême Nord ligt op 5 minuten van het station van Angoulême, dicht bij de as Parijs/Bordeaux en Limoges/Cognac. U bent 24 uur per dag welkom."
    ]
  },
