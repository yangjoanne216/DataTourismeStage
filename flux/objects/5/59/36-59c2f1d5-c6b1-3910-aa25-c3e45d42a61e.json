  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/ff5e8ef5-ed9a-3e14-a068-feb2c9096066",
      "dc:description": {
        "de": [
          "2 Schlafzimmer (1 Doppelbett, 2 Einzelbetten), Esszimmer, Küche, 2 WCs, Badezimmer, Gartenmöbel, angrenzender und geschlossener Garten. Elektroheizung bei Bedarf gegen Aufpreis. Privater Parkplatz. WIFI-Internetzugang."
        ],
        "en": [
          "2 bedrooms (1 double bed, 2 single beds), dining room, kitchen, 2 toilets, shower room, garden furniture, adjoining enclosed garden. Electric heating if needed in addition. Private parking. WIFI Internet access."
        ],
        "it": [
          "2 camere da letto (1 letto matrimoniale, 2 letti singoli), sala da pranzo, cucina, 2 WC, bagno con doccia, mobili da giardino, giardino recintato. Riscaldamento elettrico se richiesto in aggiunta. Parcheggio privato. Accesso Internet WIFI."
        ],
        "fr": [
          "2 Chambres (1 lit 2 pl., 2 lits 1 pl.), salle à manger, cuisine, 2 wc, salle d'eau, salon de jardin, jardin attenant et clos. Chauffage électrique si besoin en supplément. Parking privé. Accès Internet WIFI."
        ],
        "nl": [
          "2 slaapkamers (1 tweepersoonsbed, 2 eenpersoonsbedden), eetkamer, keuken, 2 toiletten, doucheruimte, tuinmeubels, omheinde tuin. Elektrische verwarming indien nodig. Privé parkeerplaats. WIFI internettoegang."
        ],
        "es": [
          "2 dormitorios (1 cama doble, 2 camas individuales), comedor, cocina, 2 aseos, cuarto de ducha, muebles de jardín, jardín cerrado. Calefacción eléctrica si se requiere además. Aparcamiento privado. Acceso a Internet WIFI."
        ]
      },
