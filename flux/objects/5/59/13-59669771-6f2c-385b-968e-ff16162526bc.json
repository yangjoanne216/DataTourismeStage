  "rdfs:comment": {
    "de": [
      "Mehr als 120 Aussteller (Trödelmarkt - Flohmarkt - Handwerker - Produzenten) - Verkauf von Brot aus dem Gemeindebackofen - Würstchen-Linsen-Essen um 12 Uhr, serviert von der Schule - Zahlreiche Animationen"
    ],
    "en": [
      "More than 120 exhibitors (flea market - garage sale - craftsmen - producers) - sale of bread baked in the communal oven - Sausage and lentil meal at 12 noon served by the school - Many activities"
    ],
    "it": [
      "Più di 120 espositori (mercatino delle pulci, mercatino dell'usato, artigiani, produttori) - vendita di pane cotto nel forno comune - pasto a base di salsicce e lenticchie alle 12.00 servito dalla scuola - numerose attività"
    ],
    "fr": [
      "Plus de 120 exposants (brocante - vide grenier - Artisans - producteurs) - vente de pain cuit au four banal - Repas Saucisses-lentilles à 12h servi par l'école - Nombreuses animations"
    ],
    "nl": [
      "Meer dan 120 standhouders (rommelmarkt - garageverkoop - ambachtslieden - producenten) - verkoop van in de gemeenschappelijke oven gebakken brood - Worst- en linzenmaaltijd om 12 uur geserveerd door de school - Vele activiteiten"
    ],
    "es": [
      "Más de 120 expositores (mercadillo - venta de garaje - artesanos - productores) - venta de pan cocido en el horno comunal - Comida de salchichas y lentejas a las 12 h servida por la escuela - Numerosas actividades"
    ]
  },
