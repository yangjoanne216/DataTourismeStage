  "rdfs:comment": {
    "de": [
      "Kunsthandwerkliche Keksfabrik - Schokoladenfabrik - Teesalon\r\nTraditionelle Kekse, Cakes, gefüllte Schokoladentafeln, Brotaufstriche, BIO-Fruchtsäfte... Und herzhaftes Essen am Mittag!"
    ],
    "en": [
      "Cookies - Chocolate - Tearoom\r\nTraditional cookies, cakes, filled chocolate bars, spreads, organic fruit juices... And savoury meals at lunchtime !"
    ],
    "it": [
      "Biscotti - Cioccolato - Sala da tè\r\nBiscotti tradizionali, torte, barrette di cioccolato ripiene, creme spalmabili, succhi di frutta biologici... E pasti salati a pranzo!"
    ],
    "fr": [
      "Biscuiterie - Chocolaterie - Salon de thé artisanal \r\nBiscuits traditionnels, cakes, tablettes de chocolats garnies, pâtes à tartiner, jus de fruits BIO... Et repas salé le midi !"
    ],
    "nl": [
      "Koekjes - Chocolade - Tearoom\r\nTraditionele koekjes, gebak, gevulde chocoladerepen, broodbeleg, biologisch vruchtensap... En hartige maaltijden tussen de middag!"
    ],
    "es": [
      "Galletas - Chocolate - Tearoom\r\nGalletas tradicionales, pasteles, barritas de chocolate rellenas, pastas para untar, zumos de fruta ecológicos... ¡Y comidas saladas a la hora de comer!"
    ]
  },
