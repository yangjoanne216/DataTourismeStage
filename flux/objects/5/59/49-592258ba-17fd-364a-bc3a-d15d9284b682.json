  "rdfs:comment": {
    "de": [
      "Kleines, gut ausgestattetes Hotel mit bequemer Bettwäsche. Das Hotel befindet sich nur 150 m vom SNCF-Bahnhof und 20 Minuten zu Fuß vom Stadtzentrum entfernt."
    ],
    "en": [
      "Small, well-equipped hotel with comfortable bedding. The hotel is only 150 m from the train station and 20mn walk from the city center."
    ],
    "it": [
      "Piccolo hotel ben attrezzato con biancheria da letto confortevole. L'hotel dista solo 150 m dalla stazione ferroviaria e 20 minuti a piedi dal centro città."
    ],
    "fr": [
      "Petit hôtel  bien équipé avec une literie confortable. L'hôtel se trouve seulement 150 m de la gare SNCF et à 20mn à pied du centre-ville."
    ],
    "nl": [
      "Klein, goed uitgerust hotel met comfortabel beddengoed. Het hotel ligt op slechts 150 meter van het treinstation en op 20 minuten lopen van het stadscentrum."
    ],
    "es": [
      "Hotel pequeño y bien equipado, con ropa de cama cómoda. El hotel está a sólo 150 m de la estación de tren y a 20 minutos a pie del centro de la ciudad."
    ]
  },
