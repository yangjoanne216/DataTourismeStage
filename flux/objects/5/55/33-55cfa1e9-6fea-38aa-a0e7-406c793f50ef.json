  "rdfs:comment": {
    "de": [
      "Der Bois Barrat liegt in der Gemeinde Montesquieu Lauragais und lädt Sie zu langen Nachmittagen in der Natur ein."
    ],
    "en": [
      "Located in the commune of Montesquieu Lauragais, the Barrat wood invites you to long afternoons in the middle of nature."
    ],
    "it": [
      "Situato nel comune di Montesquieu Lauragais, il Bosco di Barrat vi invita a trascorrere lunghi pomeriggi nel cuore della natura."
    ],
    "fr": [
      "Situé sur la commune de Montesquieu Lauragais, le bois Barrat vous invite à de longues après-midis en pleine Nature."
    ],
    "es": [
      "Situado en el municipio de Montesquieu Lauragais, el bosque de Barrat le invita a pasar largas tardes en plena naturaleza."
    ],
    "nl": [
      "Gelegen in de gemeente Montesquieu Lauragais, nodigt Barrat Wood u uit om lange middagen door te brengen in het hart van de natuur."
    ]
