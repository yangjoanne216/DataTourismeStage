  "rdfs:comment": {
    "de": [
      "Gegründet in Aix-en-Provence, mobile Plattform für kulturelle Aktivitäten, Workshop, um Verbindungen in der Nachbarschaft zu wecken und zu dynamisieren, Worte und Austausch in Umlauf zu bringen."
    ],
    "en": [
      "Created in Aix-en-Provence, a mobile platform for cultural activities, a workshop to encourage and energize local links, to circulate words and exchanges."
    ],
    "it": [
      "Creata ad Aix-en-Provence, una piattaforma mobile per le attività culturali, un laboratorio per incoraggiare ed energizzare i legami locali, per far circolare parole e scambi."
    ],
    "fr": [
      "Créé à Aix-en-Provence, plateforme mobile d'activités culturelles, atelier pour susciter et dynamiser les liens de proximité, faire circuler les mots et les échanges."
    ],
    "nl": [
      "Gemaakt in Aix-en-Provence, een mobiel platform voor culturele activiteiten, een werkplaats om lokale banden aan te moedigen en te stimuleren, om woorden en uitwisselingen te laten circuleren."
    ],
    "es": [
      "Creada en Aix-en-Provence, una plataforma móvil de actividades culturales, un taller para fomentar y dinamizar los vínculos locales, hacer circular las palabras y los intercambios."
    ]
  },
