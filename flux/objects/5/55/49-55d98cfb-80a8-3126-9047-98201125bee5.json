  "rdfs:comment": {
    "de": [
      "Gleich neben Le Lude empfängt Sie Baptiste, um seinen Ziegenbetrieb zu entdecken."
    ],
    "en": [
      "Just next to Le Lude, Baptiste welcomes you to discover his goat farm."
    ],
    "it": [
      "Appena fuori Le Lude, Baptiste vi accoglie per farvi scoprire il suo allevamento di capre."
    ],
    "fr": [
      "Juste à côté du Lude, Baptiste vous accueille pour découvrir son exploitation caprine."
    ],
    "nl": [
      "Net buiten Le Lude heet Baptiste u welkom op zijn geitenboerderij."
    ],
    "es": [
      "A las afueras de Le Lude, Baptiste le invita a descubrir su granja de cabras."
    ]
  },
