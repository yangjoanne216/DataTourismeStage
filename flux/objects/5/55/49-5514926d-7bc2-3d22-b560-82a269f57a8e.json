  "rdfs:comment": {
    "de": [
      "Le Logis de la Chouette liegt auf dem Land am Rande des Waldes von Chandelais, 5 Minuten von Baugé und 25 Minuten von La Flèche und Saumur entfernt."
    ],
    "en": [
      "Le Logis de la Chouette is in the countryside on the edge of the Chandelais forest, 5 minutes from Baugé and 25 minutes from La Flèche and Saumur."
    ],
    "it": [
      "Il Logis de la Chouette si trova in campagna, ai margini della foresta di Chandelais, a 5 minuti da Baugé e a 25 minuti da La Flèche e Saumur."
    ],
    "fr": [
      "Le Logis de la Chouette est en pleine campagne au bord de la forêt de Chandelais à 5 mn de Baugé et 25 mn de la Flèche et de Saumur."
    ],
    "nl": [
      "Het Logis de la Chouette ligt op het platteland aan de rand van het Chandelais-bos, op 5 minuten van Baugé en op 25 minuten van La Flèche en Saumur."
    ],
    "es": [
      "El Logis de la Chouette está situado en el campo, al borde del bosque de Chandelais, a 5 minutos de Baugé y a 25 minutos de La Flèche y Saumur."
    ]
  },
