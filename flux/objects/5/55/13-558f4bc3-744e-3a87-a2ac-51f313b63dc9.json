  "rdfs:comment": {
    "de": [
      "Montgolfière Sensation Auvergne Rhône-Alpes ändert seinen Namen in AURA MONTGOLFIERE! Bewundern Sie die Teiche der Dombes von Châtillon-sur-Chalaronne aus mit einem Heißluftballon."
    ],
    "en": [
      "Montgolfière Sensation Auvergne Rhône-Alpes changes its name and becomes AURA MONTGOLFIERE ! Admire the ponds of the Dombes, from Châtillon-sur-Chalaronne, in a hot air balloon."
    ],
    "it": [
      "Montgolfière Sensation Auvergne Rhône-Alpes cambia nome e diventa AURA MONTGOLFIERE ! Ammirate gli stagni delle Dombes, da Châtillon-sur-Chalaronne, in mongolfiera."
    ],
    "fr": [
      "Montgolfière Sensation Auvergne Rhône-Alpes change de nom et devient AURA MONTGOLFIERE ! Admirez les étangs de la Dombes, au départ de Châtillon-sur-Chalaronne, en montgolfière."
    ],
    "es": [
      "¡Montgolfière Sensation Auvergne Rhône-Alpes cambia de nombre y se convierte en AURA MONTGOLFIERE ! Admire los estanques de las Dombes, desde Châtillon-sur-Chalaronne, en globo aerostático."
    ],
    "nl": [
      "Montgolfière Sensation Auvergne Rhône-Alpes verandert van naam en wordt AURA MONTGOLFIERE ! Bewonder de vijvers van de Dombes, vanuit Châtillon-sur-Chalaronne, in een heteluchtballon."
    ]
  },
