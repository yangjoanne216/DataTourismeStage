  "rdfs:comment": {
    "de": [
      "Bastides, Dorfhäuser, verschiedene Modelle von Hütten, Bauernhäuser, Ställe, Brunnen, Taubenschlag, Fischerhütte, Jagdhütte, Waschhäuser, Bastide, Ölmühle. Keine Krippenfiguren."
    ],
    "en": [
      "Bastides, village houses, different models of sheds, farmhouses, stables, wells, dovecote, fishermen's hut, hunting station, wash houses, bastide, oil mill. No santons."
    ],
    "it": [
      "Bastides, case di paese, diversi tipi di cottage, case coloniche, stalle, pozzi, colombaie, capanne di pescatori, postazioni di caccia, lavatoi, bastide, frantoi. Niente santoni."
    ],
    "fr": [
      "Bastides, maisons de villages, différentes modèles de cabanons, mas, étables, puits, pigeonnier, cabane de pêcheurs, poste de chasse, lavoirs, bastide, moulin a huile. Pas de santons."
    ],
    "nl": [
      "Bastides, dorpshuizen, verschillende soorten huisjes, boerderijen, stallen, waterputten, duiventillen, vissershutten, jachtposten, washuizen, bastides, oliemolens. Geen santen."
    ],
    "es": [
      "Bastidas, casas de pueblo, diferentes tipos de casas de campo, granjas, establos, pozos, palomares, cabañas de pescadores, puestos de caza, lavaderos, bastidas, molinos de aceite. No hay santones."
    ]
  },
