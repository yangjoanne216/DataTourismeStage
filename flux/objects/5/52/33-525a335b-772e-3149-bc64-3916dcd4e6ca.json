  "rdfs:comment": {
    "de": [
      "Der Golfclub von Juillac liegt in einem außergewöhnlichen historischen Anwesen vor den Toren von Toulouse im Herzen des Lauragais."
    ],
    "en": [
      "The Juillac Golf Club is located in an exceptional historical estate, at the gates of Toulouse, in the heart of the Lauragais region."
    ],
    "it": [
      "Il Golf Club Juillac è situato in un'eccezionale tenuta storica, alle porte di Tolosa, nel cuore della regione del Lauragais."
    ],
    "fr": [
      "Le Golf Club de Juillac est situé dans un domaine historique exceptionnel, aux portes de Toulouse, au coeur du Lauragais."
    ],
    "es": [
      "El club de golf de Juillac está situado en una finca histórica excepcional, a las puertas de Toulouse, en el corazón de la región de Lauragais."
    ],
    "nl": [
      "De Juillac Golf Club is gelegen in een uitzonderlijk historisch domein, aan de poorten van Toulouse, in het hart van de Lauragais streek."
    ]
  },
