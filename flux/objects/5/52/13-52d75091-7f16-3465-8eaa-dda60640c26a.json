  "rdfs:comment": {
    "de": [
      "Mountainbike-Zentrum Espace Revermont mit dem Gütesiegel der Fédération Française de Cyclotourisme (FFVélo).\r\nTreffpunkt im Herzen der Berge des Revermont.\r\n8 Rundwege, 2 markierte Verbindungen von 4 bis 32 km."
    ],
    "en": [
      "Mountain bike base Espace Revermont labeled French Federation of Cycling (FFVélo).\r\nRendez-vous, in the heart of the Revermont mountains.\r\n8 circuits, 2 marked routes from 4 to 32 km."
    ],
    "it": [
      "Base per mountain bike Espace Revermont, certificata dalla Federazione ciclistica francese (FFVélo).\r\nRendez-vous, nel cuore delle montagne di Revermont.\r\n8 circuiti, 2 percorsi segnalati da 4 a 32 km."
    ],
    "fr": [
      "Base VTT Espace Revermont labélisée Fédération Française de Cyclotourisme (FFVélo).\r\nRendez-vous, au cœur des montagnes du Revermont.\r\n8 circuits, 2 liaisons balisées de 4 à 32 km."
    ],
    "nl": [
      "Espace Revermont mountainbike basis, gecertificeerd door de Franse wielerbond (FFVélo).\r\nRendez-vous, in het hart van de bergen van Revermont.\r\n8 circuits, 2 gemarkeerde routes van 4 tot 32 km."
    ],
    "es": [
      "Base de bicicletas de montaña Espace Revermont, certificada por la Federación Francesa de Ciclismo (FFVélo).\r\nRendez-vous, en el corazón de las montañas de Revermont.\r\n8 circuitos, 2 rutas marcadas de 4 a 32 km."
    ]
  },
  "rdfs:label": {
