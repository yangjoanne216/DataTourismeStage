  "rdfs:comment": {
    "de": [
      "Gönnen Sie sich eine ordentliche Meeresbrise und ein einzigartiges Erlebnis an Bord unserer Tender. Abfahrt vom Hafen Nizza."
    ],
    "en": [
      "Take a breath of fresh air between the sea and the mountains, live a unique experience on board tenders, departing from the port of Nice."
    ],
    "it": [
      "Prendete una boccata d'aria fresca tra il mare e la montagna, vivete un'esperienza unica a bordo dei tender, con partenza dal porto di Nizza."
    ],
    "fr": [
      "S'offrir un grand bol d’air frais entre mer et montagne, vivre une expérience unique à bord de tenders, au départ du port de Nice."
    ],
    "es": [
      "Tome un respiro entre el mar y la montaña, viva una experiencia única a bordo de los tenders, con salida desde el puerto de Niza."
    ],
    "nl": [
      "Een frisse neus halen tussen de zee en de bergen, een unieke ervaring beleven aan boord van tenders, vertrekkend vanuit de haven van Nice."
    ]
  },
