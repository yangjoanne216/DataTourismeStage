  "rdfs:comment": {
    "de": [
      "Kulturelle Freizeitaktivitäten für Erwachsene, die von der Vereinigung angeboten werden: Glasmalerei, Buchbinden, Kartonage, Buchmalerei, Polsterei aus Möbeln, Möbel aus Karton, Zeichnen, Malen, Theater."
    ],
    "en": [
      "Cultural leisure activities for adults offered by the association: stained glass, bookbinding, cardboard, illumination, upholstery, cardboard furniture, drawing, painting, theatre."
    ],
    "it": [
      "Attività culturali per il tempo libero per adulti offerte dall'associazione: vetrate, rilegatura, cartonaggio, illuminazione, tappezzeria, mobili in cartone, disegno, pittura, teatro."
    ],
    "fr": [
      "Activités de loisirs culturels pour adultes proposées par l'association : vitrail, reliure, cartonnage, enluminure, tapisserie en meubles, meubles en carton, dessin, peinture, théâtre."
    ],
    "nl": [
      "Culturele vrijetijdsactiviteiten voor volwassenen aangeboden door de vereniging: glas-in-lood, boekbinden, karton, verlichting, stoffering, kartonnen meubels, tekenen, schilderen, theater."
    ],
    "es": [
      "Actividades culturales de ocio para adultos ofrecidas por la asociación: vidrieras, encuadernación, cartonaje, iluminación, tapicería, muebles de cartón, dibujo, pintura, teatro."
    ]
  },
