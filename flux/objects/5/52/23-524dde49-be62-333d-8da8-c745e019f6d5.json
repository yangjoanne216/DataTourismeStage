  "rdfs:comment": {
    "de": [
      "Neue Formel für die berühmten Messen in Masseret. Bei jeder Ausgabe zahlreiche Händler und Handwerker: Lebensmittel, Kleidung, Kunsthandwerk, kommerzielle verschiedene Unternehmen."
    ],
    "en": [
      "Plants de fleurs et légumes - fruits - produits régionaux et artisanaux."
    ],
    "it": [
      "Nuova formula delle famose fiere di Masseret. Ad ogni edizione, numerosi commercianti e artigiani: alimentari, abbigliamento, artigianato, varie aziende commerciali."
    ],
    "fr": [
      "Nouvelle formule des fameuses foires de Masseret. A chaque édition nombreux commerçants et artisans : Alimentaire, vestimentaire, artisanat, commerciaux diverses entreprises."
    ],
    "es": [
      "Nueva fórmula de las famosas ferias de Masseret. En cada edición, numerosos comerciantes y artesanos: alimentación, ropa, artesanía, diversas empresas comerciales."
    ],
    "nl": [
      "Nieuwe formule van de beroemde kermissen van Masseret. Op elke editie, talrijke handelaren en ambachtslieden: voedsel, kleding, ambachten, diverse commerciële bedrijven."
    ]
  },
