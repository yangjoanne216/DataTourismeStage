  "rdfs:comment": {
    "de": [
      "Geräumiges Ferienhaus (90 m²) mit Terrasse, in ruhiger Lage im Herzen der Loire-Schlösser, 5 min vom Golfplatz Loches-Verneuil, 10 min von der Cité Royale de Loches und 45 min vom Zooparc de Beauval entfernt. Unterkunft mit allem Komfort, neue Ausstattung."
    ],
    "en": [
      "Spacious gite (90 m²) with terrace, quiet, in the heart of the Loire castles, 5 min from the golf course of Loches-Verneuil, 10 min from the Royal City of Loches and 45 min from the Zooparc of Beauval. Accommodation with all comfort, new equipment."
    ],
    "it": [
      "Spazioso gîte (90 m²) con terrazza, nel cuore dei castelli della Loira, a 5 minuti dal campo da golf di Loches-Verneuil, a 10 minuti dalla Città Reale di Loches e a 45 minuti dallo Zooparc di Beauval. Alloggio con tutti i comfort, attrezzature nuove."
    ],
    "fr": [
      "Gîte spacieux (90 m²) avec terrasse, au calme, au cœur des châteaux de la Loire, à 5 min du golf de Loches-Verneuil, 10 min de la Cité Royale de Loches et 45 min du Zooparc de Beauval. Hébergement tout confort, équipements neufs."
    ],
    "nl": [
      "Ruime gîte (90 m²) met terras, in het hart van de Loire kastelen, 5 min van de golfbaan Loches-Verneuil, 10 min van de Koninklijke Stad Loches en 45 min van het Zooparc van Beauval. Accommodatie met alle comfort, nieuwe apparatuur."
    ],
    "es": [
      "Amplia casa rural (90 m²) con terraza, en el corazón de los castillos del Loira, a 5 min del campo de golf de Loches-Verneuil, a 10 min de la Ciudad Real de Loches y a 45 min del Zooparc de Beauval. Alojamiento con todas las comodidades, equipamiento nuevo."
    ]
  },
