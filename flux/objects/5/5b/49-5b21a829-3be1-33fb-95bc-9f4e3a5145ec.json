  "rdfs:comment": {
    "de": [
      "Die Kirche Saint-Martin-de-Vertou liegt 15 Minuten von Angers entfernt im Herzen des blauen Anjou und wurde im 11. Jahrhundert mitten im Ortskern von Le Lion d'Angers errichtet."
    ],
    "en": [
      "Located 15 minutes from Angers, in the heart of the blue Anjou region, the church of Saint-Martin-de-Vertou was built in the 11th century in the heart of the Lion of Angers."
    ],
    "it": [
      "Situata a 15 minuti da Angers, nel cuore della regione dell'Anjou Bleu, la chiesa di Saint-Martin-de-Vertou fu costruita nell'XI secolo nel centro della città di Le Lion d'Angers."
    ],
    "fr": [
      "Située à 15 minutes d'Angers, au coeur de l'Anjou bleu, l'église Saint-Martin-de-Vertou est édifiée au 11e siècle en plein centre-bourg du Lion d'Angers."
    ],
    "es": [
      "Situada a 15 minutos de Angers, en el corazón de la región de Anjou Bleu, la iglesia de Saint-Martin-de-Vertou fue construida en el siglo XI en el centro de la ciudad de Le Lion d'Angers."
    ],
    "nl": [
      "Op 15 minuten van Angers, in het hart van de Anjou Bleu, ligt de kerk Saint-Martin-de-Vertou, gebouwd in de 11e eeuw in het centrum van de stad Le Lion d'Angers."
    ]
  },
