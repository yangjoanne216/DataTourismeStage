  "rdfs:comment": {
    "de": [
      "Restaurant mit regionaler Küche aus der Dauphiné, Savoyen: Tartiflette, Raclette,\r\nhuhn mit Flusskrebsen, Kalbskopf, Forelle... Froschschenkel am Sonntag!"
    ],
    "en": [
      "Restaurant serving local specialities from the Dauphiné and Savoie areas: tartiflette, raclette, chicken with crawfish, veal’s head, trout and on Sunday, frogs’ legs!"
    ],
    "it": [
      "Ristorante di cucina locale del Delfinato, Savoia: tartiflette, raclette,\r\npollo con gamberi, testa di vitello, trota? cosce di rana la domenica!"
    ],
    "fr": [
      "Restaurant de cuisine du terroir du Dauphiné, Savoie : tartiflette, raclette,\r\npoulet aux écrevisses, tête de veau, truite… cuisses de Grenouilles le dimanche !"
    ],
    "es": [
      "Restaurante de cocina local del Dauphiné, Savoie: tartiflette, raclette,\r\npollo con cangrejos de río, cabeza de ternera, trucha... ¡anillas de rana los domingos!"
    ],
    "nl": [
      "Restaurant met streekgerechten uit de Dauphiné, Savoie: tartiflette, raclette,\r\nkip met rivierkreeft, kalfskop, forel... kikkerbilletjes op zondag!"
    ]
  },
