  "rdfs:comment": {
    "de": [
      "La Bessayrie bietet Aufenthalte inmitten einer üppigen Natur. Ein Ziel, um sich aufzuladen und zu entspannen, um abzuschalten und neue Verbindungen zu knüpfen."
    ],
    "en": [
      "La Bessayrie offers guest stays among an abonce of untouched nature. Your destination to recharge and unwind, to disconnect and connect."
    ],
    "it": [
      "La Bessayrie offre soggiorni in mezzo a una natura esuberante. Una destinazione per ricaricarsi e rilassarsi, per staccare la spina e legarsi."
    ],
    "fr": [
      "La Bessayrie offre des séjours au milieu d'une nature exhuberante. Une destination pour vous recharger et vous relaxer, pour vous déconnecter et créer des liens."
    ],
    "es": [
      "La Bessayrie ofrece estancias en medio de una naturaleza exuberante. Un destino para recargar las pilas y relajarse, desconectar y estrechar lazos."
    ],
    "nl": [
      "La Bessayrie biedt verblijven te midden van een uitbundige natuur. Een bestemming om op te laden en te ontspannen, om de verbinding te verbreken en zich te verbinden."
    ]
  },
