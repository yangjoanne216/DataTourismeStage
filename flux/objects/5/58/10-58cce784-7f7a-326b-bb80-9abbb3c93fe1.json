  "rdfs:comment": {
    "de": [
      "Ausstellung Verkauf von Mineralien, Schmuck. Große Auswahl an Trommelsteinen. Täglich von 10 bis 12 Uhr und von 14 bis 19 Uhr geöffnet. Am Sonntagmorgen geschlossen. Freier Eintritt."
    ],
    "en": [
      "Exhibition sale of minerals, jewellery. Large choice of rolled stones. Open every day from 10 am to 12 am and from 2 pm to 7 pm. Closed on Sunday morning. Free entrance."
    ],
    "it": [
      "Esposizione e vendita di minerali, gioielli. Ampia scelta di pietre arrotolate. Aperto tutti i giorni dalle 10.00 alle 12.00 e dalle 14.00 alle 19.00. Chiuso la domenica mattina. Ingresso libero."
    ],
    "fr": [
      "Exposition vente de minéraux, bijouterie. Grand choix en pierres roulées. Ouvert tous les jours de 10h à 12 h et de 14h à 19h. Fermé le dimanche matin. Entrée libre."
    ],
    "nl": [
      "Tentoonstelling en verkoop van mineralen, juwelen. Grote selectie van gerolde stenen. Dagelijks geopend van 10 tot 12 uur en van 14 tot 19 uur. Gesloten op zondagochtend. Gratis toegang."
    ],
    "es": [
      "Exposición y venta de minerales, joyas. Gran selección de piedras rodadas. Abierto todos los días de 10 a 12 y de 14 a 19 horas. Cerrado el domingo por la mañana. La entrada es gratuita."
    ]
  },
