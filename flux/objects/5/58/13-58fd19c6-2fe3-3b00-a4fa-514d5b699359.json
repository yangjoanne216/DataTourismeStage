  "rdfs:comment": {
    "de": [
      "Paintballgelände - La Forêt im Wald angelegt\r\nBewaldetes Gelände mit Zäunen aus Holzstämmen, Löchern und Granattrichtern\r\n. Kapazität: 60 Spieler in einem Spiel\r\n. Möglichkeit, vor Ort zu picknicken."
    ],
    "en": [
      "Paintball field - The Managed Forest in the Woods\r\nWooded terrain with log fences, holes and shell craters\r\n. Capacity: 60 players in one game\r\n. Possibility to picnic on the spot."
    ],
    "it": [
      "Campo da paintball - La foresta nel bosco\r\nUn percorso nel bosco con recinzioni di tronchi, buche e crateri di conchiglie\r\n. Capacità: 60 giocatori in una partita\r\n. Possibilità di fare picnic in loco."
    ],
    "fr": [
      "Terrain de Paintball -  La Forêt aménagé dans les bois \r\nTerrain boisé avec des barrières en rondins de bois, des trous et des cratères d'obus. \r\n. Capacité : 60 joueurs dans une même partie\r\n. Possibilité de pique-niquer sur place."
    ],
    "nl": [
      "Paintballveld - Het bos in het bos\r\nEen bosbaan met houtwallen, holes en schelpkraters\r\n. Capaciteit: 60 spelers in één spel\r\n. Mogelijkheid tot picknicken ter plaatse."
    ],
    "es": [
      "Campo de paintball - El bosque en el bosque\r\nUn campo en el bosque con vallas de troncos, agujeros y cráteres de conchas\r\n. Capacidad: 60 jugadores en una partida\r\n. Posibilidad de hacer un picnic en el lugar."
    ]
  },
