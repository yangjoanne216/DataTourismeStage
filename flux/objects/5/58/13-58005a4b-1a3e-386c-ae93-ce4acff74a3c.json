  "rdfs:comment": {
    "de": [
      "Spezialitäten:\r\nFLEISCH VOM HOLZKOHLEGRILL\r\nCharolais, Entenbrust vom Bauernhof, Pata Negra, Produkte von lokalen Bauernhöfen, Käse, Bio-Lamm, Zicklein, Desserts, Gratins und Pommes frites \"Maison\".\r\nJeden Freitag frischer Fisch."
    ],
    "en": [
      "\"La Plancha\" specializes in top-quality local produce such as organic pork, Bounas lamb and fresh fish from Saint-Mandrier and around the port of Toulon. All ingredients are fresh and seasonal."
    ],
    "it": [
      "Specialità :\r\nCARNE ALLA GRIGLIA SU FUOCO DI LEGNA\r\nCharolais, magret de la ferme, pata negra, prodotti delle fattorie locali, formaggi, agnello biologico, cabri, dolci, gratin e patatine fritte fatte in casa.\r\nOgni venerdì pesce fresco."
    ],
    "fr": [
      "Spécialités :\r\nVIANDES GRILLEES AU FEU DE BOIS\r\nCharolaises, magret de la ferme, pata negra, produits de fermes locales, fromages, agneaux bio, cabri, desserts,gratins et frites \"Maison\".\r\nTous les vendredis Poissons frais."
    ],
    "nl": [
      "Specialiteiten :\r\nOP HOUTVUUR GEGRILD VLEES\r\nCharolais, magret de la ferme, pata negra, producten van lokale boerderijen, kazen, biologisch lamsvlees, cabri, desserts, gratins en huisgemaakte frites.\r\nElke vrijdag verse vis."
    ],
    "es": [
      "Especialidades :\r\nCARNES A LA BRASA DE LEÑA\r\nCharolais, magret de la ferme, pata negra, productos de granjas locales, quesos, cordero ecológico, cabri, postres, gratinados y patatas fritas caseras.\r\nTodos los viernes pescado fresco."
    ]
  },
