  "rdfs:comment": {
    "de": [
      "Fahren Sie mit Bmx, Mountainbike, Inlineskates, Scooter oder Skateboard in Pourrières auf einer 600 m² großen Piste inmitten von Weinbergen und Hügeln, zwischen Sainte-Victoire und Monts Aurélien."
    ],
    "en": [
      "Come and ride bmx, mountain bike, rollerblades, scooter or skateboard in Pourrières on a 600 m² track in the middle of vineyards and hills, between Sainte-Victoire and Monts Aurélien."
    ],
    "it": [
      "Venite a fare un giro in bmx, mountain bike, rollerblade, monopattino o skateboard a Pourrières su una pista di 600 m² in mezzo a vigneti e colline, tra Sainte-Victoire e Monts Aurélien."
    ],
    "fr": [
      "Venez rider en bmx, vtt, rollers, trottinette ou skate, à Pourrières sur une piste de 600 m² au milieu des vignes et des collines, entre Sainte-Victoire, Monts Aurélien."
    ],
    "es": [
      "Venga a montar en bmx, bicicleta de montaña, patines, scooter o monopatín en Pourrières en una pista de 600 m² en medio de viñedos y colinas, entre Sainte-Victoire y Monts Aurélien."
    ],
    "nl": [
      "Kom bmxen, mountainbiken, skeeleren, steppen of skateboarden in Pourrières op een parcours van 600 m² te midden van wijngaarden en heuvels, tussen Sainte-Victoire en Monts Aurélien."
    ]
  },
