  "rdfs:comment": {
    "de": [
      "L'Oriflamme bietet Ihnen französische Küche mit einem Mittagsangebot an allen Tagen der Woche außer Donnerstag.\r\nAm Sonntagmittag nur in der Wintersaison."
    ],
    "en": [
      "L'Oriflamme proposes French cuisine with a set lunch menu every day of the week except Thursday.\r\nOpens only Sunday lunchtime during the winter."
    ],
    "it": [
      "L'Oriflamme propone una cucina francese con menù fisso a pranzo tutti i giorni tranne il giovedì.\r\nDomenica a pranzo solo in inverno."
    ],
    "fr": [
      "L'Oriflamme vous propose une cuisine française avec une formule du midi tous les jours de la semaine sauf le jeudi.\r\nLe dimanche midi uniquement en période hivernale."
    ],
    "es": [
      "L'Oriflamme sirve cocina francesa con menú todos los días excepto el jueves.\r\nEn invierno, solo los domingos al mediodía."
    ],
    "nl": [
      "L'Oriflamme serveert Franse gerechten met een vast lunchmenu elke dag behalve donderdag.\r\nIn de winter alleen op zondagmiddag."
    ]
  },
