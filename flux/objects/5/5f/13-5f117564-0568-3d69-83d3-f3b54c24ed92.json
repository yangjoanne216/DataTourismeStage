  "rdfs:comment": {
    "de": [
      "Überdachtes Waschhaus aus Ziegelsteinen und Blaustein. Im Verhältnis zur Straße eingezogen. Mit Sandstein gepflasterter Gehweg in der Umgebung des Waschhauses. Bodendeckende Begrünung in der Umgebung. Nachweis der Existenz im Jahr 1598 und jüngste Restaurierung im Jahr 2007."
    ],
    "en": [
      "Covered washhouse in brick and blue stone. Set back from the street. Sidewalk paved with sandstone around the washhouse. Greening in ground cover at the edges. Certificate of existence in 1598 and recent restoration in 2007."
    ],
    "it": [
      "Lavatoio coperto in mattoni e pietra blu. Arretrato rispetto alla strada. Pavimentazione in pietra arenaria intorno al lavatoio. Copertura vegetale ai bordi. Certificato di esistenza nel 1598 e restauro recente nel 2007."
    ],
    "fr": [
      "Lavoir couvert en briques et pierre bleue. Encaissé par rapport à la rue. Trottoir pavé de grés aux abords du lavoir. Verdissement en couvre-sol aux abords. Attestation d'existence en 1598 et restauration récente en 2007."
    ],
    "nl": [
      "Overdekt washuis in baksteen en arduin. Achteraan de straat. Bestrating met zandsteen rond het washuis. Vergroening in bodembedekking rond de randen. Certificaat van bestaan in 1598 en recente restauratie in 2007."
    ],
    "es": [
      "Lavadero cubierto de ladrillo y piedra azul. Retirado de la calle. Pavimento de piedra arenisca alrededor del lavadero. Cubierta vegetal en los bordes. Certificado de existencia en 1598 y restauración reciente en 2007."
    ]
  },
  "rdfs:label": {
