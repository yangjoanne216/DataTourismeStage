  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/c3cf8b8c-0412-3855-86cd-2c18929626c6",
      "dc:description": {
        "de": [
          "BESCHREIBUNG\n\n\n\nKleines Landrestaurant im Herzen eines sehr schönen Dorfes aus rotem Sandstein. Ein sehr guter Empfang wird Ihnen hier bereitet."
        ],
        "en": [
          "DESCRIPTIVE\n\n\n\nSmall country restaurant, located in the heart of a beautiful village of red sandstone. A very good welcome will be reserved for you."
        ],
        "it": [
          "DESCRIZIONE\n\n\n\nPiccolo ristorante di campagna, situato nel cuore di un bellissimo villaggio di arenaria rossa. Sarete accolti con grande cordialità."
        ],
        "fr": [
          "DESCRIPTIF\n\n\n\nPetit restaurant de campagne, situé au cœur d'un très beau village de grès rouge. Un très bon accueil vous y sera réservé."
        ],
        "es": [
          "DESCRIPCIÓN\n\n\n\nPequeño restaurante rural, situado en el corazón de un bonito pueblo de arenisca roja. Le daremos una cálida bienvenida."
        ],
        "nl": [
          "BESCHRIJVING\n\n\n\nKlein landelijk restaurant, gelegen in het hart van een prachtig rood zandstenen dorp. U wordt hartelijk ontvangen."
        ]
      },
