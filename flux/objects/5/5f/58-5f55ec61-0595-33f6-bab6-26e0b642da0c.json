  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/12129546-73af-337e-bd48-6beb0feb6bc7",
      "dc:description": {
        "de": [
          "Einige Spezialitäten: Ziegenkäse-Tomaten-Spinat-Bruschetta, gegrilltes Fleisch vom Holzfeuer, Penne Kalb und Zucchini mit Thymian, Lendenbraten, Ratte aus Touquet und Mesclun, Ravioli mit Basilikum, Risotto Marseillais..."
        ],
        "en": [
          "Some specialities: goat's cheese, tomato and spinach bruschetta, grilled meat over a wood fire, veal penne and courgettes with thyme, sirloin steak, rattes du touquet and mesclun, bush and basil ravioli, Marseille risotto..."
        ],
        "it": [
          "Alcune specialità: bruschetta con formaggio di capra, pomodoro e spinaci, carne alla griglia, penne di vitello e zucchine al timo, bistecca di controfiletto, rattes du touquet e mesclun, ravioli al basilico, risotto alla marsigliese..."
        ],
        "fr": [
          "Quelques spécialités : bruschetta chèvre-tomate-pousse d'épinards, grillades de viande au feu de bois, penne veau et courgettes au thym, bavette d'aloyau, rattes du touquet et mesclun, raviolis brousse- basilic, risotto marseillais..."
        ],
        "es": [
          "Especialidades: bruschetta de queso de cabra, tomate y espinacas, carne a la brasa, penne de ternera y calabacín al tomillo, solomillo, rattes du touquet y mesclun, raviolis de albahaca, risotto marsellés..."
        ],
        "nl": [
          "Enkele specialiteiten: bruschetta met geitenkaas, tomaat en spinazie, gegrild vlees, penne van kalfsvlees en courgette met tijm, entrecote, rattes du touquet en mesclun, ravioli met basilicum, risotto marseillais..."
        ]
      },
