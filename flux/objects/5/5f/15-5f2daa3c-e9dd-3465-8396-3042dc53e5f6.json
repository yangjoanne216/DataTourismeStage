  "rdfs:comment": {
    "de": [
      "Verbringen Sie mit Ihrer Familie einen Moment bei den Ziegen auf der Ferme d'Ozance in Arpheuilles im Departement Indre."
    ],
    "en": [
      "Come and spend some family time with the goats at the Ferme d'Ozance in Arpheuilles in the Indre department."
    ],
    "it": [
      "Venite a trascorrere un po' di tempo in famiglia con le capre della Ferme d'Ozance ad Arpheuilles, nel dipartimento dell'Indre."
    ],
    "fr": [
      "Venez passer un moment en famille avec les chèvres à la Ferme d’Ozance à Arpheuilles dans le département de l'Indre."
    ],
