  "rdfs:comment": {
    "de": [
      "Metzgerei-Seite: Limousin-Rind mit rotem Label, Lammfleisch aus der Region, hausgemachte Wurstwaren...\r\nBrotseite: Brot, Konditoreiwaren, Gebäck, Kuchen und Spezialitäten aus dem Refuge...\r\nSelbstbedienungsseite: regionale Produkte und Weinkeller."
    ],
    "en": [
      "Butcher's : Label Rouge Limousin beef, local lamb, homemade charcuterie\r\nBakery : bread, pastries, viennoiseries, pies and Refuge specialties...\r\nSelf-service : local products and wine cellar"
    ],
    "it": [
      "Macelleria: manzo di razza Limousin, agnello locale, salumi di produzione propria...\r\nPane: pane, pasticceria, dolci, torte e specialità del Rifugio...\r\nSelf-service: prodotti regionali e cantina."
    ],
    "fr": [
      "Côté boucherie : Boeuf Limousin label rouge, agneaux de pays, charcuterie maison..\r\nCôté pain : pain, pâtisserie, viennoiserie, tarte et spécialités du Refuge..\r\nCôté libre service : produits régionaux et cave à vin."
    ],
    "nl": [
      "Vlees: Limousin rundvlees met een rood keurmerk, lamsvlees de pays, huisgemaakte vleeswaren, etc.\r\nBrood: stokbrood, gebak, viennoiserie, taart en specialiteiten uit de Refuge...\r\nZelbedieningsservice: regionale producten en wijnkelder."
    ],
    "es": [
      "Carnicería: ternera de etiqueta roja de Limousin, cordero local, embutidos caseros...\r\nPan: pan, bollería, pasteles y especialidades del Refugio...\r\nAutoservicio: productos regionales y bodega."
    ]
  },
