    "de": [
      "Bettwäsche und Handtücher bei 60 °C gewaschen. Gründliche Reinigung, Desinfektion der Kontaktzonen."
    ],
    "en": [
      "Sheets and towels washed at 60°C. Thorough cleaning, disinfection of contact areas."
    ],
    "it": [
      "Lenzuola e asciugamani lavati a 60°C. Pulizia accurata, disinfezione delle aree di contatto."
    ],
    "fr": [
      "Draps et serviettes lavées à 60 °C. Ménage approfondi, désinfection des zones de contact."
    ],
    "es": [
      "Sábanas y toallas lavadas a 60°C. Limpieza a fondo, desinfección de las zonas de contacto."
    ],
    "nl": [
      "Lakens en handdoeken gewassen op 60°C. Grondige reiniging, desinfectie van de contactzones."
    ]
  },
