  "rdfs:comment": {
    "de": [
      "Guinguette-Restaurant, das Mittag- und Abendessen anbietet: Hauptgerichte, Burger, Desserts oder Aperitifbretter.\r\nLokales Fleisch vom Bauernhof \"La combe des sources\", der weniger als 5 km vom Restaurant entfernt ist."
    ],
    "en": [
      "Restaurant guinguette which proposes lunches and dinners: dishes, burgers, desserts or aperitif boards.\r\nLocal meat from the farm \"La combe des sources\" less than 5 km from the restaurant."
    ],
    "it": [
      "Ristorante guinguette che propone pranzi e cene: piatti, hamburger, dessert o taglieri di aperitivi.\r\nCarne locale proveniente dalla fattoria \"La combe des sources\" a meno di 5 km dal ristorante."
    ],
    "fr": [
      "Restaurant guinguette qui propose des déjeuners et des dîners : plats, burgers, desserts ou planches apéritives.\r\nViande locale de la ferme \"La combe des sources\" à moins de 5 km du restaurant."
    ],
    "es": [
      "Restaurante guinguette que propone comidas y cenas: platos, hamburguesas, postres o tablas de aperitivos.\r\nCarne local de la granja \"La combe des sources\" a menos de 5 km del restaurante."
    ],
    "nl": [
      "Restaurant guinguette dat lunches en diners aanbiedt: schotels, hamburgers, desserts of aperitiefplanken.\r\nLokaal vlees van de boerderij \"La combe des sources\" op minder dan 5 km van het restaurant."
    ]
  },
