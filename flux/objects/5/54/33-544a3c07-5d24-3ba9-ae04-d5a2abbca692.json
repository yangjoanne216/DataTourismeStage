  "rdfs:comment": {
    "de": [
      "Schönes Landhaus in einem alten restaurierten Bauernhof mit einzigartigem Blick auf die Pyrenäen, sehr gemütlich und ruhig"
    ],
    "en": [
      "Lovely country gîte in an old restored farmhouse with a unique view of the Pyrenees; very welcoming, peace and quiet guaranteed!"
    ],
    "it": [
      "Bel gîte di campagna in un'antica casa colonica ristrutturata con una vista unica sui Pirenei; molto accogliente, pace e tranquillità garantite!"
    ],
    "fr": [
      "Joli gîte de campagne dans une ancienne ferme restaurée avec vue unique sur la chaîne des Pyrénées; très accueillant, tranquillité assurée!"
    ],
    "nl": [
      "Mooie landelijke gîte in een oude gerestaureerde boerderij met een uniek uitzicht op de Pyreneeën; zeer gastvrij, rust gegarandeerd!"
    ],
    "es": [
      "Bonita casa rural en una antigua granja restaurada con una vista única de los Pirineos; muy acogedora, ¡paz y tranquilidad garantizadas!"
    ]
  },
