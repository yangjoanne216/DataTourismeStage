  "rdfs:comment": {
    "de": [
      "Restaurant im Herzen von Eguisheim. Gemütlicher Weinkeller, kleine Karte mit frischen Produkten, die sich je nach Jahreszeit ändert\nConstance und Valentin freuen sich auf Ihren Besuch."
    ],
    "en": [
      "Restaurant located in the heart of Eguisheim. Warm cellar, reduced menu based on fresh products which evolves with the seasons\nConstance and Valentin will be pleased to welcome you."
    ],
    "it": [
      "Ristorante situato nel cuore di Eguisheim. Cantina calda, piccolo menu a base di prodotti freschi che si evolve con le stagioni\nConstance e Valentin saranno lieti di accogliervi."
    ],
    "fr": [
      "Restaurant situé au cœur d' Eguisheim. Caveau chaleureux, carte réduite à base de produits frais qui évolue au fil des saisons. \nConstance et Valentin se feront un plaisir de vous accueillir."
    ],
    "es": [
      "Restaurante situado en el corazón de Eguisheim. Cálida bodega, pequeña carta basada en productos frescos que evoluciona con las estaciones\nConstance y Valentin estarán encantados de recibirle."
    ],
    "nl": [
      "Restaurant gelegen in het hart van Eguisheim. Warme kelder, kleine kaart op basis van verse producten die evolueert met de seizoenen\nConstance en Valentin heten u van harte welkom."
    ]
  },
