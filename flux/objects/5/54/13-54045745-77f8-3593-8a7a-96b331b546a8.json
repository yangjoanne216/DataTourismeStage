  "rdfs:comment": {
    "de": [
      "Bar Brasserie im Stadtzentrum von Aurillac. Gemütliche Brasserie, herzlicher Empfang und Ambiente aus Ziegelsteinen und Holz. Menü von 11,50€ bis 12,50€. Karte von 9€ bis 16€. Kindermenü: 7€."
    ],
    "en": [
      "Bar-brewery located in Aurillac town centre. Cozy brasserie, warm welcome and atmosphere of stone, brick and wood. Menu from 11,50€ to 12,50€. Menu from 9€ to 16€. Children's menu: 7€."
    ],
    "it": [
      "Bar e brasserie situato nel centro di Aurillac. Accogliente brasserie con un'accoglienza calorosa e un'atmosfera di mattoni e legno. Menu dalle 11.50 alle 12.50? Menu da 9? a 16? Menu per bambini: 7?"
    ],
    "fr": [
      "Bar brasserie situé en centre ville d'Aurillac. Brasserie cosy, accueil chaleureux et ambiance pierres briques et bois. Menu de 11,50€ à 12,50€. Carte de 9€ à 16€. Menu enfant : 7€."
    ],
    "es": [
      "Bar y brasserie situados en el centro de Aurillac. Acogedora brasserie con una cálida bienvenida y un ambiente de ladrillo y madera. Menú de 11.50? a 12.50? Menú de 9? a 16? Menú infantil: 7?"
    ],
    "nl": [
      "Bar en brasserie in het centrum van Aurillac. Gezellige brasserie met een warm onthaal en een sfeer van baksteen en hout. Menu van 11,50€ tot 12,50€. Menu van 9€ tot 16€. Kindermenu: 7€."
    ]
  },
