  "rdfs:comment": {
    "de": [
      "Von Pierrepont in der Gemeinde Lantheuil aus können Sie die aus Orival-Stein gebauten Herrenhäuser und Schlösser in Colombiers-sur-Seulles und Amblie entdecken."
    ],
    "en": [
      "From Pierrepont in Lantheuil, go and discover manors and chateaux built from Orival stone in Colombiers-sur-Seulles and Amblie."
    ],
    "it": [
      "Da Pierrepont, nel comune di Lantheuil, scoprite i manieri e i castelli in pietra d'Orival di Colombiers-sur-Seulles e Amblie."
    ],
    "fr": [
      "Au départ de Pierrepont sur la commune de Lantheuil, partez découvrir les manoirs et châteaux bâtis en pierre d'Orival sur Colombiers-sur-Seulles et Amblie."
    ],
    "nl": [
      "Ontdek vanuit Pierrepont in de gemeente Lantheuil de landhuizen en kastelen van Orival-steen in Colombiers-sur-Seulles en Amblie."
    ],
    "es": [
      "Desde Pierrepont, en la comuna de Lantheuil, descubra las casas solariegas y los castillos construidos con piedra de Orival en Colombiers-sur-Seulles y Amblie."
    ]
  },
