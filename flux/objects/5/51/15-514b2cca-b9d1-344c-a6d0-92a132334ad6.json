  "rdfs:comment": {
    "de": [
      "Zwischen Tours und Poitiers gelegen, 5 km von Descartes und Le Grand-Pressigny entfernt. Loches ist 30 km entfernt. Wir sind 1 Stunde von Beauval und dem Futuroscope sowie von Chenonceaux, Amboise oder Villandry entfernt."
    ],
    "en": [
      "Located between Tours and Poitiers, 5 km from Descartes and Grand-Pressigny. Loches is 30km away. We are 1 hour from Beauval and the Futuroscope as well as Chenonceaux, Amboise or Villandry."
    ],
    "it": [
      "Situato tra Tours e Poitiers, a 5 km da Descartes e Grand-Pressigny. Loches si trova a 30 km di distanza. Siamo a 1 ora da Beauval e dal Futuroscope, nonché da Chenonceaux, Amboise o Villandry."
    ],
    "fr": [
      "Situé entre Tours et Poitiers,  à 5 km de Descartes et du Grand-Pressigny. Loches est à 30km. Nous sommes à 1h de Beauval et du Futuroscope ainsi que de Chenonceaux, Amboise ou Villandry."
    ],
    "es": [
      "Situado entre Tours y Poitiers, a 5 km de Descartes y Grand-Pressigny. Loches está a 30 km. Estamos a 1 hora de Beauval y del Futuroscope, así como de Chenonceaux, Amboise o Villandry."
    ],
    "nl": [
      "Gelegen tussen Tours en Poitiers, op 5 km van Descartes en Grand-Pressigny. Loches ligt op 30 km afstand. Wij bevinden ons op 1 uur van Beauval en de Futuroscope, evenals van Chenonceaux, Amboise of Villandry."
    ]
  },
