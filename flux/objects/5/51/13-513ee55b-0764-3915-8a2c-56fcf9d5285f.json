  "rdfs:comment": {
    "de": [
      "In einem eleganten und gemütlichen Rahmen bietet das Restaurant die Aromen des Ain auf gepflegten Tellern an."
    ],
    "en": [
      "In an elegant and warm setting, restaurant offering the flavors of Ain on neat plates."
    ],
    "it": [
      "In un ambiente elegante e accogliente, il ristorante offre i sapori dell'Ain in piatti preparati con cura."
    ],
    "fr": [
      "Dans un cadre élégant et chaleureux, restaurant proposant les saveurs de l’Ain dans des assiettes soignées."
    ],
    "nl": [
      "In een elegant en warm kader biedt het restaurant de smaken van de Ain in zorgvuldig bereide gerechten."
    ],
    "es": [
      "En un ambiente elegante y cálido, el restaurante ofrece los sabores del Ain en platos cuidadosamente preparados."
    ]
  },
