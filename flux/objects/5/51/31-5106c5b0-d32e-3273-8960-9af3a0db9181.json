  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/88557c84-7308-3032-8a7d-06746782f12d",
      "dc:description": {
        "de": [
          "Traditionelle Küche, hausgemacht. Spezialität Kalbskopf und -leber - Mousse au Chocolat. Honig vom Erzeuger (Aubrac, Grioudas). Restaurant 10 Minuten vom Stadtzentrum von Rodez entfernt."
        ],
        "en": [
          "Traditional, homemade cuisine. Head and veal liver speciality - Chocolate mousse. Producer's honey (Aubrac, Grioudas). Restaurant at 10 minutes from Rodez town centre."
        ],
        "it": [
          "Cucina tradizionale e casalinga. Specialità testa e fegato di vitello - Mousse di cioccolato. Miele del produttore (Aubrac, Grioudas). Ristorante a 10 minuti dal centro di Rodez."
        ],
        "fr": [
          "Cuisine traditionnelle, fait maison. Spécialité tête et foie de veau - Mousse au chocolat. Miel de producteur (Aubrac, Grioudas). Restaurant à 10 minutes du centre-ville de Rodez."
        ],
        "nl": [
          "Traditionele, huisgemaakte keuken. Specialiteit kalfskop en -lever - Chocolademousse. Honing van de producent (Aubrac, Grioudas). Restaurant op 10 minuten van het centrum van Rodez."
        ],
        "es": [
          "Cocina tradicional y casera. Especialidad cabeza de ternera e hígado - Mousse de chocolate. Miel del productor (Aubrac, Grioudas). Restaurante a 10 minutos del centro de Rodez."
        ]
      },
