  "rdfs:comment": {
    "de": [
      "Unterkunft im Erdgeschoss in einem Haus mit einer weiteren Unterkunft. Kochecke (Mikrowelle, Waschmaschine, Kühl-Gefrierkombination), Wohnzimmer, Sitzecke (TV), 2 Zimmer (3 Doppelbetten), Bad. Wc. Zusätzlicher Gasherd, eingezäuntes Grundstück, Grill. BABY AN BORD."
    ],
    "en": [
      "Lodging in r.d.c. in a house with another location. Kitchen area (microwave, washing machine, fridge-freezer), living room, sitting area (TV), 2 bedrooms. (3 double beds), bathroom. Wc. Gas heating in addition, closed ground, barbecue. BABY ON BOARD."
    ],
    "it": [
      "Gite in una casa con un altro affitto. Angolo cottura (microonde, lavatrice, frigorifero-congelatore), soggiorno, zona salotto (TV), 2 camere (3 letti matrimoniali), bagno. Wc. Giardino recintato, barbecue. BAMBINO A BORDO."
    ],
    "fr": [
      "Gîte en r.d.c. dans une maison comportant une autre location. Coin cuisine (micro-ondes, l-linge, frigo-congél), séjour, coin salon (TV), 2 ch. (3 lit 2 pers), salle de bains. Wc. Ch. gaz en suppl. Terrain clos, barbecue. BEBE A BORD."
    ],
    "nl": [
      "Gite in een huis met een andere verhuur. Keukenhoek (magnetron, wasmachine, koelkast met vriesvak), woonkamer, zithoek (TV), 2 slaapkamers (3 tweepersoonsbedden), badkamer. Wc. Omheinde tuin, barbecue. BABY AAN BOORD."
    ],
    "es": [
      "Gite en una casa con otro alquiler. Zona de cocina (microondas, lavadora, frigorífico-congelador), salón, zona de estar (TV), 2 dormitorios (3 camas dobles), baño. Wc. Jardín vallado, barbacoa. BEBÉ A BORDO."
    ]
  },
