  "rdfs:comment": {
    "de": [
      "Restaurant Brasserie, 50 Meter vom Jardin de Diane und 150 Meter vom Schloss Fontainebleau entfernt. La Taverne empfängt Sie in einem gemütlichen Rahmen, um Ihnen seine raffinierte und leckere Küche zu präsentieren."
    ],
    "en": [
      "Brasserie restaurant, located 50 meters from the Jardin de Diane and 150 meters from the Château de Fontainebleau. LaTaverne welcomes you in a friendly atmosphere where you are invited to enjoy  its refined and gourmet cuisine."
    ],
    "it": [
      "Ristorante brasserie, situato a 50 metri dal Jardin de Diane e a 150 metri dal Castello di Fontainebleau. La Taverne vi accoglie in un ambiente caldo per farvi scoprire la sua cucina raffinata e gourmet."
    ],
    "fr": [
      "Restaurant brasserie, situé à 50 mètres du Jardin de Diane et à 150 mètres du Château de Fontainebleau. La Taverne vous accueille dans un cadre chaleureux afin de vous faire découvrir sa cuisine raffinée et gourmande."
    ],
    "es": [
      "Restaurante brasserie, situado a 50 metros del Jardín de Diane y a 150 metros del Castillo de Fontainebleau. La Taverne le acoge en un ambiente cálido para hacerle descubrir su cocina refinada y gourmet."
    ],
    "nl": [
      "Restaurant brasserie, gelegen op 50 meter van de Jardin de Diane en op 150 meter van het Château de Fontainebleau. La Taverne ontvangt u in een warm kader om u zijn verfijnde en gastronomische keuken te laten ontdekken."
    ]
  },
