  "rdfs:comment": {
    "de": [
      "Die Schriftstellerin Alexandra David-Neel war 1924 die erste europäische Frau, die die verbotene Stadt Lhasa in Tibet betrat."
    ],
    "en": [
      "Alexandra David-Neel, a woman of letters, was the first European woman to enter the forbidden city of Lhasa, Tibet, in 1924."
    ],
    "it": [
      "Alexandra David-Neel, una donna di lettere, è stata la prima donna europea a entrare nella Città Proibita di Lhasa, in Tibet, nel 1924."
    ],
    "fr": [
      "Alexandra David-Neel, femme de lettres, a été la première femme européenne à pénétrer dans la cité interdite de Lhassa, au Tibet, en 1924."
    ],
    "es": [
      "Alexandra David-Neel, mujer de letras, fue la primera mujer europea que entró en la Ciudad Prohibida de Lhasa (Tíbet) en 1924."
    ],
    "nl": [
      "Alexandra David-Neel, een schrijfster, was de eerste Europese vrouw die in 1924 de Verboden Stad Lhasa in Tibet binnenging."
    ]
