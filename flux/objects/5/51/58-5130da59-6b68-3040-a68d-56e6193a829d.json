  "rdfs:comment": {
    "de": [
      "Dieses Wohnheim liegt in der Nähe des Stadtzentrums und der wissenschaftlichen und technischen Fakultät Montperrin. Es bietet insgesamt 71 Betten mit 27 Zimmern, 43 Studios und 1 T1 bis."
    ],
    "en": [
      "Located near the city centre and the Montperrin Faculty of Science and Technology, this residence offers a total of 71 beds with 27 rooms, 43 studios and 1 T1 bis."
    ],
    "it": [
      "Situato vicino al centro città e alla facoltà di scienze e tecnologia di Montperrin, questo residence offre un totale di 71 posti letto con 27 camere, 43 monolocali e 1 T1 bis."
    ],
    "fr": [
      "Située à proximité du centre-ville et de la faculté des sciences et techniques de Montperrin, cette résidence propose 71 lits au total avec 27 chambres, 43 studios et 1 T1 bis."
    ],
    "es": [
      "Situada cerca del centro de la ciudad y de la facultad de ciencias y tecnología de Montperrin, esta residencia ofrece un total de 71 plazas con 27 habitaciones, 43 estudios y 1 T1 bis."
    ],
    "nl": [
      "Deze residentie ligt vlakbij het stadscentrum en de wetenschappelijke en technologische faculteit van Montperrin en biedt in totaal 71 bedden met 27 kamers, 43 studio's en 1 T1 bis."
    ]
  },
