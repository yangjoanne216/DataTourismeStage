  "rdfs:comment": {
    "de": [
      "Bierbar, in der man sich mit Freunden auf Wurstplatten trifft.  Konzert jeden Freitagabend."
    ],
    "en": [
      "Beer bar where you meet up with friends over charcuterie platters. Concert every Friday evening."
    ],
    "it": [
      "Beer bar dove incontrarsi con gli amici davanti a un piatto di salumi.  Musica dal vivo ogni venerdì sera."
    ],
    "fr": [
      "Bar à bières où on se retrouve entre amis autour de plateaux de charcuterie.  Concert tous les vendredis soirs."
    ],
    "es": [
      "Cervecería donde te encuentras con amigos para disfrutar de platos de charcutería. Concierto todos los viernes por la noche."
    ],
    "nl": [
