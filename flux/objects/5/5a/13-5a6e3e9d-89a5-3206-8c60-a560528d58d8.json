  "rdfs:comment": {
    "de": [
      "Willkommen in La Danse des papillons, zwischen Lavendel, Olivenbäumen, Eichen und Linden, in der Drôme Provençale. Karine empfängt Sie für einen Aufenthalt voller Ruhe und Gelassenheit."
    ],
    "en": [
      "Welcome to the \"La Danse des papillions\" lodge. It is located in Drôme Provençale between lavender fields, olive trees, oak trees and linden trees. Karine will welcome you for a calm and peaceful stay."
    ],
    "it": [
      "Benvenuti a La Danse des papillons, tra lavanda, ulivi, querce e tigli, nella Drôme Provençale. Karine vi accoglie per un soggiorno di calma e serenità."
    ],
    "fr": [
      "Bienvenue à la Danse des papillons, entre lavandes, oliviers, chênes et tilleuls, en Drôme Provençale. Karine vous accueille pour un séjour de calme  et sérénité."
    ],
    "nl": [
      "Welkom in La Danse des papillons, tussen lavendel, olijfbomen, eiken en linden, in de Drôme Provençale. Karine verwelkomt u voor een verblijf van rust en sereniteit."
    ],
    "es": [
      "Bienvenido a La Danse des papillons, entre lavanda, olivos, robles y tilos, en la Drôme provenzal. Karine le da la bienvenida para una estancia de calma y serenidad."
    ]
  },
