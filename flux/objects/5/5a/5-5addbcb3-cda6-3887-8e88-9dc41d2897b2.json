      "dc:description": {
        "de": [
          "Voll ausgestattetes Chalet - 1 Schlafzimmer (1 Doppelbett) - 1 Schlafcouch für 2 Personen - Kochecke - Mikrowelle - Backofen - Esszimmer - Fernseher - Badezimmer (Dusche) - WC - elektrische Heizung - Gartenmöbel - Parkplatz - Grill - Terrasse - Pelletofen - Babybett zur Verfügung - Liegestühle."
        ],
        "en": [
          "Fully equipped chalet - 1 bedroom (1 double bed) - 1 double sofa bed - kitchen area - microwave - oven - dining room - TV - shower room - WC - electric heating - garden furniture - car park - barbecue - terrace - pellet stove - baby bed available - deckchairs."
        ],
        "it": [
          "Chalet completamente attrezzato - 1 camera da letto (1 letto matrimoniale) - 1 divano letto matrimoniale - angolo cottura - microonde - forno - sala da pranzo - TV - bagno con doccia - WC - riscaldamento elettrico - mobili da giardino - parcheggio - barbecue - terrazza - stufa a pellet - culla disponibile - lettini."
        ],
        "fr": [
          "Chalet tout équipé - 1 chambre (1 lit 2 pers) - 1 convertible 2 pers - coin cuisine - micro ondes - four - salle à manger - TV - salle d'eau (douche) - WC - chauffage électrique - salon de jardin - parking - barbecue - terrasse - poêle à granulés - lit bébé à disposition - chaises longues."
        ],
        "nl": [
          "Volledig ingericht chalet - 1 slaapkamer (1 tweepersoonsbed) - 1 tweepersoons slaapbank - kitchenette - magnetron - oven - eetkamer - TV - doucheruimte - WC - elektrische verwarming - tuinmeubilair - parkeerplaats - barbecue - terras - pelletkachel - kinderbedje beschikbaar - ligstoelen."
        ],
        "es": [
          "Chalet totalmente equipado - 1 dormitorio (1 cama doble) - 1 sofá cama doble - cocina americana - microondas - horno - comedor - TV - cuarto de ducha - WC - calefacción eléctrica - muebles de jardín - aparcamiento - barbacoa - terraza - estufa de pellets - cuna disponible - tumbonas."
        ]
      },
