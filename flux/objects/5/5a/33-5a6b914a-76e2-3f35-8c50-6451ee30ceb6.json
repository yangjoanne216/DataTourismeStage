  "rdfs:comment": {
    "de": [
      "Probieren Sie Cidre aus dem Südwesten Frankreichs inmitten eines neuen Obstgartens, der auf Bio umgestellt wird."
    ],
    "en": [
      "Come and taste cider from the Southwest in the heart of a new orchard in full organic conversion."
    ],
    "it": [
      "Venite a degustare il sidro del sud-ovest della Francia nel cuore di un nuovo frutteto in piena conversione biologica."
    ],
    "fr": [
      "Venez goûter du cidre du Sud-Ouest au cœur d'un nouveau verger en pleine conversion bio."
    ],
    "es": [
      "Venga a probar la sidra del suroeste de Francia en el corazón de un nuevo huerto en plena reconversión ecológica."
    ],
    "nl": [
      "Kom en proef cider uit het zuidwesten van Frankrijk in het hart van een nieuwe boomgaard in volledige biologische conversie."
    ]
  },
