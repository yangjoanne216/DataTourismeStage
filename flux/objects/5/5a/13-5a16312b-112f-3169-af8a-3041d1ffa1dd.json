  "rdfs:comment": {
    "de": [
      "Loïc, Artisan Glacier, kocht für Sie Gourmet-Eis und fruchtige Sorbets, 100% BIO, in seinem kleinen handwerklichen Atelier im Herzen der Drôme, im Dorf Piégros-La-Clastre."
    ],
    "en": [
      "Loïc, Artisan Glacier, cooks for you gourmet ice creams and fruity sorbets, 100% ORGANIC, in his small workshop, in the heart of the Drôme, in the village of Piégros-La-Clastre."
    ],
    "it": [
      "Loïc, Artisan Glacier, prepara per voi gelati gourmet e sorbetti fruttati, 100% BIOLOGICI, nel suo piccolo laboratorio artigianale, nel cuore della Drôme, nel villaggio di Piégros-La-Clastre."
    ],
    "fr": [
      "Loïc, Artisan Glacier, cuisine pour vous des glaces gourmandes et des sorbets fruités, 100% BIO, dans son petit atelier artisanal, au coeur de la Drôme, dans le village de Piégros-La-Clastre."
    ],
    "nl": [
      "Loïc, Artisan Glacier, bereidt voor u gastronomische ijsjes en fruitige sorbets, 100% ORGANISCH, in zijn kleine ambachtelijke werkplaats, in het hart van de Drôme, in het dorp Piégros-La-Clastre."
    ],
    "es": [
      "Loïc, Artesano Glaciar, cocina para usted helados gourmet y sorbetes afrutados, 100% BIO, en su pequeño taller artesanal, en el corazón de la Drôme, en el pueblo de Piégros-La-Clastre."
    ]
  },
