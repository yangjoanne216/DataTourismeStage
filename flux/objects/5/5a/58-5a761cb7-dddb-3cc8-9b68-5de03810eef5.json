  "rdfs:comment": {
    "de": [
      "Verkauf von Bio-Produkten, Bio-Obst und -Gemüse sowie Obst und Gemüse aus konventionellem Anbau, Bio-Kosmetik und Hygieneprodukte, Bio-Bäckerei."
    ],
    "en": [
      "Sales organic products, organic fruits and vegetables, organic cosmetics and hygiene products, organic bakery."
    ],
    "it": [
      "Vendita di prodotti biologici, frutta e verdura da agricoltura biologica e convenzionale, cosmetici e prodotti per l'igiene biologici, prodotti da forno biologici."
    ],
    "fr": [
      "Vente de produits Bio, Fruits et Légumes Bio et de culture conventionnelle, Cosmétique Bio et Produit d'hygiène, Boulangerie Bio."
    ],
    "nl": [
