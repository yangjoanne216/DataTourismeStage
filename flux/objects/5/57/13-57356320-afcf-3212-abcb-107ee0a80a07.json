  "rdfs:comment": {
    "de": [
      "Mond'Arverne Tourisme, das gemeindeübergreifende Fremdenverkehrsamt mit Sitz am Ufer des Lac d'Aydat, betreibt außerdem drei saisonale Informationsstellen in der Region."
    ],
    "en": [
      "Mond'Arverne Tourisme, the Intercommunity Tourist Office whose headquarters are located on the shores of Lake Aydat, also manages three seasonal information points on the territory."
    ],
    "it": [
      "Mond'Arverne Tourisme, l'ufficio turistico intercomunale con sede sulle rive del lago Aydat, gestisce anche tre punti di informazione stagionali nella zona."
    ],
    "fr": [
      "Mond'Arverne Tourisme, Office de Tourisme Intercommunautaire dont le siège se situe au bord du lac d'Aydat, gère également trois points d'informations saisonniers sur le territoire."
    ],
    "es": [
      "Mond'Arverne Tourisme, la Oficina de Turismo Intercomunitaria cuya sede se encuentra a orillas del lago Aydat, gestiona también tres puntos de información estacional en la zona."
    ],
    "nl": [
      "Mond'Arverne Tourisme, het Intercommunaal Bureau voor Toerisme waarvan de zetel aan de oevers van het Aydat-meer ligt, beheert ook drie seizoensgebonden informatiepunten in het gebied."
    ]
  },
