  "rdfs:comment": {
    "de": [
      "Neue Feinschmeckeradresse in Les Goudes! Die Gelateria des Goudes (Zweigstelle von Les Glaciers Marseillais) bietet hausgemachtes Eis, das in der Escale Borely von Jérôme Cellier, einem Meister des Eishandwerks, hergestellt wird."
    ],
    "en": [
      "New gourmet address in Les Goudes! The Gelateria des Goudes (annex of Glaciers Marseillais) offers home-made ice creams, made at Escale Borely by Jérôme Cellier, master ice cream maker."
    ],
    "it": [
      "Nuovo indirizzo gastronomico a Les Goudes! La Gelateria des Goudes (annessa ai Glaciers Marseillais) offre gelati artigianali, prodotti all'Escale Borely da Jérôme Cellier, maestro gelatiere."
    ],
    "fr": [
      "Nouvelle adresse gourmande aux Goudes ! La Gelateria des Goudes (annexe des Glaciers Marseillais) propose des glaces faites maison, fabriquées à l’Escale Borely par Jérôme Cellier, maître artisan glacier."
    ],
    "es": [
      "¡Nueva dirección gastronómica en Les Goudes! La Gelateria des Goudes (anexo de Glaciers Marseillais) ofrece helados artesanales, elaborados en la Escale Borely por Jérôme Cellier, maestro heladero."
    ],
    "nl": [
      "Nieuw gastronomisch adres in Les Goudes! De Gelateria des Goudes (dependance van Glaciers Marseillais) biedt huisgemaakt ijs, gemaakt in Escale Borely door Jérôme Cellier, meester-ijsbereider."
    ]
  },
