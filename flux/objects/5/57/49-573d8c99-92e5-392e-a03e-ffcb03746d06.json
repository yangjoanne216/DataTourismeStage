  "rdfs:comment": {
    "de": [
      "Dieses große Gebäude wird in Winternächten durch die Schönheit seiner Buntglasfenster ins rechte Licht gerückt."
    ],
    "en": [
      "This great building is illuminated by the beauty of its stained glass windows on winter nights."
    ],
    "it": [
      "La bellezza di questo grande edificio è messa in risalto dalle vetrate nelle notti d'inverno."
    ],
    "fr": [
      "Ce grand édifice est mis en lumière par la beauté de ses vitraux les nuits d'hiver."
    ],
    "es": [
      "La belleza de este gran edificio queda resaltada por la belleza de sus vidrieras en las noches de invierno."
    ],
    "nl": [
      "De schoonheid van dit grote gebouw wordt benadrukt door de schoonheid van de gebrandschilderde ramen op winternachten."
    ]
  },
