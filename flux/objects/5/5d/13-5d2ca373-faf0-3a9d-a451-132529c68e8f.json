  "rdfs:comment": {
    "de": [
      "Patricia Bourny ist Kunsthandwerkerin und diplomierte Innenarchitektin. Sie berät in Sachen Dekoration und fertigt Objekte und Schmuck aus Harz, einem Material mit besonderen Eigenschaften, an."
    ],
    "en": [
      "Craftswoman and graduate in interior design, Patricia Bourny provides decorating advice and makes resin objects, a material with special properties."
    ],
    "it": [
      "Patricia Bourny è un'artista e decoratrice d'interni che dà consigli sulla decorazione e realizza oggetti e gioielli in resina, un materiale dalle proprietà speciali."
    ],
    "fr": [
      "Artisan d'art et diplômée en décoration d'intérieur, Patricia Bourny prodigue conseils en décoration et confectionne des objets et bijoux en résine, une matière aux propriétés particulières."
    ],
    "nl": [
      "Patricia Bourny is een kunstenares en binnenhuisarchitecte die advies geeft over decoratie en objecten en sieraden maakt van hars, een materiaal met bijzondere eigenschappen."
    ],
    "es": [
      "Patricia Bourny es una artista e interiorista que asesora en decoración y fabrica objetos y joyas de resina, un material con propiedades especiales."
    ]
  },
