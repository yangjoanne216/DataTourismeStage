  "rdfs:comment": {
    "de": [
      "Minigolf mit 18 Bahnen, schattiger Parcours umgeben von Grün, Bänke, klimatisiertes Schwimmbad, Campingplatz, Spielplatz, Tennisplätze in der Nähe, sowie Paddle und City-Stadion."
    ],
    "en": [
      "Mini-golf with 18 tracks, shaded course surrounded by greenery, benches, air-conditioned swimming pool, camping, playground, tennis courts nearby, as well as paddle and city stadium."
    ],
    "it": [
      "Mini-golf con 18 piste, percorso ombreggiato e circondato dal verde, panchine, piscina climatizzata, campeggio, parco giochi, campi da tennis nelle vicinanze, nonché paddle e stadio cittadino."
    ],
    "fr": [
      "Mini-golf composé de 18 pistes, parcours ombragé entouré de verdure, bancs, piscine climatisée, camping, aire de jeux, terrains de tennis à proximité, ainsi que paddle et city stade."
    ],
    "nl": [
      "Minigolf met 18 banen, schaduwrijke baan omgeven door groen, bankjes, geklimatiseerd zwembad, camping, speeltuin, tennisbanen in de buurt, evenals paddle- en stadsstadion."
    ],
    "es": [
      "Minigolf con 18 pistas, campo sombreado rodeado de vegetación, bancos, piscina climatizada, camping, parque infantil, pistas de tenis en las inmediaciones, así como de pádel y estadio municipal."
    ]
