  "rdfs:comment": {
    "de": [
      "Samstag, 24. Juni 2023 - 15:00 und 18:00 Uhr\r\nAuditorium Cité de la musique, Romans\r\n\r\nBegleitet von einer Jazz-Combo singen und schauspielern die CHAM-Schülerinnen und -Schüler, um Ihnen eine originelle musikalische Version der Geschichte von Pinocchio zu präsentieren."
    ],
    "en": [
      "Saturday, June 24, 2023 - 3:00 pm and 6:00 pm\r\nAuditorium Cité de la musique, Romans\r\n\r\nAccompanied by a jazz combo, CHAM students sing and act to present an original musical version of the Pinocchio story."
    ],
    "it": [
      "Sabato 24 giugno 2023 - ore 15.00 e 18.00\r\nAuditorium Cité de la musique, Roma\r\n\r\nAccompagnati da un combo jazz, gli studenti dello CHAM cantano e recitano per presentare una versione musicale originale della storia di Pinocchio."
    ],
    "fr": [
      "Samedi 24 juin 2023 - 15h00 et 18h00\r\nAuditorium Cité de la musique, Romans\r\n\r\nAccompagnés d’un combo jazz, les élèves CHAM chantent et jouent la comédie pour vous présenter une version musicale tout en originalité de l’histoire de Pinocchio."
    ],
    "nl": [
      "Zaterdag 24 juni 2023 - 15.00 uur en 18.00 uur\r\nAuditorium Cité de la musique, Romeinen\r\n\r\nBegeleid door een jazzcombo presenteren de CHAM-studenten zingend en acterend een originele muzikale versie van het verhaal van Pinokkio."
    ],
    "es": [
      "Sábado 24 de junio de 2023 - 15.00 y 18.00 horas\r\nAuditorio Cité de la musique, Romans\r\n\r\nAcompañados por un combo de jazz, los alumnos del CHAM cantan y actúan para presentar una original versión musical del cuento de Pinocho."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Spectacle de la classe CHAM : Pinocchio, un opéra jazz !"
    ]
  },
