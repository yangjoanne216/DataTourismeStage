      "dc:description": {
        "de": [
          "Das Restaurant Au piment rouge bietet Ihnen eine traditionelle Küche mit regionalen und bodenständigen Produkten, die hauptsächlich aus dem Baskenland stammen.",
          "Restaurant in der Nähe des Stadtzentrums."
        ],
        "en": [
          "Restaurant located near the city centre.",
          "The restaurant Au piment rouge offers you a traditional cuisine with local and regional products coming essentially from the Basque Country."
        ],
        "it": [
          "Il ristorante Au piment rouge propone una cucina tradizionale con prodotti locali e regionali, provenienti soprattutto dai Paesi Baschi.",
          "Ristorante situato vicino al centro città."
        ],
        "fr": [
          "Le restaurant Au piment rouge vous propose une cuisine traditionnelle avec des produits du terroir et régionaux venant essentiellement du Pays Basque.",
          "Restaurant situé à proximité du centre ville."
        ],
        "nl": [
          "Het restaurant Au piment rouge biedt een traditionele keuken met lokale en regionale producten, voornamelijk uit Baskenland.",
          "Restaurant gelegen nabij het stadscentrum."
        ],
        "es": [
          "El restaurante Au piment rouge ofrece una cocina tradicional con productos locales y regionales, principalmente del País Vasco.",
          "Restaurante situado cerca del centro de la ciudad."
        ]
      },
