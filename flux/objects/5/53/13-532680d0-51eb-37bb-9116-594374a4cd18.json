  "rdfs:comment": {
    "de": [
      "Terra Verdon de Castellane, ein moderner Campingplatz mit Mietunterkünften und Stellplätzen, liegt in der Nähe der Verdonschlucht. Beheiztes Spaßbad mit zwei Planschbecken und einem beheizten Hallenbad mit Rutschen!"
    ],
    "en": [
      "Terra Verdon de Castellane, modern campsite offering rentals and pitches, located near the Gorges du Verdon. Heated leisure pool with two paddling pools and a heated indoor pool with slides!"
    ],
    "it": [
      "Terra Verdon de Castellane, un moderno campeggio che offre affitti e piazzole, situato vicino alle Gole del Verdon. Piscina riscaldata per il tempo libero con due vasche per bambini e una piscina coperta riscaldata con scivoli!"
    ],
    "fr": [
      "Terra Verdon de Castellane, camping moderne proposant locations et emplacements, situé à proximité des Gorges du Verdon. Piscine ludique chauffée avec deux pataugeoires et une piscine couverte chauffée et des toboggans !"
    ],
    "es": [
      "Terra Verdon de Castellane, un camping moderno que ofrece alquileres y parcelas, situado cerca de las Gargantas del Verdon. Piscina de recreo climatizada con dos piscinas infantiles y una piscina cubierta climatizada con toboganes"
    ],
    "nl": [
      "Terra Verdon de Castellane, een moderne camping met huuraccommodaties en staanplaatsen, gelegen nabij de Gorges du Verdon. Verwarmd recreatiebad met twee peuterbaden en een verwarmd binnenbad met glijbanen!"
    ]
  },
