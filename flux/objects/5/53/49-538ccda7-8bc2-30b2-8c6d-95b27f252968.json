  "rdfs:comment": {
    "de": [
      "Entdecken Sie die Sèvre Nantaise: ihre Fauna (Kormorane, Reiher usw.), ihre Flora (Seerosen, Binsen usw.), ihre Straßen und ihre Granitlandschaften."
    ],
    "en": [
      "Discover the Sèvre Nantaise: its fauna (cormorants, herons, etc), its flora (water lilies, rushes, etc), its roads and its granite landscapes."
    ],
    "it": [
      "Scoprite la Sèvre Nantaise: la sua fauna (cormorani, aironi, ecc.), la sua flora (ninfee, giunchi, ecc.), le sue strade rialzate e i suoi paesaggi granitici."
    ],
    "fr": [
      "Découvrez la Sèvre Nantaise: sa faune (cormorans, hérons, etc), sa flore (nénuphars, joncs, etc), ses chaussées et ses paysages granitiques."
    ],
    "nl": [
      "Ontdek de Sèvre Nantaise: zijn fauna (aalscholvers, reigers...), zijn flora (waterlelies, biezen...), zijn dijken en zijn granieten landschappen."
    ],
    "es": [
      "Descubra el Sèvre Nantaise: su fauna (cormoranes, garzas, etc.), su flora (nenúfares, juncos, etc.), sus calzadas y sus paisajes de granito."
    ]
  },
