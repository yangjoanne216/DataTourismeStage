  "rdfs:comment": {
    "de": [
      "Boutique-Hotel in einem authentischen Gebäude im Stadtzentrum von Annonay, in der Nähe von Restaurants, Bars und Geschäften. Reichhaltiges Frühstück und Formeln am Abend Etappe."
    ],
    "en": [
      "Hotel situated in a traditional building in Annonay town centre near restaurants, bars and shops. Generous breakfast and overnight packages."
    ],
    "it": [
      "Hotel di carattere situato in un edificio autentico nel centro di Annonay, vicino a ristoranti, bar e negozi. Ricca colazione e pacchetti serali."
    ],
    "fr": [
      "Hôtel de caractère situé dans un authentique bâtiment en plein centre ville d’Annonay, à proximité des restaurants, bars et commerces. Petit déjeuner copieux et formules en soirée étape."
    ],
    "nl": [
      "Hotel met karakter gelegen in een authentiek gebouw in het centrum van Annonay, vlakbij restaurants, bars en winkels. Overvloedige ontbijt- en avondpakketten."
    ],
    "es": [
      "Hotel con carácter situado en un edificio auténtico en el centro de Annonay, cerca de restaurantes, bares y tiendas. Copiosos desayunos y paquetes nocturnos."
    ]
  },
