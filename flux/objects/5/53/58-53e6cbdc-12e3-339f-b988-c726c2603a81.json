  "rdfs:comment": {
    "de": [
      "AviaSim bietet Ihnen die Möglichkeit, in mehreren Flugsimulatoren das Steuer zu übernehmen: Passagierflugzeug Airbus A320 oder Jagdflugzeug F-16 Fighting Falcon. Zugänglich für Anfänger, Begeisterte und Neugierige ab 12/14 Jahren!"
    ],
    "en": [
      "AviaSim proposes you to take the controls of several flight simulators, Airbus A320 airliner or F-16 Fighting Falcon fighter aircraft. Accessible to beginners, passionate and curious, from 12/14 years old !"
    ],
    "it": [
      "AviaSim vi offre l'opportunità di prendere i comandi di diversi simulatori di volo, Airbus A320 o F-16 Fighting Falcon. Accessibile a principianti, appassionati e curiosi, dai 12/14 anni!"
    ],
    "fr": [
      "AviaSim vous propose de prendre les commandes de plusieurs simulateurs de vol, avion de ligne Airbus A320 ou avion de chasse F-16 Fighting Falcon. Accessibles aux débutants, passionnés et curieux, dès 12/14 ans !"
    ],
    "es": [
      "AviaSim le ofrece la oportunidad de tomar los mandos de varios simuladores de vuelo, del avión de pasajeros Airbus A320 o del F-16 Fighting Falcon. Accesible para principiantes, aficionados y curiosos, ¡a partir de 12/14 años!"
    ],
    "nl": [
      "AviaSim biedt u de mogelijkheid om de controle te nemen over verschillende vluchtsimulatoren, Airbus A320 vliegtuig of F-16 Fighting Falcon. Toegankelijk voor beginners, liefhebbers en nieuwsgierigen, vanaf 12/14 jaar!"
    ]
  },
