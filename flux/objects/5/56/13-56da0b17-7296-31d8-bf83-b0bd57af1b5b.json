  "rdfs:comment": {
    "de": [
      "Crêperie - Teesalon, unter dem Dach des Midy Oriental, in Mirepoix. Kleine Gourmet- und Schnellrestaurants zum Mitnehmen oder zum Genießen auf der Terrasse. Crêpes- und Galettes-Spezialitäten aus der Bretagne - durchgehender Service."
    ],
    "en": [
      "Creperie - Tea room, under the Couvert du Midy Oriental, in Mirepoix. Small gourmet and fast food, to take away or to taste in Terrace. Specialities of Pancakes and Breton Galettes - Continuous service."
    ],
    "it": [
      "Crêperie - Salon de Thé, sotto il Couvert du Midy Oriental, a Mirepoix. Piatti veloci, da asporto o da gustare in terrazza. Specialità di crêpes e galettes bretonnes - Servizio continuo."
    ],
    "fr": [
      "Crêperie - Salon de Thé, sous le Couvert du Midy Oriental, à Mirepoix. Petite restauration gourmande et rapide, à emporter ou à déguster en Terrasse. Spécialités de Crêpes et Galettes Bretonnes - Service en continu."
    ],
    "nl": [
      "Crêperie - Salon de Thé, onder de Couvert du Midy Oriental, in Mirepoix. Fast food, om mee te nemen of om van te genieten op het terras. Specialiteiten van Crêpes en Galettes Bretonnes - Continue service."
    ],
    "es": [
      "Crêperie - Salon de Thé, bajo el Couvert du Midy Oriental, en Mirepoix. Comida rápida, para llevar o para disfrutar en la terraza. Especialidades de Crêpes y Galettes Bretonnes - Servicio continuo."
    ]
  },
