  "rdfs:comment": {
    "de": [
      "Weingut mit IGP Cevennes in Bio-Umstellung, das Weiß-, Rot- und Roséweine sowie Traubensaft, Carthagène (Likörwein) und Weinkonfit herstellt."
    ],
    "en": [
      "Wine estate in IGP Cévennes in organic conversion, producing white, red and rosé wines as well as grape juice, Carthagène (liqueur wine) and wine confit."
    ],
    "it": [
      "Azienda vitivinicola in IGP Cévennes in conversione biologica, produce vini bianchi, rossi e rosati, nonché succo d'uva, Cartagena (vino liquoroso) e vino confit."
    ],
    "fr": [
      "Domaine viticole en IGP Cévennes en conversion Bio, produisant des vins blanc, rouge et rosé ainsi que des jus de raisins, de la Carthagène (vin de liqueur) et du confit de vin."
    ],
    "nl": [
      "Wijnbouwbedrijf in IGP Cevennen in biologische omschakeling, dat witte, rode en rosé wijnen produceert, evenals druivensap, Carthagène (likeurwijn) en wijnconfit."
    ],
    "es": [
      "Finca vitícola en IGP Cévennes en conversión ecológica, que produce vinos blancos, tintos y rosados, así como zumo de uva, Carthagène (vino de licor) y vino confitado."
    ]
  },
