  "rdfs:comment": {
    "de": [
      "Charmantes Restaurant in einem sehr gemütlichen Rahmen mit einer sehr abwechslungsreichen Speisekarte und einer geselligen Atmosphäre, in der Sie mit Freunden ein Glas trinken können."
    ],
    "en": [
      "Charming restaurant in a very cosy setting with a very varied menu and a friendly atmosphere where you can have a drink with friends."
    ],
    "it": [
      "Ristorante affascinante in un ambiente molto accogliente, con un menu vario e un'atmosfera amichevole dove bere un drink con gli amici."
    ],
    "fr": [
      "Restaurant de charme dans un cadre très cosy avec une carte très variée et une ambiance conviviale où vous pouvez prendre un verre entre amis."
    ],
    "es": [
      "Restaurante con encanto en un entorno muy acogedor, con una carta variada y un ambiente agradable donde tomar una copa con los amigos."
    ],
    "nl": [
      "Charmant restaurant in een zeer gezellig kader met een gevarieerde kaart en een gezellige sfeer waar u met vrienden iets kunt drinken."
    ]
  },
