  "rdfs:comment": {
    "de": [
      "5 km langer Wanderweg in den Hügeln von La Fare-les-Oliviers, der mit 10 botanischen Informationstafeln geschmückt ist."
    ],
    "en": [
      "A 5km hiking trail in the hills of La Fare-les-Oliviers, with 10 botanical information panels."
    ],
    "it": [
      "Un percorso escursionistico di 5 km sulle colline di La Fare-les-Oliviers, con 10 pannelli informativi botanici."
    ],
    "fr": [
      "Chemin de randonnée de 5km dans les collines de la Fare-les-Oliviers, agrémenté de 10 panneaux d'informations botaniques."
    ],
    "es": [
      "Una ruta de senderismo de 5 km en las colinas de La Fare-les-Oliviers, con 10 paneles de información botánica."
