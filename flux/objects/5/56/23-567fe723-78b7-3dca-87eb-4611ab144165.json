  "rdfs:comment": {
    "de": [
      "Diese leichte Wanderung führt Sie durch die von alten Schlössern durchbrochene Landschaft von Boussac."
    ],
    "en": [
      "This easy walk offers you a dive through the countryside of Boussac pierced by its castles of yesteryear."
    ],
    "it": [
      "Questa facile passeggiata vi porta a immergervi nella campagna di Boussac, attraversata dai castelli di un tempo."
    ],
    "fr": [
      "Cette balade facile vous propose un plongeon à travers les campagnes de Boussac percées par son châteaux d'antan."
    ],
    "es": [
      "Este sencillo paseo le llevará a sumergirse en la campiña de Boussac, atravesada por los castillos de antaño."
    ],
    "nl": [
      "Deze gemakkelijke wandeling neemt je mee door het platteland van Boussac, doorboord door de kastelen van weleer."
    ]
  },
