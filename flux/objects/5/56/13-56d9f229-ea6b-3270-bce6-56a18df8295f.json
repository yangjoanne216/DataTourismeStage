  "rdfs:comment": {
    "de": [
      "Vier Hektar große Wasserfläche (Karpfen, Schleie, Rotaugen und Hechte) mit Tageskarten, die vor Ort oder in der Eisenwarenhandlung Lemeux in Le Montet abgeholt werden können."
    ],
    "en": [
      "A four-hectare lake (carp, tench, roach and pike) with day passes to be taken on site or at the Lemeux hardware store in Le Montet."
    ],
    "it": [
      "Quattro ettari di acqua (carpa, tinca, lasca e luccio) con tessere giornaliere da ritirare sul posto o presso la ferramenta Lemeux a Montet."
    ],
    "fr": [
      "Plan d'eau de quatre hectares (carpes, tanches, gardons et brochets) avec cartes à la journée à prendre sur place ou à la quincaillerie Lemeux au Montet."
    ],
    "nl": [
      "Vier hectare water (karper, zeelt, voorn en snoek) met dagkaarten die ter plaatse of in de ijzerhandel Lemeux in Montet kunnen worden genomen."
    ],
    "es": [
      "Cuatro hectáreas de agua (carpas, tencas, cucarachas y lucios) con pases de un día que se pueden coger in situ o en la ferretería Lemeux de Montet."
    ]
  },
