  "rdfs:comment": {
    "de": [
      "Kapelle mit rechteckigem Grundriss, die dem Heiligen Kreuz und der Heiligen Philomena gewidmet ist, aus Backstein und Sandstein. Auf einem Vorgebirge gelegen. Zwei große Eiben davor. Ambulatorium aus Sandstein."
    ],
    "en": [
      "Chapel of rectangular plan, dedicated to Saint Croix and Saint Philomena, in brick and sandstone. Located on a promontory. Two large yews in front. Sandstone ambulatory."
    ],
    "it": [
      "Cappella rettangolare dedicata a Santa Croce e Santa Filomena, realizzata in mattoni e pietra arenaria. Situato su un promontorio. Due grandi alberi di tasso sul davanti. Ambulatorio in pietra arenaria."
    ],
    "fr": [
      "Chapelle de plan rectangulaire, dédiée à Sainte-Croix et Sainte Philomène, en brique et grés. Située sur un promontoire. Deux grands ifs devant. Déambulatoire de grés."
    ],
    "nl": [
      "Rechthoekige kapel gewijd aan Sint Croix en Sint Philomena, gemaakt van baksteen en zandsteen. Gelegen op een voorgebergte. Twee grote taxusbomen aan de voorkant. Sandstone ambulant."
    ],
    "es": [
      "Capilla rectangular dedicada a Santa Cruz y Santa Filomena, realizada en ladrillo y piedra arenisca. Situado en un promontorio. Dos grandes tejos en la parte delantera. Ambulatorio de piedra arenisca."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Chapelle Sainte-Croix et Sainte-Philomène"
    ]
  },
