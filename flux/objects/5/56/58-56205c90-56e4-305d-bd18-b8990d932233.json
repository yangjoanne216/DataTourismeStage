  "rdfs:comment": {
    "de": [
      "Frisches und konserviertes Gemüse aus biologischem Anbau. Verkauf auf dem Bauernhof von lokalen und biologischen Produkten: Eier, Wein, Reis, Saft, Käse, Honig... AMAP (Bauernverband), Bauernhof-Buffets."
    ],
    "en": [
      "Fresh and preserved vegetables from organic farming. Sale at the farm of local and organic products: eggs, wine, rice, juice, cheese, honey... AMAP, farm buffets."
    ],
    "it": [
      "Verdure fresche e conservate da agricoltura biologica. Vendita di prodotti locali e biologici presso la fattoria: uova, vino, riso, succhi di frutta, formaggi, miele... AMAP, buffet di fattorie."
    ],
    "fr": [
      "Légumes frais et en conserves issus de l'Agriculture Biologique. Vente à la ferme de produits locaux et bio: œufs, vin, riz, jus, fromages, miel... AMAP, buffets fermiers.."
    ],
    "nl": [
      "Verse en geconserveerde groenten uit de biologische landbouw. Verkoop van lokale en biologische producten op de boerderij: eieren, wijn, rijst, sap, kaas, honing... AMAP, boerderij buffetten."
    ],
    "es": [
      "Verduras frescas y en conserva procedentes de la agricultura ecológica. Venta de productos locales y ecológicos en la granja: huevos, vino, arroz, zumo, queso, miel... AMAP, buffets de granja."
    ]
  },
