  "rdfs:comment": {
    "de": [
      "In St Sauveur Gouvernet befindet sich ein ungewöhnlicher Bauernhof: die Natur in Freiheit! Angoraziegen, Kaninchen, Hühner, Enten, Mohairwolle, Honig und Einkorn!"
    ],
    "en": [
      "In St Sauveur Gouvernet, an unusual farm: nature in freedom! Angora goats, rabbits, chickens, ducks, mohair wool, honey and small spelt!"
    ],
    "it": [
      "A St Sauveur Gouvernet, una fattoria insolita: la natura in libertà! Capre d'angora, conigli, galline, anatre, lana mohair, miele e piccolo farro!"
    ],
    "fr": [
      "A St Sauveur Gouvernet, une ferme insolite : la nature en liberté ! Chèvres angora, lapins, poules, canards, laine mohair, miel et petit épeautre !"
    ],
    "nl": [
      "In St Sauveur Gouvernet, een ongewone boerderij: natuur in vrijheid! Angorageiten, konijnen, kippen, eenden, mohairwol, honing en kleine spelt!"
    ],
    "es": [
      "En St Sauveur Gouvernet, una granja insólita: ¡naturaleza en libertad! Cabras de angora, conejos, gallinas, patos, lana de mohair, miel ¡y espelta pequeña!"
    ]
  },
