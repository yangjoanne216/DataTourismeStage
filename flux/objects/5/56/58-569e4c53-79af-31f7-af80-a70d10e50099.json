  "rdfs:comment": {
    "de": [
      "Gîte im Herzen eines typisch provenzalischen Dorfes, ideal für Besuche im Luberon, dessen erstes Dorf weniger als 10 km entfernt liegt, Salon de Provence, der Stadt von Nostradamus, Aix en Provence, Avignon, etc."
    ],
    "en": [
      "Gite in the heart of a typical Provencal village, ideal for visiting the Luberon whose first village is located less than 10kms away, Salon de Provence, the city of Nostradamus, Aix en Provence, Avignon, etc."
    ],
    "it": [
      "Gite nel cuore di un tipico villaggio provenzale, ideale per visitare il Luberon, il cui primo villaggio si trova a meno di 10 km, Salon de Provence, la città di Nostradamus, Aix en Provence, Avignone, ecc."
    ],
    "fr": [
      "Gîte au cœur d'un village typiquement provençal, idéal pour visiter Le Lubéron dont le premier village se situe à moins de 10kms, Salon de Provence, la ville de Nostradamus, Aix en Provence, Avignon, etc."
    ],
    "nl": [
      "Gite in het hart van een typisch Provençaals dorp, ideaal voor een bezoek aan de Luberon, waarvan het eerste dorp op minder dan 10 km afstand ligt, Salon de Provence, de stad van Nostradamus, Aix en Provence, Avignon, enz."
    ],
    "es": [
      "Casa rural en el corazón de un típico pueblo provenzal, ideal para visitar el Luberon, cuyo primer pueblo está a menos de 10kms, Salon de Provence, la ciudad de Nostradamus, Aix en Provence, Avignon, etc."
    ]
  },
