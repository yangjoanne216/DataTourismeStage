  "rdfs:comment": {
    "de": [
      "Eine kleine Schleife, die Sie um La Graverie und inmitten der Felder machen können. Vorsicht bei der Überquerung der RD674."
    ],
    "en": [
      "A small loop takes you round La Graverie and through the fields. Take care when crossing the RD674 trunk road."
    ],
    "it": [
      "Un piccolo anello da fare intorno a La Graverie e in mezzo ai campi. Fate attenzione quando attraversate la RD674."
    ],
    "fr": [
      "Une petite boucle à faire autour de la Graverie et au milieu des champs. Prudence pour la traversée de la RD674."
    ],
    "es": [
      "Un pequeño bucle para hacer alrededor de La Graverie y en medio de los campos. Tenga cuidado al cruzar la RD674."
    ],
    "nl": [
      "Een kleine lus te doen rond La Graverie en in het midden van de velden. Wees voorzichtig bij het oversteken van de RD674."
    ]
  },
