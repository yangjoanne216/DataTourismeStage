  "rdfs:comment": {
    "de": [
      "Le védaquais, benannt nach den Einwohnern von vaas, ist ein gemütliches Hotel-Restaurant, das inmitten von Obstgärten am Ufer des Flusses loir liegt."
    ],
    "en": [
      "The Védaquais, named after the inhabitants of Vaas, is a welcoming hotel-restaurant, located in the middle of orchards, on the banks of the Loir."
    ],
    "it": [
      "Il Védaquais, che prende il nome dagli abitanti di Vaas, è un accogliente hotel-ristorante immerso nei frutteti sulle rive del Loir."
    ],
    "fr": [
      "Le védaquais, du nom des habitants de vaas, est un accueillant hôtel-restaurant, installé au milieu des vergers, sur les bords du loir."
    ],
    "es": [
      "El Védaquais, que lleva el nombre de los habitantes de Vaas, es un acogedor hotel-restaurante situado en medio de huertos a orillas del Loir."
    ],
    "nl": [
      "De Védaquais, genoemd naar de inwoners van Vaas, is een gastvrij hotel-restaurant te midden van boomgaarden aan de oevers van de Loir."
    ]
  },
