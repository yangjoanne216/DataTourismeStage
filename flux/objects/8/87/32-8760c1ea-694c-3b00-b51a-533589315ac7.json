  "rdfs:comment": {
    "de": [
      "Dieses Hotel befindet sich im Hafenzentrum von Cap d'Agde: Yachthafen, Geschäfte, Restaurants, Eisdielen, Animation... Der Strand ist 800 m entfernt und die Atmosphäre ist familiär."
    ],
    "en": [
      "This hotel is located in the center of the port of Cap d'Agde: marina, stores, restaurants, ice cream parlors, entertainment... The beach is 800 m away and the atmosphere is family oriented."
    ],
    "it": [
      "L'hotel è situato nel centro del porto di Cap d'Agde: porto turistico, negozi, ristoranti, gelaterie, animazione... La spiaggia dista 800 m e l'atmosfera è familiare."
    ],
    "fr": [
      "Cet hôtel est situé au Centre-port du Cap d'Agde : port de plaisance, boutiques, restaurants, glaciers, animation... La plage est à 800 m et l'ambiance est familiale."
    ],
    "es": [
      "Este hotel está situado en el centro del puerto de Cap d'Agde: puerto deportivo, tiendas, restaurantes, heladerías, espectáculos... La playa está a 800 m y el ambiente es familiar."
    ],
    "nl": [
      "Dit hotel ligt in het centrum van de haven van Cap d'Agde: jachthaven, winkels, restaurants, ijssalons, animatie... Het strand ligt op 800 m afstand en de sfeer is gezinsvriendelijk."
    ]
  },
