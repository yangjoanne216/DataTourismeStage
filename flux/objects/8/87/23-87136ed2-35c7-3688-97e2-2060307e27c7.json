  "rdfs:comment": {
    "de": [
      "Gîte du château : feudales Schloss aus dem 13. Jh., im Herzen eines Weinbergs gelegen. Wohnzimmer, Esszimmer, Kamin, ausgestattete Küche, Gartenmöbel, Bettwäsche vorhanden."
    ],
    "en": [
      "Gîte du château : feudal castle of the 13th c., located in the heart of a vineyard. Living room, dining room, fireplace, equipped kitchen, garden furniture, sheets provided."
    ],
    "it": [
      "Gîte du château: castello feudale del XIII secolo, situato nel cuore di un vigneto. Soggiorno, sala da pranzo, camino, cucina attrezzata, mobili da giardino, biancheria da letto fornita."
    ],
    "fr": [
      "Gîte du château : château féodal du 13ème s., situé au coeur d'un vignoble. Salon, salle à manger, cheminée, cuisine équipée, salon de jardin, draps fournis."
    ],
    "nl": [
      "Gîte du château: feodaal kasteel uit de 13e eeuw, gelegen in het hart van een wijngaard. Woonkamer, eetkamer, open haard, ingerichte keuken, tuinmeubels, beddengoed aanwezig."
    ],
    "es": [
      "Gîte du château: castillo feudal del siglo XIII, situado en el corazón de un viñedo. Sala de estar, comedor, chimenea, cocina equipada, muebles de jardín, ropa de cama proporcionada."
    ]
  },
