  "rdfs:comment": {
    "de": [
      "Pizzas vor Ort, zum Mitnehmen oder als Lieferservice.\n7 Tage die Woche im Juli und August, in der Nebensaison (September bis Juni) mittags geschlossen"
    ],
    "en": [
      "Pizzas on the spot, to take away or delivery.\n7 days a week in July and August, closed at midday during the low season (September to June)"
    ],
    "it": [
      "Pizze sul posto, da asporto o a domicilio.\n7 giorni su 7 in luglio e agosto, chiuso a pranzo in bassa stagione (da settembre a giugno)"
    ],
    "fr": [
      "Pizzas sur place, à emporter ou en livraison.\n7j/7 en juillet août, fermé les midis hors saison (septembre à juin)"
    ],
    "es": [
      "Pizzas al momento, para llevar o para entregar.\n7 días a la semana en julio y agosto, cerrado al mediodía fuera de temporada (de septiembre a junio)"
    ],
    "nl": [
      "Pizza's ter plaatse, om mee te nemen of om te bezorgen.\n7 dagen per week in juli en augustus, 's middags gesloten buiten het seizoen (september tot juni)"
    ]
  },
