  "rdfs:comment": {
    "de": [
      "Spezialist für Cognac und Pineau des Charentes. 400 Referenzen im Laden. Verkostungen für Gruppen nach Vereinbarung. Abende zu bestimmten Themen. Versand in Frankreich und ins Ausland (Liste der Länder vor Ort zu konsultieren)."
    ],
    "en": [
      "Specialist Cognac and Pineau des Charente. 400 references in the shop. Tastings for groups by appointment. Thematic tastings. Export in France and abroad."
    ],
    "it": [
      "Specializzato in Cognac e Pineau des Charentes. 400 referenze in negozio. Degustazioni per gruppi su appuntamento. Serate a tema. Spedizione in Francia e all'estero (elenco dei Paesi da consultare in loco)."
    ],
    "fr": [
      "Spécialiste de Cognac et Pineau des Charentes. 400 références en magasin. Dégustations pour groupes sur rendez-vous. Soirées à thèmes. Expédition en France et à l'étranger (liste des pays à consulter sur place)."
    ],
    "nl": [
      "Specialist in Cognac en Pineau des Charentes. 400 referenties in de winkel. Proeverijen voor groepen op afspraak. Thema-avonden. Verzending in Frankrijk en in het buitenland (lijst van landen ter plaatse te raadplegen)."
    ],
    "es": [
      "Especialista en Cognac y Pineau des Charentes. 400 referencias en la tienda. Degustaciones para grupos con cita previa. Veladas temáticas. Envío en Francia y en el extranjero (lista de países a consultar in situ)."
    ]
