  "rdfs:comment": {
    "de": [
      "Charmante Villa in Tarascon, am Rande der Stadt, mit angrenzendem Garten und Terrasse. Für die Kinder steht ein Spielplatz mit Schaukel zur Verfügung!"
    ],
    "en": [
      "Charming villa located in Tarascon, on the outskirts of the city, with adjoining garden and terrace. A playground with swing is available for children!"
    ],
    "it": [
      "Incantevole villa situata a Tarascon, alla periferia della città, con giardino e terrazza annessi. Per i bambini è disponibile un parco giochi con altalena!"
    ],
    "fr": [
      "Charmante villa située à Tarascon, à la périphérie de la ville, avec jardin et terrasse attenants. Une aire de jeux avec balançoire est à disposition pour les enfants !"
    ],
    "es": [
      "Encantador chalet situado en Tarascón, en las afueras de la ciudad, con jardín y terraza contiguos. Los niños tienen a su disposición un parque infantil con columpios"
