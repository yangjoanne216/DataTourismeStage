  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/64cfafd2-968d-3054-b38b-0d28ce2fc680",
      "dc:description": {
        "de": [
          "Traditionelle Küche. Spezialitäten: Sauerkraut, Baeckeoffe, Bouchée à la reine und Vorschläge\n \n Bietet auch Gerichte zum Mitnehmen an. Mittagsservice: 12 Uhr bis 13:30 Uhr. Abendservice: 18:30 bis 21:00 Uhr."
        ],
        "en": [
          "Traditional cuisine. Specialities: sauerkraut, baeckeoffe, bouchée à la reine and suggestions\n \n Offers take-away meals. Lunch service : 12pm to 1:30pm. Evening service: 6:30 pm to 9 pm."
        ],
        "it": [
          "Cucina tradizionale. Specialità: crauti, baeckeoffe, bouchée à la reine e suggerimenti\n \n Offre piatti da asporto. Servizio a pranzo: dalle 12.00 alle 13.30. Servizio serale: dalle 18.30 alle 21.00."
        ],
        "fr": [
          "Cuisine traditionnelle. Spécialités : choucroute, baeckeoffe, bouchée à la reine et suggestions. \n \n Propose des plats à emporter. Service du midi : 12h à 13h30. Service du soir : 18h30 à 21h."
        ],
        "nl": [
          "Traditionele keuken. Specialiteiten: zuurkool, baeckeoffe, bouchée à la reine en suggesties\n \n Biedt afhaalgerechten aan. Lunchtijddienst: 12.00 tot 13.30 uur. Avonddienst: 18.30 tot 21.00 uur."
        ],
        "es": [
          "Cocina tradicional. Especialidades: chucrut, baeckeoffe, bouchée à la reine y sugerencias\n \n Ofrece platos para llevar. Servicio de almuerzo: de 12 a 13:30 horas. Servicio nocturno: de 18:30 a 21:00 horas."
        ]
      },
