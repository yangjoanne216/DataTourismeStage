  "rdfs:comment": {
    "de": [
      "Geselliges Restaurant im Herzen des Dorfes Roissy-en-France. Das Vivier de la Mer bezieht seine frischen Produkte direkt von der Marée de Rungis!"
    ],
    "en": [
      "A friendly restaurant in the heart of the village of Roissy-en-France, the Vivier de la Mer is supplied with fresh products directly from the Rungis fish market!"
    ],
    "it": [
      "Ristorante accogliente nel cuore del villaggio di Roissy-en-France, il Vivier de la Mer si rifornisce di prodotti freschi direttamente dal mercato del pesce di Rungis!"
    ],
    "fr": [
      "Restaurant convivial au coeur du village de Roissy-en-France, le Vivier de la Mer se fournit en produits frais en direct de la Marée de Rungis !"
    ],
    "es": [
      "Restaurante acogedor en el corazón del pueblo de Roissy-en-France, el Vivier de la Mer se abastece de productos frescos directamente de la lonja de Rungis"
    ],
    "nl": [
      "De Vivier de la Mer, een gezellig restaurant in het hart van het dorp Roissy-en-France, wordt bevoorraad met verse producten rechtstreeks van de vismarkt van Rungis!"
    ]
  },
