  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/449b627d-0523-369e-b1ba-c59fe7e9089c",
      "dc:description": {
        "de": [
          "Reitsport\n\nUnterkunft ( Via FrancIgena):\n-20 Plätze: 15 Euros/ Nacht\n-Frühstück: 5.20 Euros\n-Mahlzeit: 14.50 Euros\n-Kurtaxe: 1.10 Euros"
        ],
        "en": [
          "Horse riding.\n\n\nlodging ( via FrancIgena)\n-20 Places: 15 Euros/ night\n-Breakfast: 5.20 Euros\n-Meal: 14.50 Euros\n-Tourist tax: 1.10 Euros"
        ],
        "it": [
          "Centro ippico.\nStabilimento con brevetto di Stato (BE).\n\nAlloggio in un gîte d'étape, per gli escursionisti di passaggio (Via FrancIgena):\n-20 posti: 15 euro / notte\n-Colazione: 5,20 euro\n-Pasti: 14,50 euro\n-Tassa di soggiorno: 1,10 euro."
        ],
        "fr": [
          "Centre équestre.\nEtablissement avec encadrement Brevet d'Etat (BE).\n\nHébergement gîte d'étape, Randonneur de passage ( Via FrancIgena):\n-20 places: 15 Euros / Nuit\n-Petit déjeuner: 5.20 Euros\n-Repas:14.50 Euros\n-Taxe de séjour: 1.10 Euros."
        ],
        "nl": [
          "Paardrijden.\n\n\naccommodatie ( Via FrancIgena)\n-20 plaatsen: 15 Euros / nacht\n-ontbijt: 5.20 Euros\n-maaltijd: 14.50 Euros\n-Toeristen belasting: 1.10 Euros."
        ],
        "es": [
          "Centro ecuestre.\nEstablecimiento con patente estatal (BE).\n\nAlojamiento en una casa rural de paso, para excursionistas de paso (Via FrancIgena):\n-20 plazas: 15 Euros / Noche\n-Desayuno: 5,20 euros\n-Comidas: 14,50 euros\n-Tasa turística: 1,10 euros."
        ]
      },
