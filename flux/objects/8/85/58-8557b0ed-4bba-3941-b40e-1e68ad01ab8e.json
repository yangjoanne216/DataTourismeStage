  "rdfs:comment": {
    "de": [
      "Architektenhaus von 200 m². Sehr ruhige Umgebung, 5 Minuten von Saint-Rémy-de-Provence entfernt, im Park der Alpilles. 4 Schlafzimmer, 4 Bäder, 4 Toiletten, 4 unabhängige Eingänge möglich."
    ],
    "en": [
      "Architect house of 200 m². Very quiet environment at 5 minutes from Saint-rémy-de-provence in the park of Alpilles. 4 bedrooms, 4 bathrooms, 4 toilets, 4 possible independent entrances."
    ],
    "it": [
      "Casa dell'architetto di 200 m². Ambiente molto tranquillo, a 5 minuti da Saint-Rémy-de-Provence, nel parco delle Alpilles. 4 camere da letto, 4 bagni, 4 servizi igienici, 4 possibili ingressi indipendenti."
    ],
    "fr": [
      "Maison d'architecte de 200 m². Environnement très calme à 5 minutes de Saint-rémy-de-provence dans le parc des Alpilles. 4 chambres, 4 salles de bain, 4 toilettes, 4 entrées indépendantes possibles."
    ],
    "es": [
      "Casa de arquitecto de 200 m². Entorno muy tranquilo, a 5 minutos de Saint-Rémy-de-Provence en el parque de Alpilles. 4 dormitorios, 4 baños, 4 aseos, 4 posibles entradas independientes."
    ],
    "nl": [
      "Architectenhuis van 200 m². Zeer rustige omgeving, op 5 minuten van Saint-Rémy-de-Provence in het park van de Alpilles. 4 slaapkamers, 4 badkamers, 4 toiletten, 4 mogelijke onafhankelijke ingangen."
    ]
  },
