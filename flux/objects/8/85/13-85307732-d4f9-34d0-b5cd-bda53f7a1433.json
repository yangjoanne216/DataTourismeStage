  "rdfs:comment": {
    "de": [
      "Ecape game \"l'Elixir\" (Das Elixier)\r\nEin Abenteuer für die ganze Familie und mit Freunden!\r\nMachen Sie eine Zeitreise und lüften Sie das Geheimnis um das Elixier des Marquis de Marcieu."
    ],
    "en": [
      "Ecape game \"l'Elixir\r\nAn adventure to live with family and friends!\r\nCross the time and perceive the mystery of the elixir of the Marquis de Marcieu."
    ],
    "it": [
      "Gioco Ecape \"l'Elisir\r\nUn'avventura da vivere con la famiglia e gli amici!\r\nAttraversate il tempo e svelate il mistero dell'elisir del Marchese di Marcieu."
    ],
    "fr": [
      "Ecape game\"l'Elixir\"\r\nUne aventure à vivre en famille et entre amis !!\r\nTraversez le temps et percez le mystère de l’élixir du Marquis de Marcieu."
    ],
    "es": [
      "Juego de escape \"l'Elixir\r\n¡Una aventura para vivir en familia y con amigos!\r\nAtraviesa el tiempo y desvela el misterio del elixir del Marqués de Marcieu."
    ],
    "nl": [
      "Ecape spel \"l'Elixir\r\nEen avontuur om met familie en vrienden te beleven!\r\nSteek de tijd over en doorboor het mysterie van het elixer van de Markies de Marcieu."
    ]
  },
