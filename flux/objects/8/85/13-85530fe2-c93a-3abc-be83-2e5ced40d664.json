  "rdfs:comment": {
    "de": [
      "Willkommen im La Note Bleue, Ihrem Restaurant in Le Mourillon, direkt am Meer, in der schönsten Reede Europas: Toulon.\r\nDie von unserem Küchenchef erdachte hausgemachte Küche wird Feinschmecker begeistern!"
    ],
    "en": [
      "Welcome to La Note Bleue, your restaurant in Mourillon, facing the sea, in the most beautiful harbor in Europe: Toulon.\r\nOur chef's home cooking will delight gourmets!"
    ],
    "it": [
      "Benvenuti a La Note Bleue, il vostro ristorante a Mourillon, di fronte al mare, nel più bel porto d'Europa: Tolone.\r\nLa cucina casalinga del nostro chef delizierà i buongustai!"
    ],
    "fr": [
      "Bienvenue à La Note Bleue votre restaurant au Mourillon, face à la mer, dans la plus belle rade d'Europe : Toulon.\r\nUne cuisine maison imaginée par notre chef ravira les gourmets !"
    ],
    "nl": [
      "Welkom bij La Note Bleue, uw restaurant in Mourillon, tegenover de zee, in de mooiste haven van Europa: Toulon.\r\nDe keuken van onze chef zal de fijnproevers in vervoering brengen!"
    ],
    "es": [
      "Bienvenido a La Note Bleue, su restaurante en Mourillon, frente al mar, en el puerto más bello de Europa: Toulon.\r\n¡La cocina casera de nuestro chef hará las delicias de los gourmets!"
    ]
  },
