  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/f54bda33-4078-3c65-9d37-bc92f672cafc",
      "dc:description": {
        "de": [
          "Restaurant zwischen Louhans und Chalon-sur-Saône. Traditionelle Küche mit Tagesmenü von Montag bis Freitag. Kalbskopf am Montag und à la carte bei jedem Service. Klimatisiertes Restaurant."
        ],
        "en": [
          "Restaurant located between Louhans and Chalon-sur-Saône. Traditional cuisine with daily menu from Monday to Friday. Tête de veau on Mondays and à la carte at each service. Air-conditioned restaurant."
        ],
        "it": [
          "Ristorante situato tra Louhans e Chalon-sur-Saône. Cucina tradizionale con menù giornaliero dal lunedì al venerdì. Tête de veau il lunedì e à la carte a ogni servizio. Ristorante climatizzato."
        ],
        "fr": [
          "Restaurant situé entre Louhans et Chalon-sur-Saône. Cuisine traditionnelle avec menu du jour du lundi au vendredi. Tête de veau, le lundi et à la carte à chaque service. Restaurant climatisé."
        ],
        "es": [
          "Restaurante situado entre Louhans y Chalon-sur-Saône. Cocina tradicional con menú diario de lunes a viernes. Tête de veau los lunes y a la carta en cada servicio. Restaurante con aire acondicionado."
        ],
        "nl": [
          "Restaurant gelegen tussen Louhans en Chalon-sur-Saône. Traditionele keuken met dagmenu van maandag tot vrijdag. Tête de veau op maandag en à la carte bij elke dienst. Restaurant met airconditioning."
        ]
      },
