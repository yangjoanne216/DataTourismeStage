  "rdfs:comment": {
    "de": [
      "In der Nähe von Châteauroux, im Herzen der Champagne Berrichonne, können Sie sich am Ufer des Étang des Marais entspannen und angeln."
    ],
    "en": [
      "Near Chateauroux, in the heart of Champagne Berrichonne, come relax on the edge of the marsh pond and practice fishing."
    ],
    "it": [
      "Vicino a Châteauroux, nel cuore della regione Champagne Berrichonne, venite a rilassarvi ai bordi delle paludi e a praticare la pesca."
    ],
    "fr": [
      "A proximité de Châteauroux, au cœur de la Champagne Berrichonne, venez vous détendre au bord de l'étang des marais et pratiquer la pêche."
    ],
    "nl": [
      "In de buurt van Châteauroux, in het hart van de Champagne Berrichonne, kunt u ontspannen aan de rand van de moerassen en vissen."
    ],
    "es": [
      "Cerca de Châteauroux, en el corazón de la región de Champagne Berrichonne, venga a relajarse al borde de las marismas y a disfrutar de la pesca."
    ]
  },
