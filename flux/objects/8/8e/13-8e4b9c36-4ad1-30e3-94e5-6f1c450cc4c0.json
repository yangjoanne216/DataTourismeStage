  "rdfs:comment": {
    "de": [
      "Restaurant mit gedämpfter Atmosphäre in einem zeitgenössischen Rahmen. Zwei Essecken für Geschäftsessen für bis zu 8 Personen. Speisekarte und Schnellgericht mit Tagesgerichten. 60 Plätze."
    ],
    "en": [
      "Restaurant in a felted atmosphere, in a contemporary frame. Two corners for buisness lunches (max 8 persons). Card and quick menu with special dishes of the day. 60 seats."
    ],
    "it": [
      "Ristorante dall'atmosfera accogliente, in un ambiente contemporaneo. Due aree pranzo per pranzi di lavoro fino a 8 persone. Piatti alla carta e veloci con specialità del giorno. 60 posti a sedere."
    ],
    "fr": [
      "Restaurant à l'ambiance feutré, dans un cadre contemporain. Deux coins repas pour déjeuners d'affaires jusqu'à 8 personnes. Carte et formule rapide avec plats du jour. 60 places."
    ],
    "es": [
      "Restaurante con un ambiente acogedor, en un entorno contemporáneo. Dos comedores para comidas de negocios de hasta 8 personas. Comida a la carta y rápida con especialidades diarias. 60 plazas."
    ],
    "nl": [
      "Restaurant met een gezellige sfeer, in een eigentijds kader. Twee eetzalen voor zakenlunches tot 8 personen. A la carte en fast food met dagschotels. 60 plaatsen."
    ]
