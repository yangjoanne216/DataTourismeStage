      "dc:description": {
        "de": [
          "Fächerförmiger Wasserfall an der Nordostseite des Grand Ballon. Er ist vom Dorf Lautenbach Zell aus oder über den Lac du Ballon erreichbar."
        ],
        "en": [
          "Fan-shaped waterfall located on the northeast slope of the Grand Ballon. It is accessible from the village of Lautenbach Zell or by the Lac du Ballon."
        ],
        "it": [
          "Cascata a ventaglio sul lato nord-est del Grand Ballon. È raggiungibile dal villaggio di Lautenbach Zell o dal Lac du Ballon."
        ],
        "fr": [
          "Cascade en éventail situé sur le versant nord-est du grand Ballon. Elle est accessible à partir du village de Lautenbach Zell ou par le lac du Ballon."
        ],
        "nl": [
          "Waaiervormige waterval aan de noordoostkant van de Grand Ballon. Het is bereikbaar vanaf het dorp Lautenbach Zell of vanaf het Lac du Ballon."
        ],
        "es": [
          "Cascada en forma de abanico en el lado noreste del Grand Ballon. Se puede llegar desde el pueblo de Lautenbach Zell o desde el lago del Ballon."
        ]
