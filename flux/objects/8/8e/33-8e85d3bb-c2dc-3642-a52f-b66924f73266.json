  "rdfs:comment": {
    "de": [
      "Die alte Halle mit dem Rathaus wurde 1884 vom Bürgermeister Théodore PARLANGE gebaut."
    ],
    "en": [
      "The old market hall, with the town hall, was built in 1884 by Mayor Théodore PARLANGE."
    ],
    "it": [
      "L'antica sala del mercato, con il municipio, fu costruita nel 1884 dal sindaco Théodore PARLANGE."
    ],
    "fr": [
      "L’ancienne halle, avec la mairie, a été construite en 1884 par le Maire Théodore PARLANGE."
    ],
    "es": [
      "La antigua sala del mercado, con el ayuntamiento, fue construida en 1884 por el alcalde Théodore PARLANGE."
    ],
    "nl": [
      "De oude markthal, met het stadhuis, werd in 1884 gebouwd door burgemeester Théodore PARLANGE."
    ]
  },
