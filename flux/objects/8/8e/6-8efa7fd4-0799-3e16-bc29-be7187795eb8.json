  "rdfs:comment": {
    "de": [
      "Schöner kleiner Kokon auf den Anhöhen von Maîche. In der Nähe des Stadtzentrums für Geschäfte und für schöne Wanderungen auf dem Plateau."
    ],
    "en": [
      "Beautiful little cocoon on the heights of Maîche. Close to the town centre for the shops and to go on beautiful walks on the plateau"
    ],
    "it": [
      "Bellissimo bozzolo sulle alture di Maîche. Vicino al centro del paese per i negozi e per fare belle passeggiate sull'altopiano"
    ],
    "fr": [
      "Beau petit cocon sur les hauteurs de Maîche . Prés du Centre Ville pour les commerces et pour faire de belles randonnées sur le plateau"
    ],
    "nl": [
      "Prachtige kleine cocon op de hoogten van Maîche. Dicht bij het centrum voor de winkels en om mooie wandelingen te maken op het plateau"
    ],
    "es": [
      "Hermoso pequeño capullo en las alturas de Maîche. Cerca del centro de la ciudad para las tiendas y para ir en hermosos paseos por la meseta"
    ]
  },
