  "rdfs:comment": {
    "de": [
      "Bella Vita Pizza empfängt Sie in Roquefort la Bédoule, Avenue Barthélémy Andréis, um Sie mit seinen köstlichen Pizzen zu verwöhnen."
    ],
    "en": [
      "Bella Vita Pizza welcomes you to Roquefort la Bédoule, Avenue Barthélémy Andréis, to help you discover its delicious pizzas."
    ],
    "it": [
      "Bella Vita Pizza vi accoglie a Roquefort la Bédoule, in Avenue Barthélémy Andréis, per farvi scoprire le sue deliziose pizze."
    ],
    "fr": [
      "Bella Vita Pizza vous accueille à Roquefort la Bédoule, Avenue Barthélémy Andréis, afin de vous faire découvrir ses délicieuses pizzas."
    ],
    "nl": [
      "Bella Vita Pizza verwelkomt u in Roquefort la Bédoule, Avenue Barthélémy Andréis, om u zijn heerlijke pizza's te laten ontdekken."
    ],
    "es": [
      "Bella Vita Pizza le recibe en Roquefort la Bédoule, Avenue Barthélémy Andréis, para hacerle descubrir sus deliciosas pizzas."
