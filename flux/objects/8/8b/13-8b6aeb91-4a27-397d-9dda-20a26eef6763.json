  "rdfs:comment": {
    "de": [
      "Pub-Restaurant am Yachthafen von Saint Laurent du Var.\r\nEine große Auswahl an Whisky, Bierkeller, Fassbier (Bud, Leffe, Guinness, Magners cider, Martin's pale ale, Proper Job Ipa, Erdinger Weiss), Cocktails, Tapas, Hamburger"
    ],
    "en": [
      "Pub restaurant located on the marina of Saint Laurent du Var.\r\nA large selection of whisky, beer cellar, draught (Bud, Leffe, Guinness, Magners cider, Martin's pale ale, Proper Job Ipa, Erdinger Weiss), cocktails, tapas, hamburger"
    ],
    "it": [
      "Pub ristorante situato sul porto turistico di Saint Laurent du Var.\r\nAmpia selezione di whisky, cantina di birre alla spina (Bud, Leffe, Guinness, sidro Magners, Martin's pale ale, Proper Job Ipa, Erdinger Weiss), cocktail, tapas, hamburger"
    ],
    "fr": [
      "Pub restaurant situé sur le port de plaisance de Saint Laurent du Var.\r\nUne grande sélection de whisky, cave à bières, pressions (Bud, Leffe, Guinness, Magners cider, Martin's pale ale, Proper Job Ipa, Erdinger Weiss), cocktails, tapas, hamburger"
    ],
    "es": [
      "Pub restaurante situado en el puerto deportivo de Saint Laurent du Var.\r\nUna gran selección de whisky, bodega de cerveza, de barril (Bud, Leffe, Guinness, sidra Magners, Martin's pale ale, Proper Job Ipa, Erdinger Weiss), cócteles, tapas, hamburguesas"
    ],
    "nl": [
      "Pub restaurant gelegen aan de jachthaven van Saint Laurent du Var.\r\nGrote keuze aan whisky, bierkelder, tapbier (Bud, Leffe, Guinness, Magners cider, Martin's pale ale, Proper Job Ipa, Erdinger Weiss), cocktails, tapas, hamburger"
    ]
  },
