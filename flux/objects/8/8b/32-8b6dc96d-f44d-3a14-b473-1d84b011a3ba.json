  "rdfs:comment": {
    "de": [
      "Paillotte und Strandrestaurant, in dem Matratzen und Sonnenschirme vermietet werden. Bar und Taverne. Restaurantsaal mit Mittag- und Abendservice. Themenabende, Animationen, Hochzeiten, private Feiern."
    ],
    "en": [
      "Beach hut and restaurant offering mattress and umbrella rental. Bar-tapas. Restaurant room for lunch and dinner. Theme evenings, animations, weddings, private parties."
    ],
    "it": [
      "Capanno e ristorante sulla spiaggia con noleggio di materassini e ombrelloni. Bar-tapas. Ristorante che serve pranzo e cena. Serate a tema, intrattenimento, matrimoni, feste private."
    ],
    "fr": [
      "Paillotte et restaurant de plage proposant la location de matelas et parasols. Bar-tapas. Salle de restaurant en service midi et soir. Soirées à thèmes, animations, mariage, soirée privée."
    ],
    "nl": [
      "Strandhut en restaurant met verhuur van matrassen en parasols. Bar-tapas. Restaurant met lunch en diner. Thema-avonden, entertainment, bruiloften, privé-feesten."
    ],
    "es": [
      "Cabaña de playa y restaurante que ofrece alquiler de colchonetas y sombrillas. Bar-tapas. Restaurante que sirve comidas y cenas. Veladas temáticas, animación, bodas, fiestas privadas."
    ]
  },
