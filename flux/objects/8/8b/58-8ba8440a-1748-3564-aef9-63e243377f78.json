  "rdfs:comment": {
    "de": [
      "Die Domaine de Sulauze ist ein Weingut, das biodynamische Weine herstellt, die von einer AOC Coteaux d'Aix profitieren."
    ],
    "en": [
      "The Domaine de Sulauze is a vineyard producing biodynamic wines with an AOC Coteaux d'Aix."
    ],
    "it": [
      "Il Domaine de Sulauze è un vigneto che produce vini biodinamici con una AOC Coteaux d'Aix."
    ],
    "fr": [
      "Le Domaine de Sulauze est un vignoble élaborant des vins en biodynamie bénéficiant d'une AOC Coteaux d'Aix."
    ],
    "nl": [
      "Het Domaine de Sulauze is een wijngaard die biodynamische wijnen produceert met een AOC Coteaux d'Aix."
    ],
    "es": [
      "El Domaine de Sulauze es un viñedo que produce vinos biodinámicos con una AOC Coteaux d'Aix."
    ]
  },
