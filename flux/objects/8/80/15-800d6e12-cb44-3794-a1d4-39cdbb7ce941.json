  "rdfs:comment": {
    "de": [
      "Sich die Techniken des Drehens und Drechselns aneignen. Erfahrungen mit dem Backen im Holzofen sammeln."
    ],
    "en": [
      "Acquire the techniques of throwing and turning. Experience firing in a wood-fired kiln."
    ],
    "it": [
      "Acquisire le tecniche di tornitura e filatura. Esperienza di cottura in un forno a legna."
    ],
    "fr": [
      "Acquérir les techniques de tournage et de tournassage. Faire l’expérience de la cuisson en four à bois."
    ],
    "nl": [
      "Leer de technieken van het draaien en gooien. Ervaar het bakken in een houtgestookte oven."
    ],
    "es": [
      "Adquirir las técnicas de torneado e hilado. Experimente la cocción en un horno de leña."
    ]
  },
