  "rdfs:comment": {
    "de": [
      "Hotel-Restaurant mit Blick auf die Loire und gegenüber der Gemeinde Ingrandes sur Loire. Auf der Strecke der Loire à Vélo. 30 Minuten von Angers entfernt. Regionale Küche, die mit Produkten der Saison zubereitet wird."
    ],
    "en": [
      "Hotel-restaurant with a view of the Loire and facing the town of Ingrandes sur Loire. On the circuit of the Loire by bike. 30 minutes from Angers. Local cuisine made with seasonal products."
    ],
    "it": [
      "Hotel-ristorante con vista sulla Loira e di fronte alla città di Ingrandes sur Loire. Sul circuito della Loire à Vélo. A 30 minuti da Angers. Cucina locale a base di prodotti di stagione."
    ],
    "fr": [
      "Hôtel-restaurant avec vue sur Loire et face à la commune d'Ingrandes sur Loire. Sur le circuit de la Loire à Vélo. A 30 minutes d'Angers. Cuisine du terroir confectionnée avec les produits de saison."
    ],
    "es": [
      "Hotel-restaurante con vistas al Loira y frente a la ciudad de Ingrandes sur Loire. En el circuito de la Loire à Vélo. A 30 minutos de Angers. Cocina local elaborada con productos de temporada."
    ],
    "nl": [
      "Hotel-restaurant met uitzicht op de Loire en de stad Ingrandes sur Loire. Op het circuit van de Loire à Vélo. 30 minuten van Angers. Lokale keuken gemaakt met seizoensproducten."
    ]
  },
