  "rdfs:comment": {
    "de": [
      "Das Hotel Au Puits Enchanté empfängt Sie im Herzen der Bresse Bourguignonne für eine schmackhafte und authentische Mahlzeit."
    ],
    "en": [
      "The Hotel Au Puits Enchanté welcomes you in the heart of Bresse Bourguignonne for a tasty and authentic meal."
    ],
    "it": [
      "L'Hotel Au Puits Enchanté vi accoglie nel cuore della regione Bresse Bourguignonne per un pasto gustoso e autentico."
    ],
    "fr": [
      "L’Hôtel Au Puits Enchanté vous accueille au cœur de la Bresse Bourguignonne pour un repas savoureux et authentique."
    ],
    "nl": [
      "Hotel Au Puits Enchanté verwelkomt u in het hart van Bresse Bourguignonne voor een smakelijke en authentieke maaltijd."
    ],
    "es": [
      "El Hotel Au Puits Enchanté le da la bienvenida en el corazón de la región de Bresse Bourguignonne para una comida sabrosa y auténtica."
    ]
