  "rdfs:comment": {
    "de": [
      "Emilie Vernier ist eine Sportlerin, die Apnoe als Wettkampfsport betreibt. Nutzen Sie ihre Fähigkeiten, um sich in Ihrem Urlaub in das Schnuppertauchen einzuführen."
    ],
    "en": [
      "Emilie Vernier is an athlete who practices freediving in competition. Take advantage of her skills to introduce you to freediving during your holidays."
    ],
    "it": [
      "Emilie Vernier è un'atleta che pratica l'apnea a livello agonistico. Approfittate delle sue capacità per introdurvi all'apnea durante le vostre vacanze."
    ],
    "fr": [
      "Emilie Vernier est une athlète qui pratique l'apnée en compétition. Profitez de ses compétences pour vous initier au baptême d'apnée le temps de vos vacances."
    ],
    "nl": [
      "Emilie Vernier is een atlete die apneu beoefent in competitie. Maak gebruik van haar vaardigheden om u kennis te laten maken met freediving tijdens uw vakantie."
    ],
    "es": [
      "Emilie Vernier es una deportista que practica la apnea en competición. Aprovecha sus habilidades para iniciarte en la apnea durante tus vacaciones."
    ]
  },
