  "rdfs:comment": {
    "de": [
      "Ein Wochenende, eine Woche im Haut Rouergue? Erholen Sie sich in einem Ferienhaus aus Holz im Herzen des Lot-Tals."
    ],
    "en": [
      "A weekend or a week in the Haut Rouergue? Come and recharge your batteries in a wooden gite in the Lot valley."
    ],
    "it": [
      "Un weekend o una settimana nell'Haut Rouergue? Venite a fare il pieno di energia in un gîte in legno nel cuore della valle del Lot."
    ],
    "fr": [
      "Un week-end, une semaine dans le Haut Rouergue ? Venez vous ressourcer dans un gîte en bois au coeur de la vallée du Lot."
    ],
    "es": [
      "¿Un fin de semana o una semana en el Haut Rouergue? Venga a recargar las pilas en una casa rural de madera en el corazón del valle del Lot."
    ],
    "nl": [
      "Een weekend of een week in de Haut Rouergue? Kom nieuwe energie opdoen in een houten gîte in het hart van de Lot-vallei."
    ]
  },
