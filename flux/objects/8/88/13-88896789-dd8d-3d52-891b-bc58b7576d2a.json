  "rdfs:comment": {
    "de": [
      "Traditionelle saisonale Küche mit regionalen Produkten. Spezialitäten aus lokalen Käsesorten, Burger und hausgemachte Pommes frites, Wein-, Bier- und Rumkeller. Originelle hausgemachte Desserts."
    ],
    "en": [
      "Traditional seasonal cuisine with regional products. Specialties based on local cheeses, burger and home fries, wine, beer and rum cellars. Original homemade desserts."
    ],
    "it": [
      "Cucina tradizionale stagionale con prodotti regionali. Specialità a base di formaggi locali, hamburger e patatine fritte, cantine di vino, birra e rum. Dolci originali fatti in casa."
    ],
    "fr": [
      "Une cuisine traditionnelle de saison avec des produits régionaux. Spécialités à base de fromages locaux, burger et frites maison, caves à vin, à bières et à rhum. Desserts maison originaux."
    ],
    "nl": [
      "Traditionele seizoensgebonden keuken met regionale producten. Specialiteiten op basis van lokale kazen, hamburger en huisfriet, wijn-, bier- en rumkelders. Originele huisgemaakte desserts."
    ],
    "es": [
      "Cocina tradicional de temporada con productos regionales. Especialidades a base de quesos locales, hamburguesas y patatas fritas caseras, bodegas de vino, cerveza y ron. Originales postres caseros."
    ]
  },
