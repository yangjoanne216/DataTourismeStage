  "rdfs:comment": {
    "de": [
      "Pizzeria mit im Holzofen gebackenen Pizzen. Möglichkeit, vor Ort zu essen oder etwas mitzunehmen. Unsere Kriterien sind Qualität, Geschmack und Frische."
    ],
    "en": [
      "Pizzeria with wood-fired pizzas. You can eat in or take away. Our criteria are quality, taste and freshness."
    ],
    "it": [
      "Pizzeria con pizze cotte a legna. È possibile mangiare all'interno o portare via. I nostri criteri sono qualità, gusto e freschezza."
    ],
    "fr": [
      "Pizzeria avec pizzas cuites au feu de bois. Possibilité de se restaurer sur place ou à emporter. Nos critères sont la qualité, le goût et la fraicheur."
    ],
    "nl": [
      "Pizzeria met houtovenpizza's. Je kunt bij ons eten of afhalen. Onze criteria zijn kwaliteit, smaak en versheid."
    ],
    "es": [
      "Pizzería con pizzas al horno de leña. Se puede comer en el restaurante o para llevar. Nuestros criterios son la calidad, el sabor y la frescura."
    ]
  },
