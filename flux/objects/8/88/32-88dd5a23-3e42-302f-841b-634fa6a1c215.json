  "rdfs:comment": {
    "de": [
      "Zimmer \"Marsanne\", 17 m², 2. Stock. 3 km vom Stadtzentrum entfernt, auf dem Land, 14 km vom Meer entfernt. Gemeinsamer Swimmingpool. Privater Parkplatz, Badezimmer, private Sanitäranlagen. Fernseher. Bett 160 cm. Ehemaliger Weinkeller. Frühstück wird im Wohnraum oder auf der Terrasse serviert. Klimatisiertes Zimmer."
    ],
    "en": [
      "Room \"Marsanne\", 17 m², 2nd floor. 3km from the city centre, in the countryside, 14km from the sea. Collective swimming pool. Private parking, bathroom, private toilets. Television. Bed 160 cm. Old wine cellar. Breakfast served in the living room or on the terrace. Air-conditioned room."
    ],
    "it": [
      "Camera \"Marsanne\", 17 m², 2° piano. A 3 km dal centro città, in campagna, a 14 km dal mare. Piscina collettiva. Parcheggio privato, bagno, servizi igienici privati. Televisione. Letto 160 cm. Antica cantina. Colazione servita nel soggiorno o sulla terrazza. Camera con aria condizionata."
    ],
    "fr": [
      "Chambre \"Marsanne\", 17 m², 2ème étage. À 3km du centre-ville, à la campagne, à 14km du bord de mer. Piscine collective. Parking privé, salle de bain, sanitaires privés. Télévision. Lit 160 cm. Ancienne cave vinicole. Petit déjeuner servi dans la pièce de vie ou en terrasse. Chambre climatisée."
    ],
    "nl": [
      "Kamer \"Marsanne\", 17 m², 2e verdieping. 3 km van het stadscentrum, op het platteland, 14 km van zee. Gemeenschappelijk zwembad. Eigen parkeerplaats, badkamer, privétoilet. Televisie. Bed 160 cm. Oude wijnkelder. Ontbijt geserveerd in de woonkamer of op het terras. Kamer met airconditioning."
    ],
    "es": [
      "Habitación \"Marsanne\", 17 m², 2ª planta. A 3 km del centro de la ciudad, en el campo, a 14 km del mar. Piscina colectiva. Aparcamiento privado, cuarto de baño, aseos privados. Televisión. Cama 160 cm. Antigua bodega. Desayuno servido en el salón o en la terraza. Habitación climatizada."
    ]
  },
