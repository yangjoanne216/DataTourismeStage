  "rdfs:comment": {
    "de": [
      "Lage: Parkplatz des Schlosses. Parkplatz. Übernachtung möglich. Mögliche Services: Kartenverkauf, Bankautomat, Briefkasten, Telefonzelle, öffentliche Toiletten, Wasserstelle und Container."
    ],
    "en": [
      "Location : parking of the castle. Parking 24 hours a day. Services offered : ticket office, cash dispenser, mailbox, public toilets open during the castle opening hours, containers. No service area."
    ],
    "it": [
      "Ubicazione: parcheggio del castello. Parcheggio 24 ore su 24. Servizi offerti: biglietteria, bancomat, cassetta postale, bagni pubblici aperti durante gli orari di apertura del castello, container. Nessuna area di servizio."
    ],
    "fr": [
      "Situation : parking du château. Stationnement 24h/24. Services proposés : billetterie, distributeur de billets, boîte aux lettres, toilettes publiques ouvertes aux horaires d'ouverture du château, containers. Pas d'aire de services."
    ],
    "es": [
      "Ubicación: aparcamiento del castillo. Aparcamiento 24 horas. Servicios ofrecidos: taquilla, cajero automático, buzón, aseos públicos abiertos durante el horario de apertura del castillo, contenedores. No hay área de servicio."
    ],
    "nl": [
      "Locatie: parkeerplaats kasteel. 24 uur parkeren. Aangeboden diensten: loket, geldautomaat, brievenbus, openbare toiletten geopend tijdens de openingsuren van het kasteel, containers. Geen verzorgingsgebied."
    ]
  },
