  "rdfs:comment": {
    "de": [
      "Das im Osten von Le Mans gelegene, gerade wiedereröffnete Restaurant wurde in einem sehr modernen Design neu gestaltet."
    ],
    "en": [
      "Located east of Le Mans, this restaurant which has just reopened its doors has been refurbished in a very modern design."
    ],
    "it": [
      "Situato a est di Le Mans, questo ristorante recentemente riaperto è stato rinnovato con un design molto moderno."
    ],
    "fr": [
      "Situé à l'est du Mans, ce restaurant qui vient de rouvrir ses portes a été réaménagé dans un design très moderne."
    ],
    "es": [
      "Situado al este de Le Mans, este restaurante recientemente reabierto ha sido reformado con un diseño muy moderno."
    ],
    "nl": [
      "Dit onlangs heropende restaurant ten oosten van Le Mans is zeer modern ingericht."
    ]
  },
  "rdfs:label": {
