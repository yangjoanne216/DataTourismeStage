  "rdfs:comment": {
    "de": [
      "Schöne und geräumige Unterkunft (4 bis 8 Personen), auf den Anhöhen von Samoëns gelegen, inmitten der Natur, auf der Sonnenseite. Es bietet das ganze Jahr über einen angenehmen Aufenthalt."
    ],
    "en": [
      "Beautiful and spacious gîte (4 to 8 people), located on the heights of Samoëns, in the heart of nature, sunny side. It offers a pleasant stay all year round"
    ],
    "it": [
      "Bellissimo e spazioso gîte (da 4 a 8 posti letto), situato sulle alture di Samoëns, nel cuore della natura, sul lato soleggiato. Offre un soggiorno piacevole tutto l'anno."
    ],
    "fr": [
      "Beau et spacieux gîte (4 à 8 personnes), situé sur les hauteurs de Samoëns,  en pleine nature, coté soleil. Il offre un séjour agréable pendant toute l'année."
    ],
    "nl": [
      "Mooie en ruime gîte (4 tot 8 personen), gelegen op de hoogten van Samoëns, midden in de natuur, zonzijde. Het biedt het hele jaar door een aangenaam verblijf"
    ],
    "es": [
      "Bonita y espaciosa casa rural (de 4 a 8 plazas), situada en las alturas de Samoëns, en plena naturaleza, en el lado soleado. Ofrece una estancia agradable durante todo el año."
    ]
  },
