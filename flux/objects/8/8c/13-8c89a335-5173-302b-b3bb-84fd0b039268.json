  "rdfs:comment": {
    "de": [
      "Snackbar, hausgemachte Crêpes, Brot und Gebäck. Pizzas aus eigener Herstellung. Burger. Salate, Quiches und salzige Kuchen. Desserts aus eigener Herstellung. Nur abends und auf Reservierung."
    ],
    "en": [
      "Snack bar, homemade pancakes, bread and pastries. Homemade pizzas. Burgers. Salads, quiches and savoury pies. Homemade desserts. Only in the evening and by reservation."
    ],
    "it": [
      "Snack bar, frittelle, pane e dolci fatti in casa. Pizze fatte in casa. Hamburger. Insalate, quiche e torte salate. Dolci fatti in casa. Solo la sera e su prenotazione."
    ],
    "fr": [
      "Snack-bar, crêpes maison, pain et viennoiseries. Pizzas maison. Burgers. Salades, quiches et tartes salées. Desserts maison. Uniquement le soir et sur réservation."
    ],
    "nl": [
      "Snackbar, huisgemaakte pannenkoeken, brood en gebak. Zelfgemaakte pizza's. Burgers. Salades, quiches en hartige taarten. Zelfgemaakte desserts. Alleen 's avonds en op reservering."
    ],
    "es": [
      "Barra de aperitivos, tortitas caseras, pan y bollería. Pizzas caseras. Hamburguesas. Ensaladas, quiches y tartas saladas. Postres caseros. Sólo por la noche y con reserva."
    ]
  },
