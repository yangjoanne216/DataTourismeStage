  "rdfs:comment": {
    "de": [
      "Der Bellini... das kleine Gourmetglas, das mit Liebe vor Ort gekocht wird, um es hier oder anderswo zu genießen, allein oder zu mehreren!"
    ],
    "en": [
      "The Bellini... the little gourmet jar cooked on the spot with love, to be enjoyed here or elsewhere, alone or with others!"
    ],
    "it": [
      "Il Bellini... il vasetto gourmet cucinato sul momento con amore, da gustare qui o altrove, da soli o in compagnia!"
    ],
    "fr": [
      "Le Bellini... le p'tit bocal gourmand cuisiné sur place avec amour, à déguster ici ou ailleurs, en solo ou à plusieurs!"
    ],
    "es": [
      "El Bellini... el pequeño tarro gourmet cocinado al momento con amor, para disfrutarlo aquí o en otro lugar, solo o en compañía"
