  "rdfs:comment": {
    "de": [
      "Entspannen Sie sich und essen Sie auf dem Picknickplatz der Gemeinde Magné, der sich gegenüber dem städtischen Campingplatz befindet."
    ],
    "en": [
      "Come and relax and eat at the picnic area of the commune of Magné which is located opposite the municipal campsite."
    ],
    "it": [
      "Venite a rilassarvi e a mangiare nell'area picnic del comune di Magné, che si trova di fronte al campeggio comunale."
    ],
    "fr": [
      "Venez vous relaxer et vous restaurer à l'aire de pique-nique de la commune de Magné qui se situe en face du camping municipal."
    ],
    "es": [
      "Venga a relajarse y a comer en el merendero del municipio de Magné, situado frente al camping municipal."
    ],
    "nl": [
      "Kom ontspannen en eten op de picknickplaats van de gemeente Magné, die tegenover de gemeentelijke camping ligt."
    ]
  },
