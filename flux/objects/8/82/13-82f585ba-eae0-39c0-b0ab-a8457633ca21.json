  "rdfs:comment": {
    "de": [
      "Künstlerwerkstatt(Künstlerworkshop) Bildhauer - orfévre. Schaffungen einziger(einzigartiger) Stücke(Zimmer): Schmuckstücke, Bildhauerei(Skulptur), Beleuchtungskörper. Entschädigung(Reparatur) Gegenstände(Ziele) aus Metall (Gold, Geld, wertvolle Metalle)."
    ],
    "en": [
      "Artist studio sculptor - orfévre. Unique Creations : jewels, sculptures, lamps. Repair objects in metal (gold, money(silver), precious metals). Training courses and course of wax sculptures lost and creation of jewels."
    ],
    "it": [
      "Laboratorio d'artista - scultore - orafo. Creazione di pezzi unici: gioielli, sculture, illuminazione. Riparazione di oggetti in metallo (oro, argento, metalli preziosi...). Corsi di scultura a cera persa e creazione di gioielli."
    ],
    "fr": [
      "Atelier d'artiste sculpteur - orfévre. Créations de pièces uniques : bijoux, sculptures, luminaires. Réparation objets en métal (or, argent, métaux précieux...). Stages et cours de sculptures en cire perdue et création de bijoux."
    ],
    "es": [
      "Taller de artista - escultor - orfebre. Creación de piezas únicas: joyas, esculturas, iluminación. Reparación de objetos metálicos (oro, plata, metales preciosos...). Cursos de escultura a la cera perdida y creación de joyas."
    ],
    "nl": [
      "Kunstenaarswerkplaats - beeldhouwer - goudsmid. Creatie van unieke stukken: juwelen, sculpturen, verlichting. Reparatie van metalen voorwerpen (goud, zilver, edele metalen...). Cursussen in verloren was beeldhouwen en sieraden maken."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Eripret Fréderic - Sculpteur/Orfevre"
    ]
  },
