  "rdfs:comment": {
    "de": [
      "Ländliche Unterkunft in einem kleinen Dorf im Norden Lothringens.\nIn der Nähe von Verdun, Montmédy, der Festung von Fermont und der belgischen und luxemburgischen Grenze."
    ],
    "en": [
      "Rural gîte in a small village in northern Lorraine.\nClose to Verdun, Montmédy, the fort of Fermont and the Belgian and Luxembourg borders."
    ],
    "it": [
      "Gîte rurale in un piccolo villaggio nel nord della Lorena.\nVicino a Verdun, Montmédy, al forte di Fermont e ai confini con Belgio e Lussemburgo."
    ],
    "fr": [
      "Gîte rural dans un petit village du nord lorrain.\nA proximité de Verdun, Montmédy, du fort de Fermont et des frontières belges et luxembourgeoises."
    ],
    "nl": [
      "Landelijke gîte in een klein dorp in het noorden van Lotharingen.\nDicht bij Verdun, Montmédy, het fort van Fermont en de Belgische en Luxemburgse grens."
    ],
    "es": [
      "Casa rural en un pequeño pueblo del norte de Lorena.\nCerca de Verdún, Montmédy, el fuerte de Fermont y las fronteras belga y luxemburguesa."
    ]
  },
