      "dc:description": {
        "de": [
          "Gourmet-Salate, saftiges Fleisch mit kleinen Kartoffeln , Panisses, Käse- und Wurstplatten, um jeden Appetit zu stillen."
        ],
        "en": [
          "Gourmet salads, juicy meats with small potatoes, panisses, cheese and charcuterie plates, to satisfy every appetite."
        ],
        "it": [
          "Insalate gourmet, carni succulente con piccole patate, panisses, formaggi e salumi, per soddisfare tutti gli appetiti."
        ],
        "fr": [
          "Des salades gourmandes, des viandes juteuses avec des petites patates , des panisses, des assiettes de fromage et de charcuterie, pour combler tous les appétits."
        ],
        "nl": [
          "Heerlijke salades, sappig vlees met kleine aardappelen, panisses, kaas en vleeswaren, om elke honger te stillen."
        ],
        "es": [
          "Ensaladas gourmet, jugosas carnes con patatas pequeñas, panisses, platos de queso y charcutería, para satisfacer todos los apetitos."
        ]
      },
