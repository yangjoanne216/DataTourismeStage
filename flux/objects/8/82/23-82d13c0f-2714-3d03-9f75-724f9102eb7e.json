  "rdfs:comment": {
    "de": [
      "Haus im Perigourdine-Stil, freistehend auf privatem Grundstück. Überdachte Terrasse, Boulespielplatz, privater Pool, Kinderspielplatz. Wohnraum mit Küchenzeile, 3 Schlafzimmer für je 2 Personen, 2 Badezimmer."
    ],
    "en": [
      "Perigourdine house, independent on private land. Covered terrace, petanque ground, private swimming pool, games for children. Day room with kitchenette, 3 bedrooms for 2 persons, 2 shower rooms."
    ],
    "it": [
      "Casa di Périgourdine, indipendente su terreno privato. Terrazza coperta, campo da bocce, piscina privata, giochi per bambini. Sala da pranzo con angolo cottura, 3 camere da letto per 2 persone, 2 bagni."
    ],
    "fr": [
      "Maison Périgourdine, indépendante sur terrain privatif. Terrasse couverte, terrain de pétanque, piscine privative, jeux pour enfants. Pièce de jour avec coin cuisine, 3 chambres de 2 personnes, 2 salles d'eau."
    ],
    "nl": [
      "Périgourdine huis, onafhankelijk op eigen grond. Overdekt terras, petanquebaan, privé zwembad, spelletjes voor kinderen. Dagverblijf met kitchenette, 3 slaapkamers voor 2 personen, 2 badkamers."
    ],
    "es": [
      "Casa de Périgourdine, independiente en terreno privado. Terraza cubierta, pista de petanca, piscina privada, juegos para niños. Salón con cocina americana, 3 habitaciones para 2 personas, 2 baños."
    ]
  },
