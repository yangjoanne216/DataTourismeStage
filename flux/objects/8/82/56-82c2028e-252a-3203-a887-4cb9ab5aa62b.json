  "rdfs:comment": {
    "de": [
      "T2-Wohnung im 1. Stock einer ruhigen Residenz. Blick auf die Berge (Mont Coronat) und das Dorf. In der Nähe des Zentrums und 5 Minuten zu Fuß von den Thermen entfernt."
    ],
    "en": [
      "Apartment T2 located on the 1st floor of a quiet residence. View on the mountain (Mont Coronat) and on the village. Close to the centre and 5 minutes walk from the thermal baths."
    ],
    "it": [
      "Appartamento T2 situato al 1° piano di una residenza tranquilla. Vista sulla montagna (Mont Coronat) e sul villaggio. Vicino al centro e a 5 minuti a piedi dalle terme."
    ],
    "fr": [
      "Appartement T2 situé au 1er étage d'une résidence calme. Vue sur la montagne (Mont Coronat) et sur le village. Proche du centre et à 5 minutes à pied des Thermes."
    ],
    "nl": [
      "T2 flat gelegen op de 1e verdieping van een rustige residentie. Uitzicht op de berg (Mont Coronat) en op het dorp. Dicht bij het centrum en op 5 minuten lopen van de thermale baden."
    ],
    "es": [
      "Piso T2 situado en la 1ª planta de una tranquila residencia. Vista de la montaña (Mont Coronat) y del pueblo. Cerca del centro y a 5 minutos a pie de los baños termales."
    ]
  },
