  "rdfs:comment": {
    "de": [
      "Bei Petite Mila wird ethisch korrekt produziert, indem Körper und Geist gepflegt werden."
    ],
    "en": [
      "Petite Mila is a French brand that has been producing graphic designs based on organic cotton for several years now."
    ],
    "it": [
      "Noi di Petite Mila produciamo in modo etico, prendendoci cura del corpo e della mente."
    ],
    "fr": [
      "Chez Petite Mila, on fabrique de manière éthique en prenant soin du corps et de l'esprit."
    ],
    "es": [
      "En Petite Mila, fabricamos de forma ética, cuidando el cuerpo y la mente."
    ],
    "nl": [
      "Bij Petite Mila produceren we ethisch, met zorg voor lichaam en geest."
    ]
