  "rdfs:comment": {
    "de": [
      "Das Maison des Curistes ist ein geselliger Ort im Erdgeschoss des Kulturraums mitten im Stadtzentrum von Le Mont-Dore, der allen Kurgästen und ihren Begleitern offen steht."
    ],
    "en": [
      "The Maison des Curistes is a friendly place, located on the first floor of the cultural space, in the heart of downtown Le Mont-Dore, open to all curists and their companions."
    ],
    "it": [
      "La Maison des Curistes è un luogo accogliente, situato al piano terra dello spazio culturale, nel centro di Le Mont-Dore, aperto a tutti i curisti e ai loro accompagnatori."
    ],
    "fr": [
      "La Maison des Curistes est un lieu convivial, situé au rez-de-chaussée de l'espace culturel, en plein centre-ville du Mont-Dore, ouvert à tous les curistes et leurs accompagnants."
    ],
    "es": [
      "La Maison des Curistes es un lugar acogedor, situado en la planta baja del espacio cultural, en el centro de Le Mont-Dore, abierto a todos los curistas y sus acompañantes."
    ],
    "nl": [
      "Het Maison des Curistes is een gezellige plek, gelegen op de begane grond van de culturele ruimte, in het centrum van Le Mont-Dore, open voor alle curisten en hun begeleiders."
    ]
  },
