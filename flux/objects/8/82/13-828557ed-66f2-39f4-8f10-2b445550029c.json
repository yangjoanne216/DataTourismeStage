  "rdfs:comment": {
    "de": [
      "Die Domaine des Collettes, der Garten der Villa des Künstlers Pierre Auguste Renoir, beherbergt wunderschöne, mehrere hundert Jahre alte Olivenbäume sowie zahlreiche Pflanzenarten."
    ],
    "en": [
      "Domaine des Collettes, the garden of the artist Pierre Auguste Renoir’s villa, is home to magnificent centuries-old olive trees and many other plants."
    ],
    "it": [
      "Il Domaine des Collettes, il giardino della villa dell'artista Pierre Auguste Renoir, ospita magnifici ulivi di diverse centinaia di anni e numerose specie di piante."
    ],
    "fr": [
      "Le domaine des Collettes, jardin de la villa de l’artiste Pierre Auguste Renoir, abrite de magnifiques oliviers pluri centenaires ainsi que de nombreuses espèces végétales."
    ],
    "nl": [
      "In het Domaine des Collettes, de tuin van de villa van de kunstenaar Pierre Auguste Renoir, staan prachtige honderd jaar oude olijfbomen en talrijke plantensoorten."
    ],
    "es": [
      "El Domaine des Collettes, el jardín de la villa del artista Pierre Auguste Renoir, alberga magníficos olivos centenarios y numerosas especies vegetales."
    ]
  },
