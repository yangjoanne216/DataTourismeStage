  "rdfs:comment": {
    "de": [
      "Es ist Herbst. Sammeln Sie Kastanien und lernen Sie alle Verwendungsmöglichkeiten der Kastanie auf dem Bauernhof kennen."
    ],
    "en": [
      "It's autumn, come and pick chestnuts and discover all the uses of the chestnut tree on the farm."
    ],
    "it": [
      "È autunno, venite a raccogliere le castagne e a scoprire tutti gli usi del castagno in fattoria."
    ],
    "fr": [
      "C'est l'automne, venez ramasser des châtaignes et découvrez tous les usages du châtaignier sur la ferme."
    ],
    "nl": [
      "Het is herfst, kom kastanjes plukken en ontdek alle toepassingen van de kastanjeboom op de boerderij."
    ],
    "es": [
      "Es otoño, ven a recoger castañas y descubre todos los usos del castaño en la granja."
    ]
  },
