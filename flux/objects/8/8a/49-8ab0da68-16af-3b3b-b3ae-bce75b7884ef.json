  "rdfs:comment": {
    "de": [
      "1, 2, 3...Schlittschuhlaufen! Und mit dem Elektroroller, angeboten von der Freizeitanlage Anjou Sport Nature in La Jaille-Yvon an den Ufern der Mayenne in Anjou."
    ],
    "en": [
      "1, 2, 3...Skate! And in electric scooter, proposed by the base of leisure Anjou Sport Nature in La Jaille-Yvon on the banks of the Mayenne in Anjou."
    ],
    "it": [
      "1, 2, 3... Skate! E su uno scooter elettrico, offerto dal centro ricreativo Anjou Sport Nature di La Jaille-Yvon, sulle rive della Mayenne, nell'Anjou."
    ],
    "fr": [
      "1, 2, 3...Patinez! Et en trottinette électrique, proposée par la base de loisirs Anjou Sport Nature à La Jaille-Yvon sur les rives de la Mayenne en Anjou."
    ],
    "nl": [
      "1, 2, 3...Skate! En op een elektrische scooter, aangeboden door het recreatiecentrum Anjou Sport Nature in La Jaille-Yvon aan de oevers van de Mayenne in Anjou."
    ],
    "es": [
      "1, 2, 3... ¡Patina! Y en patinete eléctrico, que ofrece el centro de ocio Anjou Sport Nature de La Jaille-Yvon, a orillas del Mayenne, en Anjou."
    ]
