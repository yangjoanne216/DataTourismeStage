  "rdfs:comment": {
    "de": [
      "Tajines, Couscous, Pastillas, orientalisches Gebäck, Minztee... finden Sie die wahren Geschmäcker Marokkos und einige Spezialitäten französischer Gerichte."
    ],
    "en": [
      "Tajines, couscous, pastillas, oriental pastries, mint tea...find the true flavours of Morocco and some specialities of French dishes."
    ],
    "it": [
      "Tajine, couscous, pastillas, pasticcini orientali, tè alla menta... scoprite i veri sapori del Marocco e alcune specialità della cucina francese."
    ],
    "fr": [
      "Tajines, couscous, pastillas, pâtisseries orientales, thé à la menthe...retrouvez les vraies saveurs du Maroc et quelques spécialités de plats français."
    ],
    "es": [
      "Tajines, cuscús, pastillas, pastas orientales, té a la menta... encuentre los verdaderos sabores de Marruecos y algunas especialidades de la cocina francesa."
    ],
    "nl": [
      "Tajines, couscous, pastilla's, oosters gebak, muntthee... ontdek de echte smaken van Marokko en enkele specialiteiten van Franse gerechten."
    ]
