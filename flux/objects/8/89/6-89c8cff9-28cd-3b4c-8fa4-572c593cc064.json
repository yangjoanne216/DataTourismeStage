  "rdfs:comment": {
    "de": [
      "Kommen Sie mit Montgolfières en Charolais zu einer Lufttaufe im Heißluftballon und entdecken Sie das Charolais - Brionnais."
    ],
    "en": [
      "Come and take a first flight in a hot air balloon with Montgolfières en Charolais to discover the Charolais - Brionnais region."
    ],
    "it": [
      "Venite a fare un primo volo in mongolfiera con Montgolfières en Charolais per scoprire la regione Charolais - Brionnais."
    ],
    "fr": [
      "Venez faire un baptême de l'air en montgolfière avec Montgolfières en Charolais à la découverte du Charolais - Brionnais."
    ],
    "es": [
      "Venga a disfrutar de un primer vuelo en globo aerostático con Montgolfières en Charolais para descubrir la región de Charolais - Brionnais."
    ],
