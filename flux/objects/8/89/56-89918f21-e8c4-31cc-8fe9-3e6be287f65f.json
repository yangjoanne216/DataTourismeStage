  "rdfs:comment": {
    "de": [
      "Minigolf mit 18 Löchern. Leuchtende Bälle in den Nachtpartien.\nSüßer Bereich mit einer Terrasse in gemütlicher Atmosphäre. Bereiche für Geburtstage.\nEintritt 9? - kostenlos für Kinder unter 5 Jahren."
    ],
    "en": [
      "Minigolf 18 holes. Luminous balls in night games.\nSweet area with its cosy terrace. Birthday area.\nAdmission 9? - free for children under 5 years old."
    ],
    "it": [
      "Campo da minigolf a 18 buche. Palline illuminate per il gioco notturno.\nArea dolci con un'accogliente terrazza. Area compleanni.\nIngresso 9? - gratuito per i bambini sotto i 5 anni."
    ],
    "fr": [
      "Minigolf 18 trous. Balles lumineuses en parties nocturnes. \nEspace sucré avec sa terrasse ambiance cosy. Espaces anniversaires.\nEntrée 9€ - gratuit pour les moins de 5 ans."
    ],
    "es": [
      "Campo de minigolf de 18 hoyos. Bolas iluminadas para jugar de noche.\nZona dulce con su acogedora terraza. Zona de cumpleaños.\nEntrada 9? - gratis para menores de 5 años."
    ],
    "nl": [
      "18-holes midgetgolfbaan. Verlichte ballen om 's nachts te spelen.\nLiefdesruimte met gezellig terras. Verjaardagsruimte.\nToegang 9? - gratis voor kinderen onder de 5 jaar."
    ]
  },
