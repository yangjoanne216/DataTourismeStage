  "rdfs:comment": {
    "de": [
      "Ein 35 Hektar großes Gelände: frische Luft, Entspannung, Sportanlagen, Spaziergänge allein oder mit der Familie, Spiele, Picknicks, bukolisches Faulenzen... das echte Landleben, 10 km von Paris entfernt."
    ],
    "en": [
      "A 35-hectare estate: open air, relaxation, sports facilities, solo or family walks, games, picnics, bucolic idleness... the real countryside 10 km from Paris."
    ],
    "it": [
      "Una tenuta di 35 ettari: aria aperta, relax, impianti sportivi, passeggiate in solitaria o in famiglia, giochi, picnic, ozio bucolico... la vera campagna a 10 km da Parigi."
    ],
    "fr": [
      "Un domaine de 35 hectares : plein air, détente, installations sportives, promenades en solo ou en famille, jeux, pique-nique, farniente bucolique... la vraie campagne à 10 km de Paris."
    ],
    "es": [
      "Una finca de 35 hectáreas: aire libre, relajación, instalaciones deportivas, paseos en solitario o en familia, juegos, picnics, ocio bucólico... el verdadero campo a 10 km de París."
    ],
    "nl": [
      "Een domein van 35 hectare: open lucht, ontspanning, sportfaciliteiten, wandelingen met het gezin of alleen, spelletjes, picknicks, landelijk nietsdoen... het echte platteland op 10 km van Parijs."
    ]
  },
