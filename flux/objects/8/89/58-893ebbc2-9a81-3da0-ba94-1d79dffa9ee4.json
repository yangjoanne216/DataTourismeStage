  "rdfs:comment": {
    "de": [
      "Abschnitt C01 des GR2013. Von Saint Antoine aus in Richtung Sainte Marthe. Eine 4,1 km lange Strecke, auf der Sie das Reservoir des Vallon de Dol entdecken, bevor Sie in die Wohnviertel von Marseille gelangen"
    ],
    "en": [
      "Section C01 of the GR2013. From Saint Antoine to Sainte Marthe. A 4,1km walk to discover the Vallon de Dol reservoir, before entering the residential areas of Marseille"
    ],
    "it": [
      "Sezione C01 del GR2013. Da Saint Antoine a Sainte Marthe. Un percorso di 4,1 km per scoprire il bacino di Vallon de Dol, prima di entrare nei quartieri residenziali di Marsiglia"
    ],
    "fr": [
      "Tronçon C01 du GR2013. Au départ de Saint Antoine vers Sainte Marthe. Un trajet de 4,1km pour découvrir le réservoir du Vallon de Dol, avant d'entrer dans les quartiers résidentiels de Marseille"
    ],
    "es": [
      "Sección C01 del GR2013. De Saint Antoine a Sainte Marthe. Un recorrido de 4,1 km para descubrir el embalse de Vallon de Dol, antes de entrar en las zonas residenciales de Marsella"
    ],
    "nl": [
      "Afdeling C01 van de GR2013. Van Saint Antoine tot Sainte Marthe. Een route van 4,1 km om het stuwmeer van Vallon de Dol te ontdekken, voordat u de woonwijken van Marseille binnengaat"
    ]
  },
