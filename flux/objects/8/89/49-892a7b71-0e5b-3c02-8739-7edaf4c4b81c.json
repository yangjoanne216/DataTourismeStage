  "rdfs:comment": {
    "de": [
      "Amandine FRESNEAU, Önologin der Domaine de Cézin, empfängt Sie zu einer sensorischen und geschmacklichen Entdeckung der Weine des Loir-Tals."
    ],
    "en": [
      "Amandine FRESNEAU, oenologist of the Domaine de Cézin welcomes you for a sensory and gustative discovery of the wines of the Loir Valley."
    ],
    "it": [
      "Amandine FRESNEAU, enologa del Domaine de Cézin, vi accoglie per una scoperta sensoriale e gustativa dei vini della Valle del Loir."
    ],
    "fr": [
      "Amandine FRESNEAU, oenologue du Domaine de Cézin vous reçoit pour une découverte sensorielle et gustative des vins de la Vallée du Loir."
    ],
    "nl": [
      "Amandine FRESNEAU, oenologe van het Domaine de Cézin heet u welkom voor een zintuiglijke en smaakvolle ontdekking van de wijnen van de Loir-vallei."
    ],
    "es": [
      "Amandine FRESNEAU, enóloga del Domaine de Cézin le da la bienvenida para un descubrimiento sensorial y gustativo de los vinos del Valle del Loir."
