  "rdfs:comment": {
    "de": [
      "Park, der die West- und Ostseite von Terville miteinander verbindet. Eine grüne Oase, in der Sie neue Energie tanken können. Genießen Sie einen ruhigen Moment im Parc Chatillon."
    ],
    "en": [
      "Park linking the western and eastern sides of Terville. A green setting to recharge your batteries. Come and enjoy a peaceful moment in Chatillon Park."
    ],
    "it": [
      "Parco che collega i lati ovest ed est di Terville. Un ambiente verde per ricaricare le batterie. Venite a godervi un momento di pace nel Parco di Chatillon."
    ],
    "fr": [
      "Parc reliant le côté ouest et est de Terville. Un écrin de verdure pour se ressourcer. Venez profiter d'un moment paisible au parc Chatillon."
    ],
    "es": [
      "Parque que une los lados oeste y este de Terville. Un entorno verde para recargar las pilas. Venga a disfrutar de un momento de tranquilidad en el parque de Chatillon."
    ],
    "nl": [
      "Park dat de west- en oostkant van Terville verbindt. Een groene omgeving om je batterijen op te laden. Kom en geniet van een rustig moment in Chatillon Park."
    ]
  },
