  "rdfs:comment": {
    "de": [
      "Chalet auf dem Bauernhof. Versand in ganz Frankreich, Kalb, Kuh, Schwein, vakuumverpackt in Kisten, fertig zum Einfrieren, unsere Produkte und Rezepte."
    ],
    "en": [
      "Chalet at the farm. We send all over France, veal, cow, pig, vacuum packed in boxes, ready to freeze, our products and our recipes."
    ],
    "it": [
      "Chalet in fattoria. Spedizione in tutta la Francia, carne di vitello, mucca, maiale, confezionata sottovuoto in scatole, pronta da congelare, i nostri prodotti e le nostre ricette."
    ],
    "fr": [
      "Chalet à la ferme. Envoi partout en France, veau, vache, cochon, sous vide en caissette, prêt à congeler, nos produits et nos recettes."
    ],
    "es": [
      "Chalet en la granja. Envíos a toda Francia, ternera, vaca, cerdo, envasados al vacío en cajas, listos para congelar, nuestros productos y nuestras recetas."
    ],
    "nl": [
      "Chalet bij de boerderij. Verzending over heel Frankrijk, kalfsvlees, koe, varken, vacuüm verpakt in dozen, klaar om in te vriezen, onze producten en onze recepten."
    ]
  },
