  "rdfs:comment": {
    "de": [
      "27 Hektar großer Stausee am Cher (klassifiziert als 1. Kategorie), unterhalb des Stausees (8 km von Prat bis Lavault-Sainte-Anne) + Nebenflüsse 1. Kategorie (Bachforelle, Gründling, Elritze, Ukelei dominieren)."
    ],
    "en": [
      "Reservoir of 27 hectares on the Cher (classified 1st category), downstream of the reservoir (8 km from Prat to Lavault-Sainte-Anne) + tributaries 1st category (fario trout, gudgeon, minnow, bleak dominant)."
    ],
    "it": [
      "Bacino di 27 ettari sullo Cher (classificato di 1a categoria), a valle del bacino (8 km da Prat a Lavault-Sainte-Anne) + affluenti di 1a categoria (trota, scardola, minnow, alborella dominante)."
    ],
    "fr": [
      "Retenue de 27 hectares sur le Cher (classée en 1re catégorie), à l'aval de la retenue (8 km de Prat à Lavault-Sainte-Anne) + affluents 1re catégorie (truite fario, goujon, vairon, ablette dominants)."
    ],
    "nl": [
      "Reservoir van 27 hectare aan de Cher (ingedeeld als 1ste categorie), stroomafwaarts van het reservoir (8 km van Prat tot Lavault-Sainte-Anne) + zijrivieren van 1ste categorie (forel, riviergrondel, minnow, alver)."
    ],
    "es": [
      "Embalse de 27 hectáreas en el Cher (clasificado de 1a categoría), aguas abajo del embalse (8 km de Prat a Lavault-Sainte-Anne) + afluentes de 1a categoría (trucha, gobio, pececillo, dominante en el bleak)."
    ]
  },
