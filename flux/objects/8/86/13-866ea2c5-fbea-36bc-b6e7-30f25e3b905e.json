  "rdfs:comment": {
    "de": [
      "Restaurant auf dem Dorfplatz. Küche mit mediterranen Aromen und regionalen Produkten. Bar mit Austern, Muscheln und Sushi vor Ort oder zum Mitnehmen. Einladende Terrasse mit Bänken und Sesseln."
    ],
    "en": [
      "Restaurant on the village square. Mediterranean cuisine, local products. Oyster, shellfish and sushi bar on site or to take away. Welcoming terrace with benches and armchairs."
    ],
    "it": [
      "Ristorante sulla piazza del paese. Cucina di sapori mediterranei, prodotti locali. Bar di ostriche, conchiglie e sushi sul posto o da portare via. Terrazza accogliente con panchine e poltrone."
    ],
    "fr": [
      "Restaurant sur la place du village. Cuisine de saveurs méditerranéennes, produits du terroir. Bar à huîtres, coquillages et sushis sur place ou à emporter. Terrasse accueillante avec banquettes et fauteuils."
    ],
    "es": [
      "Restaurante en la plaza del pueblo. Cocina mediterránea, productos locales. Barra de ostras, marisco y sushi in situ o para llevar. Terraza acogedora con bancos y sillones."
    ],
    "nl": [
      "Restaurant op het dorpsplein. Mediterrane keuken, lokale producten. Oester-, schelpdieren- en sushibar ter plaatse of om mee te nemen. Gezellig terras met banken en fauteuils."
    ]
  },
