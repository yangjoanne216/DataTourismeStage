  "rdfs:comment": {
    "de": [
      "Nächtliche Kanufahrt auf der Creuse und anschließend durch die Oberstadt von Le Blanc mit Kostproben lokaler Produkte."
    ],
    "en": [
      "Night-time canoe trip on the Creuse and then in the upper town of Le Blanc with tasting of local products."
    ],
    "it": [
      "Gita notturna in canoa sulla Creuse e poi nella città alta di Le Blanc con degustazione di prodotti locali."
    ],
    "fr": [
      "Parcours en nocturne en canoë sur la Creuse puis dans la ville-haute du Blanc avec dégustations de produits locaux."
    ],
    "es": [
      "Paseo nocturno en canoa por el Creuse y luego en la ciudad alta de Le Blanc con degustación de productos locales."
    ],
    "nl": [
      "Nachtelijke kanotocht op de Creuse en vervolgens in de bovenstad Le Blanc met proeverij van streekproducten."
    ]
  },
