      "dc:description": {
        "de": [
          "Balkon, Küchenzeile, Backofen, Mikrowelle, Waschmaschine, Internetzugang, Wifi-Zugang, Fernseher, Wohnzimmer, elektrische Heizung, Dusche, 1 Schlafzimmer, 1 Doppelbett, 1 Klappsofa, Handtücher und Bettwäsche vorhanden. Kurtaxe und Mietkosten inbegriffen."
        ],
        "en": [
          "Balcony, kitchenette, oven, microwave, washing machine, internet access, wifi access, television, living room, electric heating, shower, 1 bedroom, 1 double bed, 1 sofa bed, bathroom and household linen provided. Tourist tax and rental charges included."
        ],
        "it": [
          "Balcone, angolo cottura, forno, microonde, lavatrice, accesso internet, accesso wifi, televisione, soggiorno, riscaldamento elettrico, doccia, 1 camera da letto, 1 letto matrimoniale, 1 divano letto, bagno e biancheria per la casa forniti. Tassa di soggiorno e spese di affitto incluse."
        ],
        "fr": [
          "Balcon, coin cuisine, four, micro-ondes, lave-linge, accès internet, accès wifi, télévision, séjour, chauffage électrique, douche, 1 chambre, 1 lit/2 pers., 1 banquette clic-clac, linge de toilette et de maison fourni. Taxe de séjour et charges locatives comprises."
        ],
        "nl": [
          "Balkon, kitchenette, oven, magnetron, wasmachine, internettoegang, wifi, televisie, woonkamer, elektrische verwarming, douche, 1 slaapkamer, 1 tweepersoonsbed, 1 slaapbank, badkamer en huishoudlinnen aanwezig. Toeristenbelasting en huurkosten inbegrepen."
        ],
        "es": [
          "Balcón, cocina americana, horno, microondas, lavadora, acceso a internet, acceso wifi, televisión, salón, calefacción eléctrica, ducha, 1 dormitorio, 1 cama doble, 1 sofá cama, cuarto de baño y ropa de hogar proporcionados. Tasa turística y gastos de alquiler incluidos."
        ]
      },
