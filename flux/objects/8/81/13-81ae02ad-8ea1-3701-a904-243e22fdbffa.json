  "rdfs:comment": {
    "de": [
      "Gleitschirmtaufen in der Umgebung von Grenoble. Teilen Sie unser Gefühl des Gleitens und der Freiheit. Werfen Sie einen ganz neuen Blick auf unsere Berge und Täler."
    ],
    "en": [
      "Paragliding baptisms around Grenoble. Share our sensations of gliding and freedom. Come and take a new look at our mountains and valleys."
    ],
    "it": [
      "Battesimo in parapendio nei dintorni di Grenoble. Condividere le nostre sensazioni di scivolamento e libertà. Venite a dare un nuovo sguardo alle nostre montagne e valli."
    ],
    "fr": [
      "Baptêmes de parapente autour de Grenoble. Partagez nos sensations de glisse et de liberté. Venez jeter un regard inédit sur nos montagnes et nos vallées."
    ],
    "nl": [
      "Paragliding dopen rond Grenoble. Deel onze sensaties van zweven en vrijheid. Kom een nieuwe blik werpen op onze bergen en valleien."
    ],
    "es": [
      "Bautizos de parapente en los alrededores de Grenoble. Comparta nuestras sensaciones de deslizamiento y libertad. Venga a echar una nueva mirada a nuestras montañas y valles."
    ]
  },
