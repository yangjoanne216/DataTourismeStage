  "rdfs:comment": {
    "de": [
      "Eiersuche um 10:30 Uhr,\nVerkostung von drei Weinen um 11:30 Uhr,\nVerkauf im Glas und in der Flasche,\nSpiele, Animationen und Überraschungen für Kinder, von 10 bis 14 Uhr."
    ],
    "en": [
      "Egg hunt at 10:30 am,\nTasting of 3 wines at 11:30 am,\nSale by the glass and by the bottle,\nGames, animations and surprises for the children, from 10 am to 2 pm."
    ],
    "it": [
      "Caccia alle uova alle 10.30,\nDegustazione di 3 vini alle 11.30,\nVendita al bicchiere e in bottiglia,\nGiochi, animazione e sorprese per i bambini, dalle 10.00 alle 14.00."
    ],
    "fr": [
      "Chasse aux œufs à 10h30,\nDégustation de 3 vins à 11h30,\nVente au verre et à la bouteille,\nJeux, animations et surprises pour les enfants, de 10h à 14h."
    ],
    "nl": [
      "Eieren zoeken om 10.30 uur,\nProeverij van 3 wijnen om 11.30 uur,\nVerkoop per glas en per fles,\nSpelletjes, entertainment en verrassingen voor kinderen, van 10.00 tot 14.00 uur."
    ],
    "es": [
      "Búsqueda de huevos a las 10.30 horas,\nDegustación de 3 vinos a las 11.30 horas,\nVenta por copa y por botella,\nJuegos, entretenimiento y sorpresas para los niños, de 10 a 14 horas."
    ]
  },
