  "rdfs:comment": {
    "de": [
      "Wir sind seit 30 Jahren im Obst- und Gemüsehandel tätig und begrüßen Sie in einer freundlichen Atmosphäre."
    ],
    "en": [
      "Professionals in fruits and vegetables for 30 years, we welcome you in a friendly atmosphere."
    ],
    "it": [
      "Professionisti del settore ortofrutticolo da 30 anni, vi accogliamo in un'atmosfera amichevole."
    ],
    "fr": [
      "Professionnels dans les fruits et légumes depuis 30 ans, nous vous accueillons dans une ambiance conviviale."
    ],
    "nl": [
      "Professionals in groenten en fruit sinds 30 jaar, wij verwelkomen u in een vriendelijke sfeer."
    ],
    "es": [
      "Profesionales de la fruta y la verdura desde hace 30 años, le damos la bienvenida en un ambiente acogedor."
    ]
