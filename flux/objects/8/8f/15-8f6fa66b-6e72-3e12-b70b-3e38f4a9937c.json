  "rdfs:comment": {
    "de": [
      "Hülsenfrüchte, Speisesamen, biologische Landwirtschaft. Verkauf auf dem Bauernhof von Montag bis Freitag nach Vereinbarung. Kontaktieren Sie Camille unter 07 88 23 04 06"
    ],
    "en": [
      "Dried vegetables, seeds for consumption, organic farming. Sale at the farm from Monday to Friday by appointment. Contact Camille on 07 88 23 04 06"
    ],
    "it": [
      "Verdure essiccate, semi per il consumo, agricoltura biologica. Vendita in azienda dal lunedì al venerdì su appuntamento. Contattare Camille al numero 07 88 23 04 06"
    ],
    "fr": [
      "Légumes secs, graines de consommation, agriculture biologique. Vente à la ferme du lundi au vendredi sur rendez-vous. Contactez Camille au  07 88 23 04 06"
    ],
    "es": [
      "Hortalizas secas, semillas para el consumo, agricultura ecológica. Venta en la granja de lunes a viernes con cita previa. Contacte con Camille en el 07 88 23 04 06"
    ],
    "nl": [
      "Gedroogde groenten, zaden voor consumptie, biologische landbouw. Verkoop op de boerderij van maandag tot vrijdag op afspraak. Neem contact op met Camille op 07 88 23 04 06"
    ]
  },
