  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/0d27ba88-82d3-322c-85d2-dd668b8305bf",
      "dc:description": {
        "de": [
          "Studio ds neues Haus - Separater Eingang - Fernseher - Waschmaschine - Bettwäsche vorhanden - Zentralheizung - Grill - Tiere erlaubt - 1. Stock: Wohnzimmer ( Bettcouch bz 2 pers ) + Zimmer Mezzanine ( Bett 2 pers ) - Kochecke Badezimmer WC."
        ],
        "en": [
          "Studio in a recent house - Independent entrance - Television - washing machine - sheets provided - Central heating - barbecue - animals allowed - 1st floor : living room ( double sofa bed ) + mezzanine bedroom ( double bed ) - kitchenette, shower room and toilet."
        ],
        "it": [
          "Monolocale in casa recente - Ingresso indipendente - Televisione - lavatrice - lenzuola fornite - riscaldamento centrale - barbecue - animali ammessi - 1° piano: soggiorno (divano letto matrimoniale) + camera da letto soppalcata (letto matrimoniale) - angolo cottura - bagno doccia WC."
        ],
        "fr": [
          "Studio ds maison récente - Entrée indépendante - Télévision - machine à laver - draps fournis - Chauffage central - barbecue - animaux admis - 1er étage : séjour ( lit canapé bz 2 pers ) + chambre mezzanine ( lit 2 pers ) - coin cuisine salle d'eau WC."
        ],
        "nl": [
          "Studio in recent huis - Onafhankelijke ingang - Televisie - wasmachine - lakens voorzien - centrale verwarming - barbecue - dieren toegelaten - 1ste verdieping: woonkamer (dubbel sofabed) + mezzanine slaapkamer (dubbel bed) - kitchenette - douchekamer WC."
        ],
        "es": [
          "Estudio en casa reciente - Entrada independiente - Televisión - lavadora - sábanas proporcionadas - calefacción central - barbacoa - animales admitidos - 1er piso: sala de estar (sofá cama doble) + dormitorio en el entresuelo (cama doble) - cocina americana - cuarto de ducha WC."
        ]
      },
