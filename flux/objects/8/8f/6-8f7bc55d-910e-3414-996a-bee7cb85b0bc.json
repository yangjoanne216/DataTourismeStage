  "rdfs:comment": {
    "de": [
      "Pizzas, Flamencokuchen aus dem Holzofen vor Ort oder zum Mitnehmen. Pizzas ab 8.10€.\nGeöffnet von Mittwoch bis Sonntag mittags und abends."
    ],
    "en": [
      "Pizzas, flamenkuchen cooked on a wood fire on the spot or to take away. Pizzas from 8.10€.\nOpen from Wednesday to Sunday for lunch and dinner."
    ],
    "it": [
      "Pizze, flamenkuchen cotti sul fuoco a legna sul posto o da portare via. Pizze a partire dalle 8.10?\nAperto da mercoledì a domenica per pranzo e cena."
    ],
    "fr": [
      "Pizzas, flamenkuchen cuites au feu de bois sur place ou à emporter. Pizzas à partir de 8.10€.\nOuvert du mercredi au dimanche midi et soir."
    ],
    "es": [
      "Pizzas, flamenkuchen cocinados al fuego de leña en el momento o para llevar. ¿Pizzas a partir de las 8.10?\nAbierto de miércoles a domingo para almuerzos y cenas."
    ],
    "nl": [
      "Pizza's, flamenkuchen bereid op houtvuur ter plaatse of om mee te nemen. Pizza's vanaf 8.10?\nGeopend van woensdag tot zondag voor lunch en diner."
    ]
  },
