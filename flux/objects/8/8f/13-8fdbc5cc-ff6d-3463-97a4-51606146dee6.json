  "rdfs:comment": {
    "de": [
      "Restaurant im Zentrum des Dorfes. Große schattige Terrasse,klimatisierten Räumen  . Traditionelle Küche, Pizzas und Imbiss. Auch Gruppen sind herzlich willkommen."
    ],
    "en": [
      "Restaurant in the village centre. Large shady terrace and air-conditioned dining rooms. Traditional cuisine and pizzeria. Takeaway dishes and pizzas. We welcome groups."
    ],
    "it": [
      "Ristorante nel centro del paese. Ampia terrazza ombreggiata, camere climatizzate. Ristorante tradizionale, pizzeria. Pizze e piatti da asporto. Possibilità di ricevere gruppi."
    ],
    "fr": [
      "Restaurant au centre du village. Grande terrasse ombragée, salles climatisées. Restauration traditionnelle, pizzéria. Pizzas et plats à emporter. Possibilité de recevoir des groupes."
    ],
    "es": [
      "Restaurante en el centro del pueblo. Amplia terraza con sombra, habitaciones con aire acondicionado. Restaurante tradicional, pizzería. Pizzas y comidas para llevar. Posibilidad de recibir grupos."
    ],
    "nl": [
      "Restaurant in dorpscentrum. Groot schaduwrijk terras. Restaurant met airconditioning. Traditionele keuken en pizzeria. Pizza en maaltijden om mee te nemen. Ook mogelijk voor groepen."
    ]
