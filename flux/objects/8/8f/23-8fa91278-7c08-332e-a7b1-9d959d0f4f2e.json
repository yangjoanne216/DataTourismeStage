  "rdfs:comment": {
    "de": [
      "Kino mit mehreren Sälen, Café mit Verkauf von Süßigkeiten und Getränken.\nPopuläre Filme und Filme mit großem Unterhaltungswert. Vorpremieren mit Besuch von Filmschauspielern."
    ],
    "en": [
      "Cinema complex, cafe with selling sweets and drinks.\nProjection of popular films and big shows."
    ],
    "it": [
      "Cinema multisala, bar con vendita di dolci e bevande.\nFilm popolari e di successo. Anteprime con attori cinematografici in visita."
    ],
    "fr": [
      "Cinéma multi-salles, café avec vente de friandises et boissons.\nFilms populaires et films à grands spectacles. Avant-premières avec venues d'acteurs de cinéma."
    ],
    "es": [
      "Cine multipantalla, cafetería con venta de dulces y bebidas.\nPelículas populares y taquilleras. Preestrenos con actores de cine visitantes."
    ],
    "nl": [
      "Bioscoop met meerdere schermen, café met verkoop van snoep en drankjes.\nPopulaire en blockbuster films. Previews met bezoekende filmacteurs."
    ]
  },
