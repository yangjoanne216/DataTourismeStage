  "rdfs:comment": {
    "de": [
      "Unabhängiges Haus am Rande eines kleinen, ruhigen Dorfes, umgeben von einer großen, eingezäunten Rasenfläche mit Bäumen, die das Tarn-Tal überblickt. Geschlossener Privatparkplatz."
    ],
    "en": [
      "Independent house on the edge of a small quiet village, surrounded by a large lawn with trees and fenced overlooking the Tarn valley. Closed private parking."
    ],
    "it": [
      "Casa indipendente ai margini di un piccolo e tranquillo villaggio, circondata da un grande prato con alberi e recinzioni con vista sulla valle del Tarn. Parcheggio privato recintato."
    ],
    "fr": [
      "Maison indépendante en bordure d'un petit village calme, entourée d'une grande pelouse arborée et clôturée dominant la vallée du Tarn. Parking privé clos."
    ],
    "nl": [
      "Vrijstaand huis aan de rand van een klein, rustig dorp, omgeven door een groot grasveld met bomen en hekwerk met uitzicht op de vallei van de Tarn. Afgesloten privé parkeerplaats."
    ],
    "es": [
      "Casa independiente en el borde de un pequeño y tranquilo pueblo, rodeado de un gran césped con árboles y vallas con vistas al valle del Tarn. Aparcamiento privado cerrado."
    ]
  },
