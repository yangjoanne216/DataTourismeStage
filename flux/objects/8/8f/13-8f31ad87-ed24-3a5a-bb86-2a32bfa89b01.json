  "rdfs:comment": {
    "de": [
      "Ruhiger Campingplatz in einem provenzalischen Dorf zwischen Nyons und Vaison la Romaine, mit Blick auf den Mont Ventoux. Gelände mit schattigen Stellplätzen von 10x10 m. Geschäfte in der Nähe. Animationen mit Verkostungen."
    ],
    "en": [
      "Campsite with individual shaded camping sites (10x10m)."
    ],
    "it": [
      "Campeggio tranquillo in un villaggio provenzale tra Nyons e Vaison la Romaine, con vista sul Mont Ventoux. Piazzole ombreggiate di 10x10 m. Negozi nelle vicinanze. Intrattenimento con degustazione di vini."
    ],
    "fr": [
      "Camping au calme dans un village provençal entre Nyons et Vaison la Romaine, avec vue sur le Mont Ventoux. Terrain avec emplacements ombragés de 10x10 m. Commerces à proximité. Animations avec dégustations."
    ],
    "nl": [
      "Rustige camping in een provençaals dorp tussen Nyons en Vaison la Romaine. Uitzicht op de Mont Ventoux. Terrein met schaduwrijke plaatsen van 10x10m. Uitstekend vertrekpunt voor uitstapjes."
    ],
    "es": [
      "Camping tranquilo en un pueblo provenzal entre Nyons y Vaison la Romaine, con vistas al Mont Ventoux. Parcelas sombreadas de 10x10 m. Tiendas cercanas. Entretenimiento con degustación de vinos."
    ]
  },
