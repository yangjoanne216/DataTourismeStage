  "rdfs:comment": {
    "de": [
      "Boutique-Atelier, Anfertigung von textilen Accessoires: Täschchen, Schals, Taschen, Schmuck... Auswahl an Stoffen für Ihre individuellen Bestellungen. Nähatelier für Anfänger ab 8 Jahren."
    ],
    "en": [
      "Workshop shop, making textile accessories: clutches, scarves, bags, jewelry ... Choice of fabrics for your personalized orders. Sewing workshop for beginners from 8 years old."
    ],
    "it": [
      "Negozio laboratorio, realizzazione di accessori tessili: pochette, sciarpe, borse, gioielli... Scelta di tessuti per i vostri ordini personalizzati. Laboratorio di cucito per principianti a partire dagli 8 anni."
    ],
    "fr": [
      "Atelier boutique, confection d'accessoires textiles : pochettes, foulards, sacs, bijoux... Choix de tissus pour vos commandes personnalisées. Atelier de couture pour débutants dès 8 ans."
    ],
    "es": [
      "Taller de confección de accesorios textiles: clutches, pañuelos, bolsos, bisutería... Elección de tejidos para sus pedidos personalizados. Taller de costura para principiantes a partir de 8 años."
    ],
    "nl": [
      "Boutique atelier, maken van textiel accessoires: clutches, sjaals, tassen, juwelen... Keuze van stoffen voor uw aangepaste bestellingen. Naaiworkshop voor beginners vanaf 8 jaar."
    ]
