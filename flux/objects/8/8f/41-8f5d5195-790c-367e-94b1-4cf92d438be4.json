  "rdfs:comment": {
    "de": [
      "5 km / Mittelschwer\nTreffpunkt am Schleusenwärterhaus, 10 Min. Fußweg vom obligatorischen Parkplatz.\nBegleitet von einem Wächter des Reservats, fahren wir abseits der Piste zum weißen Schatz.\nReservierung per SMS erforderlich.\nmax. 25 Personen."
    ],
    "en": [
      "5 km / Medium\nMeeting point at the lock house, 10 min. walk from the parking.\nAccompanied by a guard of the reserve, let's go off-road towards the white treasure.\nReservation required by SMS.\n25 people max."
    ],
    "it": [
      "5 km / Medio\nPunto d'incontro presso la casa della chiusa, a 10 minuti di cammino dal parcheggio.\nAccompagnati da una guardia di riserva, andiamo in fuoristrada verso il tesoro bianco.\nPrenotazione obbligatoria via SMS.\n25 persone al massimo."
    ],
    "fr": [
      "5 km / Moyen\nRendez-vous à la maison éclusière, 10 min. à pied du parking obligatoire.\nAccompagnés par un garde de la réserve, faisons du hors-piste vers le trésor blanc.\nRéservation obligatoire par SMS.\n25 personnes max."
    ],
    "es": [
      "5 km / Medio\nPunto de encuentro en la esclusa, a 10 min. a pie desde el aparcamiento.\nAcompañados por un guarda de la reserva, vamos fuera de pista hasta el tesoro blanco.\nReserva obligatoria por SMS.\n25 personas máx."
    ],
    "nl": [
      "5 km / Gemiddeld\nTrefpunt bij de sluis, 10 min. lopen vanaf de parkeerplaats.\nOnder begeleiding van een reservist gaan we off-road naar de witte schat.\nReserveren verplicht per SMS.\n25 personen max."
    ]
  },
