  "rdfs:comment": {
    "de": [
      "Schöne Adresse mit Terrasse am Ufer der Creuse. Französische Feinschmeckerküche... Fondu Creusois, Galettes, Crêpes, Fondue Savoyarde, Fondue Bourguignonne.\n\nÖFFNUNGSZEITEN\n- mittwoch bis samstag\n- 12:00 bis 14:00 Uhr und 19:00 bis 20:30 Uhr"
    ],
    "en": [
      "Beautiful address with terrace on the banks of the Creuse. Gourmet French cuisine... Fondu creusois, galettes, crêpes, fondue savoyarde, fondue bourguignonne.\n\nOPEN\n- wednesday to Saturday\n- 12pm to 2pm and 7pm to 8:30pm"
    ],
    "it": [
      "Bellissimo indirizzo con terrazza sulle rive della Creuse. Cucina francese gourmet... Fondu creusois, galettes, crêpes, fondue savoyarde, fondue bourguignonne.\n\nAPERTO\n- da mercoledì a sabato\n- dalle 12.00 alle 14.00 e dalle 19.00 alle 20.30"
    ],
    "fr": [
      "Belle adresse avec terrasse en bord de Creuse. Cuisine française gourmande... Fondu creusois, galettes, crêpes, fondue savoyarde, fondue bourguignonne.\n\nOUVERTURE\n- mercredi au samedi \n- 12h à 14h et 19h à 20h30"
    ],
    "nl": [
      "Prachtig adres met terras aan de oevers van de Creuse. Franse keuken voor fijnproevers... Fondu creusois, galettes, crêpes, fondue savoyarde, fondue bourguignonne.\n\nOPEN\n- woensdag tot zaterdag\n- 12u tot 14u en 19u tot 20u30"
    ],
    "es": [
      "Hermosa dirección con terraza a orillas del Creuse. Cocina francesa gastronómica... Fondu creusois, galettes, crêpes, fondue savoyarde, fondue bourguignonne.\n\nABIERTO\n- de miércoles a sábado\n- de 12:00 a 14:00 h y de 19:00 a 20:30 h"
    ]
  },
