  "rdfs:comment": {
    "de": [
      "Arkose, Saint-Austremoine, Tour de l'Horloge, Couzes, Basaltorgeln, Höhlenwohnungen, Winzerhäuser, Anatole d'Auvergne, ein Bergfried von Philippe Auguste, Königin Margot, Marchidial... Entdecken Sie die unerwarteten Reichtümer des Pays d'Issoire!"
    ],
    "en": [
      "Arkose, Saint-Austremoine, the Clock Tower, couzes, basaltic organs, troglodytes, winegrowers' houses, Anatole d'Auvergne, a Philippe Auguste dungeon, the Queen Margot, the Marchidial... Come and discover the unexpected treasures of the Pays d'Issoire!"
    ],
    "it": [
      "Arkose, Saint-Austremoine, la Torre dell'Orologio, le couzes, gli organi di basalto, i trogloditi, le case dei viticoltori, Anatole d'Auvergne, un torrione di Filippo Augusto, la Reine Margot, il Marchidial... Venite a scoprire i tesori inaspettati del Pays d'Issoire!"
    ],
    "fr": [
      "Arkose, Saint-Austremoine, la Tour de l'Horloge, couzes, orgues basaltiques,  troglodytes, maisons vigneronnes, Anatole d'Auvergne, un donjon Philippe Auguste, la Reine Margot, le Marchidial... Venez découvrir les richesses inattendues du Pays d'Issoire!"
    ],
    "es": [
      "Arkose, Saint-Austremoine, la Torre del Reloj, couzes, órganos de basalto, trogloditas, casas de viticultores, Anatole d'Auvergne, un torreón de Philippe Auguste, la Reine Margot, el Marchidial... Venga a descubrir los inesperados tesoros del País de Issoire"
    ],
    "nl": [
      "Arkose, Saint-Austremoine, de klokkentoren, couzes, basaltorgels, troglodieten, wijnbouwershuizen, Anatole d'Auvergne, een Philippe Auguste-vertrek, de Reine Margot, de Marchidial... Kom en ontdek de onverwachte schatten van de Pays d'Issoire!"
    ]
  },
