  "rdfs:comment": {
    "de": [
      "Ziegenkäse (Rasse Poitevine), Camembert, Tomme, Joghurt. Honig vom Bauernhof, Produkte aus dem Bienenstock. Handgemachte Seifen aus Ziegen-/Honigmilch."
    ],
    "en": [
      "Goat cheese (Poitevin breed), camembert, tomme, yoghurts. Honey from the farm, products of the hive. Artisanal soaps made from goat's milk/honey."
    ],
    "it": [
      "Formaggio di capra (razza Poitevin), camembert, tomme, yogurt. Miele della fattoria, prodotti dell'alveare. Saponi artigianali a base di latte di capra/miele."
    ],
    "fr": [
      "Fromages de chèvre (race poitevine), camembert, tomme, yaourts. Miel de la ferme, produits de la ruche. Savons artisanaux au lait de chèvre/miel."
    ],
    "es": [
      "Queso de cabra (raza Poitevin), camembert, tomme, yogures. Miel de la granja, productos de la colmena. Jabones artesanales de leche de cabra y miel."
    ],
    "nl": [
      "Geitenkaas (ras Poitevin), camembert, tomme, yoghurt. Honing van de boerderij, producten van de bijenkorf. Artisanale zeep gemaakt van geitenmelk/honing."
    ]
  },
