  "rdfs:comment": {
    "de": [
      "Das Kino Pathé Madeleine in Les Cinq-Avenues in Marseille empfängt Sie an 365 Tagen im Jahr, um Ihre Emotionen intensiv zu erleben!"
    ],
    "en": [
      "Located in Cinq-Avenues in Marseille, the Pathé Madeleine cinema welcomes you 365 days a year to experience your emotions intensely!"
    ],
    "it": [
      "Situato a Cinq-Avenues a Marsiglia, il cinema Pathé Madeleine vi accoglie 365 giorni all'anno per vivere intensamente le vostre emozioni!"
    ],
    "fr": [
      "Situé aux Cinq-Avenues à Marseille, le cinéma Pathé Madeleine vous accueille 365 jours par an pour vivre vos émotions intensément !"
    ],
    "es": [
      "Situado en Cinq-Avenues, en Marsella, el cine Pathé Madeleine le acoge los 365 días del año para vivir intensamente sus emociones"
