  "rdfs:comment": {
    "de": [
      "EVP - Eau Vive Passion - Büro der Bergführer.\r\nErleben Sie die Freuden der Via Ferrata, des Kletterns, des Canyonings oder des Hochgebirges dank der staatlich geprüften Bergführer."
    ],
    "en": [
      "EVP - Eau Vive Passion - guide office \nExperience the joys of Via Ferrata, climbing, canyoning or mountaineering with our state-qualified guides."
    ],
    "it": [
      "EVP - Eau Vive Passion - ufficio guida.\r\nVenite a provare le gioie della Via Ferrata, dell'arrampicata, del canyoning o dell'alta montagna grazie a guide qualificate."
    ],
    "fr": [
      "EVP - Eau Vive Passion - bureau des guides.\r\nVenez goûter aux joies de la Via Ferrata, de l'escalade, canyoning ou de la haute montagne grâce aux guides diplômés d'état."
    ],
    "es": [
      "EVP - Eau Vive Passion - oficina de la guía.\r\nVenga a saborear los placeres de la vía ferrata, la escalada, el barranquismo o la alta montaña gracias a guías cualificados por el Estado."
    ],
    "nl": [
      "EVP - Eau Vive Passion - gidsenbureau \nErvaar de geneugten van Via Ferrata, klimmen, canyoning en bergbeklimmen met onze staatsgediplomeerde gidsen."
    ]
  },
