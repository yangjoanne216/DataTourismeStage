  "rdfs:comment": {
    "de": [
      "Der Etang de Rassuen ist ein kleines Brackwassergebiet von 15 ha mit Sumpfgebieten rundherum mit einer Fläche von 40 ha in der Gemeinde Istres."
    ],
    "en": [
      "The Etang de Rassuen is a small brackish water body of 15 ha, with marshes all around a surface of 40 ha, in the commune of Istres."
    ],
    "it": [
      "L'Etang de Rassuen è un piccolo specchio d'acqua salmastra di 15 ettari, con paludi circostanti di 40 ettari, nel comune di Istres."
    ],
    "fr": [
      "L'Etang de Rassuen est une petite étendue d'eau saumâtre de 15 ha, avec des marais tout autour d'une superficie de 40 ha, sur la commune d'Istres."
    ],
    "nl": [
      "Het Etang de Rassuen is een klein brak waterlichaam van 15 ha, met omliggende moerassen van 40 ha, in de gemeente Istres."
    ],
    "es": [
      "El Etang de Rassuen es una pequeña masa de agua salobre de 15 ha, con marismas circundantes de 40 ha, en el municipio de Istres."
    ]
  },
