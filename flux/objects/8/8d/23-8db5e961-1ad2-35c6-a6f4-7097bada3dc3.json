  "rdfs:comment": {
    "de": [
      "Ein rustikaler Saal mit Mauern aus sichtbarem Stein, Balken, Schmiedeeisen und einem monumentalen Kamin. Bei schönem Wetter Abendessen auf der Terrasse."
    ],
    "en": [
      "Rustic dining room with exposed stone walls, beams, wrought iron, and monumental fireplace. Dinners on the terrace on sunny days."
    ],
    "it": [
      "Una sala rustica con pareti in pietra a vista, travi, ferro battuto e un camino monumentale. Cene in terrazza nella bella stagione."
    ],
    "fr": [
      "Une salle rustique aux murs en pierres apparentes, poutres, fer forgé, avec sa cheminée monumentale. Dîners en terrasse dès les beaux jours."
    ],
    "es": [
      "Una sala rústica con paredes de piedra vista, vigas, hierro forjado, y chimenea monumental. Cenas en la terraza en los días soleados."
    ],
    "nl": [
      "Een rustieke kamer met zichtbare stenen muren, balken, smeedijzer en een monumentale open haard. Diners op het terras bij mooi weer."
    ]
  },
