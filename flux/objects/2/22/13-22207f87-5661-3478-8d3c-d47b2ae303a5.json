  "rdfs:comment": {
    "de": [
      "Traditionelles Restaurant im Pilat - Cuisine Faite Maison (Hausgemachte Küche).\r\nJeden Mittag Tagesmenü.\r\nGerichte zum Mitnehmen.\r\nPizzas (vor Ort oder zum Mitnehmen) am Freitagabend."
    ],
    "en": [
      "Traditional restaurant in the Pilat - Home-made cooking.\r\nEvery midday menu of the day.\r\nDishes to take away.\r\nPizzas (on the spot or to take away) on Friday evenings."
    ],
    "it": [
      "Ristorante tradizionale nel Pilat - Cucina casalinga.\r\nTutti i menu del giorno a pranzo.\r\nPiatti da asporto.\r\nPizze (sul posto o da asporto) il venerdì sera."
    ],
    "fr": [
      "Restaurant traditionnel dans le Pilat - Cuisine Faite Maison.\r\nTous les midis menu du jour.\r\nPlats à emporter.\r\nPizzas (sur place ou à emporter) les vendredi soirs."
    ],
    "nl": [
      "Traditioneel restaurant in de Pilat - Huisgemaakte keuken.\r\nElk lunchmenu van de dag.\r\nAfhaalgerechten.\r\nPizza's (ter plaatse of om mee te nemen) op vrijdagavond."
    ],
    "es": [
      "Restaurante tradicional en el Pilat - Cocina casera.\r\nTodos los menús del día.\r\nPlatos para llevar.\r\nPizzas (en el lugar o para llevar) los viernes por la noche."
    ]
  },
