  "rdfs:comment": {
    "de": [
      "Eine traditionelle Küche in der Nähe des Bahnhofs Bénédictins, in der frische Produkte und Limousin-Fleisch im Mittelpunkt stehen. Empfang von Gruppen. Beheizte Terrasse. Service nach 22 Uhr."
    ],
    "en": [
      "A traditional cuisine close to the Benedictine station where fresh products and Limousin meat are honoured. Groups welcome. Heated terrace. Service after 10pm."
    ],
    "it": [
      "Cucina tradizionale vicino alla stazione dei Bénédictins, con prodotti freschi e carni del Limousin. Accoglienza gruppi. Terrazza riscaldata. Servizio dopo le 22.00."
    ],
    "fr": [
      "Une cuisine traditionnelle proche de la gare des Bénédictins où produits frais et viande limousine sont à l'honneur. Accueil de groupes. Terrasse chauffée. Service après 22h."
    ],
    "nl": [
      "Traditionele keuken vlakbij het station van Bénédictins, met verse producten en vlees uit de Limousin. Groepen zijn welkom. Verwarmd terras. Service na 22.00 uur."
    ],
    "es": [
      "Cocina tradicional cerca de la estación de Bénédictins, con productos frescos y carnes de Limousin. Acogida de grupos. Terraza climatizada. Servicio a partir de las 22 h."
    ]
  },
