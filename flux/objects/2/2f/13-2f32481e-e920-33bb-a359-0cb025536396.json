  "rdfs:comment": {
    "de": [
      "Das Hotel l'Alpin liegt in Landry, 5 Minuten vom Bahnhof entfernt und in unmittelbarer Nähe der Shuttlebushaltestelle. Es verfügt über 15 Zimmer mit Bad, TV und WLAN."
    ],
    "en": [
      "Located in Landry, 5 minutes from the train station, very close to the shuttle stop, Hotel l'Alpin has 15 en-suite rooms with TV and Wi-Fi."
    ],
    "it": [
      "Situato a Landry, a pochi passi dalla stazione ferroviaria SNCF e vicino alla fermata del bus navetta, l'Hotel l'Alpin dispone di 15 camere con bagno privato, TV e connessione Wi-Fi."
    ],
    "fr": [
      "Situé à Landry, à deux pas de la gare SNCF, tout proche de l'arrêt navette, l'hôtel l'Alpin dispose de 15 chambres en-suite avec télévision et wifi."
    ],
    "es": [
      "Situado en Landry, a un paso de la estación de tren SNCF, cerca de la parada del autobús lanzadera, el hotel Alpin dispone de 15 habitaciones con baño, TV y Wi-Fi."
    ],
    "nl": [
      "Het Alpin hotel ligt in Landry, op een steenworp afstand van het SNCF treinstation, dicht bij de halte van de pendelbus, en beschikt over 15 en-suite kamers met TV en Wi-Fi."
    ]
  },
