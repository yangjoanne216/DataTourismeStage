  "rdfs:comment": {
    "de": [
      "Zugang über das Dorf Murat von der D18 zwischen Saint-Martin la Méanne und Gros Chastang. Panorama über die Schluchten der Dordogne und den Staudamm des Chastang-Staudamms."
    ],
    "en": [
      "Access from the village of Murat from the D18 between Saint-Martin la Méanne and Gros Chastang. Panaroma over the Dordogne gorges and the Chastang dam and reservoir."
    ],
    "it": [
      "Accesso attraverso il villaggio di Murat dalla D18 tra Saint-Martin la Méanne e Gros Chastang. Vista panoramica sulle gole della Dordogna e sul lago artificiale di Chastang."
    ],
    "fr": [
      "Accès par le village de Murat à partir de la D18 entre Saint-Martin la Méanne et Gros Chastang. Panorama sur les gorges de la Dordogne et la retenue du barrage du Chastang."
    ],
    "nl": [
      "Toegang via het dorp Murat vanaf de D18 tussen Saint-Martin la Méanne en Gros Chastang. Panoramisch uitzicht op de kloven van de Dordogne en het stuwmeer van Chastang."
    ],
    "es": [
      "Acceso por el pueblo de Murat desde la D18 entre Saint-Martin la Méanne y Gros Chastang. Vistas panorámicas de las gargantas del Dordoña y del embalse de Chastang."
    ]
  },
