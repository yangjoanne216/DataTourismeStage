  "rdfs:comment": {
    "de": [
      "Gegründet im Jahr 1007 von Foulques Nerra, Graf von Anjou, der im südlichen Arm des Querschiffs begraben wurde. Freie Besichtigung der Abteikirche."
    ],
    "en": [
      "Founded in 1007 by Fulk III (Nerra), Count of Anjou, buried in the southern wing of the transept."
    ],
    "it": [
      "Fondata nel 1007 da Foulques Nerra, conte d'Angiò, che è sepolto nel braccio sud del transetto. Visita gratuita della chiesa abbaziale."
    ],
    "fr": [
      "Fondée en 1007 par Foulques Nerra, comte d’Anjou, inhumé dans le bras sud du transept. Visite libre de l'église abbatiale."
    ],
    "nl": [
      "Gesticht in 1007 door Foulques Nerra, graaf van Anjou, die in de zuidelijke arm van het transept begraven ligt. Gratis bezoek aan de abdijkerk."
    ],
    "es": [
      "Fundada en 1007 por Foulques Nerra, conde de Anjou, que está enterrado en el brazo sur del crucero. Visita gratuita a la iglesia abacial."
    ]
  },
