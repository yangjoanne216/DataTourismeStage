  "rdfs:comment": {
    "de": [
      "Zwischen Nantes und Cholet, nur einen Katzensprung von Clisson entfernt, machen Sie einen Zwischenstopp beim Château de la Gagnerie und entdecken Sie dort das Weingut Naulin."
    ],
    "en": [
      "Between Nantes and Cholet, a stone's throw from Clisson, stop off at the Château de la Gagnerie to discover the Naulin vineyard."
    ],
    "it": [
      "Tra Nantes e Cholet, a due passi da Clisson, fermatevi allo Château de la Gagnerie per scoprire il vigneto Naulin."
    ],
    "fr": [
      "Entre Nantes et Cholet, à deux pas de Clisson, faites une halte au château de la Gagnerie pour y découvrir le vignoble Naulin."
    ],
    "nl": [
      "Stop tussen Nantes en Cholet, op een steenworp afstand van Clisson, bij het Château de la Gagnerie om de wijngaard van Naulin te ontdekken."
    ],
    "es": [
      "Entre Nantes y Cholet, a un paso de Clisson, deténgase en el castillo de la Gagnerie para descubrir el viñedo de Naulin."
    ]
  },
