  "rdfs:comment": {
    "de": [
      "Entdecken Sie einen Viehzuchtbetrieb im Herzen der Bocage, in der Nähe des Tals der Creuse."
    ],
    "en": [
      "Come and visit a livestock farm in the heart of the countryside, near the Creuse Valley."
    ],
    "it": [
      "Venite a scoprire un allevamento nel cuore del bocage, vicino alla valle della Creuse."
    ],
    "fr": [
      "Venez découvrir une ferme d'élevage au coeur du bocage, à proximité de la vallée de la Creuse."
    ],
    "es": [
      "Venga a descubrir una granja de cría en el corazón del bocage, cerca del valle de Creuse."
    ],
    "nl": [
      "Kom en ontdek een fokkerij in het hart van de bocage, vlakbij de vallei van de Creuse."
    ]
