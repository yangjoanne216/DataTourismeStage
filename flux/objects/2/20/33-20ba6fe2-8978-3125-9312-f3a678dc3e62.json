    ]
  },
  "COVID19SpecialMeasures": {
    "de": [
      "Hydroalkoholisches Gel und Maske obligatorisch.\nGeöffnet von Dienstag bis Samstag mittags und von Donnerstag bis Sonntag abends."
    ],
    "en": [
      "Hydro-alcoholic gel and mask mandatory.\nOpen from Tuesday to Saturday for lunch and from Thursday to Sunday for dinner."
    ],
    "it": [
      "Gel idroalcolico e maschera obbligatori.\nAperto da martedì a sabato a pranzo e da giovedì a domenica a cena."
    ],
    "nl": [
      "Hydro-alcoholische gel en masker verplicht.\nGeopend van dinsdag tot zaterdag voor de lunch en van donderdag tot zondag voor het diner."
    ],
    "es": [
      "Gel hidroalcohólico y mascarilla obligatorios.\nAbierto de martes a sábado para el almuerzo y de jueves a domingo para la cena."
    ]
  },
