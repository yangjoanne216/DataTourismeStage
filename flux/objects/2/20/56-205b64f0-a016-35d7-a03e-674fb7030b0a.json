  "rdfs:comment": {
    "de": [
      "850 m vom Dorf, 1,5 km vom Strand entfernt, Fahrradverleih, Zug, Bus vor dem Hotel. Zimmer und Appartements, Klimaanlage, Kühlschrank, Terrasse, Wifi, geschlossener Parkplatz, Videoschutz und beheizter Pool"
    ],
    "en": [
      "At 850 m from the village, 1,5 Km from the beach, bike rental, train, bus in front of the hotel. Rooms and apartments, air conditioning, fridge, terrace, wifi, closed parking, video protection and heated swimming pool"
    ],
    "it": [
      "A 850 m dal villaggio, a 1,5 km dalla spiaggia, noleggio biciclette, treno, autobus di fronte all'hotel. Camere e appartamenti, aria condizionata, terrazza, wifi, parcheggio chiuso, video-protezione e piscina riscaldata"
    ],
    "fr": [
      "A 850 m du village, 1,5 Km de la plage, location de vélo, train, bus devant l'hôtel. Chambres et appartements, clim frigo, terrasse, wifi, parking fermé vidéo protection et piscine chauffée"
    ],
    "es": [
      "A 850 m del pueblo, a 1,5 km de la playa, alquiler de bicicletas, tren, autobús frente al hotel. Habitaciones y pisos, aire acondicionado, terraza, wifi, aparcamiento cerrado, videovigilancia y piscina climatizada"
    ],
    "nl": [
      "850 m van het dorp, 1,5 km van het strand, fietsverhuur, trein, bus voor het hotel. Kamers en appartementen, airconditioning, terras, wifi, afgesloten parkeerplaats, videobewaking en verwarmd zwembad"
    ]
  },
