  "rdfs:comment": {
    "de": [
      "Auf dem Weg nach La Cresse befindet sich ein kleiner schattiger Park, in dem es sich gut für einen Moment oder ein Picknick ausruhen lässt."
    ],
    "en": [
      "On the road to La Cresse, a small shady park where it is good to rest for a moment or the time of a picnic."
    ],
    "it": [
      "Sulla strada per La Cresse, un piccolo parco ombreggiato dove è bene riposare un attimo o fare un picnic."
    ],
    "fr": [
      "Sur la route de La Cresse, un petit parc ombragé où il fait bon s'y reposer un instant ou le temps d'un pique-nique."
    ],
    "nl": [
      "Op weg naar La Cresse, een klein schaduwrijk park waar het goed is om even uit te rusten of te picknicken."
    ],
    "es": [
      "En la carretera de La Cresse, un pequeño parque sombreado donde es bueno descansar un momento o hacer un picnic."
    ]
  },
