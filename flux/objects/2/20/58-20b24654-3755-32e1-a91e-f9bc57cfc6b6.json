  "rdfs:comment": {
    "de": [
      "Tunesisches Restaurant und Teesalon in der Nähe des Alten Hafens: frische und raffinierte Küche mit saisonalen Produkten. Spezialitäten wie Couscous. Schöne Dekoration mit blauen Mosaiken."
    ],
    "en": [
      "Tunisian restaurant and tea room near the Old Port: fresh and refined cuisine with seasonal products. Specialities of couscous. Beautiful decoration with blue mosaics."
    ],
    "it": [
      "Ristorante tunisino e sala da tè vicino al Porto Vecchio: cucina fresca e raffinata con prodotti di stagione. Specialità di couscous. Bella decorazione con mosaici blu."
    ],
    "fr": [
      "Restaurant et salon de thé tunisien près du Vieux Port : cuisine fraîche et raffinée avec des produits de saison. Spécialités de couscous. Belle décoration avec mosaiques bleues."
    ],
    "nl": [
      "Tunesisch restaurant en tearoom in de buurt van de Oude Haven: verse en verfijnde keuken met seizoensproducten. Specialiteiten van couscous. Mooie decoratie met blauwe mozaïeken."
    ],
    "es": [
      "Restaurante y salón de té tunecino cerca del Vieux Port: cocina fresca y refinada con productos de temporada. Especialidades en cuscús. Bonita decoración con mosaicos azules."
    ]
  },
