  "rdfs:comment": {
    "de": [
      "4 Personen - 30 m²: Wohnung mit 1 Wohnzimmer, 1 Badezimmer, 1 ausgestattete Küche, 1 Keller, 1 Balkon mit Blick auf den Park und 1 Parkplatz. Nähe zu den Geschäften und 5 Minuten zu Fuß von den Stränden der Chambre d'Amour entfernt."
    ],
    "en": [
      "Sleeps 4 - 30 m²: Apartment comprising 1 living room, 1 bathroom, 1 fitted kitchen , 1 cellar, 1 balcony with views over the grounds and 1 car parking area. Close to the shops and 5 minutes' walk from the beaches in the Chambre d'Amour district."
    ],
    "it": [
      "4 persone - 30 m² : Appartamento con 1 soggiorno, 1 bagno, 1 cucina attrezzata, 1 cantina, 1 balcone con vista sul parco e 1 posto auto. Vicino ai negozi e a 5 minuti a piedi dalle spiagge della Chambre d'Amour."
    ],
    "fr": [
      "4 personnes - 30 m² : Appartement comprenant 1 séjour, 1 salle de bain, 1 cuisine équipée, 1 cave, 1 balcon avec vue sur parc et 1 parking. proximité des commerces et à 5 minutes à pied des plages de la Chambre d'Amour."
    ],
    "nl": [
      "4 personen - 30 m² : Appartement met 1 woonkamer, 1 badkamer, 1 ingerichte keuken, 1 kelder, 1 balkon met uitzicht op het park en 1 parkeerplaats. Dicht bij de winkels en op 5 minuten lopen van de stranden van de Chambre d'Amour."
    ],
    "es": [
      "4 personas - 30 m² : Apartamento con 1 sala de estar, 1 baño, 1 cocina equipada, 1 bodega, 1 balcón con vistas al parque y 1 aparcamiento. Cerca de los comercios y a 5 minutos a pie de las playas de la Chambre d'Amour."
    ]
  },
