  "rdfs:comment": {
    "de": [
      "Im Herzen von Saumur genießen Sie Pizzas als Lieferservice, zum Mitnehmen oder zum Verzehr vor Ort."
    ],
    "en": [
      "In the heart of Saumur, take advantage of the pizzas in delivery, to take away or to taste on the spot."
    ],
    "it": [
      "Nel cuore di Saumur, potrete gustare la pizza a domicilio, da asporto o da mangiare in casa."
    ],
    "fr": [
      "En plein cœur de Saumur, profitez des pizzas en livraison, à emporter ou à déguster sur place."
    ],
    "nl": [
      "In het hart van Saumur kunt u pizza's laten bezorgen, afhalen of eten."
    ],
    "es": [
      "En el corazón de Saumur, disfrute de la pizza a domicilio, para llevar o para comer en casa."
    ]
