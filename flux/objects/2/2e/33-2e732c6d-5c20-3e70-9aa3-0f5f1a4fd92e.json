  "rdfs:comment": {
    "de": [
      "Das Nimp' ist der verrückteste Club in Toulouse. Gruppenessen, Tapas, Sport im Fernsehen, DJ ab 23 Uhr, um den Abend zu gestalten."
    ],
    "en": [
      "Le Nimp' is the craziest club in Toulouse. Group meal, tapas, sport on TV, dj from 11pm to set the mood for the evening."
    ],
    "it": [
      "Il Nimp' è il locale più pazzo di Tolosa. Pasti di gruppo, tapas, sport in TV, dj dalle 23 in poi per animare la serata."
    ],
    "fr": [
      "Le Nimp' est le club le plus déjanté de Toulouse. Repas de groupe, tapas, sport à la tv, dj à partir de 23h pour ambiancer la soirée ."
    ],
    "nl": [
      "The Nimp' is de gekste club van Toulouse. Groepsmaaltijden, tapas, sport op tv, dj's vanaf 23.00 uur om de avond op te leuken."
    ],
    "es": [
      "El Nimp' es el club más loco de Toulouse. Comidas para grupos, tapas, deportes en la tele, djs a partir de las 23:00 para animar la velada."
    ]
  },
