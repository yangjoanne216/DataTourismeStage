  "rdfs:comment": {
    "de": [
      "Einfaches und gemütliches Hotel im Herzen von Paray-le-Monial, einer Touristenhochburg. Hausmannskost, schöner Blick auf den blumengeschmückten Pla..."
    ],
    "en": [
      "Simple and welcoming hotel, located in the heart of Paray-le-Monial, a major tourist attraction. Family kitchen, beautiful view on the flowered Place de l'Europe."
    ],
    "it": [
      "Hotel semplice e accogliente, situato nel cuore di Paray-le-Monial, una delle principali attrazioni turistiche. Cucina casalinga, bella vista sulla Place de l'Europe in fiore."
    ],
    "fr": [
      "Hôtel simple et accueillant, situé au cœur de Paray-le-Monial, haut-lieu touristique. Cuisine familiale, belle vue sur la Place de l'Europe fleurie."
    ],
    "es": [
      "Hotel sencillo y acogedor, situado en el corazón de Paray-le-Monial, una importante atracción turística. Cocina casera, hermosas vistas a la florida Place de l'Europe."
    ],
    "nl": [
      "Eenvoudig en gastvrij hotel, gelegen in het hart van Paray-le-Monial, een belangrijke toeristische trekpleister. Huiselijke keuken, prachtig uitzicht op het in bloei staande Place de l'Europe."
    ]
  },
