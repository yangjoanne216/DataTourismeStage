  "rdfs:comment": {
    "de": [
      "Verkauf von Produkten vom Bauernhof (frischer, gereifter, aromatisierter Käse...).\nGanzjährig geöffnet von Montag bis Samstag von 9 bis 12 Uhr und von 14 bis 18 Uhr."
    ],
    "en": [
      "Sale of farm products (fresh, matured, flavored cheeses...).\nOpen all year round from Monday to Saturday from 9am to 12pm and from 2pm to 6pm."
    ],
    "it": [
      "Vendita di prodotti agricoli (formaggi freschi, stagionati e aromatizzati, ecc.).\nAperto tutto l'anno dal lunedì al sabato dalle 9.00 alle 12.00 e dalle 14.00 alle 18.00."
    ],
    "fr": [
      "Vente de produits de la ferme (fromages frais, affinés, aromatisés...).\nOuvert toute l'année du lundi au samedi de 9h à 12h et de 14h à 18h."
    ],
    "nl": [
      "Verkoop van landbouwproducten (verse, gerijpte en gearomatiseerde kazen, enz.).\nHet hele jaar geopend van maandag tot en met zaterdag van 9.00 tot 12.00 uur en van 14.00 tot 18.00 uur."
    ],
    "es": [
      "Venta de productos agrícolas (quesos frescos, curados, aromatizados, etc.).\nAbierto todo el año, de lunes a sábado, de 9.00 a 12.00 y de 14.00 a 18.00 horas."
    ]
  },
