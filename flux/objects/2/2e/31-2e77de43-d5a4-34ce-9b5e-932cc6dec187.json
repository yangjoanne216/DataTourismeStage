  "rdfs:comment": {
    "de": [
      "La belle époque ist sieben Tage die Woche geöffnet und bietet Ihnen samstags und sonntags einen gastronomischen Service und wochentags ein Tagesmenü an."
    ],
    "en": [
      "Open seven days a week, la belle époque offers you its gastronomic catering service on Saturdays and Sundays and daily menu during the week."
    ],
    "it": [
      "Aperto sette giorni su sette, La Belle Epoque offre una cena gourmet il sabato e la domenica e un menu giornaliero durante la settimana."
    ],
    "fr": [
      "Ouvert sept jours sur sept, la belle époque vous propose son service de restauration gastronomique, le samedi et dimanche et menu du jour la semaine."
    ],
    "nl": [
      "La Belle Epoque is zeven dagen per week geopend en biedt op zaterdag en zondag een gastronomisch diner en door de week een dagmenu."
    ],
    "es": [
      "Abierto los siete días de la semana, La Belle Epoque ofrece una cena gourmet los sábados y domingos y un menú diario durante la semana."
    ]
  },
