  "rdfs:comment": {
    "de": [
      "Bürgerliches Haus 200m vom Zentrum von Eymet ( alle Geschäfte) entfernt. Drei geräumige, ruhige, klimatisierte Zimmer mit eigenem Bad, Dusche und WC. Es ist ein Nichtraucherhaus. Schattiger Innenhof. Gourmet-Restaurant."
    ],
    "en": [
      "Bourgeois house 200m from the center of Eymet (all shops). Three spacious, quiet, air-conditioned bedrooms with bath, shower and private toilet. No smoking. Shaded courtyard. Gastronomic restaurant."
    ],
    "it": [
      "Casa borghese a 200 m dal centro di Eymet (tutti i negozi). Tre camere spaziose, tranquille e climatizzate con bagno privato, doccia e servizi igienici. Non si fuma. Cortile ombreggiato. Ristorante gastronomico."
    ],
    "fr": [
      "Maison bourgeoise à 200m du centre d'Eymet ( tous commerces). Trois chambres spacieuses, calmes, climatisées avec bain, douche et wc privatifs. Non fumeur. Cour ombragée. Restaurant gastronomique."
    ],
    "es": [
      "Casa burguesa a 200 m del centro de Eymet (todos los comercios). Tres habitaciones amplias, tranquilas y con aire acondicionado, con baño privado, ducha y aseo. No se puede fumar. Patio sombreado. Restaurante gastronómico."
    ],
    "nl": [
      "Bourgeois huis op 200m van het centrum van Eymet (alle winkels). Drie ruime, rustige kamers met airconditioning en eigen bad, douche en toilet. Niet roken. Schaduwrijke binnenplaats. Gastronomisch restaurant."
    ]
  },
