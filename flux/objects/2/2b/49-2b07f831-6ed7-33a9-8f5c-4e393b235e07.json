  "rdfs:comment": {
    "de": [
      "ÜBER...\n\nZwischen Nantes und Pornic gelegen, verspricht Ihnen unser elegantes Spa-Hotel mit seinen warmen und entspannenden Farben schon bei Ihrer Ankunft Ruhe."
    ],
    "en": [
      "ABOUT...\n\nLocated between Nantes and Pornic, our elegant Spa Hotel with its warm and relaxing colours promises you peace of mind from the moment you arrive."
    ],
    "it": [
      "SU DI NOI...\n\nSituato tra Nantes e Pornic, il nostro elegante Hotel Spa dai colori caldi e rilassanti vi promette tranquillità fin dal vostro arrivo."
    ],
    "fr": [
      "A PROPOS...\n\nSitué entre Nantes et Pornic, notre Hôtel Spa élégant aux couleurs chaudes et relaxantes vous promet une quiétude dès votre arrivée."
    ],
    "es": [
      "SOBRE NOSOTROS...\n\nSituado entre Nantes y Pornic, nuestro elegante hotel balneario, con sus colores cálidos y relajantes, le promete tranquilidad desde su llegada."
    ],
    "nl": [
      "OVER ONS...\n\nGelegen tussen Nantes en Pornic, belooft ons elegante Spa Hotel met zijn warme en ontspannende kleuren u gemoedsrust vanaf het moment dat u aankomt."
    ]
  },
