  "rdfs:comment": {
    "de": [
      "Water Sports 13 ist eine saisonale nautische Basis in La Couronne an der Côte Bleue in der Nähe des Hafens von Tamaris in der Nähe von Marseille und Carry-le-Rouet."
    ],
    "en": [
      "Water Sports 13 is a seasonal nautical base located in La Couronne on the Côte Bleue, near the port of Tamaris, near Marseille and Carry-le-Rouet."
    ],
    "it": [
      "Water Sports 13 è una base stagionale per gli sport acquatici situata a La Couronne sulla Côte Bleue, vicino al porto di Tamaris, nei pressi di Marsiglia e Carry-le-Rouet."
    ],
    "fr": [
      "Water Sports 13 est une base nautique saisonnière située à La Couronne sur la Côte Bleue, à proximité du port des Tamaris, près de Marseille et de Carry-le-Rouet."
    ],
    "nl": [
      "Water Sports 13 is een seizoensgebonden watersportbasis gelegen in La Couronne aan de Côte Bleue, vlakbij de haven van Tamaris, in de buurt van Marseille en Carry-le-Rouet."
    ],
    "es": [
      "Water Sports 13 es una base náutica de temporada ubicada en La Couronne en la Côte Bleue, cerca del puerto de Tamaris, cerca de Marsella y Carry-le-Rouet."
    ]
  },
