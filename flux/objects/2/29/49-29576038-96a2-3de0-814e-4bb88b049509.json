  "rdfs:comment": {
    "de": [
      "In Brain-sur-Longuenée im blauen Anjou bietet La Julière das Pflücken von Äpfeln in seinem Obstgarten an."
    ],
    "en": [
      "In Brain-sur-Longuenée, in Anjou bleu, La Julière offers apple picking in its orchard."
    ],
    "it": [
      "A Brain-sur-Longuenée, nella regione dell'Anjou Bleu, La Julière offre la raccolta delle mele nel suo frutteto."
    ],
    "fr": [
      "A Brain-sur-Longuenée, en Anjou bleu, la Julière propose la cueillette de pommes dans son verger."
    ],
    "nl": [
      "In Brain-sur-Longuenée, in de regio Anjou Bleu, biedt La Julière de mogelijkheid om in zijn boomgaard appels te plukken."
    ],
