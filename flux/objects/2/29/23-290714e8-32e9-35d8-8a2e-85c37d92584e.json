  "rdfs:comment": {
    "de": [
      "T3. 65 m². Erdgeschoss. 2 Schlafzimmer für 4 Personen. Wohnzimmer, Farbfernseher. Kochecke, Geschirrspüler, Mikrowelle. Angrenzender Abstellraum, Waschmaschine, Trockner. Badezimmer mit WC. Gemeinsamer Hof."
    ],
    "en": [
      "T3. 65 m². Ground floor. 2 bedrooms for 4 people. Living room, color TV. Kitchenette, dishwasher, microwave. Adjoining storeroom, washing machine, tumble dryer. Shower room with WC. Common courtyard."
    ],
    "it": [
      "T3. 65 m². Piano terra. 2 camere per 4 persone. Soggiorno, TV a colori. Angolo cottura, lavastoviglie, microonde. Cantina adiacente, lavatrice e asciugatrice. Bagno con doccia e WC. Cortile condiviso."
    ],
    "fr": [
      "T3. 65 m². RDC. 2 chambres pour 4 personnes. Séjour, TV couleur. Coin cuisine, lave-vaisselle, micro-ondes. Cellier attenant, lave-linge, sèche-linge. Salle d'eau avec WC. Cour commune."
    ],
    "es": [
      "T3. 65 m². Planta baja. 2 dormitorios para 4 personas. Sala de estar, televisión en color. Cocina americana, lavavajillas, microondas. Sótano adyacente, lavadora, secadora. Ducha con WC. Patio compartido."
    ],
    "nl": [
      "T3. 65 m². Begane grond. 2 slaapkamers voor 4 personen. Woonkamer, kleuren TV. Kitchenette, vaatwasser, magnetron. Aangrenzende kelder, wasmachine, wasdroger. Doucheruimte met WC. Gedeelde binnenplaats."
    ]
