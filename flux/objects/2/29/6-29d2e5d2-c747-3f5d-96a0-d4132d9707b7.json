  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/ac0cfaad-d42b-3900-82e2-c475856afb8f",
      "dc:description": {
        "de": [
          "Städtischer Campingplatz 500 m von den Geschäften entfernt, schattig, ruhig, flacher Boden, Gras und Sand. In der Nähe: 2 Tennisplätze, Schwimmbad, Rollerbladebahn. Kostenloser Internetzugang in der Bibliothek während der Öffnungszeiten."
        ],
        "en": [
          "Municipal campsite situated 500 metres from the shops, shaded, quiet, flat, grassy and sandy ground. Nearby: 2 tennis courts, swimming pool, roller rink. Free internet access at the library during opening hours."
        ],
        "it": [
          "Campeggio comunale situato a 500 metri dai negozi, ombreggiato, tranquillo, pianeggiante, con terreno erboso e sabbioso. Nelle vicinanze: 2 campi da tennis, piscina, pista di pattinaggio. Accesso gratuito a Internet presso la biblioteca durante gli orari di apertura."
        ],
        "fr": [
          "Camping municipal situé à 500 mètres des commerces, ombragé, au calme, sol plat, herbeux et sablonneux. A proximité : 2 courts de tennis, piscine, piste de roller. Accès gratuit à internet à la bibliothèque aux heures d'ouverture."
        ],
        "nl": [
          "Gemeentelijke camping op 500 meter van de winkels, schaduwrijk, rustig, vlak, gras en zandgrond. In de buurt: 2 tennisbanen, zwembad, rolschaatsbaan. Gratis internettoegang in de bibliotheek tijdens de openingsuren."
        ],
        "es": [
          "Camping municipal situado a 500 metros de los comercios, terreno sombreado, tranquilo, llano, con hierba y arena. Cerca: 2 pistas de tenis, piscina, pista de patinaje. Acceso gratuito a Internet en la biblioteca durante las horas de apertura."
        ]
      },
