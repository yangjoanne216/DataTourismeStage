  "rdfs:comment": {
    "de": [
      "Möblierte Unterkunft in Cransac-les-Thermes, dem einzigen Kurort im Aveyron und der Stadt des Wohlbefindens. Ebenerdiges Einfamilienhaus in der Stadt gelegen."
    ],
    "en": [
      "Furnished accommodation in the Aveyronnaise spa resort of Cransac. Detached bungalow situated in the town."
    ],
    "it": [
      "Alloggio turistico ammobiliato situato a Cransac-les-Thermes, unica stazione termale dell'Aveyron e città del benessere. Casa a un piano situata in città."
    ],
    "fr": [
      "Meublé de tourisme situé à Cransac-les-Thermes, l'unique station thermale aveyronnaise et la ville de bien-être. Maison individuelle de plain-pied située en ville."
    ],
    "nl": [
      "Gemeubileerde toeristische accommodatie gelegen in Cransac-les-Thermes, het enige kuuroord in Aveyron en de stad van het welzijn. Gelijkvloerse woning gelegen in de stad."
    ],
    "es": [
      "Amueblado de turismo en Cransac-les-Thermes, el unico centro termal en Aveyron y la ciudad del bienestar. Casa al nivel del suelo al frente del casino."
    ]
  },
