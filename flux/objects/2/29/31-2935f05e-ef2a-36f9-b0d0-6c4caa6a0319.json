  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/a34645fe-f248-3ade-8000-eecd1dd81215",
      "dc:description": {
        "de": [
          "1 Küche Wohnzimmer mit Mikrowelle, Fernseher, Sofa, gemeinsame Waschmaschine. 1 Schlafzimmer mit 1 Bett 140. 1 Badezimmer mit Dusche. Elektrische Heizung. Terrasse. Grünfläche. Gartenmöbel. Parkplatz."
        ],
        "en": [
          "Kitchen/living room with microwave, TV, sofa, shared washing machine. 1 bedroom with double bed, shower room, electric heating. Terrace, garden, garden furniture, parking."
        ],
        "it": [
          "1 cucina-soggiorno con microonde, televisione, divano, lavatrice in comune. 1 camera con 1 letto matrimoniale. 1 bagno con doccia. Riscaldamento elettrico. Terrazza. Area verde. Mobili da giardino. Posto auto."
        ],
        "fr": [
          "1 cuisine salle de séjour avec micro-ondes, télévision, canapé, lave-linge en commun. 1 chambre avec 1 lit 140. 1 salle d'eau avec douche. Chauffage électrique. Terrasse. Espace vert. Salon de jardin. Parking."
        ],
        "nl": [
          "1 keuken-woonkamer met magnetron, televisie, bank, gemeenschappelijke wasmachine. 1 slaapkamer met 1 tweepersoonsbed. 1 badkamer met douche. Elektrische verwarming. Terras. Groene ruimte. Tuinmeubelen. Parkeerplaats."
        ],
        "es": [
          "1 cocina salon con microondas, television, sofa, lavadora commun. 1 habitacion con una cama 140. 1 baño con ducha. Terraza. Jardin. Estacionamiento."
        ]
      },
