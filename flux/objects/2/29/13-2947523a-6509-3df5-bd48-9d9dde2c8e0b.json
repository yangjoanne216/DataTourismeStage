  "rdfs:comment": {
    "de": [
      "Ferienhaus in freier Verwaltung 140m², sonnig, mitten in der Natur, 1km vom Dorf entfernt. Panorama auf die Berge von Couspeau und 3 Becs. Für Behinderte zugänglich.\r\n6 Zimmer. 12 Personen"
    ],
    "en": [
      "Free management lodge, 140m2, peaceful, sunny, surrounded by nature, 1km away from the village. Magnificient panorama on the mountains of Couspeau and the 3 Becs. Access for disabled people. Baby friendly."
    ],
    "it": [
      "Gite indipendente di 140 m², soleggiata, in mezzo alla natura, a 1 km dal villaggio. Panorama sulle montagne di Couspeau e 3 Becs. Accesso per i disabili.\r\n6 camere da letto. 12 posti letto"
    ],
    "fr": [
      "Gîte en gestion libre 140m², ensoleillé, pleine nature à 1km du village. Panorama sur les montagnes de Couspeau et des 3 Becs. Accessible handicapés.\r\n6 chambres. 12 personnes"
    ],
    "nl": [
      "Self-catering gite 140m², zonnig, midden in de natuur, 1km van het dorp. Panorama op de bergen van Couspeau en 3 Becs. Toegang voor gehandicapten.\r\n6 slaapkamers. 12 slaapplaatsen"
    ],
    "es": [
      "Casa rural de 140m², soleada, en plena naturaleza, a 1km del pueblo. Panorama sobre las montañas de Couspeau y 3 Becs. Acceso para discapacitados.\r\n6 habitaciones. duerme 12 personas"
    ]
  },
