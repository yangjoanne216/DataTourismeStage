  "rdfs:comment": {
    "de": [
      "Freibad mit Sonnendecks, Wasser wird durch Sonnenkollektoren erwärmt.\nNur im Sommer geöffnet, vom 01-06 bis 31-08 von 10:30 bis 12:30 Uhr und von 14:30 bis 18:30 Uhr."
    ],
    "en": [
      "Open air swimming pool with solarium area, water heated by solar panels.\nOpen only in summer, from 01-06 to 31-08 from 10:30 to 12:30 and from 14:30 to 18:30."
    ],
    "it": [
      "Piscina all'aperto con zona solarium, acqua riscaldata da pannelli solari.\nAperto solo in estate, dal 01-06 al 31-08 dalle 10:30 alle 12:30 e dalle 14:30 alle 18:30."
    ],
    "fr": [
      "Piscine de plein air avec plages solarium, eau chauffée par des panneaux solaires.\nOuverte uniquement en été, du 01-06 au 31-08 de 10h30 à 12h30 et de 14h30 à 18h30."
    ],
    "es": [
      "Piscina al aire libre con zona para tomar el sol, agua calentada por paneles solares.\nAbierto sólo en verano, del 01-06 al 31-08 de 10:30 a 12:30 y de 14:30 a 18:30."
    ],
    "nl": [
      "Openluchtzwembad met ligweide, water verwarmd door zonnepanelen.\nAlleen in de zomer geopend, van 01-06 tot 31-08 van 10:30 tot 12:30 en van 14:30 tot 18:30."
    ]
  },
