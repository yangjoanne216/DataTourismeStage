  "rdfs:comment": {
    "de": [
      "Estelle und Frédéric erwarten Sie am Tag der offenen Tür des Weinguts, um Ihnen die neuesten Jahrgänge zur Verkostung anzubieten."
    ],
    "en": [
      "Estelle and Frédéric are waiting for you during the open house of the domain, to make you taste the last vintages."
    ],
    "it": [
      "Estelle e Frédéric vi aspettano all'open house della tenuta, per farvi assaggiare le ultime annate."
    ],
    "fr": [
      "Estelle et Frédéric vous attendent lors des portes ouvertes du domaine, pour vous faire déguster les dernièrs millésimes."
    ],
    "es": [
      "Estelle y Frédéric le esperan en la jornada de puertas abiertas de la finca, para hacerle degustar las últimas añadas."
    ],
    "nl": [
      "Estelle en Frédéric wachten u op bij het open huis van het landgoed, om u de nieuwste wijnen te laten proeven."
    ]
  },
