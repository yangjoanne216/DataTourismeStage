  "rdfs:comment": {
    "de": [
      "1 Euro Relais-Säule an der D956, Châteauroux-Blois auf dem hinteren Parkplatz des Fremdenverkehrsamtes. Unbegrenzte Parkdauer."
    ],
    "en": [
      "1 Euro Relais pitch on the D956 from Châteauroux to Blois in the carpark behind the tourist office. No maximum length of stay."
    ],
    "it": [
      "1 Euro Terminale a staffetta situato sulla D956, Châteauroux-Blois, nel parcheggio posteriore dell'Ufficio del Turismo. Nessun limite di tempo per il parcheggio."
    ],
    "fr": [
      "1 borne Euro Relais située sur la D956, Châteauroux-Blois sur le parking arrière de l'Office de Tourisme. Durée de stationnement non limitée."
