  "rdfs:comment": {
    "de": [
      "Schöne Girondine auf bewaldetem Park mit Pool, bestehend aus fünf großen Schlafzimmern mit Bad und WC, Einbauküche, Esszimmer, Wohnzimmer und TV-Ecke. Nebengebäude mit Brotbackofen. Geschlossener Parkplatz."
    ],
    "en": [
      "Beautiful girondine on wooded park with swimming pool with five large bedrooms with bathroom and toilet, equipped kitchen, dining room, living room and TV corner. Outbuildings with bread oven. Closed parking."
    ],
    "it": [
      "Splendida Girondine in un parco alberato con piscina composta da cinque ampie camere da letto con bagno e WC, cucina attrezzata, sala da pranzo, soggiorno e angolo TV. Annessi con forno per il pane. Parcheggio chiuso."
    ],
    "fr": [
      "Belle girondine sur parc arboré avec piscine comprenant cinq grandes chambres avec SDB et WC, cuisine équipée, salle à manger, séjour et coin TV. Dependances avec four à pain. Parking fermé."
    ],
    "nl": [
      "Mooie Girondine op een bebost park met zwembad bestaande uit vijf grote slaapkamers met badkamer en WC, ingerichte keuken, eetkamer, woonkamer en TV hoek. Bijgebouwen met broodoven. Gesloten parkeerplaats."
    ],
    "es": [
      "Hermoso Girondine en un parque arbolado con piscina que consta de cinco grandes habitaciones con baño y WC, cocina equipada, comedor, sala de estar y rincón de TV. Dependencias con horno de pan. Aparcamiento cerrado."
    ]
  },
