  "rdfs:comment": {
    "de": [
      "Direktverkauf von Rindfleisch der Rassen Charolaise oder Blonde d'Aquitaine im Einzelhandel oder in Kisten auf Bestellung.\nKalender der Verkäufe auf dem Bauernhof auf der Website von Bienvenue à la Ferme"
    ],
    "en": [
      "Direct sale of Charolais or Blonde d'Aquitaine beef by retail or in boxes on order.\nCalendar of sales at the farm on the Bienvenue à la Ferme website"
    ],
    "it": [
      "Vendita diretta di carne Charolais o Blonde d'Aquitaine al dettaglio o in box su ordinazione.\nCalendario delle vendite in fattoria sul sito Bienvenue à la Ferme"
    ],
    "fr": [
      "Vente directe de viande bovine de race Charolaise ou Blonde d'Aquitaine au détail ou en caissette sur commande. \nCalendrier des ventes à la Ferme sur le site de Bienvenue à la Ferme"
    ],
    "es": [
      "Venta directa de carne de vacuno Charolais o Blonde d'Aquitaine al por menor o en cajas por encargo.\nCalendario de ventas en la granja en la página web Bienvenue à la Ferme"
    ],
    "nl": [
      "Rechtstreekse verkoop van Charolais- of Blonde d'Aquitaine-rundvlees in de detailhandel of in dozen op bestelling.\nKalender van de verkoop op de boerderij op de website Bienvenue à la Ferme"
    ]
  },
