  "rdfs:comment": {
    "de": [
      "Das Hotel und Restaurant Ibis Orléans Nord Saran liegt 8 km von Orléans entfernt in einer ruhigen, grünen Umgebung und empfängt Sie an 7 Tagen in der Woche rund um die Uhr."
    ],
    "en": [
      "Situated just 8 km from Orléans in a peaceful and green setting, the Ibis Orléans Nord Saran hotel-restaurant offers a warm welcome, 24 hours a day, 7 days a week."
    ],
    "it": [
      "Situato a 8 km da Orléans, in un ambiente tranquillo e verdeggiante, l'hotel ristorante Ibis Orléans Nord Saran vi accoglie 7 giorni su 7, 24 ore al giorno."
    ],
    "fr": [
      "Situé à 8 km d'Orléans au calme dans un cadre de verdure, l'hôtel restaurant Ibis Orléans Nord Saran vous accueille 7 jours sur 7, 24 heures sur 24."
    ],
    "es": [
      "Situado a 8 km de Orleans, en un lugar tranquilo y rodeado de vegetación, el hotel restaurante Ibis Orléans Nord Saran abre las 24 h, los 7 días de la semana."
    ],
    "nl": [
      "Hotel-restaurant Ibis Orléans Nord Saran, dat op 8 km van Orléans ligt, in een rustige en groene omgeving, verwelkomt u 7 dagen per week en 24 uur per dag."
    ]
  },
