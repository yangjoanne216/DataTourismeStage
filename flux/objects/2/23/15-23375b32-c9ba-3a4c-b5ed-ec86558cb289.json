  "rdfs:comment": {
    "de": [
      "Vermietung von Elektrofahrzeugen mit Begleitung zur Erkundung des Regionalen Naturparks Brenne, 1 km von der Freizeitanlage Bellebouche entfernt."
    ],
    "en": [
      "Rental of electric vehicles with accompaniment to discover the Brenne Regional Natural Park, 1 km from the Bellebouche leisure centre."
    ],
    "it": [
      "Noleggio di veicoli elettrici con accompagnamento per scoprire il Parco Naturale Regionale della Brenne, a 1 km dal centro ricreativo di Bellebouche."
    ],
    "fr": [
      "Location de véhicules électriques avec accompagnement pour découvrir le Parc Naturel Régional de la Brenne, à 1 km de la base de loisirs de Bellebouche."
    ],
    "nl": [
      "Verhuur van elektrische voertuigen met begeleiding om het Regionaal Natuurpark van Brenne te ontdekken, op 1 km van het recreatiecentrum van Bellebouche."
    ],
    "es": [
      "Alquiler de vehículos eléctricos con acompañamiento para descubrir el Parque Natural Regional de Brenne, a 1 km del centro de ocio de Bellebouche."
    ]
