  "rdfs:comment": {
    "de": [
      "Bistrot Gourmand et glacier - Frische Produkte auf dem Teller von den Vorspeisen bis zu den hausgemachten Desserts. Kundenorientierter Service nonstop von 8-23 Uhr. Abwechslungsreiche und ausgesuchte Küche."
    ],
    "en": [
      "Bistrot Gourmand and ice cream parlour - From fresh products on the plate from starters to homemade desserts. Non-stop customer service 8h-23h. Varied and sophisticated cuisine."
    ],
    "it": [
      "Bistrot Gourmand e gelateria - Prodotti freschi al piatto, dagli antipasti ai dessert fatti in casa. Servizio orientato al cliente, non-stop dalle 8.00 alle 23.00. Cucina varia e ricercata."
    ],
    "fr": [
      "Bistrot Gourmand et glacier - Des produits frais dans l'assiette des entrées aux desserts faits maison. Service à l'écoute du client en non stop 8h-23h. Cuisine variée et recherchée."
    ],
    "es": [
      "Bistrot Gourmand y heladería - Productos frescos en el plato, desde los entrantes hasta los postres caseros. Servicio de atención al cliente, ininterrumpido de 8 a 23 horas. Cocina variada y solicitada."
    ],
    "nl": [
      "Bistrot Gourmand en ijssalon - Verse producten op het bord van voorgerechten tot huisgemaakte desserts. Klantgerichte service, non-stop van 8 tot 23 uur. Gevarieerde en gewilde keuken."
    ]
  },
