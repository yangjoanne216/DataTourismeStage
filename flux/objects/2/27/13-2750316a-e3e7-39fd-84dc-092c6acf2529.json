  "rdfs:comment": {
    "de": [
      "In der Kirche Notre-Dame-des-Anges, die von ihren Mönchen während der Französischen Revolution verlassen wurde, können Sie die Grotte de Coquillages (Muschelgrotte) entdecken."
    ],
    "en": [
      "Within the Church of Notre-Dame-des-Anges, left by its monks during the French Revolution, you can discover the Shell Grotto."
    ],
    "it": [
      "Nella chiesa di Notre-Dame-des-Anges, abbandonata dai suoi monaci durante la Rivoluzione francese, si può scoprire la Grotte de Coquillages."
    ],
    "fr": [
      "Au sein de l'Eglise Notre-Dame-des-Anges, quittée par ses moines à la Révolution Française, vous pourrez découvrir la Grotte de Coquillages."
    ],
    "nl": [
      "In de kerk Notre-Dame-des-Anges, die tijdens de Franse Revolutie door haar monniken werd verlaten, kunt u de Grotte de Coquillages ontdekken."
    ],
    "es": [
      "En la iglesia de Notre-Dame-des-Anges, abandonada por sus monjes durante la Revolución Francesa, podrá descubrir la Grotte de Coquillages."
    ]
  },
