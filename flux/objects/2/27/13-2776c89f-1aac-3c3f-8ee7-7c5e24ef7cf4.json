  "rdfs:comment": {
    "de": [
      "E-Bike-Route n.3 \"Entdecken Sie das authentische, unter Denkmalschutz stehende Dorf Le Monal\"\r\n\r\nAuf leichten, ausgeschilderten Wegen versprechen wir Ihnen, die schönsten Landschaften und Nuggets unserer Region zu genießen."
    ],
    "en": [
      "E-Bike route n.3 \"Discovering the authentic classified village of Monal\"\r\n\r\nOn easy and marked paths, you have the promise to enjoy the most beautiful landscapes and nuggets of our territory."
    ],
    "it": [
      "Percorso E-Bike n.3 \"Alla scoperta dell'autentico villaggio classificato di Monal\"\r\n\r\nSu sentieri facili e segnalati, si possono ammirare i paesaggi più belli e le pepite del nostro territorio."
    ],
    "fr": [
      "Itinéraire E-Bike n.3 \"A la découverte de l'authentique village Classé du Monal\"\r\n\r\nSur des chemins faciles et balisés, vous avez la promesse de profiter des plus beaux paysages et des pépites de notre territoire."
    ],
    "es": [
      "Ruta E-Bike n.3 \"Descubriendo el auténtico pueblo clasificado de Monal\"\r\n\r\nPor senderos fáciles y señalizados, podrá disfrutar de los más bellos paisajes y pepitas de nuestro territorio."
    ],
    "nl": [
      "E-Bike route n.3 \"Ontdekking van het authentieke geklasseerde dorp Monal\"\r\n\r\nOp gemakkelijke en bewegwijzerde paden kunt u genieten van de mooiste landschappen en klompjes van ons grondgebied."
    ]
  },
