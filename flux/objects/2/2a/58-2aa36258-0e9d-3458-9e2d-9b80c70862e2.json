  "rdfs:comment": {
    "de": [
      "Das im Gewerbegebiet Aix-en-Provence gelegene Restaurant Les Terrasses du Z5 bietet Ihnen eine einzigartige Umgebung im Herzen des wunderschönen Gewerbegebiets Z5."
    ],
    "en": [
      "Ideally located in the Aix-en-Provence activity cluster, the “Les Terrasses du Z5\" restaurant is in a unique environment at the heart of the magnificent Z5 centre."
    ],
    "it": [
      "Situato nel centro direzionale di Aix-en-Provence, il ristorante Les Terrasses du Z5 vi offre un ambiente unico nel cuore del magnifico centro direzionale Z5."
    ],
    "fr": [
      "Situé dans le pôle d'activités d'Aix-en-Provence, le restaurant Les Terrasses du Z5 vous propose de bénéficier d'un environnement unique au coeur du magnifique centre d'activités Z5."
    ],
    "es": [
      "Situado en el centro de negocios de Aix-en-Provence, el restaurante Les Terrasses du Z5 le ofrece un entorno único en el corazón del magnífico centro de negocios Z5."
    ],
    "nl": [
      "Het restaurant Les Terrasses du Z5, gelegen in het zakencentrum van Aix-en-Provence, biedt u een unieke omgeving in het hart van het prachtige zakencentrum Z5."
    ]
  },
