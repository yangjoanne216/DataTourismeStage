  "rdfs:comment": {
    "de": [
      "Yapu-k & Co bietet alles, was Kinder von 0 bis 10 Jahren für ihren Urlaub brauchen, zum Mieten an: Babyausstattung, Textilien, Ausrüstung, Spiele und Spielzeug."
    ],
    "en": [
      "Yapu-k & Co rents out everything children aged 0 to 10 need for their vacations: childcare equipment, textiles, equipment, games and toys."
    ],
    "it": [
      "Yapu-k & Co affitta tutto ciò di cui i bambini da 0 a 10 anni hanno bisogno per le loro vacanze: attrezzature per l'infanzia, tessuti, apparecchiature, giochi e giocattoli."
    ],
    "fr": [
      "Yapu-k & Co propose à la location tout le nécessaire pour les vacances des enfants de 0 à 10 ans : matériel de puériculture, textiles, équipements, jeux et jouets."
    ],
    "nl": [
      "Yapu-k & Co verhuurt alles wat kinderen van 0 tot 10 jaar nodig hebben voor hun vakantie: verzorgingsmateriaal, textiel, uitrusting, spelletjes en speelgoed."
    ],
    "es": [
      "Yapu-k & Co alquila todo lo que los niños de 0 a 10 años necesitan para sus vacaciones: material de puericultura, textil, equipamiento, juegos y juguetes."
    ]
  },
