  "rdfs:comment": {
    "de": [
      "Lokale und biologische Produzenten. Verkauf von vor Ort angebautem Biogemüse der Saison. Ab-Hof-Verkauf von Gemüse, Milchprodukten, Fleisch, Brot, Bier, Äpfeln."
    ],
    "en": [
      "Local and organic producers. Sale of organic seasonal vegetables grown on site. Sale of vegetables, dairy products, meat, bread, beer, apples."
    ],
    "it": [
      "Produttori locali e biologici. Vendita di verdure biologiche di stagione coltivate in loco. Vendita di verdure, prodotti caseari, carne, pane, birra e mele presso la fattoria."
    ],
    "fr": [
      "Producteurs locaux et bio. Vente de légumes bio de saison cultivés sur place. Vente à la ferme de légumes, produits laitiers, viandes, pains, bières, pommes."
    ],
    "es": [
      "Productores locales y ecológicos. Venta de verduras ecológicas de temporada cultivadas in situ. Venta de verduras, productos lácteos, carne, pan, cerveza y manzanas en la granja."
    ],
    "nl": [
      "Lokale en biologische producenten. Verkoop van ter plaatse geteelde biologische seizoensgroenten. Verkoop van groenten, zuivelproducten, vlees, brood, bier en appels op de boerderij."
    ]
  },
