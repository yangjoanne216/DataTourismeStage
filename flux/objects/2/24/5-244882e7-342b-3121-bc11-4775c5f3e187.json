  "rdfs:comment": {
    "de": [
      "Regionale Gerichte und hausgemachte Spezialitäten. Angenehmer Rahmen und herzlicher Empfang. Sehr gutes Preis-Leistungs-Verhältnis. Den ganzen Tag über von 10:30 Uhr bis Mitternacht geöffnet."
    ],
    "en": [
      "Regional dishes and house specialities. Pleasant setting and warm welcome. Very good value for money. Open all day from 10:30 am to midnight."
    ],
    "it": [
      "Piatti regionali e specialità fatte in casa. Ambiente piacevole e accoglienza calorosa. Ottimo rapporto qualità/prezzo. Aperto tutto il giorno dalle 10.30 a mezzanotte."
    ],
    "fr": [
      "Plats régionaux et spécialités maison. Cadre agréable et accueil chaleureux. Tres bon rapport qualité prix. Ouvert toute la journée de 10h30 à minuit."
    ],
    "nl": [
      "Regionale gerechten en huisgemaakte specialiteiten. Aangename omgeving en warme ontvangst. Zeer goede prijs-kwaliteitverhouding. De hele dag geopend van 10.30 tot middernacht."
    ],
    "es": [
      "Platos regionales y especialidades caseras. Un entorno agradable y una cálida acogida. Muy buena relación calidad-precio. Abierto todo el día desde las 10.30 hasta la medianoche."
    ]
  },
