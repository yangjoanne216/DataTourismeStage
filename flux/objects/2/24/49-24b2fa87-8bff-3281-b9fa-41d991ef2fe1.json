  "rdfs:comment": {
    "de": [
      "Von Joué-sur-Erdre kommend, am Eingang des Ortes. Regionale Küche, die von einem ideenreichen Küchenchef je nach Jahreszeit zubereitet wird. Tagesmenü unter der Woche und Catering am Wochenende."
    ],
    "en": [
      "Coming from Joué-sur-Erdre, at the entrance to the village. Local cuisine prepared according to the seasons by a chef full of ideas. Daily menu during the week and catered meals at weekends."
    ],
    "it": [
      "Provenendo da Joué-sur-Erdre, all'ingresso del villaggio. Cucina locale preparata secondo le stagioni da uno chef pieno di idee. Menù giornaliero durante la settimana e pasti con catering nel fine settimana."
    ],
    "fr": [
      "En venant de Joué-sur-Erdre, à l'entrée du bourg. Cuisine de terroir élaborée en fonction des saisons par un chef plein d'idées. Menu du jour en semaine et repas traiteur le week-end."
    ],
    "es": [
      "Viniendo de Joué-sur-Erdre, a la entrada del pueblo. Cocina local preparada según las estaciones por un chef lleno de ideas. Menú diario entre semana y comidas de encargo los fines de semana."
    ],
    "nl": [
      "Komende van Joué-sur-Erdre, bij de ingang van de stad. Lokale keuken bereid volgens de seizoenen door een chef vol ideeën. Dagmenu tijdens de week en catering tijdens het weekend."
    ]
  },
