  "rdfs:comment": {
    "de": [
      "Véronique Daux, die sich seit ihrer Kindheit für die Textilkunst und insbesondere für Wolle begeistert, hat in Saint-Denis-de-Gastines ihr Filzatelier Les Bêêlles laines gegründet."
    ],
    "en": [
      "Véronique Daux, passionate since her youth about textile arts and particularly about wool, has created Les Bêêlles laines, her workshop in Saint-Denis-de-Gastines."
    ],
    "it": [
      "Véronique Daux, appassionata di arti tessili e in particolare di lana fin da bambina, ha creato Les Bêlles laines, il suo laboratorio di feltro, a Saint-Denis-de-Gastines."
    ],
    "fr": [
      "Véronique Daux, passionnée depuis son plus jeune âge par les arts textiles et particulièrement par la laine a créé Les Bêêlles laines, son atelier de feutrière, à Saint-Denis-de-Gastines."
    ],
    "nl": [
      "Véronique Daux, die van jongs af aan gepassioneerd is door de textielkunst en in het bijzonder door wol, heeft in Saint-Denis-de-Gastines Les Bêêlles laines opgericht, haar viltatelier."
    ],
    "es": [
      "Véronique Daux, apasionada de las artes textiles y en particular de la lana desde niña, ha creado Les Bêêlles laines, su taller de fieltro, en Saint-Denis-de-Gastines."
    ]
  },
