  "rdfs:comment": {
    "de": [
      "Eine Genossenschaftskellerei, in der das Know-how der Erzeuger Weine aus provenzalischen Rebsorten wie Grenache, Syrah oder Cinsault hervorbringt."
    ],
    "en": [
      "A cooperative cellar where the know-how of the producers enhances the wines made from Provencal grape varieties, such as Grenache, Syrah or Cinsault."
    ],
    "it": [
      "Una cantina cooperativa dove il know-how dei produttori valorizza i vini ottenuti da vitigni provenzali, come Grenache, Syrah o Cinsault."
    ],
    "fr": [
      "Une cave coopérative où le savoir-faire des producteurs met en valeur des vins issus des cépages provençaux, comme le Grenache, la Syrah ou le Cinsault."
    ],
    "es": [
      "Una bodega cooperativa en la que el saber hacer de los productores realza los vinos elaborados con variedades de uva provenzal, como la garnacha, la syrah o la cinsault."
    ],
    "nl": [
      "Een coöperatieve kelder waar de knowhow van de producenten de wijnen van Provençaalse druivenrassen zoals Grenache, Syrah of Cinsault versterkt."
    ]
  },
