  "rdfs:comment": {
    "de": [
      "L'Esquimaude bietet Ihnen Kanufahrten auf dem Ain ab Pont d'Ain bis zu den ersten Sprudeln der Rhône an, im Kajak, auf Stand Up Paddles oder Big Sups."
    ],
    "en": [
      "Kayaking navigation on the Ain river is possible thanks to l'Esquimaude. \r\nFrom Pont d'Ain to another bridge, choose your distance 5, 10, 22 or 35km.\r\nRent your canoe-kayak, paddle-stand up or Big sup and have fun !"
    ],
    "it": [
      "L'Esquimaude propone la discesa in canoa dell'Ain da Pont d'Ain ai primi ruscelli del Rodano, in kayak, stand up paddle o Big sup."
    ],
    "fr": [
      "L'Esquimaude vous propose des descentes de l'Ain en canoë-kayak à partir de Pont d'Ain et jusqu'aux premiers bouillons du Rhône, en kayak, sur des stand up paddle ou Big sup."
    ],
    "es": [
      "L'Esquimaude propone descender el Ain en canoa desde Pont d'Ain hasta los primeros arroyos del Ródano, en kayak, en stand up paddle o en Big sup."
    ],
    "nl": [
      "L'Esquimaude biedt kanoën op de Ain vanaf Pont d'Ain tot aan de eerste beken van de Rhône, in kajaks, op stand up paddle of Big sup."
    ]
  },
