      "dc:description": {
        "de": [
          "Restaurant mit Panoramablick auf die mittelalterliche Stadt. Traditionelle und gastronomische Küche. Klimatisierter Saal. Terrasse gegenüber der Stadtmauer von Semur-en-Auxois."
        ],
        "en": [
          "Restaurant with a panoramic view of the medieval city. Traditional and gastronomic cuisine. Air-conditioned room. Terrace facing the ramparts of Semur-en-Auxois."
        ],
        "it": [
          "Ristorante con vista panoramica sulla città medievale. Cucina tradizionale e gastronomica. Camera climatizzata. Terrazza affacciata sui bastioni di Semur-en-Auxois."
        ],
        "fr": [
          "Restaurant avec vue panoramique sur la cité médiévale. Cuisine traditionnelle et gastronomique. Salle climatisée. Terrasse face aux remparts de Semur-en-Auxois."
        ],
        "es": [
          "Restaurante con vista panorámica de la ciudad medieval. Cocina tradicional y gastronómica. Habitación con aire acondicionado. Terraza frente a las murallas de Semur-en-Auxois."
        ],
        "nl": [
          "Restaurant met panoramisch uitzicht op de middeleeuwse stad. Traditionele en gastronomische keuken. Kamer met airconditioning. Terras tegenover de wallen van Semur-en-Auxois."
        ]
      },
