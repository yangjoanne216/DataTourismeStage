  "rdfs:comment": {
    "de": [
      "Die Brasserie OM Café befindet sich in der Nähe des Office du Tourisme und bietet eine raffinierte, herzhafte, mediterran inspirierte Küche mit Blick auf den Alten Hafen."
    ],
    "en": [
      "Located near the Tourist Office, the OM Café brasserie offers refined and hearty Mediterranean-inspired cuisine, with a view of the Old Port."
    ],
    "it": [
      "Situata vicino all'Ufficio del Turismo, la brasserie OM Café offre una cucina raffinata e sostanziosa di ispirazione mediterranea, con vista sul Porto Vecchio."
    ],
    "fr": [
      "Située près de l'Office du Tourisme, la brasserie OM Café propose une cuisine raffinée et copieuse d'inspiration méditerranéenne, avec vue sur le Vieux Port."
    ],
    "es": [
      "Situada cerca de la Oficina de Turismo, la brasserie OM Café sirve una cocina refinada y abundante de inspiración mediterránea, con vistas al Puerto Viejo."
    ],
    "nl": [
      "De brasserie OM Café, gelegen in de buurt van het VVV-kantoor, biedt een verfijnde en stevige mediterraanse keuken met uitzicht op de Oude Haven."
    ]
  },
