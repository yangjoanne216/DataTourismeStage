  "rdfs:comment": {
    "de": [
      "Südwestlich von Aix en Provence, zwischen Marseille und der Camargue, erscheint die Côte Bleue wie ein Schmuckkästchen aus noch unberührter Natur."
    ],
    "en": [
      "To the southwest of Aix en Provence, between Marseille and the Camargue, the Côte Bleue appears as an unspoiled natural setting."
    ],
    "it": [
      "A sud-ovest di Aix-en-Provence, tra Marsiglia e la Camargue, la Côte Bleue si presenta come uno scenario naturale incontaminato."
    ],
    "fr": [
      "Au sud-ouest d'Aix en Provence, entre Marseille et Camargue, la Côte Bleue apparait comme un écrin de nature encore préservé."
    ],
    "nl": [
      "Ten zuidwesten van Aix-en-Provence, tussen Marseille en de Camargue, verschijnt de Côte Bleue als een ongerepte natuurlijke omgeving."
    ],
    "es": [
      "Al suroeste de Aix-en-Provence, entre Marsella y la Camarga, la Côte Bleue aparece como un escenario natural intacto."
    ]
  },
