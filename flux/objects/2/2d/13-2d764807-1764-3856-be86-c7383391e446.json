  "rdfs:comment": {
    "de": [
      "2023 findet das 34. Internationale Festival für Jugendorchester und -chöre \"Eurochestries Charente-Maritime\" statt. Entdecken Sie die Ensembles bei 60 Konzerten, 330 Musiker aus 10 verschiedenen Nationalitäten."
    ],
    "en": [
      "2023 welcomes the 34th International Festival of Youth Orchestras and Choirs \"Eurochestries Charente-Maritime\", discover the ensembles during 60 concerts, 330 musicians from 10 different nationalities."
    ],
    "it": [
      "il 2023 accoglie il 34° Festival internazionale di orchestre e cori giovanili \"Eurochestries Charente-Maritime\", per scoprire gli ensemble durante 60 concerti, 330 musicisti di 10 nazionalità diverse."
    ],
    "fr": [
      "2023 accueille le 34ème Festival International d'orchestres et chœurs de jeunes \"Eurochestries Charente-Maritime\", découvrez les ensembles au cours de 60 concerts, 330 musiciens de 10 nationalités différentes."
    ],
    "nl": [
      "2023 verwelkomt het 34e Internationale Festival van Jeugdorkesten en Koren \"Eurochestries Charente-Maritime\", ontdek de ensembles tijdens 60 concerten, 330 muzikanten van 10 verschillende nationaliteiten."
    ],
    "es": [
      "2023 acoge el 34º Festival Internacional de Jóvenes Orquestas y Coros \"Eurochestries Charente-Maritime\", descubra los conjuntos durante 60 conciertos, 330 músicos de 10 nacionalidades diferentes."
    ]
  },
