  "rdfs:comment": {
    "de": [
      "1,5 Stunden von Paris entfernt (5 min Ausfahrt Mer A10) in der Nähe der Schlösser von Chambord, Blois, Talcy, Cheverny und Amboise sowie des Zoos von Beauval. Unabhängiges Ferienhaus, Kapazität für 7 Personen, mit eingezäuntem Garten und Parkplatz."
    ],
    "en": [
      "At 1h30 from Paris (5 min A10 motorway exit) near the castles of Chambord, Blois, Talcy, Cheverny and Amboise and the zoo of Beauval. Independent gîte, capacity of 7 people, with enclosed garden and parking."
    ],
    "it": [
      "A 1h30 da Parigi (5 minuti dall'uscita dell'autostrada A10), vicino ai castelli di Chambord, Blois, Talcy, Cheverny e Amboise e allo zoo di Beauval. Gîte indipendente, capacità di 7 persone, con giardino recintato e parcheggio."
    ],
    "fr": [
      "À 1h30 de Paris (5 min sortie Mer A10) à proximité des châteaux de Chambord, Blois, Talcy, Cheverny et Amboise et du zoo de Beauval. Gîte indépendant, capacité de 7 personnes, avec jardin clos et parking."
    ],
    "nl": [
      "Op 1u30 van Parijs (5 min van de afrit van de A10) in de buurt van de kastelen van Chambord, Blois, Talcy, Cheverny en Amboise en de dierentuin van Beauval. Vrijstaande gîte, capaciteit voor 7 personen, met omheinde tuin en parkeerplaats."
    ],
    "es": [
      "A 1h30 de París (5 min de la salida de la autopista A10), cerca de los castillos de Chambord, Blois, Talcy, Cheverny y Amboise y del zoo de Beauval. Casa rural independiente, con capacidad para 7 personas, con jardín cerrado y aparcamiento."
