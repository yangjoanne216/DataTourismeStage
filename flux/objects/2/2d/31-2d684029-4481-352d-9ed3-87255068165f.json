  "rdfs:comment": {
    "de": [
      "Halten Sie in diesem Dorf aus dem 12. Jahrhundert an: Sie werden dem Charme dieses identitätsstiftenden Ortes in der Ségala verfallen!"
    ],
    "en": [
      "Make a stop in this 12th century village: you will fell under the spell of this perfect illustration of the Ségala region!"
    ],
    "it": [
      "Fate una sosta in questo villaggio del XII secolo: vi lascerete incantare da questo luogo che fa parte dell'identità di Ségala!"
    ],
    "fr": [
      "Arrêtez-vous dans ce village du XIIe siècle : vous tomberez sous le charme de ce lieu identitaire du Ségala !"
    ],
    "nl": [
      "Stop in dit dorp uit de 12e eeuw: u zult in de ban raken van deze plaats die deel uitmaakt van de Ségala-identiteit!"
    ],
    "es": [
      "Haga una parada en este pueblo del siglo XII: ¡caerá en el encanto de este lugar que forma parte de la identidad de Ségala!"
    ]
  },
