  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/8f45f5fd-b4ed-3629-9670-9341e04deeb9",
      "dc:description": {
        "de": [
          "Park mit Panoramablick auf das Pays du Perche Sarthois. Schloss und Park unter Denkmalschutz. 5 charmante Gästezimmer mit angrenzendem Badezimmer. Frühstück mit lokalen Aromen."
        ],
        "en": [
          "Park with panoramic view of the Pays du Perche Sarthois. Castle and classified park. 5 charming guest rooms with en-suite bathroom. Breakfast with local flavours."
        ],
        "it": [
          "Parco con vista panoramica sulla regione del Perche Sarthois. Castello e parco tutelati. 5 incantevoli camere per gli ospiti con bagno privato. Colazione con sapori locali."
        ],
        "fr": [
          "Parc avec vue panoramique sur le Pays du Perche Sarthois. Château et Parc classés. 5 chambres d'hôtes de charme avec salle de bain attenante. Petit-déjeuner aux saveurs locales."
        ],
        "es": [
          "Parque con vista panorámica de la región del Perche Sarthois. Castillo y parque catalogados. 5 encantadoras habitaciones de huéspedes con baños en suite. Desayuno con sabores locales."
        ],
        "nl": [
          "Park met panoramisch uitzicht op de Perche Sarthois. Monumentaal kasteel en park. 5 charmante gastenkamers met eigen badkamer. Ontbijt met lokale smaken."
        ]
      },
