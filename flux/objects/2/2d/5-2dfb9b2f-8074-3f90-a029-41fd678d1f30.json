  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/11415b05-8d04-3f18-ba32-674d9d509c90",
      "dc:description": {
        "de": [
          "Munstertal, in Soultzeren. Chalet für 8 Personen. Charme, gemütliche Atmosphäre, geräumige und helle Innenräume, Terrasse, Garten. Idealer Ausgangspunkt für Wanderungen, Radtouren oder Mountainbiketouren. Skistation 15 Minuten entfernt."
        ],
        "en": [
          "Valley of Munster, in Soultzeren. Chalet for 8 persons. Charming, warm atmosphere, spacious and bright interiors, terrace, garden. Ideal starting point for hikes, bike or mountain bike outings. Ski station 15 minutes away."
        ],
        "it": [
          "Valle di Munster, a Soultzeren. Chalet per 8 persone. Atmosfera accogliente e calorosa, interni spaziosi e luminosi, terrazza, giardino. Punto di partenza ideale per escursioni a piedi, in bicicletta o in mountain bike. Stazione sciistica a 15 minuti."
        ],
        "fr": [
          "Vallée de Munster, à Soultzeren. Chalet pour 8 personnes. Charme, ambiance chaleureuse, intérieurs spacieux et lumineux, terrasse, jardin. Départ idéal pour des randonnées, sorties vélo ou vtt. Station de ski à 15 minutes."
        ],
        "nl": [
          "Vallei van Munster, in Soultzeren. Chalet voor 8 personen. Charmante, warme sfeer, ruim en licht interieur, terras, tuin. Ideaal uitgangspunt voor wandelen, fietsen of mountainbiken. Skigebied op 15 minuten afstand."
        ],
        "es": [
          "Valle de Munster, en Soultzeren. Chalet para 8 personas. Ambiente encantador y cálido, interior amplio y luminoso, terraza, jardín. Punto de partida ideal para el senderismo, el ciclismo o la bicicleta de montaña. Estación de esquí a 15 minutos."
        ]
      },
