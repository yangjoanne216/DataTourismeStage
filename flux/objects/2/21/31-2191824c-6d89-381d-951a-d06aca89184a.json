  "rdfs:comment": {
    "de": [
      "Willkommen bei Khopaï in Espalion. Wir bieten Ihnen thailändische Gerichte an. Unsere Menüs wechseln jede Woche. Gerichte zum Mitnehmen oder zum Essen vor Ort. Verkauf asiatischer Produkte"
    ],
    "en": [
      "Welcome to Khopaï in Espalion. We offer Thai food. Our menus change every week. Dishes to take away or to eat on the spot. Sale of Asian products"
    ],
    "it": [
      "Benvenuti al Khopaï di Espalion. Offriamo cibo tailandese. I nostri menu cambiano ogni settimana. Piatti da portare via o da mangiare sul posto. Vendita di prodotti asiatici"
    ],
    "fr": [
      "Bienvenue chez Khopaï à Espalion​. Nous vous proposons des plats Thaïlandais. Nos menus changent toutes les semaines. Plats à emporter ou à manger sur place. Vente produits asiatiques"
    ],
    "nl": [
      "Welkom bij Khopaï in Espalion. We bieden Thais eten aan. Onze menu's veranderen elke week. Gerechten om mee te nemen of om ter plekke op te eten. Verkoop van Aziatische producten"
    ],
    "es": [
      "Bienvenido a Khopaï en Espalion. Ofrecemos comida tailandesa. Nuestros menús cambian cada semana. Platos para llevar o para comer allí mismo. Venta de productos asiáticos"
    ]
  },
