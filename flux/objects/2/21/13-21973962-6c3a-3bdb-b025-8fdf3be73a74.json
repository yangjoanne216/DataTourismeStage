  "rdfs:comment": {
    "de": [
      "Pierre André Benoit (PAB), Drucker, Dichter, Bildhauer und Zeichner, schenkte der Stadt Alès seine Sammlung moderner Kunstwerke, um das nach ihm benannte Museum zu gründen."
    ],
    "en": [
      "Pierre André Benoit (PAB), printer, poet, sculptor and draughtsman, donated his collection of modern art to the city of Alès to create the museum that bears his name."
    ],
    "it": [
      "Pierre André Benoit (PAB), tipografo, poeta, scultore e disegnatore, ha donato la sua collezione di arte moderna alla città di Alès per creare il museo che porta il suo nome."
    ],
    "fr": [
      "Pierre André Benoit (PAB), imprimeur, poète, sculpteur et dessinateur, a fait don de sa collection d’œuvres d’art moderne à la Ville d’Alès pour créer le musée qui porte son nom."
    ],
    "es": [
      "Pierre André Benoit (PAB), impresor, poeta, escultor y dibujante, donó su colección de arte moderno a la ciudad de Alès para crear el museo que lleva su nombre."
    ],
    "nl": [
      "Pierre André Benoit (PAB), drukker, dichter, beeldhouwer en tekenaar, schonk zijn collectie moderne kunst aan de stad Alès voor de oprichting van het museum dat zijn naam draagt."
    ]
  },
