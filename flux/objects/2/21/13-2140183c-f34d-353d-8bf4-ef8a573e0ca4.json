  "rdfs:comment": {
    "de": [
      "Familienzucht von Kaninchen (7 Mütter), Hühnern, Enten, Eseln, die das Gestrüpp entfernen; biologische Bewirtschaftung des Kastanienwaldes und der Gemüsegärten; ökologische Forstwirtschaft \"pro silva\"."
    ],
    "en": [
      "Family rearing of rabbits (7 dams), chickens, ducks, presence of 'bush-clearing' donkeys; biological exploitation of chestnut groves and vegetable gardens; ecological forestry exploitation 'pro silva'."
    ],
    "it": [
      "Allevamento familiare di conigli (7 madri), galline, anatre, presenza di asini \"ripulitori\"; sfruttamento biologico del castagneto e degli orti; sfruttamento forestale ecologico \"pro silva\"."
    ],
    "fr": [
      "Élevage familial de lapins (7 mères ), poules, canards, présence d'ânes \"débroussailleurs\"; exploitation en biologie de la châtaigneraie et des jardins potagers; exploitation sylvicole écologique \"pro silva\"."
    ],
    "nl": [
      "Gezinsfokkerij van konijnen (7 moeders), kippen, eenden, aanwezigheid van ezels die \"kreupelhout ruimen\"; biologische exploitatie van het kastanjebos en de moestuinen; ecologische bosexploitatie \"pro silva\"."
    ],
    "es": [
      "Cría familiar de conejos (7 madres), pollos, patos, presencia de burros \"desbrozando\"; explotación biológica del castañar y de los huertos; explotación forestal ecológica \"pro silva\"."
    ]
  },
  "rdfs:label": {
    "fr": [
      "La ferme de L'Aulne d'Alune"
    ]
  },
