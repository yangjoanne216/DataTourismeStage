  "rdfs:comment": {
    "de": [
      "Die Winzer von Menetou-Salon öffnen ihre Keller, um ihren Beruf, ihre Leidenschaft und ihre Weine vorzustellen."
    ],
    "en": [
      "The winegrowers of Menetou-Salon open their cellars in order to make discover their job, their passion and their wines."
    ],
    "it": [
      "I viticoltori di Menetou-Salon aprono le loro cantine per far scoprire il loro mestiere, la loro passione e i loro vini."
    ],
    "fr": [
      "Les vignerons de Menetou-Salon ouvrent leurs caves afin de faire découvrir leur métier, leur passion et leurs vins."
    ],
    "es": [
      "Los viticultores de Menetou-Salon abren sus bodegas para hacer descubrir su oficio, su pasión y sus vinos."
    ],
    "nl": [
      "De wijnbouwers van Menetou-Salon stellen hun kelders open om hun vak, hun passie en hun wijnen te laten ontdekken."
    ]
