  "rdfs:comment": {
    "de": [
      "Challonges, das in der Mitte des Semine-Plateaus liegt, war früher ein wichtiger Handelsumschlagplatz, der derzeit von Seyssel abgelöst wird."
    ],
    "en": [
      "Challonges, in the center of the plateau de la Semine, was originally an important centre for trade, now replaced by Seyssel."
    ],
    "it": [
      "Challonges, situata al centro dell'altopiano della Semine, era un tempo un importante centro commerciale, ora sostituito da Seyssel."
    ],
    "fr": [
      "Challonges, située au centre du plateau de la Semine, était autrefois un important centre d'échange commercial, remplacé actuellement par Seyssel."
    ],
    "es": [
      "Challonges, situado en el centro de la meseta de Semine, fue en su día un importante centro comercial, ahora sustituido por Seyssel."
    ],
    "nl": [
      "Challonges, gelegen in het centrum van het plateau van de Semine, was ooit een belangrijk handelscentrum, nu vervangen door Seyssel."
    ]
  },
