  "rdfs:comment": {
    "de": [
      "Sammelnder Imker in Sainte-Croix, Verkauf von Bienenprodukten auf dem Bauernhof.\r\nNeu in diesem Jahr: Verarbeitung des Honigs vor Ort durch Robin Apiculteur-Confiseur."
    ],
    "en": [
      "Beekeeper harvesting on Sainte-Croix, sale at the Farm of the products of the hive.\r\nNew this year: Honey processing on site by Robin Apiculturist-Confiseur."
    ],
    "it": [
      "Raccolta delle api a Sainte-Croix, vendita alla fattoria dei prodotti dell'alveare.\r\nNovità di quest'anno: lavorazione del miele in loco da parte di Robin Apiculturist-Confiseur."
    ],
    "fr": [
      "Apiculteur récoltant sur Sainte-Croix, vente à la Ferme des produits de la ruche.\r\nNouveau cette année : Transformation du miel sur place par Robin Apiculteur-Confiseur."
    ],
    "nl": [
      "Imker oogst op Sainte-Croix, verkoop op de boerderij van de producten van de bijenkorf.\r\nNieuw dit jaar: honingverwerking ter plaatse door Robin Apiculturist-Confiseur."
    ],
    "es": [
      "Apicultor cosechando en Sainte-Croix, venta en la Granja de los productos de la colmena.\r\nNovedad de este año: Procesamiento de la miel in situ por Robin Apiculturist-Confiseur."
    ]
  },
