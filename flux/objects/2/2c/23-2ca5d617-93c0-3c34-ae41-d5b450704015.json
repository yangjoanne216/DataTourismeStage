  "rdfs:comment": {
    "de": [
      "Archi Ludo in Carsac-Aillac: gemeindeübergreifende mobile Ludothek.\n\nSpielen Sie mit Ihrer Familie vor Ort und/oder leihen Sie sich den ganzen Tag über Spiele aus."
    ],
    "en": [
      "Archi Ludo in Carsac-Aillac: intercommunal mobile toy library.\n\nCome and play on site with your family and/or borrow games all day long."
    ],
    "it": [
      "Archi Ludo a Carsac-Aillac: una ludoteca intercomunale itinerante.\n\nVenite a giocare sul posto con la vostra famiglia e/o a prendere in prestito i giochi per tutto il giorno."
    ],
    "fr": [
      "Archi Ludo à Carsac-Aillac : ludothèque intercommunale itinérante.\n\nVenez jouer sur place en famille et/ou emprunter des jeux toute la journée."
    ],
    "nl": [
      "Archi Ludo in Carsac-Aillac: een reizende intercommunale spelotheek.\n\nKom ter plaatse spelen met je gezin en/of leen de hele dag spelletjes."
    ],
    "es": [
      "Archi Ludo en Carsac-Aillac: una ludoteca intercomunal itinerante.\n\nVenga a jugar in situ con su familia y/o tome prestados juegos durante todo el día."
    ]
  },
