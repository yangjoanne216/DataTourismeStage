  "rdfs:comment": {
    "de": [
      "Zwischen Bretagne und Vendée, an den Toren des Marais Breton, am Flussufer, 500 m vom Stadtzentrum entfernt, in einer grünen Umgebung von 3 Hektar, auf 131 Stellplätzen."
    ],
    "en": [
      "Between Brittany and the Vendée, at the gateway to the Marais Breton, on the banks of the river, 500 m from the town centre, in a green setting of 3 hectares, on 131 pitches."
    ],
    "it": [
      "Tra la Bretagna e la Vandea, alle porte del Marais Breton, sul bordo del fiume, a 500 m dal centro città, in un contesto verde di 3 ettari, su 131 piazzole."
    ],
    "fr": [
      "Entre Bretagne et Vendée, aux portes du Marais Breton, en bordure de rivière, à 500 m du centre-ville, dans un cadre de verdure de 3 hectares, sur 131 emplacements."
    ],
    "nl": [
      "Tussen Bretagne en de Vendée, aan de poort van het Marais Breton, aan de rand van de rivier, op 500 m van het stadscentrum, in een groene omgeving van 3 hectare, op 131 plaatsen."
    ],
    "es": [
      "Entre Bretaña y la Vendée, a las puertas del Marais Breton, al borde del río, a 500 m del centro de la ciudad, en un entorno verde de 3 hectáreas, en 131 parcelas."
    ]
