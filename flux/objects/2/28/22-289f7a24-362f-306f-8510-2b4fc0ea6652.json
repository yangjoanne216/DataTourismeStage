        "de": [
          "5 Gerichte + 5 Weine\nAppetizer, Vorspeise, Hauptgericht, Käse, Dessert. 2 Weißweine, 3 Rotweine (im Glas serviert)\nBegrenzte Plätze!"
        ],
        "en": [
          "5 courses + 5 wines\nAppetizer, starter, main course, cheese, dessert. 2 white wines, 3 reds (served by the glass)\nLimited seating!"
        ],
        "it": [
          "5 portate + 5 vini\nMise en bouche, antipasto, piatto principale, formaggio, dessert. 2 vini bianchi, 3 rossi (serviti al bicchiere)\nPosti limitati!"
        ],
        "fr": [
          "5 plats + 5 vins\nMise en bouche, entrée, plat, fromage, dessert. 2 vins blancs, 3 rouges (servis au verre)\nPlaces limitées!"
        ],
        "es": [
          "5 platos + 5 vinos\nMise en bouche, entrada, plato principal, queso, postre. 2 vinos blancos, 3 tintos (servidos por copa)\nAforo limitado"
        ],
        "nl": [
          "5 gangen + 5 wijnen\nMise en bouche, voorgerecht, hoofdgerecht, kaas, dessert. 2 witte wijnen, 3 rode (geserveerd per glas)\nBeperkte plaatsen!"
        ]
      },
