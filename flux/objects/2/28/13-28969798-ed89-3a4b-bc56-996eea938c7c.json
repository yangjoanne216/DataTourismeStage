  "rdfs:comment": {
    "de": [
      "Ruhe und Halbschatten Campingplatz, 2 Minuten vom Dorf Dieulefit entfernt. Flach und grasbewachsenen Boden. Camping \"Glamping\" mit vielen ungewöhnliche Unterkunft und sehr geeignet für Reisemobile."
    ],
    "en": [
      "Very calm and shaded campsite, 2-minute walk from the village centre of Dieulefit. Flat area with grass. Family atmosphere. Camping \"glamping\" with many unusual accommodation and very suitable for motorhomes."
    ],
    "it": [
      "Campeggio naturalistico e antichissimo vicino al centro del villaggio di Dieulefit.  Adatto ai camper."
    ],
    "fr": [
      "Camping Nature et Anthentique à proximité du centre du village de Dieulefit.  Adapté aux camping-caristes."
    ],
    "nl": [
      "Kalm en semi-schaduwrijke camping, op 2 minuten van het dorp Dieulefit centrum. Vlakke en grasrijke grond. Camping \"glamping\" met veel bijzondere accommodatie en zeer geschikt voor kampeerauto's."
    ],
    "es": [
      "Camping de naturaleza y antítesis cerca del centro del pueblo de Dieulefit.  Apto para furgonetas camperas."
    ]
  },
