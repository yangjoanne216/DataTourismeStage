      "dc:description": {
        "de": [
          "Verkauf. Reparatur. Bootsausrüstung - Verkleidung - Lackierung - Osmosebehandlung. Mechanik - Marineelektronik - Wartung - Überwinterung. \nVertragshändler: Prestige - Jeanneau - Tempest - Motoren Yamaha - Evinrude.\n\nAgent \"Accastillage Diffusion\"."
        ],
        "en": [
          "Sale. Repairs. Fittings - Careening - painting - osmosis treatment. Mechanics - Marine electronics - Guarding - Winter storage. \nDealer : Prestige - Jeanneau - Tempest - Yamaha engines - Evinrude.\n\nAgent \"Accastillage Diffusion"
        ],
        "it": [
          "Vendita. Riparazioni. Raccordi - carenaggio - verniciatura - trattamento con osmosi. Meccanica - Elettronica marina - Guardiania - Rimessaggio invernale\nConcessionario : Prestige - Jeanneau - Tempest - Motori Yamaha - Evinrude.\n\nAgente \"Diffusione dell'accastellamento"
        ],
        "fr": [
          "Vente. Réparation. Accastillage - Carénage - peinture - traitement osmose. Mécanique - Electronique marine - Gardiennage - Hivernage. \nConcessionnaire : Prestige - Jeanneau - Tempest - Moteurs Yamaha -  Evinrude.\n\nAgent \"Accastillage Diffusion\""
        ],
        "nl": [
          "Verkoop. Reparaties. Beslag - Verzorging - verven - osmose behandeling. Mechanica - Scheepselectronica - Bewaking - Winterstalling. \nDealer: Prestige - Jeanneau - Tempest - Yamaha motoren - Evinrude.\n\nAgent \"Accastillage Verspreiding"
        ],
        "es": [
          "Venta. Reparaciones. Herrajes - Carenado - pintura - tratamiento de ósmosis. Mecánica - Electrónica marina - Vigilancia - Invernaje\nDistribuidor : Prestige - Jeanneau - Tempest - Motores Yamaha - Evinrude.\n\nAgente \"Accastillage Diffusion"
        ]
