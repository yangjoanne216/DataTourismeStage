  "rdfs:comment": {
    "de": [
      "Les Gets Tourisme, das Rathaus von Les Gets und das Musée de la Musique Mécanique empfangen Sie anlässlich der Europäischen Tage des Kulturerbes zu den Themen \"Lebendiges Kulturerbe\" und \"Kulturerbe des Sports\"."
    ],
    "en": [
      "Les Gets Tourisme, the Town Hall of Les Gets and the Mechanical Music Museum welcome you on the occasion of the European Heritage Days around the themes \"Living Heritage\" and \"Sporting Heritage\"."
    ],
    "it": [
      "Les Gets Tourisme, il Comune di Les Gets e il Museo della Musica Meccanica vi danno il benvenuto in occasione delle Giornate Europee del Patrimonio sui temi \"Patrimonio vivente\" e \"Patrimonio sportivo\"."
    ],
    "fr": [
      "Les Gets Tourisme, la Mairie des Gets et Le Musée de la Musique Mécanique vous accueillent à l'occasion des Journées Européennes du Patrimoine autour des thèmes \"Patrimoine Vivant\" et \"Patrimoine du Sport\"."
    ],
    "nl": [
      "Les Gets Tourisme, het stadhuis van Les Gets en het museum voor mechanische muziek heten u welkom ter gelegenheid van de Europese erfgoeddagen rond de thema's \"Levend erfgoed\" en \"Sportief erfgoed\"."
    ],
    "es": [
      "Les Gets Tourisme, el Ayuntamiento de Les Gets y el Museo de la Música Mecánica le dan la bienvenida con motivo de las Jornadas Europeas del Patrimonio en torno a los temas \"Patrimonio vivo\" y \"Patrimonio deportivo\"."
    ]
  },
  "rdfs:label": {
    "en": [
      "European Heritage Days"
    ],
    "fr": [
      "Journées Européennes du Patrimoine"
    ],
    "nl": [
      "Erfgoeddagen"
    ]
  },
