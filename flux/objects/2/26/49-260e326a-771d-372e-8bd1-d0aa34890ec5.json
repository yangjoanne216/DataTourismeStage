  "rdfs:comment": {
    "de": [
      "5 Minuten von Laval entfernt können Sie das Vergnügen genießen, Ihr Obst im Obstgarten selbst zu pflücken: Äpfel, Kirschen, Pflaumen, Himbeeren, Blaubeeren..."
    ],
    "en": [
      "5 minutes from Laval, taste the pleasure of picking your own fruit in the orchard: apples, cherries, plums, raspberries, blueberries..."
    ],
    "it": [
      "A 5 minuti da Laval, godetevi il piacere di raccogliere la vostra frutta nel frutteto: mele, ciliegie, prugne, lamponi, mirtilli..."
    ],
    "fr": [
      "À 5 minutes de Laval, goutez le plaisir de cueillir vous-même vos fruits au verger : pommes, cerises, prunes, framboises, myrtilles..."
    ],
    "nl": [
      "Op 5 minuten van Laval kunt u in de boomgaard uw eigen fruit plukken: appels, kersen, pruimen, frambozen, bosbessen..."
    ],
    "es": [
      "A 5 minutos de Laval, disfrute del placer de recoger su propia fruta en el huerto: manzanas, cerezas, ciruelas, frambuesas, arándanos..."
    ]
  },
