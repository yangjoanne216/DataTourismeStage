  "rdfs:comment": {
    "de": [
      "Spezialist für Bergausrüstung.\nVerkauf und Verleih von Tourenski, Langlaufski, Schneeschuhen, Textilien, Schuhen, Rucksäcken und Zubehör\nZahlreiche Marken: Montura Patagonia, Millet, La Sportiva, Icebreaker...\nVermietung / Verkauf von Mountainbikes / Elektro-Mountainbikes."
    ],
    "en": [
      "Mountain equipment specialist.\nSale and rental of ski touring, cross-country skiing, snowshoes, textiles, shoes, backpacks, accessories\nMany brands : Montura Patagonia, Millet, La Sportiva, Icebreaker...\nRental / Sale of mountain bikes / electric mountain bikes."
    ],
    "it": [
      "Specialista in attrezzature da montagna.\nVendita e noleggio di sci alpinismo, sci di fondo, racchette da neve, tessuti, scarpe, zaini, accessori\nMolte marche: Montura Patagonia, Millet, La Sportiva, Icebreaker...\nNoleggio / Vendita di mountain bike / bici elettriche."
    ],
    "fr": [
      "Spécialiste matériel montagne.\nVente et Location ski de randonnée, ski de fond, raquettes, textile, chaussures, sac à dos, accessoires. \nNombreuses marques : Montura Patagonia, Millet, La Sportiva, Icebreaker...\nLocation / Vente VTT / VTT électrique."
    ],
    "nl": [
      "Specialist in berguitrusting.\nVerkoop en verhuur van skitochten, langlaufen, sneeuwschoenen, textiel, schoenen, rugzakken, accessoires\nVeel merken: Montura Patagonia, Millet, La Sportiva, Icebreaker...\nVerhuur / verkoop van mountainbikes / elektrische fietsen."
    ],
    "es": [
      "Especialista en equipamiento de montaña.\nVenta y alquiler de esquí de travesía, esquí de fondo, raquetas de nieve, textil, calzado, mochilas, accesorios\nMuchas marcas: Montura Patagonia, Millet, La Sportiva, Icebreaker...\nAlquiler / Venta de bicicletas de montaña / bicicletas eléctricas."
    ]
  },
