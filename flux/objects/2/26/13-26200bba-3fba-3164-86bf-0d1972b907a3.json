  "rdfs:comment": {
    "de": [
      "30 Minuten vom internationalen Flughafen Nizza und 8 km von Cannes entfernt empfängt Sie das Mademoiselle Hôtel im Herzen von Juan-les-Pins."
    ],
    "en": [
      "Located just 30 minutes from the International Airport of Nice and 8km from Cannes, Mademoiselle Hôtel welcomes you in the heart of Juan-les-Pins just 50 metres from the white sandy beaches and La Pinède, famous for its Jazz Festival."
    ],
    "it": [
      "Situato a 30 minuti dall'aeroporto internazionale di Nizza e a 8 km da Cannes, l'hotel Mademoiselle vi accoglie nel cuore di Juan-les-Pins."
    ],
    "fr": [
      "Situé à 30 minutes de l'aéroport international de Nice et à 8 km de Cannes, le Mademoiselle Hôtel vous accueille en plein cœur de Juan-les-Pins."
    ],
    "es": [
      "Situado a 30 minutos del aeropuerto internacional de Niza y a 8 km de Cannes, el hotel Mademoiselle le da la bienvenida en el corazón de Juan-les-Pins."
    ],
    "nl": [
      "Het Mademoiselle Hotel ligt op 30 minuten van de internationale luchthaven van Nice en op 8 km van Cannes en verwelkomt u in het hart van Juan-les-Pins."
    ]
  },
