  "rdfs:comment": {
    "de": [
      "Restaurant im Ortszentrum eines kleinen Dorfes im blauen Anjou, zwischen Vern d'Anjou und Segré. Unter der Woche wird Ihnen mittags eine traditionelle Küche angeboten."
    ],
    "en": [
      "Restaurant located in the centre of a small village in the blue Anjou region, between Vern d'Anjou and Segré. At lunchtime during the week, traditional cuisine will be offered."
    ],
    "it": [
      "Ristorante situato nel centro di un piccolo villaggio nella regione dell'Anjou Bleu, tra Vern d'Anjou e Segré. A pranzo, durante la settimana, vi verrà offerta una cucina tradizionale."
    ],
    "fr": [
      "Restaurant situé dans le centre-bourg d'un petit village de l'Anjou bleu, entre Vern d'Anjou et Segré. Le midi en semaine, il vous sera proposé une cuisine traditionnelle."
    ],
    "es": [
      "Restaurante situado en el centro de un pequeño pueblo de la región de Anjou Bleu, entre Vern d'Anjou y Segré. A la hora de comer entre semana, se le ofrecerá una cocina tradicional."
    ],
    "nl": [
      "Restaurant gelegen in het centrum van een klein dorp in de regio Anjou Bleu, tussen Vern d'Anjou en Segré. Tijdens de lunch in de week wordt u een traditionele keuken aangeboden."
    ]
  },
