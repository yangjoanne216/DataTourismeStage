  "rdfs:comment": {
    "de": [
      "Chefkoch Poncet legt besonderen Wert auf die Beschaffung seiner Produkte, die größtenteils in kurzen Kreisläufen von lokalen Produzenten bezogen werden, mit einer Qualitätsgarantie!"
    ],
    "en": [
      "Chef Poncet pays particular attention to the sourcing of his products, mostly from local producers, with a guarantee of quality!"
    ],
    "it": [
      "Lo chef Poncet attribuisce particolare importanza all'approvvigionamento dei prodotti, per lo più da produttori locali, con garanzia di qualità!"
    ],
    "fr": [
      "Le chef Poncet porte une importance toute particulière au sourcing de ses produits, en grande partie en circuits courts, chez des producteurs locaux, avec une garantie de qualité !"
    ],
    "es": [
      "El chef Poncet concede especial importancia al abastecimiento de sus productos, en su mayoría procedentes de productores locales, con garantía de calidad"
    ],
    "nl": [
      "Chef Poncet hecht bijzonder veel belang aan de inkoop van zijn producten, meestal bij lokale producenten, met een kwaliteitsgarantie!"
    ]
  },
