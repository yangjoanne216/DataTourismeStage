  "rdfs:comment": {
    "de": [
      "Bar-Restaurant gegenüber der romanischen Kirche mit ihrem schönen Glockenturm und nur wenige Schritte vom Dorfmuseum (Wiege der Charolais-Rasse) en..."
    ],
    "en": [
      "Bar-Restaurant located in front of the Romanesque church with its beautiful bell tower and only a few steps away from the village museum (cradle of the Charolais breed)."
    ],
    "it": [
      "Bar-ristorante situato di fronte alla chiesa romanica con il suo bel campanile e a pochi passi dal museo del villaggio (culla della razza Charolais)."
    ],
    "fr": [
      "Bar-Restaurant situé en face de l'église romane avec son beau cloché et a seulement quelques pas du musée du village (berceau de la race Charolaise)."
    ],
    "es": [
      "Bar-Restaurante situado frente a la iglesia románica con su hermoso campanario y a pocos pasos del museo del pueblo (cuna de la raza charolesa)."
    ],
    "nl": [
      "Bar-Restaurant gelegen tegenover de Romaanse kerk met zijn mooie klokkentoren en op een steenworp afstand van het dorpsmuseum (bakermat van het Charolais-ras)."
    ]
  },
