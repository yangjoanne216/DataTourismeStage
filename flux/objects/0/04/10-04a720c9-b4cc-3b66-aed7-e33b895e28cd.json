  "rdfs:comment": {
    "de": [
      "Produzent von Bio-Kräuterpflanzen, die zu Kräutertees und Gewürzen verarbeitet werden.\nDirektverkauf der Produkte des Bauernhofs und gelegentliche Besuche von Freizeitaktivitäten.\nÖffnungszeiten nach Vereinbarung."
    ],
    "en": [
      "Producer of organic aromatic plants transformed into infusions and spices.\nDirect sale of farm products and occasional recreational visits.\nOpen by appointment."
    ],
    "it": [
      "Produttore di piante aromatiche biologiche trasformate in infusi e spezie.\nVendita diretta di prodotti agricoli e visite ricreative occasionali.\nAperto su appuntamento."
    ],
    "fr": [
      "Producteur de plantes aromatiques bio transformées en infusions et épices.\nVente directe des produits de la ferme et visites récréatives ponctuellement.\nOuverture sur rendez-vous."
    ],
    "es": [
      "Productor de plantas aromáticas ecológicas transformadas en infusiones y especias.\nVenta directa de productos agrícolas y visitas ocasionales de recreo.\nAbierto con cita previa."
    ],
    "nl": [
      "Producent van biologische aromatische planten verwerkt tot infusies en kruiden.\nDirecte verkoop van boerderijproducten en incidenteel recreatief bezoek.\nOpen op afspraak."
    ]
  },
