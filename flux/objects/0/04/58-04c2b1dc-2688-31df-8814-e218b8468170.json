  "rdfs:comment": {
    "de": [
      "Bagel Story bietet Ihnen echte New Yorker Bagels zum Mitnehmen oder zum Verzehr vor Ort, direkt hinter dem Justizpalast."
    ],
    "en": [
      "Bagel Story serves genuine New York bagels to eat in or take away, just behind the law courts."
    ],
    "it": [
      "Bagel Story offre veri bagel newyorkesi da asporto o da mangiare a casa, proprio dietro il tribunale."
    ],
    "fr": [
      "Bagel Story vous propose de véritables bagels de New York à emporter ou à consommer sur place, juste derrière le palais de justice."
    ],
    "es": [
      "Bagel Story ofrece auténticos bagels neoyorquinos para llevar o comer en casa, justo detrás del juzgado."
    ],
