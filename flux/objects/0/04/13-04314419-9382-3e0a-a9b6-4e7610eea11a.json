  "rdfs:comment": {
    "de": [
      "Zahlreiche Schausteller (Produzenten, Metzger, Fischhändler, Konditoren, Kleidung, Haushaltswaren, Honig, Bäcker, Käser, ...) erwarten Sie zweimal pro Woche in Lentilly."
    ],
    "en": [
      "Numerous fairgrounds (producers, butchers, fishmonger, pastry cook, clothes, household articles, honey, baker, cheesemaker, ...) are waiting for you twice a week in Lentilly."
    ],
    "it": [
      "Numerose fiere (produttori, macellai, pescivendoli, pasticceri, abbigliamento, articoli per la casa, miele, panettieri, casari, ecc.) vi aspettano due volte alla settimana a Lentilly."
    ],
    "fr": [
      "Nombreux forains (producteurs, bouchers charcutiers, poissonnier, pâtissier, vêtements, articles de la maison, miel, boulanger, fromager, …) vous attendent deux fois par semaine à Lentilly."
    ],
    "nl": [
      "Talrijke beurzen (producenten, slagers, visverkopers, banketbakkers, kleding, huishoudelijke artikelen, honing, bakkers, kaasmakers, enz.) wachten u tweemaal per week op in Lentilly."
    ],
    "es": [
      "Numerosas ferias (productores, carniceros, pescaderos, pasteleros, ropa, artículos para el hogar, miel, panaderos, queseros, etc.) le esperan dos veces por semana en Lentilly."
    ]
  },
