  "rdfs:comment": {
    "de": [
      "Traditionelle Küche mit Arbeitermenü\nNeue Menüs, neue Speisekarte seit Frühjahr 2021.\nPizzas, Pasta und Bruschetta zum Mitnehmen am Freitag- und Samstagabend."
    ],
    "en": [
      "Traditional cuisine with worker's menu\nNew menus, new menu since spring 2021.\nPizzas, pastas and bruschettas to take away on Friday and Saturday nights."
    ],
    "it": [
      "Cucina tradizionale con menù per i lavoratori\nNuovi menu, nuovi menu dalla primavera 2021.\nPizze, paste e bruschette da asporto il venerdì e il sabato sera."
    ],
    "fr": [
      "Cuisine traditionnelle avec menu ouvrier. \nNouveaux menus, nouvelle carte depuis le printemps 2021.\nPizzas, pâtes et bruschettas à emporter les vendredis et samedis soirs."
    ],
    "nl": [
      "Traditionele keuken met een arbeidersmenu\nNieuwe menu's, nieuwe kaart sinds voorjaar 2021.\nPizza's, pasta's en bruschetta's om mee te nemen op vrijdag- en zaterdagavond."
    ],
    "es": [
      "Cocina tradicional con un menú para trabajadores\nNuevos menús, nueva carta desde la primavera de 2021.\nPizzas, pastas y bruschettas para llevar los viernes y sábados por la noche."
    ]
  },
