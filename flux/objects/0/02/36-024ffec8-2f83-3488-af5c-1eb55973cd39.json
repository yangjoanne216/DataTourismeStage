  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/7b34646b-f399-33e0-a9e0-1be85b540537",
      "dc:description": {
        "de": [
          "Bar - Restaurant mit frischen Produkten von lokalen Produzenten.",
          "Bar - Restaurant mit frischen Produkten von lokalen Produzenten.\nGerichte zum Mitnehmen, hausgemachte Küche\nAm Fuße der Pisten Résidence Le Sérias, kleiner Saal im Inneren und eine Sonnenterrasse mit Blumen und einer Entspannungsecke."
        ],
        "en": [
          "Bar - Restaurant with fresh products from local producers.",
          "Bar - Restaurant with fresh products from local producers.\nTakeaway meals, home-made food\nAt the foot of the slopes of the Résidence Le Sérias, a small room inside and a sunny terrace with flowers and a relaxation area."
        ],
        "it": [
          "Bar - Ristorante con prodotti freschi di produttori locali.",
          "Bar - Ristorante con prodotti freschi di produttori locali.\nPasti da asporto, cibo fatto in casa\nAi piedi delle piste del Résidence Le Sérias, una saletta interna e una terrazza soleggiata con fiori e zona relax."
        ],
        "fr": [
          "Bar - Restaurant avec produits  frais de producteurs locaux.",
          "Bar - Restaurant avec produits  frais de producteurs locaux.\nPlats à emporter,  cuisine fait maison. \nEn pied de pistes Résidence Le Sérias, petite salle à l'intérieur et une terrasse ensoleillée fleurie et aménagée d'un coin détente."
        ],
        "es": [
          "Bar - Restaurante con productos frescos de los productores locales.",
          "Bar - Restaurante con productos frescos de los productores locales.\nComida para llevar, comida casera\nAl pie de las pistas de la Résidence Le Sérias, una pequeña sala interior y una soleada terraza con flores y zona de relax."
        ],
        "nl": [
          "Bar - Restaurant met verse producten van lokale producenten.",
          "Bar - Restaurant met verse producten van lokale producenten.\nAfhaalmaaltijden, zelfgemaakt eten\nAan de voet van de pistes van de Résidence Le Sérias, een kleine kamer binnen en een zonnig terras met bloemen en een ontspanningsruimte."
        ]
      },
