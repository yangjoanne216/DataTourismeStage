  "rdfs:comment": {
    "de": [
      "Einfamilienhaus, in einer grünen Umgebung, 5 km von Monpazier entfernt. Erdgeschoss mit überdachter Terrasse. Großer Raum mit: Küchenzeile, Esszimmer mit Kamin, Badezimmer. Schwimmbad. Kaution von 300 €"
    ],
    "en": [
      "Detached house, situated in a verdant landscape, 5 km from Monpazier with swimming pool."
    ],
    "it": [
      "Casa indipendente, situata in un ambiente verde a 5 km da Monpazier. Piano terra con terrazza coperta. Ampio locale con angolo cottura, sala da pranzo con camino, bagno. Piscina. Deposito di 300 ?"
    ],
    "fr": [
      "Maison individuelle, située dans un cadre verdoyant à 5km de Monpazier. Rez-de-chaussée avec terrasse couverte. Grande pièce comprenant : coin cuisine, salle à manger avec cheminée, salle de bains. Piscine. Caution de 300 €"
    ],
    "nl": [
      "Vrijstaand huis, gelegen in een groene omgeving op 5km van Monpazier. Begane grond met overdekt terras. Grote kamer met kitchenette, eetkamer met open haard, badkamer. Zwembad. Borg van 300 €"
    ],
    "es": [
      "Casa independiente, situada en un entorno verde a 5 km de Monpazier. Planta baja con terraza cubierta. Amplia habitación con cocina americana, comedor con chimenea, baño. Piscina. Depósito de 300 ?"
    ]
  },
