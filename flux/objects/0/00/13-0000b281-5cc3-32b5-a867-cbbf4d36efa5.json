  "rdfs:comment": {
    "de": [
      "L'Atelier du Coin, Distributeur de Créativité!\r\nHier finden Sie Leuchten, Geschirr, Schmuck, Accessoires und Kleidung, Illustrationen, Lederwaren und ... handgefertigte und einzigartige Kreationen!"
    ],
    "en": [
      "L?Atelier du Coin, Distributor of Creativity !\r\nYou will find lighting, tableware, jewelry, accessories and clothing, illustrations, leather goods and ... unique handmade creations!"
    ],
    "it": [
      "L'Atelier du Coin, distributore di creatività!\r\nTroverete illuminazione, oggetti per la tavola, gioielli, accessori e abbigliamento, illustrazioni, pelletteria e... creazioni uniche fatte a mano!"
    ],
    "fr": [
      "L’Atelier du Coin, Distributeur de Créativité ! \r\nVous y trouverez des luminaires, de la vaisselle, des bijoux, des accessoires et vêtements, des illustrations, de la maroquinerie et ...des créations artisanales et uniques !"
    ],
    "nl": [
      "L'Atelier du Coin, verdeler van creativiteit !\r\nU vindt er verlichting, serviesgoed, juwelen, accessoires en kleding, illustraties, lederwaren en ... unieke handgemaakte creaties!"
    ],
    "es": [
      "L'Atelier du Coin, ¡distribuidor de creatividad!\r\nEncontrará iluminación, vajilla, joyería, accesorios y ropa, ilustraciones, marroquinería y... ¡creaciones únicas hechas a mano!"
    ]
  },
