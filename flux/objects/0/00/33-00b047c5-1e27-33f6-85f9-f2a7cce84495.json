  "rdfs:comment": {
    "de": [
      "Wie wäre es, wenn Sie Ihre Pizza zum Mitnehmen bestellen und dabei durch das kleine Dorf Cox fahren?"
    ],
    "en": [
      "How about ordering your pizza to go through the small village of Cox?"
    ],
    "it": [
      "Che ne dite di ordinare la pizza a domicilio attraverso il piccolo villaggio di Cox?"
    ],
    "fr": [
      "Et si vous commandiez votre pizza à emporter en passant par le petit village de Cox ?"
    ],
    "es": [
      "¿Qué le parece pedir su pizza para llevar a través del pequeño pueblo de Cox?"
    ],
    "nl": [
      "Wat dacht je ervan om je pizza te bestellen via het kleine dorpje Cox?"
    ]
  },
