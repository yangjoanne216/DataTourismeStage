  "rdfs:comment": {
    "de": [
      "Macarons mit Münsterkäse, hausgemachtes Gebäck, Bretzel, Bredala und hausgemachtes Eis. Frühstücksangebot von 8,30? bis 9,80? Bretzel-Workshop auf Reservierung für mindestens 10 Personen: 50?\nBehindertengerecht, außer WC."
    ],
    "en": [
      "Macaroons with munster cheese, home made pastry, pretzel, Bredala and home made ice cream. Breakfast formula from 8,30? to 9,80? Pretzel workshop on reservation for a minimum of 10 persons : 50?\nAccessible to the handicapped, except WC."
    ],
    "it": [
      "Macaron con formaggio Munster, dolci fatti in casa, pretzel, Bredala e gelato artigianale. Formula colazione dalle 8,30 alle 9,80? Laboratorio di pretzel su prenotazione per un minimo di 10 persone: prezzo fisso di 50?\nAccessibile ai disabili, tranne il WC."
    ],
    "fr": [
      "Macarons au munster, pâtisserie maison, Bretzel, Bredala et glace artisanale. Formule petit dej. de 8,30€ à 9,80€. Atelier Bretzel sur réservation pour un minimum de 10 personnes : forfait de 50€.\nAccessible aux handicapés, sauf WC."
    ],
    "es": [
      "Macarrones con queso Munster, bollería casera, pretzels, Bredala y helado casero. Fórmula desayuno de 8,30? a 9,80? Taller de pretzels previa reserva para un mínimo de 10 personas: precio fijo de 50?\nAccesible a minusválidos, excepto el WC."
    ],
    "nl": [
      "Macarons met Munster kaas, huisgemaakt gebak, pretzels, Bredala en huisgemaakt ijs. Ontbijtformule van 8,30? tot 9,80? Pretzelworkshop op reservering voor minimaal 10 personen: vaste prijs van 50?\nToegankelijk voor gehandicapten, behalve de WC."
    ]
  },
