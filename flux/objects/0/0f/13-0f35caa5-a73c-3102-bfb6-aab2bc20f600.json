  "rdfs:comment": {
    "de": [
      "Ruhige und helle Familienwohnung (6-8 Personen) von 110 m² mit sehr großer Terrasse (140 m²) ohne Gegenüber, 4 Schlafzimmer, 1 Wohnzimmer, 1 Küche, 1 Mezzanine/Spielzimmer. Sehr gut ausgestattet. 1 Min. zu Fuß vom Strand Miramar und dem Hafen entfernt."
    ],
    "en": [
      "Family flat (6-8 people) quiet and bright 110 m² with very large terrace (140 m²) without vis-à-vis, 4 bedrooms, 1 living room, 1 kitchen, 1 mezzanine / playroom. Very well equipped. At 1 min walk from the Miramar beach and the port."
    ],
    "it": [
      "Appartamento familiare (6-8 persone) tranquillo e luminoso di 110 m² con terrazza molto grande (140 m²) senza vis-à-vis, 4 camere da letto, 1 soggiorno, 1 cucina, 1 mezzanino/sala giochi. Molto ben attrezzato. A 1 minuto a piedi dalla spiaggia di Miramar e dal porto."
    ],
    "fr": [
      "Appartement familial (6-8 personnes) calme et lumineux de 110 m² avec très grande terrasse (140 m²) sans vis-à-vis, 4 chambres, 1 salon, 1 cuisine, 1 mezzanine/salle de jeux. Très bien équipé. À 1 min à pied de la plage Miramar et du port."
    ],
    "es": [
      "Piso familiar (6-8 personas) tranquilo y luminoso 110 m² con terraza muy grande (140 m²) sin vis-à-vis, 4 habitaciones, 1 salón, 1 cocina, 1 entresuelo / sala de juegos. Muy bien equipado. A 1 min andando de la playa de Miramar y del puerto."
    ],
    "nl": [
      "Familie appartement (6-8 personen) rustig en helder 110 m² met zeer groot terras (140 m²) zonder vis-à-vis, 4 slaapkamers, 1 woonkamer, 1 keuken, 1 mezzanine / speelkamer. Zeer goed uitgerust. Op 1 minuut lopen van het Miramar strand en de haven."
    ]
  },
