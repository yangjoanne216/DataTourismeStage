  "rdfs:comment": {
    "de": [
      "Im Herzen von Vesoul können Sie eine raffinierte regionale Küche genießen, die je nach Jahreszeit variiert."
    ],
    "en": [
      "In the heart of Vesoul, you will be able to taste a refined local cuisine which varies according to the seasons."
    ],
    "it": [
      "Nel cuore di Vesoul, potrete gustare una raffinata cucina locale che cambia con le stagioni."
    ],
    "fr": [
      "En plein cœur de Vesoul, vous pourrez déguster une cuisine de terroir raffinée qui varie au gré des saisons."
    ],
    "nl": [
      "In het hart van Vesoul kunt u genieten van een verfijnde lokale keuken die met de seizoenen mee verandert."
    ],
    "es": [
      "En el corazón de Vesoul, podrá disfrutar de una refinada cocina local que cambia con las estaciones."
    ]
  },
