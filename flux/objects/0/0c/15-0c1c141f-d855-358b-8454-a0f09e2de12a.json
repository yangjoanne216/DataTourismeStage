      "dc:description": {
        "de": [
          "Innerhalb der Domäne bietet der Saal Henri VI einen außergewöhnlichen Ort, um Ihre Seminare, Arbeitstreffen und prestigeträchtigen Abendessen zu organisieren."
        ],
        "en": [
          "Within the domain, the Henri VI room is an exceptional site to organize your seminars, work meetings and prestigious dinners."
        ],
        "it": [
          "All'interno della tenuta, la sala Henri VI è un luogo eccezionale per organizzare seminari, riunioni di lavoro e cene di prestigio."
        ],
        "fr": [
          "Au sein du domaine, la salle Henri VI constitue un site d'exception pour organiser vos séminaires, réunions de travail et vos dîners prestige."
        ],
        "es": [
          "La sala Henri VI es un lugar excepcional para seminarios, reuniones de negocios y cenas de prestigio."
        ],
        "nl": [
          "De Henri VI-zaal is een uitzonderlijke locatie voor seminars, zakelijke bijeenkomsten en prestigieuze diners."
        ]
      },
