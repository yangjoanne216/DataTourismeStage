  "rdfs:comment": {
    "de": [
      "Offshore-Badeausflug am Nachmittag oder Abend bei Sonnenuntergang: offenes Meer, Ile de Brescou, Strand Grande Conque (Bereitstellung von Neoprenanzug, Flossen, Maske und Schnorchel)."
    ],
    "en": [
      "Offshore swimming trip, in the afternoon or evening at sunset: open sea, Brescou Island, Grande Conque beach (wetsuit, fins, mask and snorkel provided)."
    ],
    "it": [
      "Gita in mare aperto, nel pomeriggio o la sera al tramonto: mare aperto, Ile de Brescou, spiaggia di Grande Conque (muta, pinne, maschera e boccaglio forniti)."
    ],
    "fr": [
      "Sortie baignade en off-shore, l'après-midi ou le soir au couchant : pleine mer, Ile de Brescou, plage de la Grande Conque (fourniture de combinaison, palmes, masque et tuba)."
    ],
    "nl": [
      "Zwemtocht op zee, 's middags of 's avonds bij zonsondergang: open zee, Ile de Brescou, strand van Grande Conque (wetsuit, vinnen, masker en snorkel aanwezig)."
    ],
    "es": [
      "Excursión a nado en alta mar, por la tarde o al atardecer: mar abierto, Ile de Brescou, playa de Grande Conque (se proporciona traje de neopreno, aletas, máscara y tubo)."
    ]
  },
