  "rdfs:comment": {
    "de": [
      "Die Boutique Bambino befindet sich 3 km vom Stadtzentrum von Aix-en-Provence entfernt. Das Verkaufslager bietet ausgewählte Markenartikel für Kinder von der Geburt bis 16 Jahre."
    ],
    "en": [
      "The Bambino shop is located 3 kilometres away from Aix-en-Provence town centre. This is a second-hand store selling brand articles selected for future mums and children aged 0 to 10."
    ],
    "it": [
      "Il negozio Bambino si trova a 3 chilometri dal centro di Aix-en-Provence. Il negozio offre articoli di marca selezionati per bambini dalla nascita ai 16 anni."
    ],
    "fr": [
      "La boutique Bambino est située à 3 kilomètres du centre-ville d'Aix-en-Provence. Le dépôt vente propose des articles de marques sélectionnés pour enfants de la naissance à 16 ans."
    ],
    "es": [
      "La tienda Bambino se encuentra a 3 kilómetros del centro de Aix-en-Provence. La tienda ofrece artículos de marca seleccionados para niños desde el nacimiento hasta los 16 años."
    ],
    "nl": [
      "De Bambino winkel ligt op 3 kilometer van het centrum van Aix-en-Provence. De winkel biedt geselecteerde merkartikelen voor kinderen van 0 tot 16 jaar."
    ]
  },
