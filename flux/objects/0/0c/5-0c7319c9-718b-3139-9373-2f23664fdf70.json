      "dc:description": {
        "de": [
          "Schnellrestaurants zum Mitnehmen.\nSnack: Sandwich, Panini, Hamburger, Amerikanisch, Salat, Pommes frites ...\nCrêpe, Eis, Kaffee, Tee, Getränke ...\nDurchgehend / Telefonische Bestellung."
        ],
        "en": [
          "Fast food to go.\nSnack : Sandwich, Panini, Hamburger, American, Salad, Fries ...\nPancake, Ice cream, Coffee, Tea, Drink ...\nContinuous / Order by phone."
        ],
        "it": [
          "Fast food da asporto.\nSnack : Panino, Panini, Hamburger, Americano, Insalata, Patatine fritte ...\nPancake, Gelato, Caffè, Tè, Bevanda ...\nContinua / Ordina per telefono."
        ],
        "fr": [
          "Restauration Rapide à emporter. \nSnack : Sandwich, Panini, Hamburger, Americain, Salade, Frite ...\nCrêpe, Glace, Café,  Thé,  Boisson ...\nEn continu / Commande par téléphone."
        ],
        "nl": [
          "Fast food to go.\nSnack : Sandwich, Panini, Hamburger, Amerikaanse, Salade, Frites ...\nPannenkoeken, IJs, Koffie, Thee, Drank ...\nDoorlopend / telefonisch bestellen."
        ],
        "es": [
          "Comida rápida para llevar.\nSnack : Sandwich, Panini, Hamburguesa, Americano, Ensalada, Patatas fritas ...\nTortitas, Helados, Café, Té, Bebida ...\nContinuo / Pedido por teléfono."
        ]
      },
