  "rdfs:comment": {
    "de": [
      "Brigitte und Olivier lieben die Geselligkeit und empfangen Sie das ganze Jahr über in ihren Gästezimmern, nur 150 Meter vom See Saint-Ferréol entfernt!"
    ],
    "en": [
      "Lovers of conviviality, Brigitte and Olivier welcome you all year long in their guest rooms, 150 meters from the Lake of Saint-Ferréol!"
    ],
    "it": [
      "Amanti della convivialità, Brigitte e Olivier vi accolgono tutto l'anno nelle loro camere, a 150 metri dal lago di Saint-Ferréol!"
    ],
    "fr": [
      "Amoureux de convivialité, Brigitte et Olivier vous accueillent toute l'année dans leurs chambres d'hôtes, à 150 mètres du Lac de Saint-Ferréol !"
    ],
    "nl": [
      "Liefhebbers van gezelligheid, Brigitte en Olivier verwelkomen u het hele jaar door in hun gastenkamers, op 150 meter van het meer van Saint-Ferréol!"
    ],
    "es": [
      "Amantes de la convivencia, Brigitte y Olivier le dan la bienvenida todo el año en sus habitaciones de huéspedes, a 150 metros del lago de Saint-Ferréol"
    ]
  },
