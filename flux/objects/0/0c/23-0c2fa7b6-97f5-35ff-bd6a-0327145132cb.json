  "rdfs:comment": {
    "de": [
      "Kostenloser Parkplatz am Ortseingang von Laruns vor dem Schwimmbad Ayguebère. Er bietet Zugang zum Campingplatz Ayguebère, aber auch zum Animationsraum \"Espace 2015\"."
    ],
    "en": [
      "Free parking located at the entrance of the village of Laruns in front of the Ayguebère swimming pool, giving access to the Ayguebère campsite but also to the animation room \"Espace 2015\"."
    ],
    "it": [
      "Parcheggio gratuito situato all'ingresso del villaggio di Laruns, di fronte alla piscina di Ayguebère, con accesso al campeggio di Ayguebère e alla sala di intrattenimento \"Espace 2015\"."
    ],
    "fr": [
      "Parking gratuit situé à l'entrée du village de Laruns devant la piscine Ayguebère, donnant accès au camping Ayguebère mais aussi à la salle d'animation \"Espace 2015\"."
    ],
    "nl": [
      "Gratis parkeerplaats bij de ingang van het dorp Laruns voor het zwembad van Ayguebère, met toegang tot de camping Ayguebère en de animatieruimte \"Espace 2015\"."
    ],
    "es": [
      "Aparcamiento gratuito situado a la entrada del pueblo de Laruns, frente a la piscina de Ayguebère, que da acceso al camping Ayguebère y a la sala de animación \"Espace 2015\"."
    ]
  },
