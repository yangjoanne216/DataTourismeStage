  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/39bee8bc-54d1-3cd6-b700-9948b9feac26",
      "dc:description": {
        "de": [
          "Küche aus frischen und saisonalen Produkten, die direkt von lokalen Produzenten stammen.",
          "Montags bis samstags von 12.00 bis 15.00 Uhr geöffnet.\nFreitag- und Samstagabend von 19:30 bis 23:00 Uhr.",
          "Traditionelle und bodenständige Küche."
        ],
        "en": [
          "Cuisine made from fresh and seasonal products, directly from local producers.",
          "Open Monday to Saturday from 12:00 to 15:00.\nFriday and Saturday evenings from 7:30 pm to 11 pm.",
          "Traditional and local cuisine."
        ],
        "it": [
          "Aperto dal lunedì al sabato dalle 12:00 alle 15:00.\nVenerdì e sabato sera dalle 19.30 alle 23.00.",
          "Cucina preparata con prodotti freschi e di stagione, direttamente dai produttori locali.",
          "Cucina tradizionale e locale."
        ],
        "fr": [
          "Cuisine élaborée à partir de produits frais et de saison, provenant  directement de producteurs locaux.",
          "Cuisine traditionnelle et de terroir.",
          "Ouverture du lundi au samedi de 12h00 à 15h00.\nLe vendredi et samedi soir de 19h30 à 23h00."
        ],
        "nl": [
          "Keuken bereid met verse, seizoensgebonden producten, rechtstreeks van lokale producenten.",
          "Open van maandag tot zaterdag van 12:00 tot 15:00 uur.\nVrijdag- en zaterdagavond van 19.30 tot 23.00 uur.",
          "Traditionele en lokale gerechten."
        ],
        "es": [
          "Abierto de lunes a sábado de 12:00 a 15:00.\nViernes y sábado por la noche, de 19.30 a 23.00 horas.",
          "Cocina elaborada con productos frescos y de temporada, directamente de los productores locales.",
          "Cocina tradicional y local."
        ]
      },
