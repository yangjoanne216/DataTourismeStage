  "rdfs:comment": {
    "de": [
      "Musikalisches Schauspiel BARBARINE, eine Reise mit Kinderreimen, Bourrées, Melodien, die am Kopf kleben, und Stepptanz...nach Barbarine: ein großes Gastland, ein Land der Freiheit, der Liebe und des Lebens."
    ],
    "en": [
      "Musical show BARBARINE, a journey of nursery rhymes, bourrées, heady tunes and tap dancing...to Barbarine: a great land of welcome, freedom, love and life."
    ],
    "it": [
      "Spettacolo musicale BARBARINE, un viaggio fatto di filastrocche, bourrées, melodie che si fissano in testa e tip tap... verso Barbarine: una grande terra di accoglienza, una terra di libertà, amore e vita."
    ],
    "fr": [
      "Spectacle musical BARBARINE, un voyage fait de comptines, de bourrées,d'airs qui collent à la tête et aux claquettes...vers Barbarine : un grand pays d'accueil, un pays de liberté, d'amour et de vie."
    ],
    "nl": [
      "Musicalvoorstelling BARBARINE, een reis gemaakt van kinderrijmpjes, bourrées, deuntjes die in je hoofd blijven hangen en tapdansen... naar Barbarine: een groot land van welkom, een land van vrijheid, liefde en leven."
    ],
    "es": [
      "Espectáculo musical BARBARINE, un viaje hecho de canciones infantiles, bourrées, melodías que se te quedan grabadas en la cabeza y claqué... a Barbarine: una gran tierra de acogida, una tierra de libertad, amor y vida."
    ]
  },
  "rdfs:label": {
    "fr": [
      "SPECTACLE MUSICAL DUO DE CHANTEUSES...AU FIL DE L'EAU"
    ]
  },
