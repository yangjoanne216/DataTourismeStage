      "dc:description": {
        "de": [
          "Erdgeschoss+2. Etage, 80m2, 2 Schlafzimmer: 2 Betten 140cm, 1 Bett 90cm, Kochecke, Wohn-Esszimmer, Kamin, Terrasse, Balkon, Grill, Park. portable/demand.",
          "Restauriertes Bauernhaus in einem 2ha großen Park, Vermietung an das Haus des Eigentümers, 800m vom Dorf entfernt."
        ],
        "en": [
          "Ground floor+2nd floor, 80m2, 2 bedrooms: 2 beds 140cm, 1 bed 90cm, kitchenette, Living room, fireplace, Terrace, Balcony, Barbecue, Park. portable/application.",
          "Restored farmhouse in park 2ha, semi-detached rental owner's house, 800m village."
        ],
        "it": [
          "Casa colonica ristrutturata in 2 ettari di parco, adiacente alla casa del proprietario, a 800 m dal paese.",
          "Piano terra+2 piani, 80m2, 2 camere da letto: 2 letti 140cm, 1 letto 90cm, angolo cottura, Soggiorno, camino, Terrazza, Balcone, Barbecue, Parco."
        ],
        "fr": [
          "Corps de ferme restauré dans parc 2ha, location mitoyenne maison du propriétaire, 800m village.",
          "RDC+2étage, 80m2, 2 chambres : 2 lits 140cm, 1 lit 90cm, coin cuisine, Séjour-salon, cheminée, Terrasse, Balcon, Barbecue, Park. portable/demande."
        ],
        "es": [
          "Masía restaurada en 2ha de parque, junto a la casa del propietario, a 800m del pueblo.",
          "Planta baja+2planta, 80m2, 2 dormitorios: 2 camas 140cm, 1 cama 90cm, cocina americana, Salón, chimenea, Terraza, Balcón, Barbacoa, Parque."
        ],
        "nl": [
          "Begane grond+2 verdieping, 80m2, 2 slaapkamers: 2 bedden 140cm, 1 bed 90cm, kitchenette, Woonkamer, open haard, Terras, Balkon, Barbecue, Park.",
          "Gerestaureerde boerderij in een park van 2ha, grenzend aan het huis van de eigenaar, op 800m van het dorp."
        ]
      },
