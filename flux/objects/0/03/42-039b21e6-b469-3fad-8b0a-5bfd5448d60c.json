  "rdfs:comment": {
    "de": [
      "24 Personen - 9 ZimmerNur wenige Meter vom Schloss La Noue-le-Coq entfernt, mit Zugang zum Freibad des Schlosses.ausgestattete Küche, großes Esszimmer, doppeltes Wohnzimmer, Zimmer für 2 bis 4 Personen, 4 Duschen.separater Parkplatz."
    ],
    "en": [
      "24 persons - 9 bedroomsA few meters from the Château de la Noue-le-Coq, possibility to access the open air swimming pool of the castle.fitted kitchen, large dining room, double living room, bedrooms from 2 to 4 persons, 4 showers.independent parking."
    ],
    "it": [
      "24 persone - 9 camereA pochi metri dallo Château de la Noue-le-Coq, possibilità di accesso alla piscina all'aperto dello Château.Cucina attrezzata, ampia sala da pranzo, doppio soggiorno, camere da 2 a 4 persone, 4 docce.Parcheggio indipendente."
    ],
    "fr": [
      "24 personnes - 9 chambresA quelques mètres du Château de la Noue-le-Coq, possibilité d'accéder à la piscine en plein air du château.Cuisine aménagée, grande salle à manger, double salon, chambres allant de 2 à 4 personnes, 4 douches.Parking indépendant."
    ],
    "nl": [
      "24 personen - 9 slaapkamersEen paar meter van het Château de la Noue-le-Coq, mogelijkheid van toegang tot het openluchtzwembad van het Château.Ingerichte keuken, grote eetkamer, dubbele woonkamer, slaapkamers voor 2 tot 4 personen, 4 douches.Onafhankelijke parkeerplaats."
    ],
    "es": [
      "24 personas - 9 habitacionesA pocos metros del Château de la Noue-le-Coq, posibilidad de acceso a la piscina al aire libre del château.Cocina equipada, gran comedor, salón doble, habitaciones de 2 a 4 personas, 4 duchas.Aparcamiento independiente."
    ]
  },
