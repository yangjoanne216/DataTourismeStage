  "rdfs:comment": {
    "de": [
      "Die Freizeitanlage Familly Park öffnet wieder ihre Pforten mit Accrobranche, Paintball und Angeln!"
    ],
    "en": [
      "An official site and the largest in the Loiret. Come and discover our different play areas on a magnificent site of 11 hectares under the watchful eye of our monitors: fun and adrenalin surges guaranteed."
    ],
    "it": [
      "Il parco divertimenti Familly Park riapre i battenti con Accrobranche, paintball e pesca!"
    ],
    "fr": [
      "La base de loisirs Familly Park rouvre ses portes avec Accrobranche, paint-ball et pêche !"
    ],
    "nl": [
      "Officieel en grootste terrein van de Loiret. Kom kennismaken met onze verschillende spelzones op een prachtig terrein van 11 hectare, onder begeleiding van onze instructeurs, waar plezier en adrenaline hand in hand gaan"
    ],
    "es": [
      "Arborismo en una zona de más de 4 hectáreas."
    ]
