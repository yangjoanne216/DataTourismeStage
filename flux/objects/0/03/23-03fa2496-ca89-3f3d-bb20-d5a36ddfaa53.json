  "rdfs:comment": {
    "de": [
      "Hotel - Restaurant - Bar - Catering\nTraditionelle Küche (Menüs + Karte) - Arbeitermahlzeit am Mittag, 2 Säle, Terrasse.\nGanzjährig geöffnet, abends und außerhalb der Saison am Wochenende auf Reservierung, sonntags geschlossen."
    ],
    "en": [
      "Hotel - restaurant - bar - caterer\nTraditional cuisine (Menus + a la carte) - working class meals at lunchtime, 2 rooms, terrace.\nOpen all year round, evenings and weekends out of season on reservation, closed on Sundays."
    ],
    "it": [
      "Hotel - ristorante - bar - catering\nCucina tradizionale (menu + à la carte) - pasti di lavoro a pranzo, 2 sale, terrazza.\nAperto tutto l'anno, sera e fine settimana fuori stagione su prenotazione, chiuso la domenica."
    ],
    "fr": [
      "Hôtel - restaurant - bar - traiteur\nCuisine traditionnelle (Menus + carte) - repas ouvrier le midi, 2 salles, terrasse.\nOuvert toute l’année, le soir et hors saison le week-end sur réservation, fermé le dimanche."
    ],
    "es": [
      "Hotel - restaurante - bar - catering\nCocina tradicional (menús + a la carta) - comidas de trabajo al mediodía, 2 salas, terraza.\nAbierto todo el año, tardes y fines de semana fuera de temporada con reserva, cerrado los domingos."
    ],
    "nl": [
      "Hotel - restaurant - bar - traiteur\nTraditionele keuken (menu's + a la carte) - arbeidersmaaltijden tussen de middag, 2 zalen, terras.\nHet hele jaar geopend, 's avonds en in het weekend buiten het seizoen op reservering, op zondag gesloten."
    ]
  },
