  "rdfs:comment": {
    "de": [
      "L'Effet des mains ist ein 2014 gegründeter Vereinsladen für lokales Kunsthandwerk, in dem rund 20 lokale Kunsthandwerker zusammenarbeiten, die sich solidarisch abwechseln, um Sie zu empfangen."
    ],
    "en": [
      "This associative shop which sells local crafts opened in 2014. L'Effet des Mains brings together about twenty local craftspeople, who take turns in solidarity to welcome you."
    ],
    "it": [
      "L'Effet des mains, un negozio di artigianato locale creato nel 2014, riunisce una ventina di artigiani locali che si alternano per accogliervi."
    ],
    "fr": [
      "Boutique associative d'artisanat local  créée depuis 2014, L'Effet des mains regroupe une vingtaine d'artisans locaux, qui se relayent solidairement pour vous accueillir."
    ],
    "es": [
      "Tienda asociativa de artesanía local creada en 2014, L'Effet des mains reúne a una veintena de artesanos locales, que se turnan para darte la bienvenida."
    ],
    "nl": [
      "L'Effet des mains, een in 2014 opgerichte plaatselijke ambachtswinkel, verenigt een twintigtal plaatselijke ambachtslieden die u om beurten ontvangen."
    ]
  },
