  "rdfs:comment": {
    "de": [
      "Ein der virtuellen Realität gewidmetes Freizeitzentrum in der Drôme Ardèche: Raum mit 7 Virtual-Reality-Stationen, an denen Sie alleine oder im Netzwerk spielen können, wobei Ihre Spielzeit minutengenau abgerechnet wird."
    ],
    "en": [
      "Leisure centre dedicated to virtual reality in Drôme Ardèche: space of 7 virtual reality stations allowing you to play alone or in network with a countdown of your game time to the minute."
    ],
    "it": [
      "Centro ricreativo dedicato alla realtà virtuale nella Drôme Ardèche: 7 postazioni di realtà virtuale che permettono di giocare da soli o in rete con un conto alla rovescia del tempo di gioco al minuto."
    ],
    "fr": [
      "Centre de loisir dédié à la réalité virtuelle en Drôme Ardèche : espace de 7 stations de réalité virtuelle vous permettant de jouer seul ou en réseau avec un décompte de votre temps de jeu à la minute."
    ],
    "nl": [
      "Vrijetijdscentrum gewijd aan virtual reality in de Drôme Ardèche: 7 virtual reality stations waar u alleen of in een netwerk kunt spelen met een countdown van uw speeltijd per minuut."
    ],
    "es": [
      "Centro de ocio dedicado a la realidad virtual en Drôme Ardèche: 7 estaciones de realidad virtual que le permiten jugar solo o en red con una cuenta atrás de su tiempo de juego por minutos."
    ]
  },
