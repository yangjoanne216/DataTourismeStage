  "rdfs:comment": {
    "de": [
      "Gästezimmer, Gästetisch an den Toren des Comminges auf halbem Weg zwischen Toulouse und den Pyrenäen.\nMotorradfahrer-Etappen mit gesicherter Garage und spezieller Ausrüstung."
    ],
    "en": [
      "Bed and breakfast, table d'hôtes at the gates of Le Comminges halfway between Toulouse and the Pyrenees.\nBiker stages with secure garage and specific equipment."
    ],
    "it": [
      "Bed and breakfast, table d'hôtes alle porte delle Comminges, a metà strada tra Tolosa e i Pirenei.\nSosta per motociclisti con garage custodito e attrezzature specifiche."
    ],
    "fr": [
      "Chambres d'hôtes, table d'hôtes aux portes du Comminges à mi-chemin entre Toulouse et les Pyrénées.\nEtapes motards avec garage sécurisé et matériel spécifique."
    ],
    "nl": [
      "Bed and breakfast, table d'hôtes aan de poort van de Comminges, halverwege tussen Toulouse en de Pyreneeën.\nStopplaatsen voor motorrijders met beveiligde garage en specifieke uitrusting."
    ],
    "es": [
      "Bed and breakfast, table d'hôtes a las puertas del Comminges, a medio camino entre Toulouse y los Pirineos.\nParada de motoristas con garaje seguro y equipamiento específico."
    ]
