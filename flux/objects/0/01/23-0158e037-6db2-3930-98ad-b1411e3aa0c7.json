  "COVID19SpecialMeasures": {
    "de": [
      "Physische Distanzierung.\nBereitstellung von hydroalkoholischem Gel.\nBewegungsrichtung innerhalb des Geländes\nMaskentragepflicht für Besucher."
    ],
    "en": [
      "Physical distancing.\nProvision of hydro-alcoholic gel.\nDirection of circulation inside the site\nMandatory wearing of masks for visitors."
    ],
    "it": [
      "Allontanamento fisico.\nFornitura di gel idroalcolico.\nDirezione di movimento all'interno del sito\nI visitatori devono indossare maschere."
    ],
    "fr": [
      "Distanciation physique.\nMise à disposition de gel hydro-alcoolique.\nSens de circulation à l'intérieur du site. \nPort du masque obligatoire pour les visiteurs."
    ],
    "nl": [
      "Fysieke afstand.\nVerstrekking van hydro-alcoholische gel.\nBewegingsrichting binnen het gebied\nBezoekers moeten maskers dragen."
    ],
    "es": [
      "Distanciamiento físico.\nSuministro de gel hidroalcohólico.\nSentido de la circulación dentro del recinto\nLos visitantes deben llevar máscaras."
    ]
  },
