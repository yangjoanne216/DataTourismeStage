  "rdfs:comment": {
    "de": [
      "Eine leidenschaftliche und mystische Malerei, in der die Farben sprießen und die Linien sich verdrehen."
    ],
    "en": [
      "A passionate and mystical painting where colours spurt out and lines twist."
    ],
    "it": [
      "Un dipinto appassionato e mistico in cui i colori esplodono e le linee si contorcono."
    ],
    "fr": [
      "Une peinture passionnée et mystique où les couleurs jaillissent et les lignes se tordent."
    ],
    "es": [
      "Una pintura apasionada y mística en la que los colores estallan y las líneas se retuercen."
    ],
    "nl": [
      "Een gepassioneerd en mystiek schilderij waarin de kleuren uitbarsten en de lijnen verwrongen zijn."
    ]
  },
