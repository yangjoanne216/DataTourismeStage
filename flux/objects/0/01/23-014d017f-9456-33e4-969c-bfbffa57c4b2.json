  "rdfs:comment": {
    "de": [
      "Begonia bietet einen atemberaubenden Blick auf das Dorf und das Schloss. Das Ferienhaus bietet eine geräumige Unterkunft auf zwei Etagen für bis zu sechs Personen."
    ],
    "en": [
      "Begonia offers a breathtaking view of the village and the castle. The gîte offers spacious accommodation on two floors that can accommodate up to six people."
    ],
    "it": [
      "Begonia offre una vista mozzafiato sul paese e sul castello. Il gîte offre sistemazioni spaziose su due piani per un massimo di sei persone."
    ],
    "fr": [
      "Bégonia offre une vue imprenable sur le village et le château. Le gîte propose un hébergement spacieux sur deux étages pouvant accueillir jusqu'à six personnes."
    ],
    "es": [
      "Begonia ofrece una vista impresionante del pueblo y del castillo. La casa rural ofrece un amplio alojamiento en dos plantas para un máximo de seis personas."
    ],
    "nl": [
      "Begonia biedt een adembenemend uitzicht op het dorp en het kasteel. De gîte biedt ruime accommodatie op twee verdiepingen voor maximaal zes personen."
    ]
  },
