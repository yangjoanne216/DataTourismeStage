  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/18d841cd-bda5-3202-9dc4-c5f918624583",
      "dc:description": {
        "de": [
          "4 Schlafzimmer, Wohnzimmer, Küche, Badezimmer mit Badewanne und Dusche, Liegestühle.",
          "Unabhängiges altes Haus auf 2 Ebenen mit Stilmöbeln, Garten, Hof, Terrasse."
        ],
        "en": [
          "4 bedrooms, living room, kitchen, bathroom with bath and shower, deckchairs.",
          "Independent old house on 2 levels with period furniture, garden, courtyard, terrace."
        ],
        "it": [
          "4 camere da letto, soggiorno, cucina, bagno con vasca e doccia, lettini.",
          "Antica casa indipendente su 2 livelli con mobili d'epoca, giardino, cortile, terrazza."
        ],
        "fr": [
          "4 chambres, séjour, cuisine, salle de bains avec baignoire et douche, chaises longues.",
          "Maison ancienne indépendante sur 2 niveaux avec meubles d'époque, jardin, cour, terrasse."
        ],
        "nl": [
          "4 slaapkamers, woonkamer, keuken, badkamer met bad en douche, ligstoelen.",
          "Onafhankelijk oud huis op 2 niveaus met antieke meubels, tuin, binnenplaats, terras."
        ],
        "es": [
          "4 dormitorios, salón, cocina, baño con bañera y ducha, tumbonas.",
          "Casa antigua independiente en 2 niveles con muebles de época, jardín, patio, terraza."
        ]
      },
