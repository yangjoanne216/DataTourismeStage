      "dc:description": {
        "de": [
          "Zahlreiche Aktivitäten, die Sie mit anderen teilen können: Canyoning , Klettern , 3000er-Besteigungen , Bergsteigen, Skitouren , Splitboard... Willkommen in den Bergen das ganze Jahr über!"
        ],
        "en": [
          "Many activities to share : canyoning, climbing, 3000 climbs, mountaineering, ski touring, splitboard... Welcome to the mountains all year round!"
        ],
        "it": [
          "Molte attività da condividere: canyoning, arrampicata, 3000 scalate, alpinismo, sci alpinismo, splitboard... Benvenuti in montagna tutto l'anno!"
        ],
        "fr": [
          "De nombreuses activités à partager : canyoning , escalade , ascensions 3000 , alpinisme, ski de randonnée , splitboard... Bienvenue en montagne toute l'année !"
        ],
        "nl": [
          "Veel activiteiten om te delen: canyoning, klimmen, 3000 beklimmingen, bergbeklimmen, skitochten, splitboard... Welkom in de bergen het hele jaar door!"
        ],
        "es": [
          "Muchas actividades para compartir: barranquismo, escalada, 3000 escaladas, alpinismo, esquí de travesía, splitboard... ¡Bienvenido a las montañas durante todo el año!"
        ]
      },
