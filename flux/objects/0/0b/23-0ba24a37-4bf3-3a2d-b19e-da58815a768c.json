  "rdfs:comment": {
    "de": [
      "Ehemaliges Kloster des Ordens von Fontevrault für Nonnen adliger Herkunft, umgebaut zu 3 nebeneinander liegenden Unterkünften, am Ufer des Lot."
    ],
    "en": [
      "Former convent of the order of Fontevrault intended for nuns of noble origin, refurbished into 3 adjoining gîtes, on the banks of the Lot."
    ],
    "it": [
      "Ex convento dell'ordine di Fontevrault per suore di origine nobile, trasformato in 3 gîtes adiacenti, sulle rive del Lot."
    ],
    "fr": [
      "Ancien couvent de l'ordre de Fontevrault destiné aux religieuses d'origine noble, réaménagé en 3 gîtes mitoyens, en bordure du Lot."
    ],
    "nl": [
      "Voormalig klooster van de orde van Fontevrault voor nonnen van adellijke afkomst, verbouwd tot 3 aangrenzende gîtes, aan de oever van de Lot."
    ],
    "es": [
      "Antiguo convento de la orden de Fontevrault para monjas de origen noble, convertido en 3 casas rurales contiguas, a orillas del Lot."
    ]
