      "dc:description": {
        "de": [
          "52 m² großer Saal für 50 Personen, professionelle Einbauküche, 16 Schlafplätze (4 Zimmer), Geschirr vorhanden, gesichertes Gelände. 450 € pro Wochenende für die gesamte Unterkunft."
        ],
        "en": [
          "Room of 52 m² that can accommodate 50 people, professional fitted kitchen, sleeps 16 (4 bedrooms), crockery provided, secure grounds. 450 € the weekend for the whole gîte."
        ],
        "it": [
          "Sala di 52 m² per 50 persone, cucina professionale, 16 posti letto (4 camere), stoviglie fornite, terreno protetto. 450 ? per week-end per l'intera struttura."
        ],
        "fr": [
          "Salle de 52 m² pouvant accueillir 50 personnes, cuisine aménagée professionnelle, 16 couchages (4 chambres), vaisselle fournie, terrain sécurisé. 450 € le week-end pour la totalité du gîte."
        ],
        "es": [
          "Sala de 52 m² para 50 personas, cocina profesional, 16 camas (4 habitaciones), vajilla incluida, terreno seguro. 450€ por fin de semana para toda la casa rural."
        ],
        "nl": [
          "52 m² zaal voor 50 personen, professionele keuken, 16 bedden (4 slaapkamers), vaatwerk voorzien, beveiligd terrein. 450 € per weekend voor de hele gîte."
        ]
      },
