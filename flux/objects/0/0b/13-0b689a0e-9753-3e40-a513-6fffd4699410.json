  "rdfs:comment": {
    "de": [
      "Gleitschirmfliegen mit professionellen Gleitschirmfliegern in den höchsten Tälern der Hautes-Alpes ist auf dem Gelände von Ceillac mit Tête en l'air im Herzen des Regionalen Naturparks Queyras möglich."
    ],
    "en": [
      "Paragliding with paragliding professionals in the highest valleys of the Hautes-Alpes, it is possible on the site of Ceillac with Tête en l'air, in the heart of the Queyras Regional Natural Park."
    ],
    "it": [
      "Il parapendio con i professionisti del parapendio nelle valli più alte delle Hautes-Alpes, è possibile sul sito di Ceillac con Tête en l'air, nel cuore del Parc Naturel Régional du Queyras."
    ],
    "fr": [
      "Voler en parapente avec des professionnels du vol libre dans les plus hautes vallées des Hautes-Alpes, c'est possible sur le site de Ceillac avec Tête en l'air, au cœur du Parc Naturel Régional du Queyras."
    ],
    "es": [
      "Parapente con profesionales de parapente en los valles más altos de los Hautes-Alpes, es posible en el sitio de Ceillac con Tête en l'air, en el corazón del Parc Naturel Régional du Queyras."
    ],
    "nl": [
      "Paragliding met professionals in de hoogste valleien van de Hautes-Alpes, het is mogelijk op de site van Ceillac met Tête en l'air, in het hart van het Parc Naturel Régional du Queyras."
    ]
  },
  "rdfs:label": {
    "en": [
      "\"Tête en l'Air\" Paragliding School of Queyras"
    ],
    "fr": [
      "\"Tête en l'Air\" Ecole de Parapente Du Queyras"
    ]
  },
