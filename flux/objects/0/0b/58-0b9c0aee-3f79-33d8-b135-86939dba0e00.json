  "rdfs:comment": {
    "de": [
      "Brasserie-Restaurant in der Nähe des historischen Zentrums und des Luma-Turms. Platz innen und auf der Terrasse, herzlicher Empfang,reichhaltige Gerichte und gutes Preis-Leistungs-Verhältnis."
    ],
    "en": [
      "Brasserie-restaurant close to the historic centre and the Luma tower. Indoor and outdoor seating, warm welcome, hearty dishes and good value for money."
    ],
    "it": [
      "Brasserie-ristorante vicino al centro storico e alla torre Luma. Posti a sedere all'interno e all'esterno, accoglienza calorosa, piatti abbondanti e buon rapporto qualità-prezzo."
    ],
    "fr": [
      "Brasserie-restaurant à deux pas du centre historique et de la tour Luma. Place à l'intérieur et en terrasse, accueil chaleureux,plats copieux et bon rapport qualité-prix."
    ],
    "es": [
      "Brasserie-restaurante cerca del centro histórico y de la torre Luma. Asientos en el interior y al aire libre, cálida bienvenida, platos abundantes y buena relación calidad-precio."
    ],
    "nl": [
      "Brasserie-restaurant vlakbij het historische centrum en de Luma-toren. Zitplaatsen binnen en buiten, warme ontvangst, stevige gerechten en goede prijs-kwaliteitverhouding."
    ]
  },
