  "rdfs:comment": {
    "de": [
      "Entdecken Sie Les Craquants de Delme ©, eine eingetragene Marke und die Spezialität von Fabrice Bazzara! Diese mit feiner Zartbitterschokolade umhüllten Blätterteig-Pralinenquadrate tragen das Gütesiegel Produits Moselle Passion."
    ],
    "en": [
      "Discover Les Craquants de Delme ©, registered trademark; Fabrice Bazzara's speciality! These puff pastry praline squares coated with fine dark chocolate have the Moselle Passion Products label."
    ],
    "it": [
      "Scoprite Les Craquants de Delme ©, marchio registrato; la specialità di Fabrice Bazzara! Questi quadratini di pralina a scaglie ricoperti di finissimo cioccolato fondente hanno il marchio Produits Moselle Passion."
    ],
    "fr": [
      "Découvrez Les Craquants de Delme ©,marque déposée; la spécialité de Fabrice Bazzara ! Ces carrés de praliné feuilleté enrobés de fin chocolat noir bénéficient du label Produits Moselle Passion."
    ],
    "nl": [
      "Ontdek Les Craquants de Delme ©, gedeponeerd handelsmerk; de specialiteit van Fabrice Bazzara! Deze blokjes schilferpraliné omhuld met fijne pure chocolade hebben het label Produits Moselle Passion."
    ],
    "es": [
      "Descubra Les Craquants de Delme ©, marca registrada; ¡la especialidad de Fabrice Bazzara! Estos cuadrados de praliné hojaldrado recubiertos de fino chocolate negro llevan la etiqueta Produits Moselle Passion."
    ]
  },
