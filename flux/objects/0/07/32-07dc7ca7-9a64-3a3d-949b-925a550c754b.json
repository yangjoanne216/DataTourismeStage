  "rdfs:comment": {
    "de": [
      "Burg aus der Zeit der Katharer (11. und 12. Jahrhundert).\nJahrhundert. Im Herzen des kreisförmigen Dorfes Puisserguier, auf einem der Wege nach Santiago de Compostela."
    ],
    "en": [
      "Castle of the Cathar period (11th and 12th century).\nIn the heart of the circular village of Puisserguier, on one of the paths to St Jacques de Compostelle."
    ],
    "it": [
      "Castello di epoca catara (XI e XII secolo).\nNel cuore del villaggio circolare di Puisserguier, su uno dei percorsi di Santiago de Compostela."
    ],
    "fr": [
      "Château de la période cathare (XIe et XIIe siècle).\nAu cœur du village circulaire de Puisserguier, sur un des chemins de St Jacques de Compostelle."
    ],
    "nl": [
      "Kasteel uit de Kathaarse periode (11e en 12e eeuw).\nIn het hart van het ronde dorp Puisserguier, op een van de routes naar Santiago de Compostela."
    ],
    "es": [
      "Castillo de la época cátara (siglos XI y XII).\nEn el corazón del pueblo circular de Puisserguier, en uno de los caminos de Santiago de Compostela."
    ]
  },
