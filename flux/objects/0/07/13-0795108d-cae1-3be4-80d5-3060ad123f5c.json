  "rdfs:comment": {
    "de": [
      "Tagesmenüs von Montag bis Freitag mittags, wobei der Service bis ca. 14 Uhr angeboten wird. Karte unter der Woche und am Wochenende mittags möglich. Hausgemachte Küche.  Bar PMU zur Verfügung."
    ],
    "en": [
      "Daily menus from Monday to Friday at noon, with a service offered until about 2:00 pm. A la carte menu available during the week and at weekends, at lunchtime. Home-made cooking.  PMU bar at disposal."
    ],
    "it": [
      "Menù giornalieri dal lunedì al venerdì a pranzo, con servizio fino alle 14 circa. Menu alla carta disponibile durante la settimana e nei fine settimana, a pranzo. Cucina casalinga.  Barra PMU disponibile."
    ],
    "fr": [
      "Menus du jour du lundi au vendredi midi, avec un service proposé jusqu'à 14h00 environ. Carte possible la semaine et le week-end, les midis. Cuisine maison.  Bar PMU à disposition."
    ],
    "es": [
      "Menús diarios de lunes a viernes al mediodía, con servicio hasta las 14:00 horas aproximadamente. Menú a la carta disponible durante la semana y los fines de semana, a la hora del almuerzo. Cocina casera.  Barra PMU disponible."
    ],
    "nl": [
      "Dagmenu's van maandag tot vrijdagmiddag, met bediening tot ongeveer 14.00 uur. A la carte menu beschikbaar tijdens de week en in het weekend, tijdens de lunch. Thuis koken.  PMU bar beschikbaar."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Café de l'Hôtel de Ville"
    ]
  },
