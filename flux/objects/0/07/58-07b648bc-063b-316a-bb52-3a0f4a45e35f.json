  "rdfs:comment": {
    "de": [
      "Einfamilienhaus auf 2 Ebenen, mit 700 m² eingezäuntem Garten. 2 Terrassen, davon 1 überdacht mit Gartenmöbeln. Parkplatz im Inneren. Boulodrome. Ruhige Gegend."
    ],
    "en": [
      "Detached house on 2 levels, with enclosed garden of 700 m². 2 terraces including 1 covered with garden furniture. Indoor parking. Bowling alley. Quiet area."
    ],
    "it": [
      "Casa indipendente su 2 livelli, con giardino recintato di 700 m². 2 terrazze, 1 coperta con mobili da giardino. Parcheggio interno. Boulodrome. Zona tranquilla."
    ],
    "fr": [
      "Maison individuelle sur 2 niveaux, avec jardin clos de 700 m². 2 terrasses dont 1 couverte avec mobilier de jardin. Parking intérieur. Boulodrome. Quartier calme."
    ],
    "nl": [
      "Vrijstaand huis op 2 niveaus, met omheinde tuin van 700 m². 2 terrassen, waarvan 1 overdekt met tuinmeubelen. Binnen parkeren. Boulodrome. Rustig aan."
    ],
    "es": [
      "Casa independiente en 2 niveles, con jardín cerrado de 700 m². 2 terrazas, 1 cubierta con muebles de jardín. Aparcamiento interior. Boulodrome. Zona tranquila."
    ]
  },
