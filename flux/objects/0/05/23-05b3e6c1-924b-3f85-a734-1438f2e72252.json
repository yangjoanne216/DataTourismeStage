  "rdfs:comment": {
    "de": [
      "Verkauf von regionalen Bauernprodukten (Foie gras, Pastete, Confit....) und Lammfleischkonserven, ganzjährig von 10 bis 19 Uhr (außer sonntags). Verkauf von frischen Produkten vom 15/10 bis 15/05 (Entenbrust...). Verkauf von frischem Lammfleisch ab Hof auf Bestellung."
    ],
    "en": [
      "Sale of local farm products (foie gras, pâté, confit....) and canned lamb, all year round from 10am to 7pm (except Sundays). Sale of fresh products from 15/10 to 15/05 (magret...). Sale of fresh lamb at the farm on order."
    ],
    "it": [
      "Vendita di prodotti agricoli locali (foie gras, paté, confit....) e conserve di agnello, tutto l'anno dalle 10 alle 19 (tranne la domenica). Vendita di prodotti freschi dal 15/10 al 15/05 (magret...). Vendita di agnello fresco in fattoria su ordinazione."
    ],
    "fr": [
      "Vente de produits fermiers du terroir (foie gras, pâté, confit....) et conserves d'agneau, toute l'année de 10h à 19h (sauf le dimanche). Vente de produits frais du 15/10 au 15/05 (magret...). Vente d'agneau frais à la ferme sur commande."
    ],
    "nl": [
      "Verkoop van lokale boerderijproducten (foie gras, paté, confit....) en lamsvleesconserven, het hele jaar door van 10 tot 19 uur (behalve op zondag). Verkoop van verse producten van 15/10 tot 15/05 (magret...). Verkoop van vers lamsvlees op de boerderij op bestelling."
    ],
    "es": [
      "Venta de productos agrícolas locales (foie gras, paté, confit....) y conservas de cordero, todo el año de 10 a 19 horas (excepto los domingos). Venta de productos frescos del 15/10 al 15/05 (magret...). Venta de cordero fresco en la granja por encargo."
    ]
  },
