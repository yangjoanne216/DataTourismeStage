  "rdfs:comment": {
    "de": [
      "Kanu und Kajak Vermietung – Fahrt durch die Ardèche-Schlucht."
    ],
    "en": [
      "Canoe-Kayak hire - Trips down the Ardèche Gorges."
    ],
    "it": [
      "Noleggio di canoe e kayak. Discesa delle Gole dell'Ardèche. mini-discesa del Moulin à Saint-Just. Scoperta del canyon sud: partenza e ritorno alla nostra base nautica, senza vincoli di tempo\r\nPercorsi: 25 km, 32 km, 55 km e 70 km."
    ],
    "fr": [
      "Location de canoës- kayak. Descente des Gorges de l'Ardèche. , mini-descente du Moulin à Saint-Just. Découverte du canyon Sud : départ et retour à notre base nautique, pas de contrainte d'horaire\r\nParcours : 25 km, 32 km, 55km et 70 km."
    ],
    "nl": [
      "Verhuur van kano's-kajakken - afdaling in de Gorges de l'Ardèche."
    ],
    "es": [
      "Alquiler de canoas y kayaks. Descenso de las Gargantas del Ardèche. mini-descenso del Moulin à Saint-Just. Descubrimiento del cañón Sur: salida y regreso a nuestra base náutica, sin límite de tiempo\r\nRutas: 25 km, 32 km, 55 km y 70 km."
    ]
  },
