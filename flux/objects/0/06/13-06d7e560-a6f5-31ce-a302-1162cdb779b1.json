  "rdfs:comment": {
    "de": [
      "Johanna Brechet empfängt Sie von Montag bis Freitag auf dem Rond point de Cognac in Barbezieux-Saint-Hilaire.\r\nEntdecken Sie ihre Burger und Paninis."
    ],
    "en": [
      "Johanna Brechet welcomes you from Monday to Friday at the Rond point de Cognac in Barbezieux-Saint-Hilaire.\r\nCome and discover its burgers and paninis formulas."
    ],
    "it": [
      "Johanna Brechet vi accoglie dal lunedì al venerdì sul Rond Point de Cognac a Barbezieux-Saint-Hilaire.\r\nVenite a scoprire le sue formule di hamburger e panini."
    ],
    "fr": [
      "Johanna Brechet vous accueille du lundi au vendredi sur le Rond point de Cognac à Barbezieux-Saint-Hilaire.\r\nVenez découvrir ses formules burgers et ses paninis."
    ],
    "nl": [
      "Johanna Brechet ontvangt u van maandag tot vrijdag op het Rond Point de Cognac in Barbezieux-Saint-Hilaire.\r\nKom zijn hamburger- en paniniformules ontdekken."
    ],
    "es": [
      "Johanna Brechet le recibe de lunes a viernes en el Rond Point de Cognac, en Barbezieux-Saint-Hilaire.\r\nVenga a descubrir sus fórmulas de hamburguesa y panini."
    ]
  },
