  "rdfs:comment": {
    "de": [
      "Schöne Wanderung in Saint-Léon sur L'Isle. Entdecken Sie verschiedene kleine Gebäude, die erhalten werden sollen. Diese große Schleife wird Sie durch verschiedene Landschaften führen."
    ],
    "en": [
      "Nice walk in Saint-Léon sur L'Isle. Discovery of various small buildings. This large loop will make you escape through different landscapes."
    ],
    "it": [
      "Bella passeggiata a Saint-Léon sur L'Isle. Scoperta di vari piccoli edifici. Questo lungo percorso ad anello vi farà attraversare diversi paesaggi."
    ],
    "fr": [
      "Belle randonnée à Saint-Léon sur L'Isle. Découverte de divers petit bâti sauvegarder. Cette grande boucle saura vous faire évader au travers de différents paysages."
    ],
    "nl": [
      "Mooie wandeling in Saint-Léon sur L'Isle. Ontdekking van verschillende kleine gebouwen. Deze lange lus laat u ontsnappen door verschillende landschappen."
    ],
    "es": [
      "Bonito paseo en Saint-Léon sur L'Isle. Descubrimiento de varios edificios pequeños. Este largo bucle le hará escapar a través de diferentes paisajes."
    ]
  },
