  "rdfs:comment": {
    "de": [
      "200 m2 Holzskulpturen aus Schrott, Bilder, Zeichnungen und Skizzenbücher.\nTäglich 15-19 Uhr und nach Vereinbarung. Für jedes Publikum zugänglich. Kostenlos."
    ],
    "en": [
      "200 m2 of scrap wood sculptures, paintings, drawings and sketchbooks.\nDaily 3pm-7pm and by appointment. Open to all. Free admission."
    ],
    "it": [
      "200 m2 di sculture in legno di scarto, dipinti, disegni e quaderni di schizzi.\nAperto tutti i giorni dalle 15.00 alle 19.00 e su appuntamento. Aperto al pubblico. Ingresso libero."
    ],
    "fr": [
      "200 m2 de sculptures bois ferrailles, tableaux, dessins et carnets de dessins. \nTous les jours 15h-19h et sur rendez-vous. Tout public. Gratuit."
    ],
    "nl": [
      "200 m2 sculpturen van sloophout, schilderijen, tekeningen en schetsboeken.\nDagelijks geopend van 15.00 tot 19.00 uur en op afspraak. Open voor publiek. Gratis toegang."
    ],
    "es": [
      "200 m2 de esculturas de madera de desecho, pinturas, dibujos y cuadernos de bocetos.\nAbierta todos los días de 15.00 a 19.00 horas y con cita previa. Abierto al público en general. Entrada gratuita."
    ]
  },
