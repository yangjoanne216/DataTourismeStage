  "rdfs:comment": {
    "de": [
      "Chefkoch Gérald Potron und sein Team bieten eine einfache, originelle und kreative Küche, die Sie verwöhnen und Ihre Geschmacksknospen wecken wird!"
    ],
    "en": [
      "Chef Gérald Potron and his team offer simple, original and creative cuisine that will delight your taste buds!"
    ],
    "it": [
      "Lo chef Gérald Potron e il suo team offrono una cucina semplice, originale e creativa che delizierà il vostro palato!"
    ],
    "fr": [
      "Le Chef Gérald Potron et son équipe, proposent une cuisine simple, originale et créative qui saura vous régaler et réveiller vos papilles !"
    ],
    "es": [
      "El Chef Gérald Potron y su equipo ofrecen una cocina sencilla, original y creativa que hará las delicias de su paladar"
    ],
    "nl": [
      "Chef Gérald Potron en zijn team bieden eenvoudige, originele en creatieve gerechten die uw smaakpapillen zullen verrukken!"
    ]
