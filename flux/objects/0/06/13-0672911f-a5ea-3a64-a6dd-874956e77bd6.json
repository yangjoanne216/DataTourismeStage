  "rdfs:comment": {
    "de": [
      "Swimmingpool mit 2 Becken plus schönem Kinderbecken, Sonnenterrasse. Restaurant direkt nebenan."
    ],
    "en": [
      "Open just for summer. Swimming pool with 2 pools, paddling pool, solarium."
    ],
    "it": [
      "Piscina con 2 vasche più una bella piscina per bambini, solarium. Ristorante proprio accanto."
    ],
    "fr": [
      "Piscine avec 2 bassins plus belle pataugeoire, solarium. Restaurant juste à côté."
    ],
    "nl": [
      "Zwembad met 2 baden plus een mooi peuterbad, solarium. Restaurant naast de deur."
    ],
    "es": [
      "Piscina con 2 piscinas más una bonita piscina infantil, solarium. Restaurante justo al lado."
    ]
  },
