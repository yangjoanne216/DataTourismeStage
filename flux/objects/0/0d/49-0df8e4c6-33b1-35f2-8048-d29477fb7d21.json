  "rdfs:comment": {
    "de": [
      "Haben Sie Lust auf rote Früchte? Die Jardins du Tenu bieten Ihnen eine große Auswahl an Früchten zum Selbstpflücken oder in Schalen!"
    ],
    "en": [
      "Do you feel like red fruits? Les Jardins du Tenu offers you a wide choice of fruit, picked or in trays!"
    ],
    "it": [
      "Avete voglia di frutti rossi? Les Jardins du Tenu offrono un'ampia gamma di frutta da raccogliere o confezionare in vaschette!"
    ],
    "fr": [
      "Vous avez envie de fruits rouges ? Les Jardins du Tenu vous proposent un large choix de fruits à la cueillette ou en barquette !"
    ],
    "es": [
      "¿Tienes antojo de frutos rojos? Les Jardins du Tenu ofrece una amplia gama de frutas para recoger o envasar en bandejas"
    ],
    "nl": [
      "Heb je trek in rood fruit? Les Jardins du Tenu biedt een breed scala aan fruit om te plukken of te verpakken in trays!"
    ]
  },
