  "rdfs:comment": {
    "de": [
      "Dieser Taubenschlag aus Backstein aus dem 18. Jahrhundert befindet sich hinter der Kirche, am Eingang des Bauernhofs."
    ],
    "en": [
      "This 18th century brick porch dovecote is located behind the church, at the entrance to the farm."
    ],
    "it": [
      "Questa colombaia in mattoni del XVIII secolo si trova dietro la chiesa, all'ingresso della fattoria."
    ],
    "fr": [
      "Ce pigeonnier porche du XVIIIè siècle en brique se trouve derrière l'église, en entrée de la ferme."
    ],
    "es": [
      "Este palomar con porche de ladrillo del siglo XVIII se encuentra detrás de la iglesia, a la entrada de la granja."
    ],
    "nl": [
      "Deze bakstenen duiventil uit de 18e eeuw bevindt zich achter de kerk bij de ingang van de boerderij."
    ]
  },
