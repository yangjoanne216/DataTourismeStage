  "rdfs:comment": {
    "de": [
      "HAUS. 98 m². Erdgeschoss. 2 Schlafzimmer für 4 Personen. Wohnzimmer, Fernseher, Kamineinsatz. Küche, Mikrowelle, Nebenküche, Waschmaschine. Badezimmer mit Dusche. Überdachte Terrasse, eingezäunter Garten, Gartenmöbel, Grill. Privater Parkplatz."
    ],
    "en": [
      "HOUSE. 98 m². Ground floor. 2 bedrooms for 4 persons. Living room, TV, fireplace. Kitchen, microwave, scullery, washing machine. Shower room. Covered terrace, enclosed garden, garden furniture, barbecue. Private car park."
    ],
    "it": [
      "CASA. 98 m². Piano terra. 2 camere per 4 persone. Soggiorno, TV, camino. Cucina, microonde, retrocucina, lavatrice. Bagno con doccia. Terrazza coperta, giardino recintato, mobili da giardino, barbecue. Parcheggio privato."
    ],
    "fr": [
      "MAISON. 98 m². RDC. 2 chambres pour 4 personnes. Salon, TV, cheminée insert. Cuisine, micro-ondes, arrière cuisine, lave-linge. Salle d'eau. Terrasse couverte, jardin clos, salon de jardin, barbecue. Parking privé."
    ],
    "nl": [
      "HUIS. 98 m². Begane grond. 2 slaapkamers voor 4 personen. Woonkamer, TV, open haard. Keuken, magnetron, bijkeuken, wasmachine. Doucheruimte. Overdekt terras, omheinde tuin, tuinmeubels, barbecue. Privé parkeerplaats."
    ],
    "es": [
      "CASA. 98 m². Planta baja. 2 dormitorios para 4 personas. Sala de estar, TV, chimenea. Cocina, microondas, fregadero, lavadora. Ducha. Terraza cubierta, jardín cerrado, muebles de jardín, barbacoa. Aparcamiento privado."
    ]
  },
