  "rdfs:comment": {
    "de": [
      "Maora JetGliss befindet sich am Strand La Romaniquette in Istres und bietet Wassersportaktivitäten und den Verleih von Wassersportgeräten auf dem Étang de Berre an."
    ],
    "en": [
      "Maora JetGliss is installed on the beach of La Romaniquette in Istres and offers activities and rentals of nautical equipment on the Etang de Berre."
    ],
    "it": [
      "Maora JetGliss si trova sulla spiaggia di Romaniquette a Istres e offre attività e noleggio di attrezzature per sport acquatici sull'Etang de Berre."
    ],
    "fr": [
      "Maora JetGliss est installé sur la plage de la Romaniquette à Istres et propose des activités et locations de matériels nautiques sur l'étang de Berre."
    ],
    "es": [
      "Maora JetGliss se encuentra en la playa de Romaniquette, en Istres, y ofrece actividades y alquiler de equipos para deportes acuáticos en el Etang de Berre."
    ],
    "nl": [
      "Maora JetGliss ligt op het strand van Romaniquette in Istres en biedt activiteiten en verhuur van watersportuitrusting op het Etang de Berre."
    ]
  },
