  "rdfs:comment": {
    "de": [
      "Die SCA du Mas Daussan ist seit 15 Jahren am Eingang des Regionalparks Camargue in Arles ansässig und hat sich der Apfelproduktion verschrieben."
    ],
    "en": [
      "Established 15 years ago at the entrance to the Camargue Regional Park, in Arles, the SCA du Mas Daussan is dedicated to producing apples."
    ],
    "it": [
      "Fondata 15 anni fa all'ingresso del parco regionale della Camargue, ad Arles, la SCA du Mas Daussan produce mele."
    ],
    "fr": [
      "Installée depuis 15 ans à l'entrée du Parc Régional de Camargue, sur Arles, la SCA du Mas Daussan a pour vocation de produire des pommes."
    ],
    "nl": [
      "De SCA du Mas Daussan is 15 jaar geleden opgericht bij de ingang van het regionale park van de Camargue, in Arles, en produceert appels."
    ],
    "es": [
      "Creada hace 15 años a la entrada del Parque Regional de la Camarga, en Arles, la SCA du Mas Daussan produce manzanas."
    ]
  },
