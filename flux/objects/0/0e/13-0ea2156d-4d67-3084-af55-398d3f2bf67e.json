  "rdfs:comment": {
    "de": [
      "Philippe Creusefond ist Holzdreher und Tischler. Er empfängt Sie nur nach Vereinbarung für Vorführungen, Animationen oder einfach nur, um seine Produkte zu sehen. Seine Objekte sind aus einheimischen Hölzern gefertigt."
    ],
    "en": [
      "Philippe Creusefond is a woodturner and carpenter. He welcomes you only by appointment for demonstrations, animations or simply to see his products. His objects are made of local wood."
    ],
    "it": [
      "Philippe Creusefond è un tornitore e falegname. Vi accoglie solo su appuntamento per dimostrazioni, animazioni o semplicemente per vedere i suoi prodotti. I suoi oggetti sono realizzati in legno locale."
    ],
    "fr": [
      "Philippe Creusefond est tourneur sur bois et menuisier. Il vous accueille uniquement sur rendez-vous pour des démonstrations, animations ou tout simplement pour voir ses produits. Ses objets sont en bois locaux."
    ],
    "nl": [
      "Philippe Creusefond is houtdraaier en timmerman. Hij ontvangt u uitsluitend op afspraak voor demonstraties, animaties of gewoon om zijn producten te bekijken. Zijn voorwerpen zijn gemaakt van lokaal hout."
    ],
    "es": [
      "Philippe Creusefond es tornero y carpintero. Le recibe con cita previa para demostraciones, animaciones o simplemente para ver sus productos. Sus objetos son de madera local."
    ]
  },
