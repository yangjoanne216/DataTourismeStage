  "rdfs:comment": {
    "de": [
      "Keller und handwerkliche Weinbar. Große Auswahl an biologischen, biodynamischen oder natürlichen Weinen. Craft Biere. Teller und Tapas auf der Terrasse oder im Obergeschoss."
    ],
    "en": [
      "Cellar and artisan wine bar. Wide choice of organic, biodynamic or natural wine.\r\nCraft beers. Plates and tapas on the terrace or upstairs."
    ],
    "it": [
      "Cantina e wine bar di artigiani. Ampia scelta di vini biologici, biodinamici e naturali\r\nBirre artigianali. Piatti e tapas sulla terrazza o al piano superiore."
    ],
    "fr": [
      "Cave et bar à vins d'artisans. Large choix de vin Bio, en Biodynamie ou nature. \r\nBières artisanales. Planches et tapas en terrasse ou à l'étage."
    ],
    "nl": [
      "Kelder en wijnbar van ambachtslieden. Ruime keuze aan biologische, biodynamische en natuurlijke wijnen\r\nArtisanale bieren. Borden en tapas op het terras of boven."
    ],
    "es": [
      "Bodega y vinoteca artesanal. Amplia variedad de vinos orgánicos, biodinámicos o naturales. Cervezas artesanales. Platos y tapas en la terraza o arriba."
    ]
  },
