  "rdfs:comment": {
    "de": [
      "Ein Restaurant, in dem alles hausgemacht ist, eine Küche mit Holzfeuer aus frischen Zutaten, die aus einer lokalen, umweltfreundlichen, bäuerlichen Landwirtschaft stammen!"
    ],
    "en": [
      "A restaurant where everything is homemade, a wood-fired kitchen with fresh ingredients, from local farmers and respectful of the environment!"
    ],
    "it": [
      "Un ristorante dove tutto è fatto in casa, cucinato sul fuoco a legna con ingredienti freschi provenienti da agricoltura locale ed ecologica!"
    ],
    "fr": [
      "Un restaurant où tout est fait maison, une cuisine au feu de bois à partir d'ingrédients frais, issus d'une agriculture paysanne locale et respectueuse de l'environnement !"
    ],
    "es": [
      "Un restaurante donde todo es casero, cocinado a la leña con ingredientes frescos procedentes de la agricultura local y respetuosa con el medio ambiente"
    ],
    "nl": [
      "Een restaurant waar alles huisgemaakt is, gekookt boven een houtvuur met verse ingrediënten van lokale en milieuvriendelijke landbouw!"
    ]
  },
