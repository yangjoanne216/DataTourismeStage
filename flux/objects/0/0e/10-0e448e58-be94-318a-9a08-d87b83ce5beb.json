  "rdfs:comment": {
    "de": [
      "Bar und Restaurant 7 Tage die Woche geöffnet. Traditionelle Küche. Karte. Tagesmenüs. Große Auswahl an Eis auf der Terrasse. Pizzas / Flammkuchen im Sommer und am Wochenende ab 15:00 Uhr."
    ],
    "en": [
      "Bar and restaurant open 7 days a week Traditional cuisine. A la carte menu. Menus of the day. Large choice of ice creams on the terrace. Pizzas / flamms in summer and on weekends from 15h00."
    ],
    "it": [
      "Bar e ristorante aperti 7 giorni su 7. Cucina tradizionale. Menu alla carta. Menu giornalieri. Ampia scelta di gelati sulla terrazza. Pizze/fiamme in estate e nei fine settimana a partire dalle 15.00."
    ],
    "fr": [
      "Bar et restaurant ouvert 7j/7.. Cuisine traditionnelle. Carte. Menus du jour. Large choix de glaces en terrasse. Pizzas / flamms en été et le week-end à partir de 15h00."
    ],
    "es": [
      "Bar y restaurante abiertos los 7 días de la semana. Cocina tradicional. Menú a la carta. Menús diarios. Amplia oferta de helados en la terraza. Pizzas / flamas en verano y los fines de semana a partir de las 15:00 horas."
    ],
    "nl": [
      "Bar en restaurant 7 dagen per week geopend. Traditionele keuken. A la carte menu. Dagmenu's. Ruime keuze aan ijsjes op het terras. Pizza's/vlammen in de zomer en in het weekend vanaf 15.00 uur."
    ]
  },
