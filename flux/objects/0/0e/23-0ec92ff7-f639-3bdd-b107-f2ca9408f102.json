  "rdfs:comment": {
    "de": [
      "Bistro, Weinstube, gutbürgerliche Küche\nKanadische Gerichte und Weine aus allen Regionen von kleinen Besitzern.\nMusikalische Atmosphäre mit Spätdienst am Wochenende."
    ],
    "en": [
      "Bistro, wine bar, bourgeois cuisine.\nCanaille dishes and wines from all regions from small owners.\nMusical atmosphere with late service on weekends."
    ],
    "it": [
      "Bistrot, wine bar, cucina borghese\nPiatti e vini deliziosi di tutte le regioni da piccoli proprietari.\nAtmosfera musicale con servizio fino a tardi nei fine settimana."
    ],
    "fr": [
      "Bistrot, bar à vin, cuisine bourgeoise. \nPlats canailles et vins de toutes régions de petits propriétaires.\nAmbiance musicale avec service tardif le week-end."
    ],
    "nl": [
      "Bistro, wijnbar, burgerlijke keuken\nHeerlijke gerechten en wijnen uit alle regio's van kleine eigenaren.\nMuzikale sfeer met late bediening in het weekend."
    ],
    "es": [
      "Bistro, bar de vinos, cocina burguesa.\nPlatos y vinos de Canaille de todas las regiones de pequeños propietarios.\nAmbiente musical con servicio hasta tarde los fines de semana."
    ]
  },
