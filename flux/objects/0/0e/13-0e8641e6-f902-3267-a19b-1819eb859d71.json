  "rdfs:comment": {
    "de": [
      "Die Berge bieten einzigartige Möglichkeiten: abenteuerliche, entspannende oder adrenalinreiche Momente, entdecken Sie zahlreiche sportliche oder genussvolle Aktivitäten."
    ],
    "en": [
      "The mountains offer unique opportunities for adventures, relaxation and thrills. Come and discover a wide range of sports and fun activities."
    ],
    "it": [
      "La montagna offre opportunità uniche: avventura, relax o adrenalina, per scoprire tante attività sportive o di piacere."
    ],
    "fr": [
      "La montagne offre des opportunités uniques : moment d'aventure, de détente ou riche en adrénaline, découvrez de nombreuses activités sportives ou plaisir."
    ],
    "es": [
      "La montaña ofrece oportunidades únicas: aventura, relajación o adrenalina, descubra una amplia gama de deportes y actividades de ocio."
    ],
    "nl": [
      "De bergen bieden unieke mogelijkheden: avontuur, ontspanning of adrenaline. Ontdek onze sport- en recreatie-activiteiten."
    ]
  },
