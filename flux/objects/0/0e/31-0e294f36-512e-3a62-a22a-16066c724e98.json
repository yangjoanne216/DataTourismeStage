  "rdfs:comment": {
    "de": [
      "Schlendern Sie durch die Gassen des Dorfes Montjaux und entdecken Sie seine historischen Sehenswürdigkeiten: zwei romanische Häuser im Herzen des Dorfes."
    ],
    "en": [
      "Stroll through the narrow streets of the village of Montjaux and discover its historical monuments: two Romanesque houses in the heart of the village."
    ],
    "it": [
      "Passeggiate per le stradine del villaggio di Montjaux e scoprite i suoi monumenti storici: due case romaniche nel cuore del villaggio."
    ],
    "fr": [
      "Arpentez les ruelles du village de Montjaux et découvrez ses monuments historiques : deux maisons romanes au coeur du village."
    ],
    "nl": [
      "Wandel door de smalle straatjes van het dorp Montjaux en ontdek de historische monumenten: twee Romaanse huizen in het hart van het dorp."
    ],
    "es": [
      "Pasee por las estrechas calles del pueblo de Montjaux y descubra sus monumentos históricos: dos casas románicas en el corazón del pueblo."
    ]
  },
