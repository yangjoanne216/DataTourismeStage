  "rdfs:comment": {
    "de": [
      "Kunstgraveur/Typograf. Weicher Druck vom Kupfer auf Papier. Er verbringt die Zeit mit Gravieren, und betrachtet genau den Himmel, die Bäume und die Landschaften."
    ],
    "en": [
      "Artist engraver/Typographer. Uses copper engravings to print on paper. He engraves passing time as he contemplates the sky, trees and the horizon."
    ],
    "it": [
      "Artista incisore/tipografo. Impronta morbida dal rame alla carta. Incide il tempo che passa, contempla il cielo, gli alberi, il rilievo."
    ],
    "fr": [
      "Artiste graveur/Typographe. Empreinte douce du cuivre au papier. Il grave le temps qui passe, contemple le ciel, les arbres, le relief."
    ],
    "nl": [
      "Graveerder/typograaf. Zachte opdruk van koper op papier. Hij graveert de voorbijgaande tijd, overdenkt de lucht, de bomen, het reliëf."
    ],
    "es": [
      "Artista grabador/tipógrafo. Impresión suave del cobre al papel. Graba el paso del tiempo, contempla el cielo, los árboles, el relieve."
    ]
