  "rdfs:comment": {
    "de": [
      "Am Fuße der Pisten Alpette, Raverettes, Thurn und Forestier auf dem Rochebrune-Massiv können Sie mit dem Sessellift Alpette den Gipfel von Rochebrune erreichen."
    ],
    "en": [
      "The Alpette chairlift, located at the foot of the Alpette, Raverettes, Thurn and Forestier slopes on the Rochebrune mountain range enables you to reach the summit of Rochebrune."
    ],
    "it": [
      "Ai piedi delle piste Alpette, Raverettes, Thurn e Forestier sul massiccio di Rochebrune, la seggiovia Alpette permette di raggiungere la vetta di Rochebrune."
    ],
    "fr": [
      "Au pied des pistes Alpette, Raverettes, Thurn et Forestier sur le massif de Rochebrune, le télésiège de l'alpette vous permet de rejoindre le sommet de Rochebrune."
    ],
    "es": [
      "Al pie de las pistas Alpette, Raverettes, Thurn y Forestier, en el macizo de Rochebrune, el telesilla Alpette permite alcanzar la cumbre de Rochebrune."
    ],
    "nl": [
      "Aan de voet van de pistes Alpette, Raverettes, Thurn en Forestier op het massief van Rochebrune kunt u met de stoeltjeslift Alpette de top van Rochebrune bereiken."
    ]
  },
  "rdfs:label": {
    "en": [
      "The Alpette chairlift"
    ],
    "fr": [
      "Télésiège de l'Alpette"
    ]
  },
