  "rdfs:comment": {
    "de": [
      "Die Domaine de la Tendrière liegt in Poncé sur Loir, einem kleinen, charaktervollen Dorf zwischen Tours und Le Mans, im von Ronsard geliebten Tal der Loir."
    ],
    "en": [
      "The Domaine de la Tendrière is located in Poncé sur Loir, a small village of character, between Tours and Le Mans, in the Loir valley, dear to Ronsard."
    ],
    "it": [
      "Il Domaine de la Tendrière si trova a Poncé sur Loir, un piccolo villaggio di carattere, tra Tours e Le Mans, nella valle del Loir, tanto cara a Ronsard."
    ],
    "fr": [
      "Le Domaine de la Tendrière se situe à Poncé sur Loir, petit village de caractère, entre Tours et Le Mans, dans la vallée du Loir, chère à Ronsard."
    ],
    "nl": [
      "Het Domaine de la Tendrière ligt in Poncé sur Loir, een karaktervol dorpje tussen Tours en Le Mans, in het dal van de Loir, dat Ronsard zo dierbaar was."
    ],
    "es": [
      "El Domaine de la Tendrière se encuentra en Poncé sur Loir, un pequeño pueblo con carácter, entre Tours y Le Mans, en el valle del Loir, tan querido por Ronsard."
    ]
  },
