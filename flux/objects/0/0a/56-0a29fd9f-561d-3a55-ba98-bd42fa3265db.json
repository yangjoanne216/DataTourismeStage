  "rdfs:comment": {
    "de": [
      "Im Herzen von Torreilles, einem mediterranen Dorf mit Charakter in den östlichen Pyrenäen, authentisches Haus aus Naturstein, ruhig gelegen und in direkter Nähe zu den Geschäften."
    ],
    "en": [
      "Located in the heart of Torreilles, a Mediterranean village of character in the Pyrenees-Orientales, authentic stone house, quiet and close to the shops."
    ],
    "it": [
      "Nel cuore di Torreilles, un villaggio mediterraneo di carattere nei Pirenei Orientali, un'autentica casa in pietra in una posizione tranquilla vicino ai negozi."
    ],
    "fr": [
      "Située au coeur de Torreilles, village méditerranéen de caractère des Pyrénées-Orientales, authentique maison en pierres apparentes, au calme et à proximité directe des commerces."
    ],
    "es": [
      "Situado en el corazón de Torreilles, un pueblo mediterráneo de carácter en los Pirineos Orientales, una auténtica casa de piedra en una ubicación tranquila cerca de las tiendas."
    ],
    "nl": [
      "Gelegen in het hart van Torreilles, een mediterraan dorp met karakter in de Pyrénées-Orientales, een authentiek stenen huis op een rustige locatie dicht bij de winkels."
    ]
  },
