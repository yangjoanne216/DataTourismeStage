  "rdfs:comment": {
    "de": [
      "La Pivoine: das Restaurant mit All-you-can-eat-Buffet! Sushi, Vorspeisen, Meeresfrüchte, warme Gerichte und andere asiatische Spezialitäten. Man kommt satt wieder heraus!"
    ],
    "en": [
      "La Pivoine: the restaurant with all-you-can-eat buffet! Sushi, starters, seafood, hot dishes and other Asian specialities. You'll come out full!"
    ],
    "it": [
      "La Pivoine: il ristorante con buffet a volontà! Sushi, antipasti, frutti di mare, piatti caldi e altre specialità asiatiche. Ne uscirete sazi!"
    ],
    "fr": [
      "La Pivoine : le restaurant avec buffet à volonté ! Des sushis, des entrées, des fruits de mer, des plats chauds et autres spécialités asiatiques. On en ressort rassasié !"
    ],
    "nl": [
      "La Pivoine: het restaurant met all-you-can-eat buffet! Sushi, voorgerechten, zeevruchten, warme gerechten en andere Aziatische specialiteiten. Je komt er vol uit!"
    ],
    "es": [
      "La Pivoine: el restaurante con buffet libre Sushi, entrantes, marisco, platos calientes y otras especialidades asiáticas. ¡Saldrás lleno!"
    ]
  },
