  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/bcd2bddb-e5c9-300d-ae46-6b0d1d553c37",
      "dc:description": {
        "de": [
          "Lauch, Karotten, Sellerie, Rüben, Salate, Spinat, Zwiebeln, Schalotten, grüne Bohnen, Erbsen, Zucchini, Kürbis, Rote Bete, Tomaten, Kartoffeln aus eigener Produktion.\nVerkauf von Obst der Saison."
        ],
        "en": [
          "Leeks, carrots, celery, turnips, salads, spinach, onions, shallots, green beans, peas, zucchini, pumpkins, beets, tomatoes, potatoes produced on site.\nSale of seasonal fruits."
        ],
        "it": [
          "Porri, carote, sedano, rape, lattuga, spinaci, cipolle, scalogni, fagiolini, piselli, zucchine, zucche, barbabietole, pomodori, patate prodotti in loco.\nVendita di frutta di stagione."
        ],
        "fr": [
          "Poireaux, carottes, céleris, navets, salades, épinard, oignons, échalottes, haricots-verts, petit pois, courgettes, potirons, betteraves, tomates, pommes-de-terre produits sur place.\nVente de fruits de saison."
        ],
        "es": [
          "Puerros, zanahorias, apio, nabos, lechugas, espinacas, cebollas, chalotas, judías verdes, guisantes, calabacines, calabazas, remolachas, tomates y patatas producidos in situ.\nVenta de fruta de temporada."
        ],
        "nl": [
          "Prei, wortelen, selderij, rapen, sla, spinazie, uien, sjalotten, sperziebonen, erwten, courgettes, pompoenen, bieten, tomaten, aardappelen die ter plaatse worden geproduceerd.\nVerkoop van seizoensfruit."
        ]
      },
