      "@id": "https://data.datatourisme.fr/8f3656fc-47bd-3009-a224-217a879b0d71",
      "dc:description": {
        "de": [
          "Kochecke, Mikrowelle, Waschmaschine, Internetzugang, Wifi-Zugang, Fernseher, Wohnzimmer, Dusche, 1 Schlafzimmer, 1 Bett/2 Pers., Handtücher und Haushaltswäsche vorhanden, Föhn, Bügeleisen und Bügelbrett, Netflix. Kurtaxe nicht inbegriffen. Mietnebenkosten inbegriffen."
        ],
        "en": [
          "Kitchenette, microwave, washing machine, internet access, wifi access, television, living room, shower, 1 bedroom, 1 double bed, towels and household linen provided, hair dryer, iron and ironing board, Netflix. Tourist tax not included. Rental charges included."
        ],
        "it": [
          "Angolo cottura, microonde, lavatrice, accesso a internet, accesso wifi, televisione, soggiorno, doccia, 1 camera da letto, 1 letto matrimoniale, asciugamani e biancheria per la casa forniti, asciugacapelli, ferro e asse da stiro, Netflix. Tassa di soggiorno non inclusa. Spese di noleggio incluse."
        ],
        "fr": [
          "Coin cuisine, micro-ondes, lave-linge, accès internet, accès wifi, télévision, séjour, douche, 1 chambre, 1 lit/2 pers., linge de toilette et de maison fourni, sèche-cheveux, fer et table à repasser, Netflix. Taxe de séjour non comprise. Charges locatives comprises."
        ],
        "nl": [
          "Keukenhoek, magnetron, wasmachine, internettoegang, wifi, televisie, woonkamer, douche, 1 slaapkamer, 1 tweepersoonsbed, handdoeken en huishoudlinnen aanwezig, haardroger, strijkijzer en strijkplank, Netflix. Toeristenbelasting niet inbegrepen. Huurkosten inbegrepen."
        ],
        "es": [
          "Cocina americana, microondas, lavadora, acceso a internet, acceso wifi, televisión, salón, ducha, 1 dormitorio, 1 cama doble, toallas y ropa de hogar incluidas, secador de pelo, plancha y tabla de planchar, Netflix. Tasa turística no incluida. Gastos de alquiler incluidos."
        ]
      },
