  "rdfs:comment": {
    "de": [
      "Folies de Provence ist leidenschaftlicher Provence-Fan und vertreibt und wählt mit großer Sorgfalt die Marken seiner Region aus, die ihm so sehr am Herzen liegt"
    ],
    "en": [
      "Passionate about Provence, Folies de Provence distributes and selects with great care the brands of his region so dear to him"
    ],
    "it": [
      "Appassionato di Provenza, Folies de Provence distribuisce e seleziona con grande cura i marchi della sua regione a lui tanto cari"
    ],
    "fr": [
      "Passionné de Provence, Folies de Provence distribue et sélectionne avec grand soin les marques de sa région si chère à ses yeux"
    ],
    "nl": [
      "Folies de Provence, gepassioneerd door de Provence, verdeelt en selecteert met veel zorg de merken uit zijn regio die hem zo dierbaar zijn"
    ],
    "es": [
      "Apasionado de la Provenza, Folies de Provence distribuye y selecciona con esmero las marcas de su región que tanto aprecia"
    ]
  },
