  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/f8dd8eb0-d57d-32fe-ae24-7e46736f2105",
      "dc:description": {
        "de": [
          "Verkostung/Verkauf von Apfelsaft aus einheimischen Äpfeln und schmackhaften Mischungen (mit Kirschen, schwarzen Johannisbeeren, Himbeeren, Pfirsich, Brombeeren) und süßem Apfelwein."
        ],
        "en": [
          "Tasting/Sale of local apple juice and tasty blends (with cherries, blackcurrants, raspberries, peaches, blackberries) and sweet cider."
        ],
        "it": [
          "Degustazione/vendita di succo di mela locale e di gustose miscele (con ciliegie, ribes, lamponi, pesche, more) e sidro dolce."
        ],
        "fr": [
          "Dégustation/vente de jus de pommes du pays et de mélanges savoureux (avec cerises, cassis, framboises, pêche, mûre) et de cidre doux."
        ],
        "nl": [
          "Proeverij/verkoop van lokaal appelsap en lekkere mengsels (met kersen, zwarte bessen, frambozen, perziken, bramen) en zoete cider."
        ],
        "es": [
          "Degustación/venta de zumo de manzana local y de sabrosas mezclas (con cerezas, grosellas, frambuesas, melocotones, moras) y sidra dulce."
        ]
      },
      "@type": [
        "Description"
