  "rdfs:comment": {
    "de": [
      "Küche mit Fleisch vom AOP-Stier und frischem Fisch aus dem Mittelmeer. Im Inneren erwartet Sie eine Camargue-Atmosphäre!"
    ],
    "en": [
      "Cuisine based on PDO bull meat and fresh Mediterranean fish. A Camargue atmosphere awaits you inside!"
    ],
    "it": [
      "Cucina a base di carne di toro DOP e pesce fresco del Mediterraneo. L'atmosfera della Camargue vi aspetta all'interno!"
    ],
    "fr": [
      "Cuisine à base de viande de taureau AOP, et de poissons frais de méditerranée. Une ambiance camarguaise vous attends à l'intérieur !"
    ],
    "nl": [
      "Keuken op basis van BOB-stierenvlees en verse mediterrane vis. Binnen wacht u een Camargue-sfeer!"
    ],
    "es": [
      "Cocina basada en la carne de toro de la DOP y el pescado fresco del Mediterráneo. En su interior le espera un ambiente de Camarga"
    ]
  },
