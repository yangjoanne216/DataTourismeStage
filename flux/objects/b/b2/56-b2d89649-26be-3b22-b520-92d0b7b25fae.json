  "rdfs:comment": {
    "de": [
      "Charmantes Studio, sehr gut ausgestattet, im Erdgeschoss, typisches Viertel von Cap d'Oune. Parkplatz in der Nähe (Bahnhof ). Kleine Terrasse. Blick auf die Berge, das Dorf und den Strand."
    ],
    "en": [
      "Charming studio, very well equipped, on the ground floor, typical neighborhood of Cap d'Oune. Parking nearby (station). Small terrace. View on mountain, village and beach."
    ],
    "it": [
      "Grazioso monolocale, molto ben attrezzato, al piano terra, nella tipica zona di Cap d'Oune. Parcheggio nelle vicinanze (stazione). Piccola terrazza. Vista sulle montagne, sul villaggio e sulla spiaggia."
    ],
    "fr": [
      "Charmant studio, très bien équipé, au RDC, Quartier typique du Cap d'Oune. Parking à proximité (gare ). Petite terrasse. Vue sur montagne, village et plage."
    ],
    "es": [
      "Encantador estudio, muy bien equipado, en la planta baja, en la zona típica de Cap d'Oune. Aparcamiento cerca (estación). Pequeña terraza. Vista a las montañas, al pueblo y a la playa."
    ],
    "nl": [
      "Charmante studio, zeer goed uitgerust, op de begane grond, in de typische wijk Cap d'Oune. Parkeerplaats in de buurt (station). Klein terras. Uitzicht op de bergen, het dorp en het strand."
    ]
  },
