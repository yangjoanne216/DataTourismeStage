  "rdfs:comment": {
    "de": [
      "Familienrestaurant am Flussufer, wo Qualität und Einfachheit auf der Speisekarte angeboten werden. Ideal für Kinder (Spielplatz, geschlossene Terrasse). Service bis 14 Uhr und 22 Uhr."
    ],
    "en": [
      "Family restaurant located on the riverside where quality and simplicity are on the menu. Ideal for children (playground, closed terrace). Service provided until 2pm and 10pm."
    ],
    "it": [
      "Ristorante a conduzione familiare situato in riva al fiume, dove qualità e semplicità sono il menù. Ideale per i bambini (parco giochi, terrazza chiusa). Servizio fornito fino alle 14:00 e alle 22:00."
    ],
    "fr": [
      "Restaurant familial situé en bord de rivière où qualité et simplicité sont proposées au menu. Idéal pour les enfants (aire de jeux, terrasse fermée). Service assuré jusqu'à 14h et 22h."
    ],
    "es": [
      "Restaurante familiar situado junto al río donde la calidad y la sencillez están en el menú. Ideal para los niños (parque infantil, terraza cerrada). El servicio se presta hasta las 14 y las 22 horas."
    ],
    "nl": [
      "Familierestaurant aan de rivier waar kwaliteit en eenvoud op het menu staan. Ideaal voor kinderen (speeltuin, gesloten terras). Dienst verleend tot 14 en 22 uur."
    ]
  },
