  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/eec0a3dc-66fe-34af-bc46-d5f2bb62e88d",
      "dc:description": {
        "de": [
          "Restaurant im Stadtzentrum gelegen. Käsespezialitäten: Steinpilzfondue, Pyrenäen-Raclette, Raclette des Gaves... Fleischfondues. Desserts nach Art des Hauses. Im Sommer geschlossen."
        ],
        "en": [
          "Restaurant located in the town centre. Cheese specialities: cep fondue, Pyrenean raclette, raclette des gaves... Meat fondues. Home-made desserts. Closed in summer."
        ],
        "it": [
          "Ristorante situato nel centro della città. Specialità casearie: fonduta di porcini, raclette dei Pirenei, raclette des gaves? Fondute di carne. Dolci fatti in casa. Chiuso in estate."
        ],
        "fr": [
          "Restaurant situé en centre ville. Spécialités de fromage : fondue aux cèpes, raclette pyrénéenne, raclette des gaves… Fondues aux viandes. Desserts maison. Fermé l'été."
        ],
        "es": [
          "Restaurante situado en el centro de la ciudad. Especialidades de queso: fondue de cep, raclette pirenaica, raclette des gaves? Fondues de carne. Postres caseros. Cerrado en verano."
        ],
        "nl": [
          "Restaurant gelegen in het stadscentrum. Kaasspecialiteiten: eekhoorntjesbrood, raclette uit de Pyreneeën, raclette des gaves... Vleesfondues. Zelfgemaakte desserts. Gesloten in de zomer."
        ]
      },
