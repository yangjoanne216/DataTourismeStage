  "rdfs:comment": {
    "de": [
      "10 Uhr: Angelwettbewerb, Wanderung\n12 Uhr: Picknick aus der Tasche - Grill zur Verfügung\n15 Uhr: Wettbewerb im Boulespiel\nDen ganzen Tag über: Getränkeausschank und verschiedene Animationen am Ufer des Flusses Auvézère."
    ],
    "en": [
      "10 a.m. : fishing contest, walking tour\n12 h : picnic - barbecue available\n3 p.m. : petanque contest\nAll day long : refreshment stand and various animations on the banks of the Auvézère."
    ],
    "it": [
      "ore 10: gara di pesca, visita a piedi\nore 12: picnic - barbecue a disposizione\nore 15.00: gara di bocce\nPer tutto il giorno: rinfreschi e attività varie sulle rive dell'Auvézère."
    ],
    "fr": [
      "10 h : concours de pêche, randonnée pédestre\n12 h : pique-nique tiré du sac - barbecue à disposition\n15 h : concours de pétanque\nToute la journée : buvette et animations diverses au bord de l'Auvézère."
    ],
    "nl": [
      "10 uur: viswedstrijd, wandeling\n12u: picknick - barbecue beschikbaar\n15 uur: petanquewedstrijd\nDe hele dag: versnaperingen en diverse activiteiten aan de oevers van de Auvézère."
    ],
    "es": [
      "10 h: concurso de pesca, recorrido a pie\n12h: picnic - barbacoa disponible\n15.00 h: concurso de petanca\nTodo el día: refrescos y actividades diversas a orillas del Auvézère."
    ]
  },
