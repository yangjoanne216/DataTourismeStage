  "rdfs:comment": {
    "de": [
      "Im Square Pierre Leroux erwarten Sie Holztische mit Kinderspielen für Ihr Picknick mit der Familie oder mit Freunden."
    ],
    "en": [
      "Wooden tables in Square Pierre Leroux welcome you for picnics with family and friends, and children's games."
    ],
    "it": [
      "Nella piazza Pierre Leroux ci sono tavoli di legno per i picnic con la famiglia e gli amici e giochi per bambini."
    ],
    "fr": [
      "Des tables en bois vous accueillent dans le Square Pierre Leroux pour votre pique nique en famille ou entre amis, avec des jeux d'enfants."
    ],
    "nl": [
      "Er zijn houten tafels op het plein Pierre Leroux om te picknicken met familie en vrienden, en er zijn spelletjes voor kinderen."
    ],
    "es": [
      "En la plaza Pierre Leroux hay mesas de madera para hacer picnics con la familia y los amigos, y juegos infantiles."
    ]
  },
