  "rdfs:comment": {
    "de": [
      "Traditionelles Restaurant mit hausgemachten Speisen im oberen Teil des Dorfes, in der Nähe der Chapelle des Pénitents Blancs.\r\nSnacks zum Verkauf vor Ort und zum Mitnehmen."
    ],
    "en": [
      "Traditional restaurant, home-made cooking located at the top of the village, near the White Penitents Chapel.\r\nSnack sale on the spot and to take away."
    ],
    "it": [
      "Ristorante tradizionale, cucina casalinga situato nella parte alta del villaggio, vicino alla Chapelle des Pénitents Blancs.\r\nSnack bar in vendita sul posto e da asporto."
    ],
    "fr": [
      "Restauration traditionnel, cuisine fait maison situé en haut du village, près de la Chapelle des Pénitents Blancs.\r\nSnack vente sur place et à emporter."
    ],
    "nl": [
      "Traditioneel restaurant, huisgemaakte keuken gelegen op de top van het dorp, vlakbij de Chapelle des Pénitents Blancs.\r\nSnackbar te koop ter plaatse en om mee te nemen."
    ],
    "es": [
      "Restaurante tradicional, de cocina casera, situado en lo alto del pueblo, cerca de la Chapelle des Pénitents Blancs.\r\nBarra de aperitivos a la venta en el lugar y para llevar."
    ]
  },
