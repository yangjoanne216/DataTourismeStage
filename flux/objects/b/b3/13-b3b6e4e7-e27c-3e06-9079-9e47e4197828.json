  "rdfs:comment": {
    "de": [
      "Traditionelle Zucht von Alpenziegen, Verkostung und Direktverkauf von Ziegenkäse auf dem Hof (Frischkäse, gereifter Käse und Gewürzkäse, Ziegenkäse, Ziegenwurst und Chorizo) ."
    ],
    "en": [
      "Traditional breeding of alpine goats.\r\nTasting of cheeses goat and sale at the farm or in the sunday morning market in Rosans."
    ],
    "it": [
      "Allevamento tradizionale di capre alpine, degustazione e vendita diretta in azienda di formaggi di capra (freschi, stagionati e speziati, faisselles, salsicce e chorizo di capra)."
    ],
    "fr": [
      "Elevage traditionnel de chèvres alpines, dégustation et vente directe à la ferme de fromages de chèvres (fromages frais, maturés et aux épices, faisselles, saucissons et chorizo de chèvres) ."
    ],
    "nl": [
      "Traditioneel fokken van alpengeiten, proeven en directe verkoop op de boerderij van geitenkazen (verse, gerijpte en gekruide kazen, faisselles, worsten en geitenchorizo)."
    ],
    "es": [
      "Cría tradicional de cabras alpinas, degustación y venta directa en la granja de quesos de cabra (frescos, curados y con especias, faisselles, salchichas y chorizo de cabra)."
    ]
  },
