  "rdfs:comment": {
    "de": [
      "Das Restaurant Le Coin Catalan liegt im Herzen des kleinen mittelalterlichen Dorfes Castelnou und bietet Ihnen Teller und Schiefertafeln voller origineller Aromen."
    ],
    "en": [
      "Located in the heart of the small medieval village of Castelnou, the restaurant Le Coin Catalan will make you discover its plates and its slates full of original flavors."
    ],
    "it": [
      "Situato nel cuore del piccolo borgo medievale di Castelnou, il ristorante Le Coin Catalan vi farà scoprire i suoi piatti e le sue tavole piene di sapori originali."
    ],
    "fr": [
      "Situé au coeur du petit village médiéval de Castelnou, le restaurant Le Coin Catalan vous fera découvrir ses assiettes et ses ardoises pleines de saveurs originales."
    ],
    "es": [
      "Situado en el corazón del pequeño pueblo medieval de Castelnou, el restaurante Le Coin Catalan le hará descubrir sus platos y sus pizarras llenas de sabores originales."
    ],
    "nl": [
      "Gelegen in het hart van het kleine middeleeuwse dorp Castelnou, zal het restaurant Le Coin Catalan u zijn borden en zijn leistenen vol originele smaken laten ontdekken."
    ]
  },
