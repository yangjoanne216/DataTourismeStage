  "rdfs:comment": {
    "de": [
      "3 akrobatische Hochseilgärten, in denen man sich von Baum zu Baum über zahlreiche Workshops bewegen kann (Steigbügel, Fässer, Affenbrücke, Kabel, Trapez...)."
    ],
    "en": [
      "3 acrobatic courses in height to evolve from trees to trees on multiple workshops (Stirrups, barrels, monkey bridge, cables, trapeze...)."
    ],
    "it": [
      "3 percorsi acrobatici in altezza per spostarsi da un albero all'altro su più laboratori (staffe, barili, ponte delle scimmie, cavi, trapezio...)."
    ],
    "fr": [
      "3 parcours acrobatiques en hauteur pour évoluer d’arbres en arbres sur de multiples ateliers (Étriers, tonneaux, pont de singe, câbles, trapèze...)."
    ],
    "es": [
      "3 recorridos acrobáticos en altura para pasar de árbol en árbol sobre múltiples talleres (estribos, barriles, puente mono, cables, trapecio...)."
    ],
    "nl": [
      "3 acrobatische parcours in de hoogte om van boom naar boom te gaan op meerdere werkplaatsen (stijgbeugels, tonnen, apenbrug, kabels, trapeze...)."
    ]
  },
