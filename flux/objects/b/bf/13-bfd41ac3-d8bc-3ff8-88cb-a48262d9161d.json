  "rdfs:comment": {
    "de": [
      "Der Campingplatz besteht aus zwei Lagern: \r\n- Einem im Norden: Das Camp du Paradou, das vom 1. April bis zum 6. November geöffnet ist,\r\n- und eines im Süden: Le Camp des Oliviers, das das ganze Jahr über geöffnet ist."
    ],
    "en": [
      "Campsite with 2 camps : \r\n- one in the North: Le camp du Paradou open from April 1st to November 6th,\r\n- and one in the South : Le Camp des Oliviers open all year round."
    ],
    "it": [
      "Campeggio con 2 campi: \r\n- uno nel nord: Le camp du Paradou aperto dal primo di aprile al 6 di novembre,\r\n- e uno nel sud: Le Camp des Oliviers aperto tutto l'anno."
    ],
    "fr": [
      "Camping comprenant 2 camps : \r\n- un au Nord : Le camp du Paradou ouvert du 1er Avril au 6 Novembre,\r\n- et un au Sud : Le Camp des Oliviers ouvert à l'année."
    ],
    "nl": [
      "Camping met 2 kampen \r\n- een in het noorden : Le camp du Paradou geopend van 1 april tot 6 november,\r\n- en één in het zuiden: Le Camp des Oliviers, het hele jaar geopend."
    ],
    "es": [
      "Camping con 2 campamentos \r\n- uno en el Norte: Le camp du Paradou abierto del 1 de abril al 6 de noviembre,\r\n- y uno en el Sur: Le Camp des Oliviers abierto todo el año."
    ]
  },
