  "rdfs:comment": {
    "de": [
      "Bar-Restaurant, das das ganze Jahr über geöffnet ist, mit einer großen Terrasse auf dem Cours Julien. Es bietet schnelle Küche (Kuchen, Hamburger , Salate...) und veranstaltet Ausstellungen und Konzerte."
    ],
    "en": [
      "Bar restaurant open all year round, with a large terrace on the Cours Julien. It offers fast food (pies, hamburgers, salads...) and hosts exhibitions and concerts."
    ],
    "it": [
      "Bar ristorante aperto tutto l'anno, con un'ampia terrazza sul Cours Julien. Offre fast food (torte, hamburger, insalate...) e ospita mostre e concerti."
    ],
    "fr": [
      "Bar restaurant ouvert toute l'année, avec une grande terrasse sur le cours Julien. Il propose une cuisine rapide (tartes, hamburgers , salades...) et accueille des expositions, des concerts."
    ],
    "es": [
      "Bar restaurante abierto todo el año, con una gran terraza en el Cours Julien. Ofrece comida rápida (tartas, hamburguesas, ensaladas...) y acoge exposiciones y conciertos."
    ],
    "nl": [
      "Bar restaurant het hele jaar geopend, met een groot terras op de Cours Julien. Het biedt fastfood (taarten, hamburgers, salades...) en organiseert tentoonstellingen en concerten."
    ]
  },
