  "rdfs:comment": {
    "de": [
      "Anemone bietet einen atemberaubenden Blick auf das Dorf und das Schloss. Das Ferienhaus bietet eine geräumige Unterkunft auf zwei Etagen für bis zu fünf Personen."
    ],
    "en": [
      "Anemone offers a breathtaking view of the village and the castle. The gîte offers spacious accommodation on two floors that can accommodate up to five people."
    ],
    "it": [
      "Anemone offre una vista mozzafiato sul villaggio e sul castello. Il gîte offre una sistemazione spaziosa su due piani per un massimo di cinque persone."
    ],
    "fr": [
      "Anemone offre une vue imprenable sur le village et le château. Le gîte propose un hébergement spacieux sur deux étages pouvant accueillir jusqu'à cinq personnes."
    ],
    "es": [
      "Anemone ofrece una vista impresionante del pueblo y del castillo. La casa rural ofrece un amplio alojamiento en dos plantas para un máximo de cinco personas."
    ],
    "nl": [
      "Anemone biedt een adembenemend uitzicht op het dorp en het kasteel. De gîte biedt ruime accommodatie op twee verdiepingen voor maximaal vijf personen."
    ]
  },
