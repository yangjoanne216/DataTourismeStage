  "rdfs:comment": {
    "de": [
      "Brasserie mit Terrasse im Herzen des Dorfes, am Fuße der Kirche. Genießen Sie hier eine provenzalische Küche mit saisonalen Gerichten. Spezialität: Aioli."
    ],
    "en": [
      "Brasserie with terrace located in the heart of the village, at the foot of the church, you will enjoy a Provencal cuisine composed of seasonal dishes. Speciality: aïoli."
    ],
    "it": [
      "Brasserie con terrazza situata nel cuore del villaggio, ai piedi della chiesa, potrete gustare una cucina provenzale composta da piatti di stagione. Specialità: aïoli."
    ],
    "fr": [
      "Brasserie avec terrasse située au coeur du village, au pied de l'église, vous y dégusterez une cuisine provençale composée de plats de saison. Spécialité : aïoli."
    ],
    "nl": [
      "Brasserie met terras gelegen in het hart van het dorp, aan de voet van de kerk, geniet u van een Provençaalse keuken samengesteld uit seizoensgebonden gerechten. Specialiteit: aïoli."
    ],
    "es": [
      "Brasserie con terraza situada en el corazón del pueblo, a los pies de la iglesia, podrá disfrutar de una cocina provenzal compuesta de platos de temporada. Especialidad: aïoli."
    ]
  },
