  "rdfs:comment": {
    "de": [
      "Probieren Sie die köstlichen Teller von Marylou, die aus hochwertigen lokalen Produkten bestehen!"
    ],
    "en": [
      "Come and taste Marylou's delicious plates, made with quality local products!"
    ],
    "it": [
      "Venite a gustare i deliziosi piatti di Marylou, realizzati con prodotti locali di qualità!"
    ],
    "fr": [
      "Venez déguster les délicieuses assiettes de Marylou, composées de produits locaux de qualité !"
    ],
    "es": [
      "Venga a degustar los deliciosos platos de Marylou, elaborados con productos locales de calidad"
    ],
    "nl": [
      "Kom en proef Marylou's heerlijke gerechten, gemaakt met lokale kwaliteitsproducten!"
    ]
  },
