  "rdfs:comment": {
    "de": [
      "Kleine und große Spaziergänge auf Eseln (bis zu 50 kg) in Begleitung Ihrer Führerin Danièle. Möglichkeit von Workshops \"Putzen-Bürsten-Füttern\". Aktivität für Erwachsene und Kinder."
    ],
    "en": [
      "Small and large donkey rides (up to 50 kg) with your guide Danièle. Possibility of \"grooming-brushing-feeding\" workshops. Activity for adults and children."
    ],
    "it": [
      "Passeggiate brevi e lunghe a dorso d'asino (fino a 50 kg) con la guida Danièle. Possibilità di laboratori di \"grooming-brushing-feeding\". Attività per adulti e bambini."
    ],
    "fr": [
      "Petites et grandes balades à dos d'ânes (jusqu'à 50 kg) en compagnie de votre guide Danièle. Possibilité d'ateliers \"pansage-brossage-nourrissage\". Activité pour adulte et enfant."
    ],
    "es": [
      "Paseos cortos y largos en burro (hasta 50 kg) con su guía Danièle. Posibilidad de talleres de \"aseo-cepillado-alimentación\". Actividades para adultos y niños."
    ],
    "nl": [
      "Korte en lange ezeltochten (tot 50 kg) met je gids Danièle. Mogelijkheid tot workshops \"verzorgen-borstelen-voeden\". Activiteiten voor volwassenen en kinderen."
    ]
  },
