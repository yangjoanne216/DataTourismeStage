      "dc:description": {
        "de": [
          "Unterkunft in einem ehemaligen Bauernhaus. Veranda, große Grünfläche, ein Schlafzimmer und ein Hauptraum mit 2 zusätzlichen Schlafplätzen."
        ],
        "en": [
          "Gîte fitted out in an old farmhouse. Veranda, large green area, a bedroom and a main room with 2 extra beds."
        ],
        "it": [
          "Gîte allestito in un'antica casa colonica. Veranda, ampio spazio verde, una camera da letto e una camera principale con 2 letti supplementari."
        ],
        "fr": [
          "Gîte aménagé dans un ancien corps de ferme. Véranda, grand espace vert, une chambre et une pièce principale avec 2 couchages supplémentaires."
        ],
        "nl": [
          "Gîte ingericht in een oude boerderij. Veranda, grote groene ruimte, een slaapkamer en een hoofdkamer met 2 extra bedden."
        ],
        "es": [
          "Casa rural habilitada en una antigua granja. Veranda, gran espacio verde, un dormitorio y una habitación principal con 2 camas supletorias."
        ]
