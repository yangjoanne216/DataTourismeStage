  "rdfs:comment": {
    "de": [
      "Méjanassère, ein Gästebauernhof, die schönen Steinhäuser beherbergen Ferienwohnungen, Zimmer und Gästetisch. Die Besitzer, die Winzer sind, freuen sich auf Ihren Besuch."
    ],
    "en": [
      "Méjanassère, farm of reception, the beautiful stone houses, shelter lodgings, rooms and table of hosts. The winegrower owners will be happy to welcome you."
    ],
    "it": [
      "Méjanassère, una fattoria accogliente, le belle case in pietra, le gîtes, le camere e la table d'hôtes. I proprietari dei vigneti saranno lieti di accogliervi."
    ],
    "fr": [
      "Méjanassère, ferme d'accueil, les belles maisons de pierres, abritent gîtes, chambres et table d'hôtes. Les propriétaires vignerons seront heureux de vous accueillir."
    ],
    "es": [
      "Méjanassère, una granja acogedora, las hermosas casas de piedra, casas rurales, habitaciones y una mesa d'hôtes. Los viticultores estarán encantados de recibirle."
    ],
    "nl": [
      "Méjanassère, een gastvrije boerderij, de prachtige stenen huizen, huis gîtes, kamers en een table d'hôtes. De wijnboeren zullen u graag ontvangen."
    ]
