  "rdfs:comment": {
    "de": [
      "Segeln Sie an Bord eines Gabarot de Loire, das von der Association des Chalandoux du Vème Vent gebaut wurde, durch das Wasser und die Musik!"
    ],
    "en": [
      "Sail along the water and the music on board a Loire barges built by the Association des Chalandoux du Vème Vent!"
    ],
    "it": [
      "Navigate insieme all'acqua e alla musica a bordo di una chiatta della Loira costruita dall'Association des Chalandoux du Vème Vent!"
    ],
    "fr": [
      "Voguez au fil de l'eau et de la musique à bord d’un gabarot de Loire construit par l’Association des Chalandoux du Vème Vent !"
    ],
    "nl": [
      "Vaar mee met het water en de muziek aan boord van een Loire schuit gebouwd door de Association des Chalandoux du Vème Vent!"
    ],
    "es": [
      "Navegue con el agua y la música a bordo de una gabarra del Loira construida por la Association des Chalandoux du Vème Vent"
    ]
  },
