  "rdfs:comment": {
    "de": [
      "Saint-Germain-sur-Rhône ist eine kleine ländliche Gemeinde in der Region Rhône-Alpes und liegt im Bezirk Haute-Savoie. Die Gemeinde gehört zum Kanton Seyssel und zum Arrondissement Saint-Julien-en-Genevois."
    ],
    "en": [
      "Saint-Germain-sur-Rhône is a small rural commune in the Rhône-Alpes region, located in Haute-Savoie. The commune is part of the canton of Seyssel and the district of Saint-Julien-en-Genevois."
    ],
    "it": [
      "Saint-Germain-sur-Rhône è un piccolo comune rurale della regione Rodano-Alpi, situato in Alta Savoia. Il comune fa parte del cantone di Seyssel e dell'arrondissement di Saint-Julien-en-Genevois."
    ],
    "fr": [
      "Saint-Germain-sur-Rhône est une petite commune rurale de la région Rhône-Alpes, située en Haute-Savoie. La commune fait partie du canton de Seyssel et de l'arrondissement de Saint-Julien-en-Genevois."
    ],
    "nl": [
      "Saint-Germain-sur-Rhône is een kleine plattelandsgemeente in de regio Rhône-Alpes, gelegen in de Haute-Savoie. De gemeente maakt deel uit van het kanton Seyssel en het arrondissement Saint-Julien-en-Genevois."
    ],
    "es": [
      "Saint-Germain-sur-Rhône es un pequeño municipio rural de la región de Ródano-Alpes, situado en la Alta Saboya. El municipio forma parte del cantón de Seyssel y del distrito de Saint-Julien-en-Genevois."
    ]
  },
