  "rdfs:comment": {
    "de": [
      "Familiencampingplatz mit Pool, Sportplatz, und Restaurant (La Paillote). Nah am Fluss gelegen, am Anfang der Ardècheschlucht, 900m vom Dorf, idealer Startpunkt für eine Kanufahrt durch die Schlucht."
    ],
    "en": [
      "Family campsite with a pool, a multi-sport area and a restaurant (La Paillote). Located by the river at the mouth Ardèche Gorges, 900m from the village, the perfect base for a canoe trip down the Ardèche Gorges."
    ],
    "it": [
      "Un campeggio familiare con piscina e campo polisportivo, sul bordo del fiume all'ingresso delle Gorges de l'Ardèche, a 900 m dal villaggio, ideale per iniziare la discesa delle Gorges in canoa."
    ],
    "fr": [
      "Camping familial équipé d'une piscine, d'un terrain multisports , en bordure de rivière à l'entrée des Gorges de l'Ardèche, à 900m du village, idéal pour le départ de la descente des Gorges en canoës."
    ],
    "es": [
      "Un camping familiar con piscina y terreno polideportivo, a orillas del río a la entrada de las Gargantas del Ardèche, a 900 m del pueblo, ideal para iniciar el descenso de las Gargantas en canoa."
    ],
    "nl": [
      "Familiecamping met zwembad en multisportterrein, aan de rand van de rivier bij de ingang van de Gorges de l'Ardèche, op 900m van het dorp, ideaal om de afdaling van de Gorges in kano's te beginnen."
    ]
  },
