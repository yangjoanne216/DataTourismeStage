  "rdfs:comment": {
    "de": [
      "Ein Verleihservice für Fahrräder mit elektrischer Unterstützung (VAE). 4 Fahrräder für Erwachsene (in Partnerschaft mit tempo vélo) gibt es während der Sommersaison am Empfang in Agen."
    ],
    "en": [
      "A rental service for electrically assisted bicycles (EAB). 4 adult bikes (in partnership with tempo vélo) are available at the Agen reception desk during the summer season."
    ],
    "it": [
      "Un servizio di noleggio di biciclette a pedalata assistita (VAE). 4 biciclette per adulti (in collaborazione con tempo vélo) sono disponibili presso la reception di Agen durante la stagione estiva."
    ],
    "fr": [
      "Un service de location de vélos à assistance électrique (VAE). 4 vélos adultes (en partenariat avec tempo vélo) est en place à l'accueil d'Agen pendant la saison estivale."
    ],
    "es": [
      "Un servicio de alquiler de bicicletas con asistencia eléctrica (VAE). 4 bicicletas para adultos (en colaboración con tempo vélo) está disponible en la recepción de Agen durante la temporada de verano."
    ],
    "nl": [
      "Een verhuurdienst voor elektrisch ondersteunde fietsen (VAE). 4 fietsen voor volwassenen (in samenwerking met tempo vélo) is beschikbaar bij de receptie van Agen tijdens het zomerseizoen."
    ]
  },
