      "dc:description": {
        "de": [
          "Einfamilienhaus mit 2 Wohnungen 5 km von Lourdes entfernt, Wohnung 65 m2, Erdgeschoss +1 Stockwerk, Küche, Gefrierschrank, 3Schlafzimmer: 3 Betten 140 cm, , Dusche, WC, Farbfernseher, Mikrowellengerät, Grill, Gartenmöbel, Garten, Privatraum, Überdachter Parkplatz."
        ],
        "en": [
          "Detached house with 2 apartments at 5 km from Lourdes, Apartment 65 m2, ground floor + 1 floor, Kitchen, freezer, 3 bedrooms: 3 beds 140 cm, shower, toilet, color TV, microwave, barbecue, garden furniture, garden, private room, covered parking."
        ],
        "it": [
          "Casa indipendente con 2 appartamenti, a 5 km da Lourdes, Appartamento 65 m2, Piano terra + 1 piano, Cucina, congelatore, 3 camere: 3 letti 140 cm, doccia, WC, TV color, Microonde, barbecue, Mobili da giardino, Giardino, locale privato, Parcheggio coperto."
        ],
        "fr": [
          "Maison individuelle avec 2 appartements à 5 km de Lourdes., Appartement 65 m2, RDC +1 étage, Cuisine, congélateur, 3chambres : 3 lits en 140 cm, , douche, WC, TV Couleur, Micro-Ondes, barbecue, Salon de Jardin, Jardin, local privé, Parking couvert."
        ],
        "nl": [
          "Vrijstaand huis met 2 woningen, 5 km van Lourdes, Appartement 65 m2, Begane grond + 1 verdieping, Keuken, vriezer, 3 slaapkamers: 3 bedden 140 cm, douche, WC, Kleuren TV, Magnetron, barbecue, Tuinmeubels, Tuin, privé kamer, Overdekte parkeerplaats."
        ],
        "es": [
          "Casa independiente con 2 pisos, a 5 km de Lourdes, Apartamento 65 m2, Planta baja + 1 piso, Cocina, congelador, 3 dormitorios: 3 camas 140 cm, ducha, WC, TV color, Microondas, barbacoa, Muebles de jardín, Jardín, habitación privada, Aparcamiento cubierto."
        ]
      },
