      "dc:description": {
        "de": [
          "Am Fuß der Pisten gelegen",
          "Studio für 5 Personen - Residenz Ramondia\nWohnzimmer mit Bz - ausgestattete Küchenecke - Schlafbereich mit 1 Etagenbett und Schubladenbett (90 cm) - Badezimmer - separates WC."
        ],
        "en": [
          "Situated at the foot of ski runs",
          "Studio for 5 people - Ramondia residence - Living room wtih sofa bed (2 people) - equipped kitchen area - corner harms with bunk beds and another single bed - bathroom and toilets separated."
        ],
        "it": [
          "Situato ai piedi delle piste",
          "Studio 5 posti - Residenza Ramondia\nSoggiorno con bz - angolo cottura attrezzato - zona notte con 1 letto a castello e 1 letto estraibile (90 cm) - bagno - WC separato."
        ],
        "fr": [
          "Situé en pied de pistes",
          "Studio 5 places - Résidence Ramondia\nSéjour avec bz - coin cuisine équipé - coin nuit avec 1 lit superposé et lit tiroir (90 cm) - salle de bain - wc séparé."
        ],
        "nl": [
          "Gelegen aan de voet van de pistes",
          "Studio 5 plaatsen - Residence Ramondia\nWoonkamer met bz - ingerichte keukenhoek - slaapgedeelte met 1 stapelbed en 1 uitschuifbaar bed (90 cm) - badkamer - apart toilet."
        ],
        "es": [
          "Apartamento para 5 personas . Résidencia Ramondia\nSalón con sofá - cocina equipada - puerta de separación entre el espacio para dormir y el salón - zona de descanso con 3 literas - cuarto de baño - Aseos separados.",
          "Situado al pie de las pistas"
        ]
      },
