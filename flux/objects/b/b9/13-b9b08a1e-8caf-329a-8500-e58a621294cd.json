  "rdfs:comment": {
    "de": [
      "Servicebereich mit Nachtparken möglich. 7 Stellplätze. Poller Euro-relais. Wasser zum Trinken. Entleerung von Abwasser, WC-Kassette. Tische für Picknicks. Tennis in der Nähe. Restaurant in der Nähe (100 m). Kostenlose Parkplätze."
    ],
    "en": [
      "Service area with night parking possible. 7 places. Euro-relay terminal. Drinking water. Waste water disposal, WC cassette. Picnic tables. Tennis court nearby. Restaurant nearby (100 meters). Free parking."
    ],
    "it": [
      "Area di servizio con parcheggio notturno disponibile. 7 posti. Terminale Euro-relay. Acqua potabile. Smaltimento delle acque reflue, cassetta WC. Tavoli da picnic. Campo da tennis nelle vicinanze. Ristorante nelle vicinanze (100 metri). Parcheggio gratuito."
    ],
    "fr": [
      "Aire de services avec stationnement nuit possible. 7 emplacements. Borne Euro-relais. Eau potable. Vidange eaux usées, cassette WC. Tables de pique-nique. Tennis à proximité. Restaurant à proximité (100 mètres). Stationnement gratuit."
    ],
    "es": [
      "Área de servicio con aparcamiento nocturno disponible. 7 plazas. Terminal Euro-relé. Agua potable. Desagüe, WC cassette. Mesas de picnic. Pista de tenis cercana. Restaurante cercano (100 metros). Aparcamiento gratuito."
    ],
    "nl": [
      "Dienstruimte met overnachtingsmogelijkheid. 7 plaatsen. Euro-relais terminal. Drinkwater. Riolering, WC-cassette. Picknicktafels. Tennisbaan in de buurt. Restaurant in de buurt (100 meter). Gratis parkeren."
    ]
  },
