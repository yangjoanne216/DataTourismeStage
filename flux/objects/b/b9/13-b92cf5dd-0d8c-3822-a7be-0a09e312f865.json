  "rdfs:comment": {
    "de": [
      "Gegenüber der Kirche Notre-Dame können Sie eine authentische Küche genießen. Probieren Sie die lokalen Produkte von Erzeugern aus den Bergen von Bourbonnaise."
    ],
    "en": [
      "In front of the Notre-Dame church, come and taste an authentic cuisine, come and taste the local products of producers from the Bourbonnais mountains."
    ],
    "it": [
      "Di fronte alla chiesa di Notre-Dame, venite a gustare la cucina autentica e i prodotti locali dei produttori delle montagne del Bourbonnais."
    ],
    "fr": [
      "Face à l'église Notre-Dame, venez goûter une cuisine authentique, venez déguster les produits locaux de producteurs de la montagne bourbonnaise."
    ],
    "nl": [
      "Kom tegenover de Notre-Dame kerk authentieke gerechten en streekproducten proeven van producenten uit de bergen van Bourbonnais."
    ],
    "es": [
      "Frente a la iglesia de Notre-Dame, venga a degustar la auténtica cocina y los productos locales de los productores de las montañas de Bourbonnais."
    ]
  },
