  "rdfs:comment": {
    "de": [
      "Charmante Unterkunft, 65 m2 in einem typisch elsässischen Haus. Im historischen Zentrum von Kaysersberg gelegen. Vollständig renoviert mit Holztäfelungen und Fachwerk in allen Räumen."
    ],
    "en": [
      "Charming gite, 65 m2 in a typical Alsatian house. Located in the historical center of Kaysersberg. Completely renovated with woodwork and half-timbering in all rooms."
    ],
    "it": [
      "Incantevole gîte di 65 m2 in una tipica casa alsaziana. Situato nel centro storico di Kaysersberg. Completamente rinnovato con boiserie e mezzane in tutte le stanze."
    ],
    "fr": [
      "Gîte de charme, 65 m2 dans une maison typiquement alsacienne. Situé dans le centre historique de Kaysersberg. Entièrement rénové avec boiseries et colombages dans toutes les pièces."
    ],
    "nl": [
      "Charmante gîte, 65 m2 in een typisch Elzasser huis. Gelegen in het historische centrum van Kaysersberg. Volledig gerenoveerd met houtwerk en vakwerk in alle kamers."
    ],
    "es": [
      "Casa rural con encanto, 65 m2 en una casa típica alsaciana. Situado en el centro histórico de Kaysersberg. Completamente renovado con carpintería y entramado de madera en todas las habitaciones."
    ]
  },
