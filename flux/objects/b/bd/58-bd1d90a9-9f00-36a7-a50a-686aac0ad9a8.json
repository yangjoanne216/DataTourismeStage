  "rdfs:comment": {
    "de": [
      "Die Aufgabe der Archives nationales d'outre-mer (ANOM) ist es, die Archive der französischen Kolonialexpansion zu bewahren."
    ],
    "en": [
      "The National Overseas Archives, established in Aix-en-Provence in 1966, keep documents relating to the French colonial presence overseas, except for in Morocco and Tunisia, the archives of these countries being kept at the Ministry of Foreign Affairs."
    ],
    "it": [
      "La missione dell'Archivio Nazionale d'Oltremare (ANOM) è quella di conservare gli archivi dell'espansione coloniale francese."
    ],
    "fr": [
      "Les Archives nationales d’outre-mer (ANOM) ont pour mission la conservation des archives de l’expansion coloniale française."
    ],
    "es": [
      "La misión de los Archivos Nacionales de Ultramar (ANOM) es preservar los archivos de la expansión colonial francesa."
    ],
    "nl": [
      "De missie van het Nationaal Overzees Archief (ANOM) is het bewaren van de archieven van de Franse koloniale expansie."
    ]
