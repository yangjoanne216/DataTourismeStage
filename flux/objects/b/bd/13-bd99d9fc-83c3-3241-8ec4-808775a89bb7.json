  "rdfs:comment": {
    "de": [
      "Von Saint-Nicolas-des-Biefs oder La Chabanne aus führt dieser Rundweg über die Höhen der Montagne Bourbonnaise durch Wälder und Heidekrautfelder zum Plateau de la Verrerie."
    ],
    "en": [
      "Starting from Saint-Nicolas-des-Biefs or La Chabanne, this loop goes through the heights of the Montagne Bourbonnaise through forests and heather fields to the plateau of La Verrerie."
    ],
    "it": [
      "Partendo da Saint-Nicolas-des-Biefs o da La Chabanne, questo anello attraversa le alture della Montagne Bourbonnaise, passando per boschi e campi di erica fino all'altopiano della Verrerie."
    ],
    "fr": [
      "Au départ de Saint-Nicolas-des-Biefs ou de La Chabanne, cette boucle parcourt les hauteurs de la Montagne Bourbonnaise à travers forêts et champs de bruyère au plateau de la Verrerie."
    ],
    "nl": [
      "Vanuit Saint-Nicolas-des-Biefs of La Chabanne gaat deze lus door de hoogten van de Montagne Bourbonnaise door bossen en heidevelden naar het plateau de la Verrerie."
    ],
    "es": [
      "Partiendo de Saint-Nicolas-des-Biefs o de La Chabanne, este bucle atraviesa las alturas de la Montagne Bourbonnaise a través de bosques y campos de brezo hasta la meseta de la Verrerie."
    ]
  },
