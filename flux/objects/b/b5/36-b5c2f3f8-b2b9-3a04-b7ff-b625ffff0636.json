  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/020aabe5-d879-39e8-af4f-4f7e65bdf74c",
      "dc:description": {
        "de": [
          "Die Aromen von Bigorre beleben Ihren Gaumen...",
          "Traditionelle Produktion von Stopfenten. Direktverkauf von \"Aromen aus der Region\": Garbure, gekochte Bohnen aus Tarbais, Foie Gras, Confits, Rillettes, Pasteten..."
        ],
        "en": [
          "The flavours of Bigorre will delight your palate...",
          "Traditional production of fattened ducks. Direct sale of \"local flavours\": garbure, cooked Tarbais beans, foie gras, confits, rillettes, pâtés..."
        ],
        "it": [
          "I sapori della Bigorre delizieranno il vostro palato...",
          "Produzione tradizionale di anatre ingrassate. Vendita diretta di \"sapori locali\": garbure, fagioli di Tarbais cotti, foie gras, confits, rillettes, paté..."
        ],
        "fr": [
          "Les saveurs de Bigorre ravivent votre palais...",
          "Production traditionnelle de canards gras. Vente directe des \"saveurs du terroir\": garbure, haricots tarbais cuisinés, foie gras, confits, rillettes, pâtés..."
        ],
        "es": [
          "Los sabores de Bigorre harán las delicias de su paladar...",
          "Producción tradicional de patos de engorde. Venta directa de \"sabores locales\": garbure, judías cocidas de Tarbais, foie gras, confits, rillettes, patés..."
        ],
        "nl": [
          "De smaken van Bigorre zullen uw gehemelte verrukken...",
          "Traditionele productie van gemeste eenden. Directe verkoop van \"lokale smaken\": garbure, gekookte Tarbais bonen, foie gras, confits, rillettes, patés..."
        ]
      },
      "@type": [
        "Description"
