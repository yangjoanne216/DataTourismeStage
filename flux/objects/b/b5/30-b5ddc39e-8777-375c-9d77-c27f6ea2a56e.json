  "rdfs:comment": {
    "de": [
      "Handwerkliche Herstellung von normannischen Bieren durch die beiden Braumeister. Flaschen- und Fassbiere vom Fass: Weißbier, Blondbier, Bernsteinbier, Braunbier..."
    ],
    "en": [
      "Small-scale traditional Normandy beer making by two master brewers. Bottled and draught beers: white, lager, amber, dark ale."
    ],
    "it": [
      "Produzione artigianale di birre Norman da parte dei due mastri birrai. Birre in bottiglia e alla spina: bianche, bionde, ambrate, brune..."
    ],
    "fr": [
      "Fabrication artisanale de bières normandes par les deux maîtres brasseurs. Bières bouteilles et fûts pression : blanche, blonde, ambrée, brune..."
    ],
    "es": [
      "Producción artesanal de cervezas normandas por parte de los dos maestros cerveceros. Cervezas embotelladas y de barril: blanca, rubia, ámbar, marrón..."
    ],
    "nl": [
      "Ambachtelijke productie van Norman bieren door de twee meesterbrouwers. Gebottelde en getapte bieren: wit, blond, amber, bruin..."
    ]
  },
  "rdfs:label": {
    "en": [
      "Brasserie artisanale de La Lie"
    ],
    "fr": [
      "Brasserie artisanale de La Lie"
    ]
  },
