  "rdfs:comment": {
    "de": [
      "Gilles Ventalon, staatlich geprüfter Klavierbauer, ausgebildet am Institut Français de Facture Instrumentale, ist seit 1987 Kunsthandwerker, Klavierstimmer und -restaurator."
    ],
    "en": [
      "Gilles Ventalon, a state graduate, trained at the French Institute of Instrument Making, has been a piano tuner and restorer since 1987."
    ],
    "it": [
      "Gilles Ventalon, diplomato all'Institut Français de Facture Instrumentale, è accordatore e restauratore di pianoforti dal 1987."
    ],
    "fr": [
      "Gilles Ventalon, diplômé d'état, formé à l'Institut Français de Facture Instrumentale est depuis 1987 artisan d'art accordeur et restaurateur de piano."
    ],
    "es": [
      "Gilles Ventalon, licenciado en el Instituto Francés de Fabricación Instrumental, es afinador y restaurador de pianos desde 1987."
    ],
    "nl": [
      "Gilles Ventalon, afgestudeerd aan het Institut Français de Facture Instrumentale, is sinds 1987 pianostemmer en restaurateur."
    ]
