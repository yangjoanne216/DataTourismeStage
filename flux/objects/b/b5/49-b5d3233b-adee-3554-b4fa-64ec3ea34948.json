  "rdfs:comment": {
    "de": [
      "Gehen Sie mit Ihrer Familie an Bord eines eleganten und komfortablen Bootes, um bei Sonnenuntergang in der Bucht von Bourgneuf zu segeln."
    ],
    "en": [
      "Bring the whole family aboard an elegant and comfortable boat for a sunset cruise in the Bay of Bourgneuf."
    ],
    "it": [
      "Portate tutta la famiglia a bordo di una barca elegante e confortevole per una crociera al tramonto nella baia di Bourgneuf."
    ],
    "fr": [
      "Embarquez en famille à bord d’un bateau élégant et confortable pour une navigation au coucher du soleil dans la baie de Bourgneuf."
    ],
    "nl": [
      "Neem het hele gezin mee aan boord van een elegante en comfortabele boot voor een boottocht bij zonsondergang in de baai van Bourgneuf."
    ],
    "es": [
      "Lleve a toda la familia a bordo de un elegante y confortable barco para disfrutar de un crucero al atardecer por la bahía de Bourgneuf."
    ]
  },
