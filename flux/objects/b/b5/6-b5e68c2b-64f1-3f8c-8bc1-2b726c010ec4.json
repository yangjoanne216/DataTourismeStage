  "rdfs:comment": {
    "de": [
      "Als Wellness-Praktikerin in Pontailler-sur-Saône bietet Gaelle Ihnen an, sich Zeit zu nehmen, um neue Energie zu tanken und sich in einer sanften Atmosphäre zu entfalten."
    ],
    "en": [
      "Gaelle is a wellness practitioner in Pontailler-sur-Saône. She offers to take time to recharge your batteries and to blossom in a gentle atmosphere."
    ],
    "it": [
      "Gaelle è un'operatrice del benessere a Pontailler-sur-Saône e propone di prendersi del tempo per ricaricarsi e svilupparsi in un'atmosfera dolce."
    ],
    "fr": [
      "Praticienne en bien-être à Pontailler-sur-Saône, Gaelle propose de prendre du temps pour vous ressourcer, vous épanouir dans une ambiance de douceur."
    ],
    "nl": [
      "Gaelle is een wellness-beoefenaar in Pontailler-sur-Saône. Zij biedt u de tijd om u te herbronnen en uzelf te ontwikkelen in een zachte sfeer."
    ],
    "es": [
      "Gaelle es practicante de bienestar en Pontailler-sur-Saône. Le propone tomarse un tiempo para recargar las pilas y desarrollarse en un ambiente apacible."
    ]
  },
