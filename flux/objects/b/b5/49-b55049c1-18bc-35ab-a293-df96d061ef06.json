  "rdfs:comment": {
    "de": [
      "Ihr Gourmet-Gemüsegarten in La Pommeraye, 5 Minuten vom Ufer der Loire und einige Kilometer von Angers entfernt. Verkauf von Gemüse und Setzlingen. Pflücken von Erdbeeren und Himbeeren."
    ],
    "en": [
      "Your gourmet kitchen garden in La Pommeraye, 5 minutes from the banks of the Loire and a few kilometres from Angers. Sale of vegetables and seedlings. Strawberry and raspberry picking."
    ],
    "it": [
      "Il vostro orto gourmet a La Pommeraye, a 5 minuti dalle rive della Loira e a pochi chilometri da Angers. Vendita di ortaggi e piante. Raccolta di fragole e lamponi."
    ],
    "fr": [
      "Votre potager gourmand à la Pommeraye, à 5 minutes des bords de Loire et à quelques kilomètres d'Angers. Vente de légumes et plants. Cueillette de fraises et de framboises."
    ],
    "nl": [
      "Uw moestuin voor fijnproevers in La Pommeraye, op 5 minuten van de oevers van de Loire en op enkele kilometers van Angers. Verkoop van groenten en planten. Aardbeien en frambozen plukken."
    ],
    "es": [
      "Su huerto gastronómico en La Pommeraye, a 5 minutos de las orillas del Loira y a pocos kilómetros de Angers. Venta de verduras y plantas. Recogida de fresas y frambuesas."
    ]
  },
