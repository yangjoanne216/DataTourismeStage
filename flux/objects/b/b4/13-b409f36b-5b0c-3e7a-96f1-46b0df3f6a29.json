  "rdfs:comment": {
    "de": [
      "3* Logis de France-Hotel. Wellnessbereich mit Sauna und Hammam. Ruhige Lage am Flussufer, 900 m vom Zentrum entfernt. Großer Privatparkplatz und großer Garten. Zimmer mit Bad oder Dusche, separatem WC und TV."
    ],
    "en": [
      "Hotel 3* Logis de France. Wellness area with sauna and hammam. Quietly located on the riverside, 900m from the centre. Large private car park and garden. Rooms with bathroom or shower, separate WC and TV."
    ],
    "it": [
      "Hotel Logis de France 3*. Area benessere con sauna e hammam. Situato in una zona tranquilla sulle rive del fiume, a 900 m dal centro. Ampio parcheggio privato e giardino. Camere con bagno o doccia, servizi igienici separati e TV."
    ],
    "fr": [
      "Hôtel 3* Logis de France. Espace bien-être avec sauna et hammam. Situé au calme en bordure de rivière à 900m du centre. Grand parking et grand jardin  privés. Chambres avec salle de bains ou douche, WC séparé et TV."
    ],
    "es": [
      "Hotel Logis de France 3*. Zona de bienestar con sauna y hammam. Situado en una zona tranquila a orillas del río, a 900 metros del centro. Amplio aparcamiento privado y jardín. Habitaciones con baño o ducha, aseo separado y TV."
    ],
    "nl": [
      "3* Logis de France hotel. Wellnessruimte met sauna en hammam. Gelegen in een rustige omgeving aan de oever van de rivier, 900m van het centrum. Grote privé parkeerplaats en tuin. Kamers met badkamer of douche, apart toilet en TV."
    ]
  },
