  "rdfs:comment": {
    "de": [
      "Als Adlernest an der Côte d'Azur und echte mittelalterliche Festung überragt Gourdon das grüne Wolftal."
    ],
    "en": [
      "Built on a rocky base at an altitude of 760m, Gourdon dominates the Vallée du Loup. It enjoys an incomparable 80 km panoramic view of the French Riviera coastline from Théoule-sur-Mer to Nice."
    ],
    "it": [
      "Nido d'aquila della Costa Azzurra e vera e propria fortezza medievale, Gourdon domina la verdeggiante Vallée du Loup."
    ],
    "fr": [
      "Nid d'aigle de la Côte d'Azur et véritable forteresse médiévale, Gourdon surplombe la verdoyante Vallée du Loup."
    ],
    "es": [
      "Nido de águilas en la Costa Azul y verdadera fortaleza medieval, Gourdon domina el verde Valle del Loup."
    ],
    "nl": [
      "Gourdon, een adelaarsnest aan de Côte d'Azur en een echt middeleeuws fort, kijkt uit over de groene Vallée du Loup."
    ]
  },
