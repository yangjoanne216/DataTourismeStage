  "rdfs:comment": {
    "de": [
      "Hotel mit 6 Zimmern, eingebettet im Herzen des alten Dorfes Laguiole.                             \nWohnzimmer, Esszimmer, Küche mit Selbstversorgung und Außenhof."
    ],
    "en": [
      "Hotel with 6 rooms nestled in the heart of the old village of Laguiole.                             \nLiving room, dining room, kitchen and outdoor courtyard."
    ],
    "it": [
      "Hotel con 6 camere situato nel cuore dell'antico villaggio di Laguiole.                             \nSoggiorno, sala da pranzo, cucina indipendente e cortile esterno."
    ],
    "fr": [
      "Hôtel de 6 chambres niché au coeur du vieux village de Laguiole.                             \nSalon, salle a manger, cuisine en gestion libre et cour extérieure."
    ],
    "nl": [
      "Hotel met 6 kamers gelegen in het hart van het oude dorp Laguiole.                             \nWoonkamer, eetkamer, keuken en binnenplaats."
    ],
    "es": [
      "Hotel de 6 habitaciones situado en el corazón del antiguo pueblo de Laguiole.                             \nSala de estar, comedor, cocina independiente y patio exterior."
    ]
  },
