  "rdfs:comment": {
    "de": [
      "Gehen Sie auf Entdeckungsreise durch das Tal der Creuse und zu Beobachtungen rund um den Fluss."
    ],
    "en": [
      "Explore the Creuse valley and observe the surroundings of the river."
    ],
    "it": [
      "Scoprite la valle della Creuse e le osservazioni intorno al fiume."
    ],
    "fr": [
      "Partez à la découverte de la vallée de la Creuse et des observations autour de la rivière."
    ],
    "nl": [
      "Ontdek de vallei van de Creuse en de observaties rond de rivier."
    ],
    "es": [
      "Descubra el valle de Creuse y las observaciones en torno al río."
    ]
  },
