  "rdfs:comment": {
    "de": [
      "Entdecken Sie den Canal du midi in Toulouse ganz unabhängig mit dem Elektrobootverleih ohne Führerschein!"
    ],
    "en": [
      "Discover the Canal du Midi in Toulouse in complete autonomy thanks to the rental of electric boats without a license!"
    ],
    "it": [
      "Scoprite il Canal du Midi a Tolosa in completa autonomia grazie al noleggio di barche elettriche senza patente!"
    ],
    "fr": [
      "Découvrez le Canal du midi à Toulouse en toute autonomie grâce à la location de bateaux électrique sans permis !"
    ],
    "nl": [
      "Ontdek het Canal du Midi in Toulouse in volledige autonomie dankzij de verhuur van elektrische boten zonder vaarbewijs!"
    ],
    "es": [
      "Descubra el Canal du Midi en Toulouse con total autonomía gracias al alquiler de barcos eléctricos sin licencia"
    ]
  },
