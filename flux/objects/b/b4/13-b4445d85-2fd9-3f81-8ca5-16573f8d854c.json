  "rdfs:comment": {
    "de": [
      "Traditionelle Küche, savoyardische Spezialitäten. Sonnenterrasse, Bergstation der Seilbahn Rosay nebenan."
    ],
    "en": [
      "Traditional cuisine, Savoyard specialities. Sunny terrace on the slopes in winter, in moutain pastures in summer. In La Côte, at the top of the Rosay gondolas."
    ],
    "it": [
      "Cucina tradizionale, specialità savoiarde. Terrazza soleggiata sulle piste in inverno, cucina raffinata negli alpeggi in estate. A La Côte, arrivo della funivia di Rosay."
    ],
    "fr": [
      "Cuisine traditionnelle, spécialités savoyardes. Terrasse ensoleillée sur les pistes l'hiver, cuisine soignée en alpage l'été. A la Côte, arrivée de la télécabine du Rosay."
    ],
    "nl": [
      "Traditionele keuken, Savoyaardse specialiteiten. Zonnig terras op de piste in de winter, fijne keuken in de bergweiden in de zomer. Bij La Côte, aankomst van de Rosay kabelbaan."
    ],
    "es": [
      "Cocina tradicional, especialidades saboyanas. Terraza soleada en las pistas en invierno, cocina fina en los pastos de montaña en verano. En La Côte, llegada del teleférico de Rosay."
    ]
  },
