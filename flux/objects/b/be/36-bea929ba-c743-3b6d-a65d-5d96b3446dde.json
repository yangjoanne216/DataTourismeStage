  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/a22450a0-b2bf-31d9-9173-f04b5a4ef73b",
      "dc:description": {
        "de": [
          "Bar-Brasserie am Ufer des Gave mit Außenterrasse in Südlage, die einen freien Blick auf den Cirque de Gavarnie bietet. Imbiss, Crêpes, Heidelbeerkuchen."
        ],
        "en": [
          "Bar-brasserie located on the edge of the gave, with an outdoor terrace facing south offering an unobstructed view of the Cirque de Gavarnie. Snacks, pancakes, blueberry tarts."
        ],
        "it": [
          "Bar-brasserie situato sulle rive del Gave, con terrazza esterna esposta a sud e vista sul Cirque de Gavarnie. Merendine, crêpes, crostate di mirtilli."
        ],
        "fr": [
          "Bar-brasserie situé en bordure de gave, avec terrasse extérieure plein Sud offrant une vue dégagée sur le Cirque de Gavarnie. Casse-croûte, crêpes, tartes myrtilles."
        ],
        "es": [
          "Bar-brasserie situado a orillas del Gave, con una terraza exterior orientada al sur y con vistas al Circo de Gavarnie. Aperitivos, crepes, tartas de arándanos."
        ],
        "nl": [
          "Bar-brasserie aan de rand van de Gave, met een buitenterras op het zuiden met uitzicht op het Cirque de Gavarnie. Snacks, crêpes, bosbessentaartjes."
        ]
      },
