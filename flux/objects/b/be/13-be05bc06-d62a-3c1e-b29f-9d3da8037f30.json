  "rdfs:comment": {
    "de": [
      "Campingplatz Le Soleil des Bastides *** 62 Stellplätze im Herzen des goldenen Dreiecks Gaillac 12 km, Cordes sur Ciel 12 km und Albi 18 km. Ruhiger Familiencampingplatz ohne Unterhaltung, beheizter Pool, Tennis und Multisportplatz. Das hoteleigene Restaurant ist die ganze Saison geöffnet."
    ],
    "en": [
      "Le Soleil des Bastides campsite *** 62 pitches located in the heart of the golden triangle Gaillac 12 km, Cordes sur Ciel 12 km and Albi 18 km. Quiet family campsite, without entertainment, heated swimming pool, tennis and multisports court, on-site restaurant open all season."
    ],
    "it": [
      "Campeggio con 62 piazzole situato nel cuore del triangolo d'oro Gaillac 12 km, Cordes sur Ciel 12 km e Albi 18 km. Tranquillo campeggio per famiglie, senza animazione, piscina riscaldata, campo da tennis e campo polisportivo, ristorante in loco aperto tutta la stagione."
    ],
    "fr": [
      "Camping de 62 emplacements situé au cœur du triangle d'or Gaillac 12 km, Cordes sur Ciel 12 km et Albi 18 km. Camping familial et calme, sans animation, piscine chauffée, terrain de tennis et multisports, restaurant sur place ouvert toute la saison."
    ],
    "nl": [
      "Camping Le Soleil des Bastides *** 62 plaatsen gelegen in het hart van de gouden driehoek Gaillac 12 km, Cordes sur Ciel 12 km en Albi 18 km. Rustige familiecamping, zonder animatie, verwarmd zwembad, tennis en multisportterrein, eigen restaurant het hele seizoen geopend."
    ],
    "es": [
      "Camping Le Soleil des Bastides *** 62 parcelas situadas en el corazón del triángulo dorado Gaillac a 12 km, Cordes sur Ciel a 12 km y Albi a 18 km. Camping familiar tranquilo, sin animación, piscina climatizada, pista de tenis y polideportiva, restaurante propio abierto toda la temporada"
    ]
  },
