  "rdfs:comment": {
    "de": [
      "Erleben Sie das Privileg, in das Herz des Naturreservats von Chérine vorzudringen, um seine Vögel zu entdecken."
    ],
    "en": [
      "Have the privilege of entering the heart of the nature reserve of Chérine to discover its birds."
    ],
    "it": [
      "Avrete il privilegio di entrare nel cuore della riserva naturale di Chérine per scoprire i suoi uccelli."
    ],
    "fr": [
      "Ayez le privilège de pénétrer au coeur de la réserve naturelle de Chérine afin d'en découvrir ses oiseaux."
    ],
    "es": [
      "Tenga el privilegio de adentrarse en el corazón de la reserva natural de Chérine para descubrir sus aves."
    ],
    "nl": [
      "Heb het voorrecht om het hart van het natuurreservaat van Chérine binnen te gaan om de vogels te ontdekken."
    ]
  },
