      "dc:description": {
        "de": [
          "Konzert des DJAGDA Trios\n\nJean-Charles Agou: Saxophone, Flöten, Gitarre, Kompositionen.\nEmmanuel Pi Djob: Stimme, Gitarre, Keyboards, Kompositionen.\nSega Seck: Bass, Perkussion."
        ],
        "en": [
          "DJAGDA Trio concert\n\nJean-Charles Agou: saxophones, flutes, guitar, compositions.\nEmmanuel Pi Djob: vocals, guitar, keyboards, compositions.\nSega Seck: bass, percussion."
        ],
        "it": [
          "Concerto del Trio DJAGDA\n\nJean-Charles Agou: sassofoni, flauti, chitarra, composizioni.\nEmmanuel Pi Djob: voce, chitarra, tastiere, composizioni.\nSega Seck: basso, percussioni."
        ],
        "fr": [
          "Concert du DJAGDA Trio\n\nJean-Charles Agou: saxophones, flûtes, guitare, compositions.\nEmmanuel Pi Djob : voix, guitare, claviers, compositions.\nSega Seck : basse, percussions."
        ],
        "es": [
          "Concierto del Trío DJAGDA\n\nJean-Charles Agou: saxos, flautas, guitarra, composiciones.\nEmmanuel Pi Djob: voz, guitarra, teclados, composiciones.\nSega Seck: bajo, percusión."
        ],
        "nl": [
          "Concert door het DJAGDA Trio\n\nJean-Charles Agou: saxofoons, fluiten, gitaar, composities.\nEmmanuel Pi Djob: zang, gitaar, keyboards, composities.\nSega Seck: bas, percussie."
        ]
      },
