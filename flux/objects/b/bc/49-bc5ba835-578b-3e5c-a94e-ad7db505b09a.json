  "rdfs:comment": {
    "de": [
      "Willkommen in Saint-Hilaire-de-Chaléons auf dem Circuit des Templiers: eine Wanderung, auf der Sie vor allem zahlreiche große, mittlerweile hundertjährige Eichen bewundern können."
    ],
    "en": [
      "Welcome to Saint-Hilaire-de-Chaléons on the Circuit des Templiers: a hike where you will be able to admire numerous large oak trees that have turned 100 years old."
    ],
    "it": [
      "Benvenuti a Saint-Hilaire-de-Chaléons sul Sentiero dei Templari: un'escursione in cui si possono ammirare molte grandi querce ormai secolari."
    ],
    "fr": [
      "Bienvenue à Saint-Hilaire-de-Chaléons sur le circuit des Templiers : une randonnée où vous pourrez notamment admirer de nombreux grands chênes devenus centenaires."
    ],
    "nl": [
      "Welkom in Saint-Hilaire-de-Chaléons op het Tempelierspad: een wandeling waar u vele eeuwenoude grote eiken kunt bewonderen."
    ],
    "es": [
      "Bienvenido a Saint-Hilaire-de-Chaléons en el Sendero de los Templarios: una excursión en la que podrá admirar numerosos robles centenarios de gran tamaño."
    ]
  },
