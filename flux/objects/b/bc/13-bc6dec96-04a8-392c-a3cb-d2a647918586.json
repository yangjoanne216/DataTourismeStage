  "rdfs:comment": {
    "de": [
      "Bernard Gay kocht mit Blick auf die Bauges: Produkte aus seinem Gemüsegarten, Fisch aus den umliegenden Seen, Käse aus Tamié."
    ],
    "en": [
      "Bernard Gay has the privilege of gazing at the spectacular Bauges Mountains while cooking gourmet dishes using ingredients from his garden, fish from the surrounding lakes and Tamié cheese."
    ],
    "it": [
      "Bernard Gay cucina guardando i Bauges: prodotti del suo orto, pesce dei laghi circostanti, formaggio di Tamié."
    ],
    "fr": [
      "Bernard Gay cuisine en regardant les Bauges : produits de son potager, poissons des lacs environnants, fromage de Tamié."
    ],
    "nl": [
      "Bernard Gay kookt terwijl hij naar de Bauges kijkt: producten uit zijn moestuin, vis uit de omliggende meren, kaas uit Tamié."
    ],
    "es": [
      "Bernard Gay cocina mirando a los Bauges: productos de su huerto, pescado de los lagos de los alrededores, queso de Tamié."
    ]
