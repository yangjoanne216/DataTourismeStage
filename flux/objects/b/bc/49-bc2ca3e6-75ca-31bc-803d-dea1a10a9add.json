  "rdfs:comment": {
    "de": [
      "Interkommunales Tourismusbüro, in unmittelbarer Nähe der Strandpromenade und des Stadtzentrums von Saint-Brevin-les-Pins gelegen. Angebotene Dienstleistungen: Empfang, Touristeninformation, Aufenthaltsberatung, Kartenverkauf, Boutique, Veranstaltungen."
    ],
    "en": [
      "Intercommunal Tourist Office, located close to the seafront and the town centre of Saint-Brevin-les-Pins. Services offered: reception, tourist information, stay advice, ticket offices, shop, events."
    ],
    "it": [
      "Ufficio turistico intercomunale, situato nelle immediate vicinanze del mare e del centro di Saint-Brevin-les-Pins. Servizi offerti: reception, informazioni turistiche, consigli per le vacanze, biglietteria, negozio, eventi."
    ],
    "fr": [
      "Office de Tourisme Intercommunal, situé à proximité immédiate du bord de mer et du centre-ville de Saint-Brevin-les-Pins. Services proposés : accueil, information touristique, conseil en séjour, billetteries, boutique, manifestations."
    ],
    "es": [
      "Oficina de Turismo Intercomunal, situada en las inmediaciones de la playa y del centro de la ciudad de Saint-Brevin-les-Pins. Servicios ofrecidos: recepción, información turística, asesoramiento vacacional, taquilla, tienda, eventos."
    ],
    "nl": [
      "Intercommunaal Bureau voor Toerisme, gelegen in de onmiddellijke nabijheid van de zee en het stadscentrum van Saint-Brevin-les-Pins. Aangeboden diensten: receptie, toeristische informatie, vakantieadvies, kassa, winkel, evenementen."
    ]
  },
