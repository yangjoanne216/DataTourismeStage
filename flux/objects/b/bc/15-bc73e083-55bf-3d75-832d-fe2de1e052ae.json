  "rdfs:comment": {
    "de": [
      "Animation für die ganze Familie ab 5 Jahren. Lernen Sie spielerisch, einige Schmetterlinge zu identifizieren."
    ],
    "en": [
      "Animation to do with your family from 5 years old. Learn to identify some butterflies while having fun."
    ],
    "it": [
      "Un'attività per famiglie per bambini dai 5 anni in su. Imparate a identificare alcune farfalle divertendovi."
    ],
    "fr": [
      "Animation à faire en famille à partir de 5 ans. Apprenez à identifier quelques papillons tout en s'amusant."
    ],
    "es": [
      "Una actividad familiar para niños a partir de 5 años. Aprende a identificar algunas mariposas mientras te diviertes."
    ],
    "nl": [
      "Een gezinsactiviteit voor kinderen vanaf 5 jaar. Leer enkele vlinders identificeren terwijl je plezier hebt."
    ]
  },
