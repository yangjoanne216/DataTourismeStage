  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/cee7f1ec-ee03-386b-9677-f17b54a800be",
      "dc:description": {
        "de": [
          "Tomettes-Boden, Kalkwände, Bett 160 x 200 cm, große Dusche, WC, Küchenbereich.\nTerrasse mit Möbeln und eigenem Garten und Blick auf den Fluss \"la Sarthe\". Parkplatz in der Nähe möglich."
        ],
        "en": [
          "Tiled floor, whitewashed walls, bed 160 x 200 cm, large shower, WC, kitchen area.\nTerrace with furniture and private garden and view on the river \"la Sarthe\". Parking nearby possible."
        ],
        "it": [
          "Pavimento in piastrelle, pareti in calce, letto 160 x 200 cm, grande doccia, WC, angolo cottura.\nTerrazza con mobili e giardino privato e vista sul fiume \"la Sarthe\". Possibilità di parcheggio nelle vicinanze."
        ],
        "fr": [
          "Sol en tomettes, murs à la chaux, lit 160 x 200 cm,  grande douche, WC, espace cuisine.\nTerrasse avec mobilier et jardin privatif et vue sur la rivière \"la Sarthe\". Parking à proximité possible."
        ],
        "nl": [
          "Tegelvloer, kalkwanden, bed 160 x 200 cm, grote douche, WC, keukenhoek.\nTerras met tuinmeubelen en uitzicht op de rivier \"la Sarthe\". Parkeren in de buurt mogelijk."
        ],
        "es": [
          "Suelo de baldosas, paredes de cal, cama de 160 x 200 cm, ducha grande, WC, zona de cocina.\nTerraza con mobiliario y jardín privado y vista al río \"la Sarthe\". Posibilidad de aparcamiento en las inmediaciones."
        ]
      },
