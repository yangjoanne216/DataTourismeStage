  "rdfs:comment": {
    "de": [
      "Schattiger Campingplatz inmitten der Natur auf einem landwirtschaftlichen Betrieb, der auf Reiten spezialisiert ist. Besitzer eines Reiterhofs, Ausritte möglich.\nMahlzeiten möglich."
    ],
    "en": [
      "Shady camp site deep in the countryside on a farm specialising in horse-riding. Owners of an equestrian farm, opportunity for horse-riding. Meals available."
    ],
    "it": [
      "Un campeggio ombreggiato in mezzo alla natura, situato in una fattoria specializzata in equitazione. Proprietario di un allevamento di cavalli, possibilità di passeggiate a cavallo.\nPasti disponibili."
    ],
    "fr": [
      "Camping ombragé, en pleine nature situé sur une exploitation agricole spécialisée dans l'équitation. Propriétaire d'une ferme équestre, possibilité de promenades à cheval.\nRepas possibles."
    ],
    "nl": [
      "Een schaduwrijke camping midden in de natuur, gelegen op een boerderij gespecialiseerd in paardrijden. Eigenaar van een paardenboerderij, mogelijkheid tot paardrijden.\nBeschikbare maaltijden."
    ],
    "es": [
      "Camping con sombra, en plena naturaleza situado en una explotación agrícola especializada en la equitación. Propietario de una granja ecuestre, posibilidad de paseos a caballo. Comidas posibles."
    ]
  },
