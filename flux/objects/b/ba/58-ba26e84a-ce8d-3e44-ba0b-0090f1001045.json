  "rdfs:comment": {
    "de": [
      "Fleuriste de la Barque ein großzügiger handwerklicher Florist, der Ihnen zuhört und Sie mit seinen Blumenarrangements und der Frische seiner Blumen verführen wird."
    ],
    "en": [
      "Fleuriste de la Barque is a generous florist who will seduce you with his floral arrangements and the freshness of his flowers."
    ],
    "it": [
      "Fleuriste de la Barque è un fiorista generoso che vi sedurrà con le sue composizioni floreali e la freschezza dei suoi fiori."
    ],
    "fr": [
      "Fleuriste de la Barque un artisan fleuriste généreux à votre écoute, qui vous séduira par ses compositions florales et la fraîcheur de ses fleurs."
    ],
    "nl": [
      "Fleuriste de la Barque is een gulle bloemist die u zal verleiden met zijn bloemstukken en de versheid van zijn bloemen."
    ],
    "es": [
      "Fleuriste de la Barque es un florista generoso que le seducirá con sus arreglos florales y la frescura de sus flores."
    ]
  },
