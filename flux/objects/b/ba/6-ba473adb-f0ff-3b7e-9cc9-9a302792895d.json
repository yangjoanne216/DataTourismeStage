  "rdfs:comment": {
    "de": [
      "Genießen Sie einen ruhigen Aufenthalt am Waldrand, aber nur einen Katzensprung von der Stadt entfernt in einer separaten Wohnung."
    ],
    "en": [
      "Come and enjoy a quiet stay on the edge of the forest but close to the city in a self-contained flat."
    ],
    "it": [
      "Venite a godervi un soggiorno tranquillo ai margini della foresta ma vicino alla città in un appartamento indipendente."
    ],
    "fr": [
      "Venez profiter d'un séjour au calme en bordure de forêt mais à deux pas de la ville dans un appartement indépendatnt."
    ],
    "es": [
      "Ven a disfrutar de una estancia tranquila al borde del bosque pero cerca de la ciudad en un piso independiente."
    ],
    "nl": [
      "Kom genieten van een rustig verblijf aan de rand van het bos maar dicht bij de stad in een zelfstandige flat."
    ]
  },
