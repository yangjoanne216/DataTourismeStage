  "rdfs:comment": {
    "de": [
      "Das Schloss Thégra öffnet Ihnen seine Türen, um auf Entdeckungsreise durch seine Deckenmalereien, Fresken und monumentalen Kamine zu gehen."
    ],
    "en": [
      "The Château de Thégra opens its doors to you to discover its painted ceilings, its frescoes and its monumental fireplaces."
    ],
    "it": [
      "Il Castello di Thégra vi apre le porte per scoprire i suoi soffitti dipinti, i suoi affreschi e i suoi camini monumentali."
    ],
    "fr": [
      "Le Château de Thégra vous ouvre ses portes pour aller à la découverte de ses plafonds peints, de ses fresques et de ses cheminées monumentales."
    ],
    "nl": [
      "Het kasteel van Thégra opent zijn deuren voor u om zijn beschilderde plafonds, zijn fresco's en zijn monumentale open haarden te ontdekken."
    ],
    "es": [
      "El castillo de Thégra le abre sus puertas para que descubra sus techos pintados, sus frescos y sus chimeneas monumentales."
    ]
  },
