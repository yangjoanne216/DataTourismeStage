  "rdfs:comment": {
    "de": [
      "Abschnitt B16 des GR2013. Von Beaureceuil aus in Richtung Aix-en-Provence. Eine 12,8 km lange Strecke, auf der Sie die Landschaften des Sainte-Victoire-Massivs, den Zola-See und die Steinbrüche von Bibemus entdecken können."
    ],
    "en": [
      "Section B16 of the GR2013. From Beaureceuil to Aix-en-Provence. A 12,8km route to discover the landscapes of the Sainte-Victoire massif, the Zola lake and the Bibemus quarries."
    ],
    "it": [
      "Sezione B16 del GR2013. Da Beaureceuil a Aix-en-Provence. Un percorso di 12,8 km per scoprire i paesaggi del massiccio della Sainte-Victoire, del lago di Zola e delle cave di Bibemus."
    ],
    "fr": [
      "Tronçon B16 du GR2013. Au départ de Beaureceuil vers Aix-en-Provence. Un trajet de 12,8km pour découvrir Les paysages du massif Sainte-Victoire, le lac Zola et les carrières de Bibemus."
    ],
    "es": [
      "Sección B16 del GR2013. De Beaureceuil a Aix-en-Provence. Un recorrido de 12,8 km para descubrir los paisajes del macizo de Sainte-Victoire, el lago de Zola y las canteras de Bibemus."
    ],
    "nl": [
      "Afdeling B16 van de GR2013. Van Beaureceuil naar Aix-en-Provence. Een route van 12,8 km om de landschappen van het massief van Sainte-Victoire, het meer van Zola en de steengroeven van Bibemus te ontdekken."
    ]
  },
  "rdfs:label": {
    "fr": [
      "GR2013 - B16 - De Beaureceuil à Aix en Provence"
    ]
  },
