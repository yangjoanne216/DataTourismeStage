  "rdfs:comment": {
    "de": [
      "Am Ufer der Sarthe, in dem hübschen Dorf Juvardeil, befindet sich ein Drittort, der Restaurants, Bars und Lebensmittelgeschäfte mit Kultur vermischt."
    ],
    "en": [
      "On the banks of the Sarthe, in the pretty village of Juvardeil, a third place combining restaurant, bar and grocery store and culture."
    ],
    "it": [
      "Sulle rive della Sarthe, nel grazioso villaggio di Juvardeil, un terzo luogo che unisce ristoranti, bar e negozio di alimentari alla cultura."
    ],
    "fr": [
      "En bord de Sarthe, dans le joli village de Juvardeil, tiers-lieu mêlant restauration, bar et épicerie et culture."
    ],
    "nl": [
      "Aan de oever van de Sarthe, in het mooie dorpje Juvardeil, een derde plaats die restaurants, een bar en een kruidenierswinkel combineert met cultuur."
    ],
    "es": [
      "A orillas del Sarthe, en el bonito pueblo de Juvardeil, un tercer lugar que combina restaurantes, bar y tienda de comestibles con cultura."
    ]
  },
