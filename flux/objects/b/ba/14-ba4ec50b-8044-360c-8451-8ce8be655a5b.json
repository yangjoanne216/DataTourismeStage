  "rdfs:comment": {
    "de": [
      "Die Ferme Boulanger ist ein Bauernhof mit Mischkultur und Milchviehhaltung. Catherine hat den Wunsch, die Welt der Landwirtschaft durch ihren pädagogischen Bauernhof zu präsentieren."
    ],
    "en": [
      "The Boulanger Farm is a mixed farming farm with dairy cattle. Catherine wishes to present the agricultural world through her educational farm."
    ],
    "it": [
      "La Ferme Boulanger è un'azienda agricola e casearia mista. Catherine desidera presentare il mondo agricolo attraverso la sua fattoria didattica."
    ],
    "fr": [
      "La Ferme Boulanger est une ferme de polyculture d’élevage laitier. Catherine a le souhait de présenter le monde agricole grâce à sa ferme pédagogique."
    ],
    "nl": [
      "La Ferme Boulanger is een gemengd landbouw- en melkveebedrijf. Catherine wil de agrarische wereld presenteren via haar educatieve boerderij."
    ],
    "es": [
      "La Ferme Boulanger es una explotación mixta agrícola y lechera. Catherine desea presentar el mundo agrícola a través de su granja educativa."
    ]
  },
