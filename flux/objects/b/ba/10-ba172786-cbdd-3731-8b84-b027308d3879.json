  "rdfs:comment": {
    "de": [
      "3 km von Gérardmer entfernt. 17 Zimmer, 6 Suiten, Wohnzimmer am Kamin, Bar, Garten, Außen- und Innenpool. Sauna, Hammam Behandlungen und Massagen auf Reservierung. Geschlossene Garagen, Parkplatz."
    ],
    "en": [
      "3 km from Gérardmer. 17 rooms, 6 suites, lounge around the fireplace, bar, garden, outdoor and indoor swimming pools. Sauna, Hammam and massages on reservation. Closed garages, parking."
    ],
    "it": [
      "3 km da Gérardmer. 17 camere, 6 suite, salone intorno al camino, bar, giardino, piscina esterna e interna. Sauna, Hammam, trattamenti e massaggi su prenotazione. Garage chiusi, parcheggio."
    ],
    "fr": [
      "A 3 km de Gérardmer. 17 chambres, 6 suites, salon autour de la cheminée, bar, jardin, piscines extérieure et intérieure. Sauna, Hammam soins et massages sur réservation. Garages fermés, parking."
    ],
    "es": [
      "A 3 km de Gérardmer. 17 habitaciones, 6 suites, salón alrededor de la chimenea, bar, jardín, piscina exterior y cubierta. Sauna, Hammam, tratamientos y masajes bajo reserva. Garajes cerrados, aparcamiento."
    ],
    "nl": [
      "3 km van Gérardmer. 17 kamers, 6 suites, lounge rond de open haard, bar, tuin, buiten- en binnenzwembaden. Sauna, Hammam, behandelingen en massages op reservering. Gesloten garages, parkeren."
    ]
  },
