  "rdfs:comment": {
    "de": [
      "Im Herzen von Thonon empfängt Sie Le Pretext in freundlicher und geselliger Atmosphäre für eine Kaffeepause."
    ],
    "en": [
      "At the heart of downtown Thonon, Le Pretext welcomes you with warmth and friendliness for a coffee break."
    ],
    "it": [
      "I negozi La Dame En Bois presentano Le Pretext Bar à Jeux, un luogo ludico per scoprire, condividere e divertirsi insieme con i giochi da tavolo."
    ],
    "fr": [
      "Les boutiques La Dame En Bois vous présente Le Pretext Bar à Jeux, un lieu ludique pour vous permettre de découvrir, partager et profiter ensemble autour des jeux de société."
    ],
    "es": [
      "Las tiendas La Dame En Bois presentan Le Pretext Bar à Jeux, un lugar lúdico para descubrir, compartir y disfrutar juntos de los juegos de mesa."
