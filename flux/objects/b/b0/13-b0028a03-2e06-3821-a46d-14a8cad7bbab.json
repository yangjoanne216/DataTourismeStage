  "rdfs:comment": {
    "de": [
      "Der Zugang ist frei. Angenehmer Ort für einen Spaziergang, der sich mit dem Plan d'eau des Percières verbindet (Picknickbereich, Kinderspielplatz, City-Stadion, Gesundheitsparcours...)."
    ],
    "en": [
      "Open access. Pleasant place of walk that joins the lake of Percières (picnic area, children’s games, city stadium, health course...)."
    ],
    "it": [
      "Accesso libero. Un luogo piacevole per le passeggiate che si uniscono al lago di Percières (area pic-nic, parco giochi per bambini, stadio cittadino, percorso salute...)."
    ],
    "fr": [
      "Accès libre. Agréable lieu de promenade qui rejoint le plan d'eau des Percières (aire de pique-nique, de jeux pour enfants, city stade, parcours santé...)."
    ],
    "nl": [
      "Vrije toegang. Aangename plaats om te wandelen die aansluit op het meer van Percières (picknickplaats, speeltuin, stadsstadion, gezondheidsparcours...)."
    ],
    "es": [
      "Acceso gratuito. Lugar agradable para pasear que se une al lago de Percières (zona de picnic, parque infantil, estadio municipal, curso de salud...)."
    ]
  },
