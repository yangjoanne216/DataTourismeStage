  "rdfs:comment": {
    "de": [
      "15 m² großes Studio im Gartengeschoss für 2 Personen in Noyant-la-Plaine, zwischen Brissac und Doué-la-Fontaine. Eine schöne Lage, um sich auszuruhen und die umliegenden Reichtümer von oben... und von unten zu besichtigen!"
    ],
    "en": [
      "Studio of 15 m² in garden level, for 2 persons, located in Noyant-la-Plaine, between Brissac and Doué-la-Fontaine. A beautiful situation to take a break and visit the surrounding wealth from above... and below!"
    ],
    "it": [
      "Monolocale di 15 m² al piano terra, per 2 persone, situato a Noyant-la-Plaine, tra Brissac e Doué-la-Fontaine. Una bella location per fare una pausa e visitare i dintorni dall'alto... e dal basso!"
    ],
    "fr": [
      "Studio de 15 m² en rez-de-jardin, pour 2 personnes, situé à Noyant-la-Plaine, entre Brissac et Doué-la-Fontaine. Une belle situation pour se pauser et visiter les richesses alentour du dessus... et du dessous !"
    ],
    "nl": [
      "Studio van 15 m² op de begane grond, voor 2 personen, gelegen in Noyant-la-Plaine, tussen Brissac en Doué-la-Fontaine. Een prachtige locatie om even uit te rusten en de omgeving van boven... en onder te bekijken!"
    ],
    "es": [
      "Estudio de 15 m² en la planta baja, para 2 personas, situado en Noyant-la-Plaine, entre Brissac y Doué-la-Fontaine. Un lugar precioso para hacer una pausa y visitar los alrededores desde arriba... ¡y desde abajo!"
    ]
  },
