  "rdfs:comment": {
    "de": [
      "In der Landschaft des Dorfes Orgon, im Herzen der Alpilles und an der Grenze zum Luberon, befindet sich das Mas des Cigales."
    ],
    "en": [
      "In the countryside of the village of Orgon, in the heart of the Alpilles and on the edge of the Luberon, is the Mas des Cigales."
    ],
    "it": [
      "Nella campagna del villaggio di Orgon, nel cuore delle Alpilles e ai margini del Luberon, si trova il Mas des Cigales."
    ],
    "fr": [
      "Dans la campagne du village d’Orgon, au cœur des Alpilles et à la limite du Luberon, se trouve le Mas des Cigales."
    ],
    "nl": [
      "Op het platteland van het dorp Orgon, in het hart van de Alpilles en aan de rand van de Luberon, ligt de Mas des Cigales."
    ],
    "es": [
