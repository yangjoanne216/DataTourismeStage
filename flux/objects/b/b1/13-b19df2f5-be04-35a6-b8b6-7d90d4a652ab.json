  "rdfs:comment": {
    "de": [
      "Der Parc Omnisports Auvergne-Rhône-Alpes-Vichy verfügt über einen Disc-Golf-Parcours mit 18 Körben, die über den ganzen Park verteilt sind und so einen sportlichen und spielerischen Spaziergang bieten."
    ],
    "en": [
      "The Parc Omnisports Auvergne-Rhône-Alpes-Vichy has a disc golf course with 18 baskets spread throughout the park, offering a sporty and playful stroll."
    ],
    "it": [
      "Il Parc Omnisports Auvergne-Rhône-Alpes-Vichy dispone di un campo da disc golf con 18 canestri distribuiti in tutto il parco, che offre una corsa sportiva e divertente."
    ],
    "fr": [
      "Le Parc Omnisports Auvergne-Rhône-Alpes-Vichy dispose d'un parcours de disc golf de 18 paniers répartis dans tout le Parc offrant ainsi une balade sportive et ludique."
    ],
    "nl": [
      "Het Parc Omnisports Auvergne-Rhône-Alpes-Vichy heeft een disc golf parcours met 18 baskets verspreid over het park, die een sportieve en leuke rit bieden."
    ],
    "es": [
      "El Parc Omnisports Auvergne-Rhône-Alpes-Vichy cuenta con un campo de disc golf con 18 cestas repartidas por todo el parque, que ofrecen un recorrido deportivo y divertido."
    ]
  },
  "rdfs:label": {
