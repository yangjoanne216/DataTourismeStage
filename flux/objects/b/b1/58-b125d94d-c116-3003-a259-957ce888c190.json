  "rdfs:comment": {
    "de": [
      "Entdecken Sie die Camargue zu Pferd mit unserem Verein Lou Souleu \"Le Soleil\". Entfliehen Sie dem Alltag bei einem Ausritt in die unberührte Natur."
    ],
    "en": [
      "Discover the Camargue on horseback with our Association Lou Souleu \"Le Soleil\". Escape during a walk in the middle of preserved nature."
    ],
    "it": [
      "Scoprite la Camargue a cavallo con la nostra Associazione Lou Souleu \"Le Soleil\". Allontanatevi da tutto con un viaggio nella natura incontaminata."
    ],
    "fr": [
      "Découverte de la Camargue à cheval avec notre Association Lou Souleu \"Le Soleil\". Evadez-vous lors d'une promenade en pleine nature préservée."
    ],
    "nl": [
      "Ontdek de Camargue te paard met onze Association Lou Souleu \"Le Soleil\". Ga er even tussenuit tijdens een rit door de ongerepte natuur."
    ],
    "es": [
      "Descubra la Camarga a caballo con nuestra Asociación Lou Souleu \"Le Soleil\". Aléjese de todo en un paseo por la naturaleza virgen."
    ]
  },
