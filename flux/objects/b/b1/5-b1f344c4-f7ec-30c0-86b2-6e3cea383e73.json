  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/2014a971-add9-3bea-847b-f5842027f252",
      "dc:description": {
        "de": [
          "Käser, Weinhändler, Käsebar.\nFüllkörbe, Raclette, Fondue, biodynamische Weine, Käseplatten, Cheese Box.\nÖffnungszeiten je nach Saison (http://lepiciergrandcru.com/infos)"
        ],
        "en": [
          "Cheese shop, wine shop, cheese bar.\nPacked baskets, raclette, fondue, biodynamic wines, cheese trays, cheese box.\nOpening hours according to season (http://lepiciergrandcru.com/infos)"
        ],
        "it": [
          "Negozio di formaggi, enoteca, bar di formaggi.\nCestini confezionati, raclette, fonduta, vini biodinamici, vassoi di formaggi, box di formaggi.\nOrari di apertura secondo la stagione (http://lepiciergrandcru.com/infos)"
        ],
        "fr": [
          "Fromager, caviste, cheese bar.\nPaniers garnis, raclette, fondue, vins biodynamie, plateaux de formage, cheese box.\nHoraires selon saison (http://lepiciergrandcru.com/infos)"
        ],
        "es": [
          "Tienda de quesos, tienda de vinos, bar de quesos.\nCestas envasadas, raclette, fondue, vinos biodinámicos, bandejas de queso, caja de quesos.\nHorario de apertura según la temporada (http://lepiciergrandcru.com/infos)"
        ],
        "nl": [
          "Kaaswinkel, wijnwinkel, kaasbar.\nVerpakte manden, raclette, fondue, biodynamische wijnen, kaasplateaus, kaasdoos.\nOpeningstijden volgens seizoen (http://lepiciergrandcru.com/infos)"
        ]
      },
