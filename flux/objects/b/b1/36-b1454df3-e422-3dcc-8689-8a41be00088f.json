  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/0e7971e6-e798-37d7-b0fb-45eaca82042a",
      "dc:description": {
        "de": [
          "Kochnische, Schlafbereich, Essbereich, 2 x 90 cm Etagenbett, Schlafsofa, Farbfernseher, Badezimmer, WC. Skiraum, Balkon. Nebenkosten nicht inbegriffen.",
          "Neue Residenz mit Aufzug im Stadtzentrum von Cauterets, in der Nähe der Thermen und der Seilbahn von Lys. Studio Alkoven im 1. Stock, Fläche: 22 m2."
        ],
        "en": [
          "Kitchenette, sleeping area, dining area, 2 x 90 cm bunk beds, sofa bed, colour television, bathroom, wc. Ski rack, balcony. Charges not included.",
          "Recent residence with lift located in the city centre of Cauterets, near the thermal baths and the Lys cable car. Studio alcove on the 1st floor, surface area: 22 m2."
        ],
        "it": [
          "Angolo cottura, zona notte, zona pranzo, 2 letti a castello da 90 cm, divano letto, TV a colori, bagno, WC. Porta sci, balcone. Spese non incluse.",
          "Residenza recente con ascensore situata nel centro di Cauterets, vicino alle terme e alla funivia del Lys. Studio alcova al 1° piano, superficie: 22 m2."
        ],
        "fr": [
          "Kitchenette, coin nuit, coin repas, 2 x 90 cm superposés, canapé convertible, télévision couleur, salle de bain, wc. Ratelier à skis, balcon. Charges non comprises.",
          "Résidence récente avec ascenseur située en plein centre ville de Cauterets, à proximité des thermes et de la télécabine du Lys. Studio alcôve au 1er étage, superficie : 22 m2."
        ],
        "nl": [
          "Kitchenette, slaapgedeelte, eethoek, 2 x 90 cm stapelbed, slaapbank, kleuren-tv, badkamer, toilet. Ski-rek, balkon. Kosten niet inbegrepen.",
          "Recente residentie met lift gelegen in het centrum van Cauterets, vlakbij de thermale baden en de Lys kabelbaan. Alkoof studio op de 1e verdieping, oppervlakte: 22 m2."
        ],
        "es": [
          "Cocina americana, zona de dormir, zona de comedor, 2 literas de 90 cm, sofá cama, TV en color, baño, aseo. Portaesquís, balcón. Gastos no incluidos.",
          "Residencia reciente con ascensor situada en el centro de Cauterets, cerca de los baños termales y del teleférico del Lys. Estudio alcoba en el 1er piso, superficie: 22 m2."
        ]
      },
