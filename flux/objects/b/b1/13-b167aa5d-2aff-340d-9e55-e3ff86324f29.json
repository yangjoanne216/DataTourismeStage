  "rdfs:comment": {
    "de": [
      "Familienweingut mit Maxime:\r\n << Wenn ich großzügig bin,\r\n     Wenn man mit mir singt,\r\n     Ich verdanke meine Qualitäten,\r\n     Den Kieselsteinen der Grands Bois >>"
    ],
    "en": [
      "Family wine estate with Maxime:\r\n << If I am generous,\r\n     If they sing with me,\r\n     I owe my qualities,\r\n     To the stones of the Great Woods >>"
    ],
    "it": [
      "Azienda vinicola di famiglia con Maxime:\r\n << Se sono generoso,\r\n     Se cantano con me,\r\n     Devo le mie qualità,\r\n     Alle pietre del Grands Bois >>"
    ],
    "fr": [
      "Domaine viticole familial  avec Maxime:\r\n << Si je suis généreux,\r\n     Si l'ont chante avec moi,\r\n     Je dois mes qualités,\r\n     Aux cailloux des Grands Bois >>"
    ],
    "es": [
      "Finca vinícola familiar con Maxime:\r\n << Si soy generoso,\r\n     Si cantan conmigo,\r\n     Debo mis cualidades,\r\n     A las piedras del Grands Bois >>"
    ],
    "nl": [
      "Familie wijngaard met Maxime:\r\n << Als ik gul ben,\r\n     Als ze met mij zingen,\r\n     Ik heb mijn kwaliteiten te danken,\r\n     Naar de stenen van de Grands Bois >>"
    ]
  },
