  "rdfs:comment": {
    "de": [
      "Haus Osco. Zwischen Kunsthandwerk & charaktervoller Gastronomie. Unser Restaurant befindet sich im Herzen des historischen Zentrums von Saint-Raphael: Schreiten Sie über das Kopfsteinpflaster der Stadt und entdecken Sie unsere bis-gastronomische Welt."
    ],
    "en": [
      "Osco House. Between craftsmanship & gastronomy of character. Our restaurant is located in the heart of the historic center of Saint-Raphael: walk the cobblestones of the city and discover our bis-gastronomic universe."
    ],
    "it": [
      "Casa Osco. Tra artigianato e gastronomia di carattere. Il nostro ristorante si trova nel cuore del centro storico di Saint-Raphael: passeggiate lungo i ciottoli della città e scoprite il nostro universo bis-gastronomico."
    ],
    "fr": [
      "Maison Osco. Entre artisanat & gastronomie de caractère. Notre restaurant est logé au coeur du centre historique de Saint-Raphael : arpentez les pavés de la ville et découvrez notre univers bis-gastronomique."
    ],
    "nl": [
      "Osco House. Tussen vakmanschap en karaktervolle gastronomie. Ons restaurant ligt in het hart van het historische centrum van Saint-Raphael: wandel langs de kasseien van de stad en ontdek ons bis-gastronomisch universum."
    ],
    "es": [
      "Casa Osco. Entre la artesanía y la gastronomía de carácter. Nuestro restaurante está situado en el corazón del centro histórico de Saint-Raphael: pasee por los adoquines de la ciudad y descubra nuestro universo bis-gastronómico."
    ]
  },
