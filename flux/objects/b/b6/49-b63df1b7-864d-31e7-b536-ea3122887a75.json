  "rdfs:comment": {
    "de": [
      "Charmantes Häuschen auf dem Land zum Entspannen, Naturbeobachten und Entdecken der Geschichte in Parçay les Pins"
    ],
    "en": [
      "Charming country house to recharge your batteries, observe nature and discover history in Parçay les Pins"
    ],
    "it": [
      "Incantevole cottage in campagna per rilassarsi, osservare la natura e scoprire la storia a Parçay les Pins"
    ],
    "fr": [
      "Charmante maisonnette à la campagne pour se ressourcer, observer la nature et découvrir l'histoire à Parçay les Pins"
    ],
    "es": [
      "Encantadora casa de campo para relajarse, observar la naturaleza y descubrir la historia en Parçay les Pins"
    ],
