  "rdfs:comment": {
    "de": [
      "Gänsestopfleber und Fleisch von Blonde d'Aquitaine. Kostenlose Führungen mit Verkostung von Mitte Juni bis September, dienstags um 15 Uhr (Anmeldung bis zum Vortag). Laden das ganze Jahr über nach Vereinbarung geöffnet."
    ],
    "en": [
      "Goose foie gras and Blonde d'Aquitaine meat. Free guided tours with tasting from mid-June to September, Tuesdays at 3pm (registration before the day before). Shop open all year round by appointment."
    ],
    "it": [
      "Foie gras d'oca e carne Blonde d'Aquitaine. Visite guidate gratuite con degustazione da metà giugno a settembre, il martedì alle 15.00 (è necessario iscriversi il giorno prima). Negozio aperto tutto l'anno su appuntamento."
    ],
    "fr": [
      "Foie gras d'oie et viande de Blonde d'Aquitaine. Visites guidées gratuites avec dégustation de mi-juin à septembre, le mardi à 15h (inscription avant la veille). Boutique ouverte toute l'année sur rdv."
    ],
    "nl": [
      "Ganzenlever en Blonde d'Aquitaine vlees. Gratis rondleidingen met proeverij van half juni tot september, op dinsdag om 15.00 uur (de dag ervoor aanmelden). Winkel het hele jaar geopend op afspraak."
    ],
    "es": [
      "Foie gras de oca y carne de rubia de Aquitania. Visitas guiadas gratuitas con degustación de mediados de junio a septiembre, los martes a las 15:00 horas (hay que inscribirse el día anterior). Tienda abierta todo el año con cita previa."
    ]
  },
