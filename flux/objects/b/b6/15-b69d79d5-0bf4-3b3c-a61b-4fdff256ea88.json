  "rdfs:comment": {
    "de": [
      "Kunst im weitesten Sinne mit dem Cercle des Beaux Arts! Workshops für Kinder, Jugendliche und Erwachsene, in denen Sie mehr über Ton, Öl, Tinte, Farben usw. erfahren."
    ],
    "en": [
      "Every kind of Art with the Circle of Fine Arts! Workshops for children, teenagers and adults, to discover pottery, oil, ink, colors, etc..."
    ],
    "it": [
      "Arte in senso lato con il Cercle des Beaux Arts! Laboratori per bambini, adolescenti e adulti, alla scoperta di argilla, olio, inchiostro, colori, ecc."
    ],
    "fr": [
      "L'Art au sens large avec le Cercle des Beaux Arts ! Ateliers pour enfants, adolescents et adultes, pour découvrir la terre, l'huile, l'encre, les couleurs, etc."
    ],
    "nl": [
      "Kunst in de breedste zin van het woord met de Cercle des Beaux Arts! Workshops voor kinderen, tieners en volwassenen, om klei, olie, inkt, kleuren, enz. te ontdekken."
    ],
    "es": [
      "Arte en el sentido más amplio con el Círculo de Bellas Artes Talleres para niños, adolescentes y adultos, para descubrir la arcilla, el óleo, la tinta, los colores, etc."
    ]
  },
