  "rdfs:comment": {
    "de": [
      "Traditionelles Restaurant: Salate, Fleisch, Pizzas und hausgemachte Desserts. Das Kochen unserer Gerichte wird größtenteils durch einen Holzofen gewährleistet. Familiäre und entspannte Atmosphäre."
    ],
    "en": [
      "Traditional restaurant : salads, meats, pizzas in a wood oven and homemade desserts.\r\nFriendly atmosphere"
    ],
    "it": [
      "Ristorante tradizionale: insalate, carni, pizze e dolci fatti in casa. La cottura dei nostri piatti avviene in gran parte nel forno a legna. Atmosfera familiare e rilassata."
    ],
    "fr": [
      "Restaurant traditionnel : salades, viandes, pizzas et desserts maison. la cuisson de nos plats sont assurés en grande partie par un four à bois. Ambiance familiale et décontractée."
    ],
    "nl": [
      "Traditioneel restaurant: salades, vleeswaren, pizza's en huisgemaakte desserts. De bereiding van onze gerechten gebeurt grotendeels in een houtoven. Familiale en ontspannen sfeer."
    ],
    "es": [
      "Restaurante tradicional: ensaladas, carnes, pizzas y postres caseros. La cocción de nuestros platos se realiza en gran parte en horno de leña. Ambiente familiar y relajado."
    ]
  },
  "rdfs:label": {
    "fr": [
      "La Grange aux pizzas"
    ]
  },
