  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/3ec2c2b8-36ff-332b-971e-998acd375602",
      "dc:description": {
        "de": [
          "F3, große Terrasse mit Blick, ausgestattete Küche (amerikanischer Kühlschrank, Induktion, ...), Mezzanine, Schlafzimmer, WC, Badezimmer (italienische Dusche), Wifi, Canal +, Box, Aufzug, voll ausgestattet (Waschmaschine, Trockner)."
        ],
        "en": [
          "A fully-equipped kitchen, a mezzanine, a bedroom, restrooms, a bathroom. A wide terrace with different points of view. Wifi, a washing-machine + dryer, elevator."
        ],
        "it": [
          "F3, grande terrazza con vista, cucina attrezzata (frigorifero americano, induzione, ...), mezzanino, camera da letto, WC, bagno (doccia italiana), Wifi, Canal +, box, ascensore, completamente attrezzata (lavatrice, asciugatrice)."
        ],
        "fr": [
          "F3, grande terrasse avec vue, cuisine équipée (frigo américain, induction, ...), mezzanine, chambre, WC, salle de bains (douche italienne), Wifi, Canal +, box, ascenseur, tout équipé (lave-linge, sèche-linge)."
        ],
        "nl": [
          "F3, groot terras met uitzicht, ingerichte keuken (Amerikaanse koelkast, inductie, ...), mezzanine, slaapkamer, WC, badkamer (Italiaanse douche), Wifi, Canal +, box, lift, volledig uitgerust (wasmachine, droger)."
        ],
        "es": [
          "F3, gran terraza con vistas, cocina equipada (nevera americana, inducción, ...), altillo, dormitorio, WC, baño (ducha italiana), Wifi, Canal +, caja, ascensor, totalmente equipado (lavadora, secadora)."
        ]
      },
