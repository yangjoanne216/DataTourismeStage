  "rdfs:comment": {
    "de": [
      "Internationales Mandolinenfestival: Im Juli finden vier Tage lang Konzerte und Veranstaltungen mit internationalen Künstlern im prächtigen Rahmen der Kirche Saint-Pierre statt. Das Repertoire reicht vom Barock bis zu zeitgenössischen Stücken."
    ],
    "en": [
      "The Mandoline International Festival is 4 days of concerts, animations and master classes, with international artists. Concerts in the sumptuous setting of the Saint-Pierre church, with a repertoire that extends from baroque to contemporary."
    ],
    "it": [
      "Il Festival Internazionale del Mandoline è 4 giorni di concerti, animazioni e master classes, con artisti internazionali. Concerti nella sontuosa cornice della chiesa Saint-Pierre, con un repertorio che va dal barocco al contemporaneo."
    ],
    "fr": [
      "Le Festival International de Mandoline : au mois de juillet, 4 jours de concerts et animations, avec des artistes internationaux, dans le somptueux cadre de l'église Saint-Pierre, avec un répertoire qui s’étend du baroque au contemporain."
    ],
    "nl": [
      "Het Internationale Mandolinefestival: in juli, 4 dagen van concerten en evenementen, met internationale artiesten, in het prachtige kader van de Saint-Pierre kerk, met een repertoire gaande van barok tot hedendaags."
    ],
    "es": [
      "Festival Internacional de la Mandolina: en julio, 4 días de conciertos y actos, con artistas internacionales, en el suntuoso marco de la iglesia Saint-Pierre, con un repertorio que va del barroco al contemporáneo."
    ]
  },
