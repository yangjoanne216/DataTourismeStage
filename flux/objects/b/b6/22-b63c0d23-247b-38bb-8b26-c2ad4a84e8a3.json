      "dc:description": {
        "de": [
          "Foyer du Petit Mot, Rue du Petit Mot (Straße senkrecht zur Rue St James oder Zugang über die Rue Clemenceau an der Ecke des Restaurants Marie Colline)\n\nFreier Eintritt montags bis freitags von 9.00 bis 12.00 Uhr und von 14.00 bis 17.00 Uhr."
        ],
        "en": [
          "Foyer du Petit Mot, rue du Petit Mot (street perpendicular to rue St James or access by rue Clemenceau at the corner of the restaurant Marie Colline)\n\nFree admission from Monday to Friday from 9am to 12pm and from 2pm to 5pm."
        ],
        "it": [
          "Foyer du Petit Mot, rue du Petit Mot (strada perpendicolare a rue St James o accesso da rue Clemenceau all'angolo del ristorante Marie Colline)\n\nIngresso libero dal lunedì al venerdì dalle 9.00 alle 12.00 e dalle 14.00 alle 17.00."
        ],
        "fr": [
          "Foyer du Petit Mot, rue du Petit Mot (rue perpendiculaire à la rue St James ou accès par la rue Clemenceau à l'angle du restaurant Marie Colline). \n\nEntrée libre du lundi au vendredi de 9h à 12h et de 14h à 17h."
        ],
        "nl": [
          "Foyer du Petit Mot, rue du Petit Mot (straat loodrecht op de rue St James of toegang vanaf de rue Clemenceau op de hoek van het restaurant Marie Colline)\n\nGratis toegang van maandag tot vrijdag van 9 tot 12 uur en van 14 tot 17 uur."
        ],
        "es": [
          "Foyer du Petit Mot, rue du Petit Mot (calle perpendicular a la rue St James o acceso desde la rue Clemenceau en la esquina del restaurante Marie Colline)\n\nEntrada gratuita de lunes a viernes de 9 a 12 y de 14 a 17 horas."
        ]
      },
