  "rdfs:comment": {
    "de": [
      "Möblierte Unterkunft in Cransac-les-Thermes, dem einzigen Kurort im Aveyron und der Stadt des Wohlbefindens. Geräumige Unterkunft, hoher Komfort, Ruhe und Erholung garantiert."
    ],
    "en": [
      "Furnished let located at the Aveyron spa resort for rheumatism."
    ],
    "it": [
      "Alloggio turistico ammobiliato a Cransac-les-Thermes, unico centro termale dell'Aveyron e città del benessere. Alloggio spazioso, grande comfort, calma e riposo assicurato."
    ],
    "fr": [
      "Meublé de tourisme à Cransac-les-Thermes, l'unique station thermale en Aveyron et la ville du bien-être. Logement spacieux, grand confort, calme et repos assurés."
    ],
    "nl": [
      "Gemeubileerde toeristische accommodatie in Cransac-les-Thermes, het enige kuuroord in Aveyron en de stad van het welzijn. Ruime accommodatie, groot comfort, kalmte en rust verzekerd."
    ],
    "es": [
      "Amueblado de turismo en Cransac-les-Thermes, el unico centro termal en Aveyron y la ciudad del bienestar."
    ]
