  "rdfs:comment": {
    "de": [
      "Dieses provenzalische Landhaus, zehn Minuten vom Zentrum von Aix-en-Provence entfernt, empfängt Sie inmitten der Natur."
    ],
    "en": [
      "This Provencal farmhouse at ten minutes from the center of Aix-en-Provence welcomes you in the heart of nature."
    ],
    "it": [
      "Questo casale provenzale, a dieci minuti dal centro di Aix-en-Provence, vi accoglie in mezzo alla natura."
    ],
    "fr": [
      "Ce mas provençal à dix minutes du centre d'Aix-en-Provence vous accueille en pleine nature."
    ],
    "es": [
      "Esta granja provenzal, a diez minutos del centro de Aix-en-Provence, le acoge en plena naturaleza."
    ],
    "nl": [
      "Deze Provençaalse boerderij, op tien minuten van het centrum van Aix-en-Provence, verwelkomt u midden in de natuur."
    ]
  },
