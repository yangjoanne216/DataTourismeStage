  "rdfs:comment": {
    "de": [
      "Die Kinder lernen die Techniken und Methoden der archäologischen Ausgrabung kennen, und zwar an einem falschen Ort aus dem Ersten Weltkrieg."
    ],
    "en": [
      "Children are introduced to the techniques and methods of archaeological excavation, on a mock site of the Great War."
    ],
    "it": [
      "I bambini vengono introdotti alle tecniche e ai metodi di scavo archeologico in un finto sito della Grande Guerra."
    ],
    "fr": [
      "Les enfants s’initient aux techniques et aux méthodes de la fouille archéologique, sur un faux-site de la Grande Guerre."
    ],
    "es": [
      "Los niños se inician en las técnicas y métodos de la excavación arqueológica en un simulacro de la Gran Guerra."
    ],
    "nl": [
      "Kinderen maken kennis met de technieken en methoden van archeologische opgravingen op een nagebouwde site uit de Grote Oorlog."
    ]
  },
