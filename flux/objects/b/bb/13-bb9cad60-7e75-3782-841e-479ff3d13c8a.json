  "rdfs:comment": {
    "de": [
      "Rochefort-Samson liegt in der Nähe einer der schönsten Schluchten der Monts du Matin, der Combe d'Oyans, die sich zu einer Landschaft aus Tälern und Hügeln hin öffnet."
    ],
    "en": [
      "Rochefort-Samson is located near one of the most beautiful gullies of the Monts du Matin, the Combe d'Oyans, which opens onto a landscape of valleys and hills."
    ],
    "it": [
      "Rochefort-Samson si trova vicino a uno dei più bei canaloni dei Monts du Matin, la Combe d'Oyans, che si apre su un paesaggio di valli e colline."
    ],
    "fr": [
      "Rochefort-Samson est située près d'un des plus beaux goulets des Monts du Matin, la Combe d'Oyans, qui s’ouvre sur un paysage de vallons et de collines."
    ],
    "es": [
      "Rochefort-Samson se encuentra cerca de uno de los más bellos barrancos de los Montes del Mato, la Combe d'Oyans, que se abre a un paisaje de valles y colinas."
    ],
    "nl": [
      "Rochefort-Samson ligt vlakbij de Combe d'Oyans, een van de mooiste bergengtes van de Monts du Matin, die uitloopt in een heuvelachtig landschap."
    ]
  },
