  "rdfs:comment": {
    "de": [
      "In Bonneval im Haute Vallée du Loir stellt der Betrieb Glandin handwerklich gebraute Biere her, vom Brauen bis zur Etikettierung."
    ],
    "en": [
      "Bonn’ Vallée beer is made from spring hops harvested at Le Glandin Farm in Bonneval."
    ],
    "it": [
      "A Bonneval, nell'Alta Valle del Loir, l'azienda agricola Glandin produce birre tradizionali, dalla produzione all'etichettatura."
    ],
    "fr": [
      "A Bonneval en Haute Vallée du Loir, l'exploitation du Glandin produit des bières artisanales, du brassage à l'étiquetage."
    ],
    "nl": [
      "In Bonneval in de Oberloirvallei produceert de boerderij Glandin traditionele bieren, van het brouwen tot het etiketteren."
    ],
    "es": [
      "En Bonneval, en el valle del Alto Loir, la granja Glandin produce cervezas tradicionales, desde la elaboración hasta el etiquetado."
    ]
  },
