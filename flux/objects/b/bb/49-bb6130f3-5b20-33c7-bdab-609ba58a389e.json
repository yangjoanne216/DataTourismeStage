  "rdfs:comment": {
    "de": [
      "Fun Archery ist eine Mischung aus Dodgeball und Paintball und ein offener Krieg zwischen zwei Teams von Bogenschützen."
    ],
    "en": [
      "Halfway between Dodgeball and Paintball, Fun Archery is an open war between two teams of archers."
    ],
    "it": [
      "A metà strada tra Dodgeball e Paintball, Fun Archery è una guerra aperta tra due squadre di arcieri."
    ],
    "fr": [
      "A mi-chemin entre le Dodgeball et le Paintball, le Fun Archery est une guerre ouverte opposant deux équipes d'archers."
    ],
    "nl": [
      "Halverwege tussen Dodgeball en Paintball, is Fun Archery een open oorlog tussen twee teams van schutters."
    ],
    "es": [
      "A medio camino entre el Dodgeball y el Paintball, Fun Archery es una guerra abierta entre dos equipos de arqueros."
    ]
  },
