  "rdfs:comment": {
    "de": [
      "Hotel de Charme im Stadtzentrum, 5 Minuten vom SNCF-Bahnhof, der Cité des Congrès, dem Château des ducs de Bretagne, Kultur- und Verwaltungszentren, öffentlichen Verkehrsmitteln und dem Ufer der Erdre entfernt."
    ],
    "en": [
      "Charming hotel located in the town centre, 5 minutes from the SNCF railway station, the congress centre, the Castle of the Dukes of Brittany, the cultural and administrative centres, public transport and the banks of the Erdre river."
    ],
    "it": [
      "Hotel di charme situato nel centro della città, a 5 minuti dalla stazione ferroviaria SNCF, dal centro congressi, dallo Château des ducs de Bretagne, dai centri culturali e amministrativi, dai trasporti pubblici e dalle rive dell'Erdre."
    ],
    "fr": [
      "Hôtel de Charme situé en centre ville, à 5 mn de la gare SNCF, de la cité des congrès, du Château des ducs de Bretagne, des centres culturels et administratifs, des transports en commun et des bords de l'Erdre."
    ],
    "es": [
      "Hotel con encanto situado en el centro de la ciudad, a 5 minutos de la estación de tren SNCF, del centro de congresos, del Château des ducs de Bretagne, de los centros culturales y administrativos, del transporte público y de las orillas del Erdre."
    ],
    "nl": [
      "Charmant hotel gelegen in het stadscentrum, op 5 minuten van het SNCF station, het congrescentrum, het Château des ducs de Bretagne, culturele en administratieve centra, openbaar vervoer en de oevers van de Erdre."
    ]
  },
