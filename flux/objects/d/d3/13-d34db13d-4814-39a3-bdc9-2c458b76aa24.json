  "rdfs:comment": {
    "de": [
      "Der Campingplatz Le Chêne Gris liegt in dem ländlichen Dorf Pommeuse , nur wenige Schritte vom SNCF-Bahnhof entfernt und bietet Ihnen einen Wasserkomplex für Groß und Klein!"
    ],
    "en": [
      "Located in the country village of Pommeuse, the Chêne Gris campsite, a stone's throw from the SNCF train station, offers an aquatic complex for young and old!"
    ],
    "it": [
      "Situato nel villaggio di campagna di Pommeuse, il campeggio Chêne Gris, a due passi dalla stazione ferroviaria SNCF, offre un complesso di piscine per grandi e piccini!"
    ],
    "fr": [
      "Situé dans le village de campagne de Pommeuse , le camping le Chêne Gris, à deux pas de gare SNCF, vous offre un complexe aquatique pour petits et grands !"
    ],
    "es": [
      "Situado en el pueblo rural de Pommeuse, el camping Chêne Gris, a dos pasos de la estación de tren SNCF, ofrece un complejo de piscinas para grandes y pequeños"
    ],
    "nl": [
      "De camping Chêne Gris, gelegen in het landelijke dorpje Pommeuse, op een steenworp afstand van het SNCF-station, biedt een zwemparadijs voor groot en klein!"
    ]
  },
