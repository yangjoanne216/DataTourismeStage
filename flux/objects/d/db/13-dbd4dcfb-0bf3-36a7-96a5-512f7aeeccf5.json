  "rdfs:comment": {
    "de": [
      "Annecy Zentrum, Altstadt, Bahnhof in unmittelbarer Nähe, 8 Gehminuten zum See. Das 2-Sterne Hôtel des Alpes (Nichtraucherhotel) wurde renoviert und verfügt über TV und W-Lan. Parken kostenlos von 19 bis 8 Uhr."
    ],
    "en": [
      "Central Annecy, old town, nearby train station, 8 minute walk to the lake. Renovated 2* Hôtel des Alpes with TV and free Wi-Fi. Free parking from 7pm to 8am."
    ],
    "it": [
      "Annecy centro, centro storico, stazione vicina, 8 minuti a piedi dal lago. L'hotel des Alpes 2 stelle rimodernato, TV, Wi-Fi gratuito. Parcheggio gratuito dalle 19 alle 8."
    ],
    "fr": [
      "Annecy centre, vieille ville, gare immédiat, 8 min à pied du lac. L'hôtel des Alpes 2 étoiles rénové, TV, Wifi gratuit. Parking offert 19h à 8h."
    ],
    "es": [
      "Centro de Annecy, casco antiguo, estación de tren inmediata, 8 minutos a pie del lago. Hôtel des Alpes renovado de 2 estrellas, TV, Wifi gratuito. Aparcamiento gratuito de 19 a 8 horas."
    ],
    "nl": [
      "Annecy centrum, oude stad, bij het station, op 8 min. lopen van het meer. Hotel des Alpes, 2 sterren gerenoveerd, TV, wifi. Parkeren gratis van 19.00 tot 08.00 uur."
    ]
