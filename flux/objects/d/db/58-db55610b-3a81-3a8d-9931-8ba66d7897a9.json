  "rdfs:comment": {
    "de": [
      "Pub in Les Goudes, direkt am Meer, mit einer Terrasse mit Panoramablick, die auch kleine Snacks ( Pizza, Salat... ) und Sportübertragungen anbietet. Happy Hours jeden Tag von 17:00 bis 21:00 Uhr."
    ],
    "en": [
      "Pub in Les Goudes, on the seafront with a terrace with a panoramic view, which also offers small restaurants (pizza, salad ...) and sports broadcasts. Happy hours every day from 5pm to 9pm."
    ],
    "it": [
      "Pub a Les Goudes, sul lungomare con terrazza panoramica, che offre anche piccoli ristoranti (pizza, insalata...) e trasmissioni sportive. Happy hour tutti i giorni dalle 17.00 alle 21.00."
    ],
    "fr": [
      "Pub aux Goudes, en bord de mer avec une terrasse à la vue panoramique, qui propose aussi de la petite restauration( pizza, salade... ) et les retransmissions sportives. Happy hours tous les jours de 17h00 à 21h00."
    ],
    "nl": [
      "Pub in Les Goudes, aan zee, met een terras met panoramisch uitzicht, waar ook snacks (pizza, salade...) en sportuitzendingen worden aangeboden. Happy hours elke dag van 17.00 tot 21.00 uur."
    ],
    "es": [
      "Pub en Les Goudes, en primera línea de mar, con terraza con vistas panorámicas, que también ofrece aperitivos (pizza, ensalada...) y retransmisiones deportivas. Happy hours todos los días de 17:00 a 21:00."
    ]
  },
