  "rdfs:comment": {
    "de": [
      "Konzert der Schüler der Musikschule von Loches. Blasinstrumente (Saxofon, Flöte, Klarinette, Horn...), Streichinstrumente (Geige, Klavier, Harfe, Gitarre) und Schlaginstrumente (Schlagzeug). Instrumentalensembles, Orchester für junge Musiker."
    ],
    "en": [
      "Concert of the students of the music school of Loches. Wind instruments (sax, flute, clarinet, horn...), strings (violin, piano, harp, guitar), and percussion (drums). Instrumental ensembles, young musicians' orchestras."
    ],
    "it": [
      "Concerto degli studenti della scuola di musica di Loches. Strumenti a fiato (sax, flauto, clarinetto, corno...), a corda (violino, pianoforte, arpa, chitarra) e a percussione (batteria). Ensemble strumentali, orchestre di giovani musicisti."
    ],
    "fr": [
      "Concert des élèves de l'école de musique de Loches. Instruments à vent (saxo, flûte, clarinette, cor...), à cordes (violon, piano, harpe, guitare), et à percussion (batterie). Ensembles instrumentaux, orchestres de jeunes musiciens."
    ],
    "es": [
      "Concierto de los alumnos de la escuela de música de Loches. Instrumentos de viento (saxo, flauta, clarinete, trompa...), cuerda (violín, piano, arpa, guitarra) y percusión (batería). Conjuntos instrumentales, orquestas de jóvenes músicos."
    ],
    "nl": [
      "Concert van de leerlingen van de muziekschool van Loches. Blaasinstrumenten (sax, fluit, klarinet, hoorn...), strijkers (viool, piano, harp, gitaar) en slagwerk (drums). Instrumentale ensembles, orkesten van jonge muzikanten."
    ]
  },
