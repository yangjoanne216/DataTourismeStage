  "rdfs:comment": {
    "de": [
      "Von Prugnanes aus, auf dem Katharerpfad oder auf den Wegen des Fenouillèdes, stundenweise, halbtags, am Wochenende oder wochenweise, lassen Sie sich führen... von einem Esel."
    ],
    "en": [
      "From Prugnanes, on the Cathar path or on the paths of the Fenouillèdes, for an hour, a half-day, a weekend or a week, let yourself be guided... by a donkey."
    ],
    "it": [
      "Partendo da Prugnanes, sul sentiero cataro o sui sentieri di Fenouillèdes, a ore, mezza giornata, weekend o settimana, lasciatevi guidare... da un asino."
    ],
    "fr": [
      "Au départ de Prugnanes, sur le sentier cathare ou sur les chemins du Fenouillèdes, à l'heure, à la demi-journée, au week-end ou à la semaine, laissez-vous guider... par un âne."
    ],
    "nl": [
      "Vanuit Prugnanes, op het Katharenpad of op de paden van Fenouillèdes, per uur, halve dag, weekend of week, laat u leiden... door een ezel."
    ],
    "es": [
      "Partiendo de Prugnanes, por el camino de los cátaros o por los senderos de las Fenouillèdes, por horas, media jornada, fin de semana o semana, déjese guiar... por un burro."
    ]
  },
