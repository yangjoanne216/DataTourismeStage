  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/e66a366a-4a35-3760-a4c7-2c9240895912",
      "dc:description": {
        "de": [
          "Croissanterie, hausgemachte Konditorei, Brot, Tagesgericht, Suppen, Saladerie, Sandwiches, Teesalon.\nIn der Nähe des Bahnhofs, authentischer Rahmen, WIFI-Zugang.\nVor Ort oder zum Mitnehmen."
        ],
        "en": [
          "Croissant shop, homemade pastries, bread, dish of the day, soups, salad, sandwich shop, tea room.\nNear train station, authentic setting, WIFI access.\nOn the spot or to take away."
        ],
        "it": [
          "Croissanterie, pasticceria artigianale, pane, specialità del giorno, zuppe, insalateria, paninoteca, sala da tè.\nVicino alla stazione, ambiente autentico, accesso WIFI.\nSul posto o da portare via."
        ],
        "fr": [
          "Croissanterie, pâtisserie maison, pain, plat du jour, soupes, saladerie, sandwicherie, salon de thé.\nProche gare, cadre authentique, accès WIFI.\nSur place ou à emporter."
        ],
        "es": [
          "Croissanterie, pastelería casera, pan, platos del día, sopas, bar de ensaladas, sandwichería, salón de té.\nCerca de la estación, entorno auténtico, acceso WIFI.\nEn el acto o para llevar."
        ],
        "nl": [
          "Croissanterie, huisgemaakt gebak, brood, dagschotels, soepen, saladebar, broodjeszaak, tearoom.\nDicht bij het station, authentieke omgeving, WIFI toegang.\nTer plaatse of om mee te nemen."
        ]
      },
      "@type": [
        "Description"
      ],
