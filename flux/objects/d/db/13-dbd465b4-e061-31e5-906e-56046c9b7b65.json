  "rdfs:comment": {
    "de": [
      "Jeden Samstagmittag Mahlzeiten vor Ort oder zum Mitnehmen.  Juli und August Mittagsmahlzeiten von Montag bis Samstag, Freitag- und Samstagabend auf Reservierung."
    ],
    "en": [
      "Every Saturday lunchtime meals on the spot or to take away.  July and August lunchtime meals from Monday to Saturday, Friday and Saturday evenings by reservation."
    ],
    "it": [
      "Tutti i sabati a pranzo pasti in loco o da asporto.  Luglio e agosto pasti a pranzo dal lunedì al sabato, venerdì e sabato sera su prenotazione."
    ],
    "fr": [
      "Tous les samedis midis repas sur place ou à emporter.  Juillet et aout repas  le midi du lundi au samedi, vendredi et samedi soir sur réservation."
    ],
    "es": [
      "Todos los sábados, comidas in situ o para llevar.  Julio y agosto comidas de lunes a sábado, viernes y sábados por la noche previa reserva."
    ],
    "nl": [
      "Elke zaterdag middagmaal ter plaatse of om mee te nemen.  In juli en augustus middagmaaltijden van maandag tot zaterdag, vrijdag- en zaterdagavond op reservering."
    ]
  },
  "rdfs:label": {
