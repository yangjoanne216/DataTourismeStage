  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/2f918df8-ff91-3f5f-b9d8-ee41428636ef",
      "dc:description": {
        "de": [
          "Bearbeitung von Steingut, Steinzeug und Porzellan mit anschließender Emaillierung der Stücke. Arbeiten mit Acrylmalerei, Tinte und Gouache auf Leinwand. Arbeiten von Kindern mit verschiedenen Materialien."
        ],
        "en": [
          "Earthenware, stoneware and porcelain work followed by enamelling of the pieces. Work of acrylic paint, ink, gouache on canvas. Children's work with various materials."
        ],
        "it": [
          "Lavorazione di terracotta, gres e porcellana e successiva smaltatura dei pezzi. Pittura ad acrilico, inchiostro e gouache su tela. Lavori dei bambini con vari materiali."
        ],
        "fr": [
          "Travail de la faïence, du grès et de la porcelaine suivi d'émaillage des pièces. Travaux de peinture acrylique, encre, gouache sur toile. Travail des enfants avec des matériaux divers."
        ],
        "nl": [
          "Werken met aardewerk, steengoed en porselein gevolgd door het glazuren van de stukken. Acryl, inkt en gouache op doek. Kinderwerk met verschillende materialen."
        ],
        "es": [
          "Trabajo con loza, gres y porcelana y posterior esmaltado de las piezas. Pintura acrílica, tinta y gouache sobre lienzo. Trabajo de los niños con diversos materiales."
        ]
      },
      "@type": [
        "Description"
      ]
    }
