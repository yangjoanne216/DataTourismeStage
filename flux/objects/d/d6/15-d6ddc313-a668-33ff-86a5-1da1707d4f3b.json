  "rdfs:comment": {
    "de": [
      "8 Rennveranstaltungen pro Jahr, die von der Pferderenngesellschaft von Lignières-en-Berry organisiert werden. Trab, Galopp und Springen - ein abwechslungsreiches Programm!"
    ],
    "en": [
      "8 race meetings each year organised by the Lignières-en-Berry horse racing society. Trotting, galloping, jumping, a rich and varied program!"
    ],
    "it": [
      "8 riunioni di corse all'anno organizzate dalla società ippica Lignières-en-Berry. Trotto, galoppo, salto, un programma ricco e vario!"
    ],
    "fr": [
      "8 réunions de courses chaque année organisées par la société des courses hippiques de Lignières-en-Berry. Du trot du galop, du saut d'obstacles un programme riche et varié!"
    ],
    "nl": [
      "8 renwedstrijden per jaar georganiseerd door de paardenrenvereniging van Lignières-en-Berry. Draven, galopperen, springen, een rijk en gevarieerd programma!"
    ],
    "es": [
      "8 carreras anuales organizadas por la sociedad hípica de Lignières-en-Berry. Trote, galope, salto, ¡un programa rico y variado!"
    ]
  },
