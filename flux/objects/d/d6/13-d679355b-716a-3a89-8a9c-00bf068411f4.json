  "rdfs:comment": {
    "de": [
      "Lionel Richard, Accompagnateur de Tourisme Equestre, empfängt Sie auf der Ranch la Ramasse in Val Cenis Termignon für Ponyausritte (1/4h, 1/2h, 1h) oder Reitausflüge (1h, 2h, halber Tag, ganzer Tag) im Wald und in den Bergen."
    ],
    "en": [
      "Lionel Richard, Equestrian Tourism guide, welcomes you to the ranch la Ramasse in Val Cenis Termignon for pony rides (1/4h, 1/2, 1 hour) or horseback riding (1h, 2h, half day, day) in the forest and mountain."
    ],
    "it": [
      "Lionel Richard, guida del turismo equestre, vi accoglie al ranch la Ramasse in Val Cenis Termignon per passeggiate in pony (1/4h, 1/2h, 1h) o a cavallo (1h, 2h, mezza giornata, giornata) nella foresta e in montagna."
    ],
    "fr": [
      "Lionel Richard, Accompagnateur de Tourisme Equestre, vous accueille au ranch la Ramasse à Val Cenis Termignon pour des balades à poney (1/4h, 1/2h, 1h) ou randonnée à cheval (1h, 2h, demi-journée, journée) en forêt et montagne."
    ],
    "es": [
      "Lionel Richard, Guía de Turismo Ecuestre, le da la bienvenida al rancho la Ramasse en Val Cenis Termignon para realizar paseos en poni (1/4h, 1/2h, 1h) o a caballo (1h, 2h, medio día, día) por el bosque y la montaña."
    ],
    "nl": [
      "Lionel Richard, Gids voor ruitertoerisme, heet u welkom op de Ramasse-ranch in Val Cenis-Termignon voor ponyrijden (1 / 4u, 1 / 2u, 1u) of paardrijden (1u, 2u, halve dag, dag) in het bos en berg."
    ]
  },
