  "rdfs:comment": {
    "de": [
      "Adezzo Beauty ist ein Online-Shop, der es jungen Make-up-Künstlerinnen ermöglicht, Kosmetikprodukte nach ihrem eigenen Bild zu kreieren und so ihre Träume zu verwirklichen."
    ],
    "en": [
      "Adezzo Beauty is an online store that allows young make-up artists to create cosmetic products in their image and thus realize their dream."
    ],
    "it": [
      "Adezzo Beauty è un negozio online che permette ai giovani make-up artist di creare prodotti cosmetici a loro immagine e somiglianza, realizzando così i loro sogni."
    ],
    "fr": [
      "Adezzo Beauty est une boutique en ligne qui permet aux jeunes artistes Make-up de créer des produits cosmétiques à leur image et ainsi réaliser leur rêve."
    ],
    "nl": [
      "Adezzo Beauty is een online winkel die jonge visagisten in staat stelt cosmetische producten naar hun eigen beeld te creëren en zo hun dromen waar te maken."
    ],
    "es": [
      "Adezzo Beauty es una tienda online que permite a los jóvenes maquilladores crear productos cosméticos a su imagen y semejanza y así hacer realidad sus sueños."
    ]
  },
