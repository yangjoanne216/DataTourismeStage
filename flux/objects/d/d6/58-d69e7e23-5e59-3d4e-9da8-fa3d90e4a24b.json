  "rdfs:comment": {
    "de": [
      "Handwerklich hergestelltes Eis, Crêpes, Waffeln, Eisgebäck und frische Fruchtsäfte in Aix-en-Provence, nur wenige Meter vom Cours Mirabeau entfernt!"
    ],
    "en": [
      "Artisanal ice creams, pancakes, waffles, frozen pastries and fresh fruit juices in Aix-en-Provence, a few meters from the Cours Mirabeau!"
    ],
    "it": [
      "Gelati artigianali, crêpes, waffles, pasticceria surgelata e succhi di frutta freschi ad Aix-en-Provence, a pochi metri dal Cours Mirabeau!"
    ],
    "fr": [
      "Glaces artisanales, crêpes, gaufres, pâtisseries glacées et jus de fruits frais à Aix-en-Provence, à quelques mètres du cours Mirabeau !"
    ],
    "es": [
      "Helados artesanales, crêpes, gofres, bollería helada y zumos de fruta fresca en Aix-en-Provence, ¡a pocos metros del Cours Mirabeau!"
    ],
    "nl": [
      "Ambachtelijk ijs, crêpes, wafels, diepvriesgebak en verse vruchtensappen in Aix-en-Provence, op enkele meters van de Cours Mirabeau!"
    ]
  },
