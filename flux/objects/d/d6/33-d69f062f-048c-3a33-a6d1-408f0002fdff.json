  "COVID19SpecialMeasures": {
    "de": [
      "GESUNDHEITSMASSNAHMEN: Drive-by-Abholung, Verpflegung vor Ort im Freien, hydroalkoholisches Gel zur Verfügung, soziale Distanzierung. Verkauf zum Mitnehmen und Lieferung zu den üblichen Zeiten."
    ],
    "en": [
      "HEALTH MEASURES: drive-through, outdoor catering, hydro-alcohol gel available, social distancing. Take-away and delivery at the usual times."
    ],
    "it": [
      "MISURE SANITARIE: drive-through, ristorazione all'aperto, disponibilità di gel idroalcolico, allontanamento sociale. Asporto e consegna negli orari consueti."
    ],
    "fr": [
      "MESURES SANITAIRES : retrait drive, restauration sur place en extérieur, gel hydroalcoolique à disposition, distanciation sociale. Vente à emporter et livraison aux horaires habituels."
    ],
    "es": [
      "MEDIDAS SANITARIAS: drive-through, restauración al aire libre, gel hidroalcohólico disponible, distanciamiento social. Comida para llevar y entrega en los horarios habituales."
    ],
    "nl": [
      "GEZONDHEIDSMAATREGELEN: drive-through, catering in de open lucht, hydro-alcohol gel beschikbaar, sociale afstand. Afhalen en bezorgen op de gebruikelijke tijden."
    ]
  },
