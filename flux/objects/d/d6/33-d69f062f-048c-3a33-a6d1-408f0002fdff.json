  "rdfs:comment": {
    "de": [
      "Sind Sie auf der Durchreise in Saint-Paul-sur-Save? Haben Sie einen kleinen Hunger? Machen Sie Halt im Pizzasnack von O'Régal, um Ihren Magen zu verwöhnen!"
    ],
    "en": [
      "Are you passing through Saint-Paul-sur-Save? Are you hungry? Stop by the O'Régal Snack-Pizzas, to treat your stomach!"
    ],
    "it": [
      "Siete in visita a Saint-Paul-sur-Save? Hai fame? Fermatevi allo Snack-Pizza O'Régal, per curare il vostro stomaco!"
    ],
    "fr": [
      "Vous êtes de passage à Saint-Paul-sur-Save ? Vous avez un petit creux ? Arrêtez-vous au Snack-Pizzas d'O'Régal, pour régaler votre estomac !"
    ],
    "es": [
      "¿Va a visitar Saint-Paul-sur-Save? ¿Tienes hambre? Pásate por el O'Régal Snack-Pizzas, para darle un capricho a tu estómago"
    ],
    "nl": [
      "Bezoekt u Saint-Paul-sur-Save? Heb je honger? Stop bij de O'Régal Snack-Pizza's, om uw maag te verwennen!"
    ]
  },
  "rdfs:label": {
