  "rdfs:comment": {
    "de": [
      "Produktionshof mit BIO-Gemüseanbau in Rencurel auf 750 m Höhe. Verkauf ab Hof am Samstag von Saisongemüse, Beeren, Kräutern. Besuche auf dem Bauernhof."
    ],
    "en": [
      "Organic market gardening production farm located in Rencurel at an altitude of 750 m. Sale at the farm on Saturdays of seasonal vegetables, small fruits, aromatic plants. Visits to the farm."
    ],
    "it": [
      "Azienda agricola biologica ad indirizzo orticolo situata a Rencurel a 750 m di altitudine. Sabato vendita in azienda di ortaggi di stagione, piccoli frutti, piante aromatiche. Visite alla fattoria."
    ],
    "fr": [
      "Ferme de production en maraîchage BIO située à Rencurel à 750 m d'altitude. Vente à la ferme le samedi de légumes de saison, petits fruits, plantes aromatiques. Visites à la ferme."
    ],
    "es": [
      "Granja de horticultura ecológica situada en Rencurel a 750 m de altitud. Ventas agrícolas los sábados de hortalizas de temporada, pequeños frutos, plantas aromáticas. Visitas a la granja."
    ],
    "nl": [
      "Biologische tuinderij gelegen in Rencurel op 750 m hoogte. Verkoop op zaterdag van seizoensgroenten, klein fruit, aromatische planten. Bezoeken aan de boerderij."
    ]
  },
  "rdfs:label": {
