  "rdfs:comment": {
    "de": [
      "Das Restaurant BALM (Bienvenue A La Maison) bietet Frühstück, Brunch, Mittag- und Abendessen. Ein wahrer Kokon, in dem sich Sanftheit, Gastfreundschaft und Wärme vereinen."
    ],
    "en": [
      "The BALM (Bienvenue A La Maison) restaurant offers breakfast, brunch, lunch and dinner. A real cocoon where softness, hospitality and warmth are combined."
    ],
    "it": [
      "Il ristorante BALM (Bienvenue A La Maison) offre colazione, brunch, pranzo e cena. Un vero e proprio bozzolo dove si combinano morbidezza, ospitalità e calore."
    ],
    "fr": [
      "Le restaurant BALM (Bienvenue A La Maison) propose petit déjeuner, brunch, déjeuner, dîner. Un véritable cocon où se mêlent douceur, hospitalité et chaleur."
    ],
    "nl": [
      "Het restaurant BALM (Bienvenue A La Maison) biedt ontbijt, brunch, lunch en diner. Een echte cocon waar zachtheid, gastvrijheid en warmte worden gecombineerd."
    ],
    "es": [
      "El restaurante BALM (Bienvenue A La Maison) ofrece desayunos, brunch, comidas y cenas. Un verdadero capullo donde se combinan la suavidad, la hospitalidad y la calidez."
    ]
  },
