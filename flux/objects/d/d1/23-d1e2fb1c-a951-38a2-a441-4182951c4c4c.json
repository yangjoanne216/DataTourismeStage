  "rdfs:comment": {
    "de": [
      "Folgen Sie dem Goupil, entdecken Sie das Kulturerbe von Saint-Aulaye, indem Sie Rätsel lösen, und finden Sie das Versteck von Zéroïks Schatz."
    ],
    "en": [
      "Follow the Goupil, discover the heritage of Saint-Aulaye by finding riddles and find the hiding place of Zéroïk's treasure."
    ],
    "it": [
      "Seguite il Goupil, scoprite il patrimonio di Saint-Aulaye trovando gli enigmi e trovate il nascondiglio del tesoro di Zéroïk."
    ],
    "fr": [
      "Suivez le Goupil, découvrez  le patrimoine de Saint-Aulaye en trouvant des énigmes et trouvez la cachette du trésor de Zéroïk."
    ],
    "nl": [
      "Volg de Goupil, ontdek het erfgoed van Saint-Aulaye door de raadsels te vinden en vind de verstopplaats van de schat van Zéroïk."
    ],
    "es": [
      "Siga a los Goupil, descubra el patrimonio de Saint-Aulaye encontrando los enigmas y encuentre el escondite del tesoro de Zéroïk."
    ]
  },
