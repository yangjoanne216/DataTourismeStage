  "rdfs:comment": {
    "de": [
      "Thierry Blondeau, Küchenchef, lässt Sie durch seine Spezialitäten, seine Speisekarte und seine Menüs die Region und die lokale Gastronomie mit frischen Produkten entdecken."
    ],
    "en": [
      "Thierry Blondeau, head chef, will make you discover through his specialities, his a la carte and his menus: the terroir and the local gastronomy from fresh products ."
    ],
    "it": [
      "Thierry Blondeau, capo chef, vi farà scoprire attraverso le sue specialità, la sua carta e i suoi menu: la gastronomia locale a partire da prodotti freschi."
    ],
    "fr": [
      "Thierry Blondeau,  chef de cuisine, vous fera découvrir à travers ses spécialités, sa carte et ses menus : le terroir et la Gastronomie locale à partir de produit frais ."
    ],
    "es": [
      "Thierry Blondeau, jefe de cocina, le hará descubrir a través de sus especialidades, su carta y sus menús: la gastronomía local a partir de productos frescos."
    ],
    "nl": [
      "Thierry Blondeau, chef-kok, laat u via zijn specialiteiten en zijn menu de lokale gastronomie van verse producten ontdekken."
    ]
  },
