  "rdfs:comment": {
    "de": [
      "Restaurant - Lounge - Weinbar - Grillspezialitäten in Moûtiers\r\nTraditionelle Küche, savoyardische Spezialitäten.\r\nLoungebereich, um ein Glas Wein mit einer großen Auswahl an Tapas zu genießen."
    ],
    "en": [
      "Restaurant - Lounge - Wine Bar - Grill Speciality in Moûtiers\r\nTraditional cuisine, Savoyard specialties.\r\nLounge space to taste a glass of wine and a large range of Tapas.\r\nThe W labeled \"Home bike\", also offers services and a menu adapted to cyclists."
    ],
    "it": [
      "Ristorante - Lounge - Wine bar - Specialità grill a Moûtiers\r\nCucina tradizionale, specialità savoiarde.\r\nArea lounge per gustare un bicchiere di vino con un'ampia scelta di tapas."
    ],
    "fr": [
      "Restaurant - Lounge - Bar à vins - Spécialité Grill à Moûtiers\r\nCuisine traditionnelle, spécialités savoyardes.\r\nEspace Lounge pour déguster un verre de vin accompagné d'un grand choix de Tapas."
    ],
    "es": [
      "Restaurante - Salón - Bar de vinos - Especialidad en parrilla en Moûtiers\r\nCocina tradicional, especialidades saboyanas.\r\nZona de estar para disfrutar de una copa de vino con una amplia selección de tapas."
    ],
    "nl": [
      "Restaurant - Lounge - Wijnbar - Grillspecialiteit in Moûtiers\r\nTraditionele keuken, Savoyaardse specialiteiten.\r\nLoungehoek om te genieten van een glas wijn met een ruime keuze aan tapas."
    ]
  },
