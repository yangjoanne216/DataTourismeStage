  "rdfs:comment": {
    "de": [
      "La boutique chez Kiki. Verkauf regionaler Produkte: Fruchtsäfte, Konfitüren, Nüsse, Lavendel, Diois-Weine, Herbstfrüchte. Verkostung von Weinen aus Châtillon-en-Diois sowie der Clairette Jaillance."
    ],
    "en": [
      "La boutique chez Kiki. Shop with local products: fruit juices, jams, walnuts, lavender, Diois’ wines, autumnal fruits… Wine tasting of Châtillon-en-Diois and Clairette Jaillance wines."
    ],
    "it": [
      "Negozio da Kiki, prodotti locali: succhi di frutta, marmellate, miele, noci, lavanda, vini Diois, Clairette, vino di noci, frutti autunnali... Degustazione dei vini di Châtillon-en-Diois e della Clairette de Die Jaillance."
    ],
    "fr": [
      "Boutique chez Kiki, produits du terroir : jus de fruits, confitures, miel, noix, lavande, vins du Diois, Clairette, vin de noix, fruits d'automne... Dégustation des vins de Châtillon-en-Diois et de la Clairette de Die Jaillance."
    ],
    "es": [
      "Tienda en Kiki's, productos locales: zumos de frutas, mermeladas, miel, nueces, lavanda, vinos de Diois, Clairette, vino de nueces, frutas de otoño... Degustación de vinos de Châtillon-en-Diois y Clairette de Die Jaillance."
    ],
    "nl": [
      "Bij boutique chez Kiki. Winkel met streekproducten: vruchtensap, jam, noten, lavendel, wijnen uit Diois, herfstfruit, etc. Proeverij van de Châtillon-en-Diois en de Clairette Jaillance wijnen."
    ]
  },
