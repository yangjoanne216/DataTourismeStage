  "rdfs:comment": {
    "de": [
      "Familiencampingplatz in einem geschützten, ruhigen und grünen Gebiet in der Nähe von Rochefort, Saintes, Royan, Marennes, der Ile d'Oléron... Alter Golf von Saintonge, 1 km von der Tour de Broue entfernt, außergewöhnliches Panorama über die Sümpfe von Brouage."
    ],
    "en": [
      "Family campsite in a preserved, calm and green site near Rochefort, Saintes, Royan, Marennes, Ile d'Oléron...Ancient gulf of Saintonge, 1km from the Tour de Broue, exceptional panorama on the marshes of Brouage."
    ],
    "it": [
      "Campeggio familiare in un sito preservato, tranquillo e verde, vicino a Rochefort, Saintes, Royan, Marennes, Ile d'Oléron... Ex golfo di Saintonge, a 1 km dalla Tour de Broue, panorama eccezionale sulle paludi di Brouage."
    ],
    "fr": [
      "Camping familial dans un site préservé, calme et verdoyant à proximité de Rochefort, Saintes, Royan, Marennes, l'Ile d'Oléron...Ancien golfe de Saintonge, à 1km de la Tour de Broue, panorama exceptionnel sur les marais de Brouage."
    ],
    "nl": [
      "Familiecamping in een behouden, rustige en groene omgeving nabij Rochefort, Saintes, Royan, Marennes, Ile d'Oléron... Voormalige golf van Saintonge, 1km van de Tour de Broue, uitzonderlijk panorama op de moerassen van Brouage."
    ],
    "es": [
      "Camping familiar en un sitio preservado, tranquilo y verde, cerca de Rochefort, Saintes, Royan, Marennes, Ile d'Oléron... Antiguo golfo de Saintonge, a 1 km de la Tour de Broue, panorama excepcional sobre las marismas de Brouage."
    ]
  },
