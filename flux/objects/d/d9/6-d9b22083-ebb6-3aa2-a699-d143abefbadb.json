  "rdfs:comment": {
    "de": [
      "Heißluftballonfahrten. Lassen Sie sich vom Wind treiben, die herrlichen Landschaften des Burgunds ziehen langsam vorbei."
    ],
    "en": [
      "Hot air balloon flights. As the wind blows, discover the superb landscapes of Burgundy that will slowly unfold before your eyes."
    ],
    "it": [
      "Voli in mongolfiera. Mentre il vento soffia, scoprite i superbi paesaggi della Borgogna che si dispiegano lentamente davanti ai vostri occhi."
    ],
    "fr": [
      "Vols en montgolfière. Au gré des vents, découvrez les superbes paysages de Bourgogne qui défileront lentement sous vos yeux."
    ],
    "es": [
      "Vuelos en globo aerostático. Mientras sopla el viento, descubra los magníficos paisajes de Borgoña que se desplegarán lentamente ante sus ojos."
    ],
    "nl": [
      "Luchtballonvaarten. Terwijl de wind waait, ontdekt u de prachtige landschappen van de Bourgogne die zich langzaam voor uw ogen ontvouwen."
    ]
  },
