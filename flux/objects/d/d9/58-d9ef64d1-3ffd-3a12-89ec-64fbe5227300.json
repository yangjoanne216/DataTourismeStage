  "rdfs:comment": {
    "de": [
      "Traditionelle neapolitanische Küche: culatura di alici, Oktopus aus dem Felsen mit Salat, sece alla Luciana ...Und Pizza von Rang! Restaurant mit dem Gütesiegel Ospitalità Italianna."
    ],
    "en": [
      "Traditional Neapolitan cuisine: culatura di alici, rock octopus in salad, sece alla Luciana ... And renowned pizza! Restaurant labeled Ospitalità Italianna."
    ],
    "it": [
      "Cucina tradizionale napoletana: culatura di alici, polpo di scoglio in insalata, sece alla Luciana...e la famosa pizza! Ristorante con il marchio Ospitalità Italiana."
    ],
    "fr": [
      "Cuisine traditionnelle napolitaine : culatura di alici, poulpe de roche en salade, sece alla Luciana ...Et pizza de renom ! Restaurant labellisé Ospitalità Italianna."
    ],
    "nl": [
      "Traditionele Napolitaanse keuken: culatura di alici, rotsoctopus in salade, sece alla Luciana ...en beroemde pizza! Restaurant met het label Ospitalità Italianna."
    ],
    "es": [
      "Cocina tradicional napolitana: culatura di alici, pulpo de roca en ensalada, sece alla Luciana... ¡Y las famosas pizzas! Restaurante con el sello de calidad Ospitalità Italianna."
    ]
  },
