  "rdfs:comment": {
    "de": [
      "Der Uhrenturm wurde 1672 mit Steinen aus dem Schloss erbaut und ist ein Symbol für die Unabhängigkeit von Eygalières."
    ],
    "en": [
      "Built in 1672 with stones from the castle, the Clock Tower is the symbol of Eygalières' independence."
    ],
    "it": [
      "Costruita nel 1672 con le pietre del castello, la Torre dell'Orologio è il simbolo dell'indipendenza di Eygalières."
    ],
    "fr": [
      "Bâtie en 1672 avec des pierres venant du château, la Tour de l'Horloge est le symbole de l'indépendance d'Eygalières."
    ],
    "nl": [
      "De in 1672 met stenen van het kasteel gebouwde klokkentoren is het symbool van de onafhankelijkheid van Eygalières."
    ],
    "es": [
      "Construida en 1672 con piedras del castillo, la Torre del Reloj es el símbolo de la independencia de Eygalières."
    ]
  },
