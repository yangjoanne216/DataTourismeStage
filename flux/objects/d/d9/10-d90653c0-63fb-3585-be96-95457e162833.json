  "rdfs:comment": {
    "de": [
      "Das Festival d'Anicé empfängt Groß und Klein in La Bresse für drei Tage voller Animationen, Aufführungen, Spiele und Lachen... ein Moment des Teilens 100% für die ganze Familie."
    ],
    "en": [
      "The Anicé Festival welcomes young and old to La Bresse for 3 days of entertainment, shows, games and laughter... a moment of sharing 100% family."
    ],
    "it": [
      "Il Festival Anicé accoglie grandi e piccini a La Bresse per 3 giorni di animazione, spettacoli, giochi e risate... un momento di condivisione al 100% in famiglia."
    ],
    "fr": [
      "Le Festival d'Anicé accueille petits et grands à La Bresse pour 3 jours d'animations, de spectacles, de jeux et de rire... un moment de partage 100% famille."
    ],
    "nl": [
      "Het Anicé Festival verwelkomt jong en oud in La Bresse voor 3 dagen van entertainment, shows, spelletjes en lachen... een moment van 100% familie delen."
    ],
    "es": [
      "El Festival Anicé acoge a grandes y pequeños en La Bresse durante 3 días de animaciones, espectáculos, juegos y risas... un momento para compartir al 100% en familia."
    ]
  },
