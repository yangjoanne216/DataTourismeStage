  "rdfs:comment": {
    "de": [
      "Kommen Sie mit Ihren Kindern und nutzen Sie die Orientierungsparcours auf dem historischen Gelände der Kapelle Saint Michel."
    ],
    "en": [
      "Come with your children and enjoy the orientation courses on the historical site of the Saint Michel chapel."
    ],
    "it": [
      "Venite con i vostri bambini e godetevi i corsi di orientamento sul sito storico della cappella Saint Michel."
    ],
    "fr": [
      "Venez avec vos enfants et profitez des parcours d'orientation sur le site historique de la chapelle Saint Michel."
    ],
    "es": [
      "Venga con sus hijos y disfrute de los cursos de orientación en el sitio histórico de la capilla Saint Michel."
    ],
    "nl": [
      "Kom met uw kinderen en geniet van de oriëntatielessen op de historische site van de Sint-Michielskapel."
    ]
  },
