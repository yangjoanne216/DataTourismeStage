  "rdfs:comment": {
    "de": [
      "La Table (Gastronomie) empfängt Sie Freitagabend, Samstagmittag und Samstagabend -- La Bagatelle (Bistronomie), die Sie von Dienstagmittag bis Frei..."
    ],
    "en": [
      "La Table (gastronomy) welcomes you on Friday evening, Saturday lunchtime and Saturday evening -- La Bagatelle (bistronomy) welcomes you from Tuesda..."
    ],
    "it": [
      "La Table (gastronomia) vi accoglie il venerdì sera, il sabato a pranzo e il sabato sera -- La Bagatelle (bistronomia) vi accoglie dal martedì a pranzo al venerdì a pranzo."
    ],
    "fr": [
      "La Table (gastronomie) vous accueille vend soir, sam midi et sam soir -- La Bagatelle (bistronomie) qui vous accueille du mardi midi au vendredi midi."
    ],
    "nl": [
      "La Table (gastronomie) ontvangt u op vrijdagavond, zaterdagmiddag en zaterdagavond -- La Bagatelle (bistronomie) ontvangt u van dinsdagmiddag tot vrijdagmiddag."
    ],
    "es": [
      "La Table (gastronomía) le da la bienvenida el viernes por la noche, el sábado al mediodía y el sábado por la noche -- La Bagatelle (bistronomía) le da la bienvenida del martes al mediodía al viernes al mediodía."
    ]
  },
