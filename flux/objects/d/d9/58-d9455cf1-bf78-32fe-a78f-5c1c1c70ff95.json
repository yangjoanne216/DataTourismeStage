  "rdfs:comment": {
    "de": [
      "Marie Rebuffat ist auf glutenfreie Lebensmittel spezialisiert. Sie knetet Brot und kreiert Backwaren von einfach bis aufwendig, laktose- und glutenfrei."
    ],
    "en": [
      "Specialized in gluten-free, Marie Rebuffat kneads bread and creates pastries from the simplest to the most elaborate, without lactose or gluten."
    ],
    "it": [
      "Specializzata in prodotti senza glutine, Marie Rebuffat impasta il pane e crea dolci dai più semplici ai più elaborati, senza lattosio né glutine."
    ],
    "fr": [
      "Spécialisée dans le sans gluten, Marie Rebuffat pétrit le pain et crée des pâtisseries des plus simples aux plus élaborées, sans lactose ni gluten."
    ],
    "nl": [
      "Marie Rebuffat, gespecialiseerd in glutenvrije producten, kneedt brood en maakt gebak van de meest eenvoudige tot de meest uitgebreide, zonder lactose of gluten."
    ],
    "es": [
      "Especializada en productos sin gluten, Marie Rebuffat amasa el pan y crea pasteles desde los más sencillos hasta los más elaborados, sin lactosa ni gluten."
    ]
  },
