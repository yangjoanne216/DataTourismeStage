  "rdfs:comment": {
    "de": [
      "Astronomieausflug mit dem Verein Ciel en Carladez zur Sternwarte von Frons in Thérondels. Der Verein veranstaltet im Sommer 6 Abende, an denen Sie den Sternenhimmel über Carladez entdecken können."
    ],
    "en": [
      "Astronomy outing with the association Ciel en Carladez, at the observatory of Frons, in Thérondels. The association animates 6 summer evenings to discover the starry sky of Carladez."
    ],
    "it": [
      "Uscita astronomica con l'associazione Ciel en Carladez, presso l'osservatorio Frons, a Thérondels. L'associazione organizza 6 serate estive per scoprire il cielo stellato di Carladez."
    ],
    "fr": [
      "Sortie astronomie avec l'association Ciel en Carladez, à l'observatoire de Frons, à Thérondels. L'association anime 6 soirées d'été pour découvrir le ciel étoilé du Carladez."
    ],
    "es": [
      "Salida astronómica con la asociación Ciel en Carladez, en el observatorio de Frons, en Thérondels. La asociación organiza 6 veladas estivales para descubrir el cielo estrellado de Carladez."
    ],
    "nl": [
      "Astronomisch uitstapje met de vereniging Ciel en Carladez, in het observatorium van Frons, in Thérondels. De vereniging organiseert 6 zomeravonden om de sterrenhemel van Carladez te ontdekken."
    ]
  },
