      "dc:description": {
        "de": [
          "Pizzas\n- Schinken (Tomate, Schinken, Käse) = 8.80€\n- Reine (Tomate, Pilze, Schinken, Käse) = 9.40€\n- Royale (Tomate, Zwiebeln, Schinken, Speck, Käse) = 10.30€\n- Ziege (Tomate, Speck, Ziege, Honig, Käse) = 11€\n- 4 Käse (Tomate, Roquefort, Ziege, Mozzarella, Käse) = 11.80€\n- Kebab (Crème fraîche, Zwiebeln, Kebab, Kebab-Sauce, Käse) = 10.60€\nSavoyarde (Crème fraîche, Speck, Zwiebeln, Reblochon, Käse) = 12.20€\n\nHamburger\n- Klassisch (Röstzwiebeln, Salat, Tomate, Hacksteak, geschmolzener Cheddar) = 7.20€\n- Larzac (gebratene Zwiebeln, Salat, Tomate, Hacksteak, Ziegenkäse, Honig) = 8.30€\n- Double (gebratene Zwiebeln, Salat, Tomate, 2 Hacksteaks, geschmolzener Cheddar) = 9.70€\n- Schale mit Pommes frites = 2.50"
        ],
        "en": [
          "Pizzas\n- Ham (tomato, ham, cheese) = 8.80\n- Queen (tomato, mushrooms, ham, cheese) = 9.40€ (€)\n- Royale (tomato, onions, ham, bacon, cheese) = 10.30\n- Goat (tomato, bacon, goat, honey, cheese) = 11\n- 4 cheeses (tomato, Roquefort, goat, mozzarella, cheese) = 11.80\n- Kebab (fresh cream, onions, kebab, kebab sauce, cheese) = 10.60\nSavoyarde (fresh cream, bacon, onions, reblochon, cheese) = 12.20\n\nHamburgers\n- Classic (fried onions, salad, tomato, chopped steak, melted cheddar) = 7.20\n- Larzac (fried onions, lettuce, tomato, chopped steak, goat cheese, honey) = 8.30\n- Double (fried onions, salad, tomato, 2 chopped steaks, melted cheddar) = 9.70\n- French fries = 2.50"
        ],
        "it": [
          "Pizze\n- Prosciutto (pomodoro, prosciutto, formaggio) = 8,80?\n- Regina (pomodoro, funghi, prosciutto, formaggio) = 9,40?\n- Royale (pomodoro, cipolle, prosciutto, pancetta, formaggio) = 10,30?\n- Capra (pomodoro, pancetta, capra, miele, formaggio) = 11?\n- 4 formaggi (pomodoro, Roquefort, capra, mozzarella, formaggio) = 11,80?\n- Kebab (panna acida, cipolle, kebab, salsa kebab, formaggio) = 10,60?\nSavoyarde (panna, pancetta, cipolle, formaggio reblochon) = 12,20?\n\nHamburger\n- Classico (cipolle fritte, lattuga, pomodoro, bistecca a pezzi, cheddar fuso) = 7,20?\n- Larzac (cipolle fritte, lattuga, pomodoro, bistecca a pezzi, formaggio di capra, miele) = 8,30?\n- Doppio (cipolle fritte, insalata, pomodoro, 2 bistecche tagliate, cheddar fuso) = 9,70?\n- Patatine = 2,50"
        ],
        "fr": [
          "Pizzas\n- Jambon (tomate, jambon, fromage) = 8.80€\n- Reine (tomate, champignons, jambon, fromage) = 9.40€\n- Royale (tomate, oignons, jambon, lardons, fromage) = 10.30€\n- Chèvre (tomate, bacon, chèvre, miel, fromage) = 11€\n- 4 fromages (tomate, roquefort, chèvre, mozzarella, fromage) = 11.80€\n- Kebab (crème fraîche, oignons, kebab, sauce kebab, fromage) = 10.60€\nSavoyarde (crème fraîche, lardons, oignons, reblochon, fromage) = 12.20€\n\nHamburgers\n- Classique (oignons frits, salade, tomate, steak haché, cheddar fondu) = 7.20€\n- Larzac (oignons frits, salade, tomate, steak haché, chèvre, miel) = 8.30€\n- Double (oignons frits, salade, tomate, 2 steaks hachés, cheddar fondu) = 9.70€\n- Barquette de frites = 2.50"
        ],
        "es": [
          "Pizzas\n- Jamón (tomate, jamón y queso) = 8,80?\n- Reina (tomate, champiñones, jamón, queso) = 9,40?\n- Royale (tomate, cebolla, jamón, bacon, queso) = 10,30?\n- Cabra (tomate, bacon, cabra, miel, queso) = 11?\n- 4 quesos (tomate, roquefort, cabra, mozzarella, queso) = 11,80?\n- Brocheta (crema agria, cebolla, brocheta, salsa de brocheta, queso) = 10,60?\nSavoyarde (crema, tocino, cebolla, queso reblochon) = 12,20?\n\nHamburguesas\n- Clásico (cebolla frita, lechuga, tomate, filete picado, queso cheddar fundido) = 7,20?\n- Larzac (cebollas fritas, lechuga, tomate, filete picado, queso de cabra, miel) = 8,30?\n- Doble (cebolla frita, ensalada, tomate, 2 filetes picados, queso cheddar fundido) = 9,70?\n- Patatas fritas = 2,50"
        ],
        "nl": [
          "Pizza's\n- Ham (tomaat, ham, kaas) = 8,80\n- Reine (tomaat, champignons, ham, kaas) = 9.40€ (€9.40)\n- Royale (tomaat, uien, ham, spek, kaas) = 10.30\n- Geit (tomaat, spek, geit, honing, kaas) = 11€\n- 4 kazen (tomaat, Roquefort, geit, mozzarella, kaas) = 11,80\n- Kebab (verse room, uien, kebab, kebab saus, kaas) = 10,60\nSavoyarde (verse room, spek, uien, reblochon, kaas) = 12,20\n\nHamburgers\n- Classic (gebakken uien, sla, tomaat, gehakte biefstuk, gesmolten cheddar) = 7.20\n- Larzac (gebakken uien, sla, tomaat, gehakte biefstuk, geitenkaas, honing) = 8.30\n- Dubbel (gebakken uien, salade, tomaat, 2 gehakte steaks, gesmolten cheddar) = 9,70\n- Chips = 2,50"
        ]
