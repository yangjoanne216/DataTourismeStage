  "rdfs:comment": {
    "de": [
      "Eigenes Spa-Chalet und Sauna. Panoramablick auf den See von Matemale. 4 Schlafzimmer mit Doppelbetten. Entspannungsbereich mit Sauna und Spa/Jacuzzi. Große Terrasse mit Blick auf den See und die Pisten. 2 private Parkplätze...."
    ],
    "en": [
      "Chalet Spa and private Sauna. Panoramic view on the Lake of Matemale. 4 rooms with double beds. Relaxation area with sauna and spa/jacuzzi. Large terrace with view on the lake and the slopes. 2 private parking spaces...."
    ],
    "it": [
      "Chalet Spa e sauna privata. Vista panoramica sul Lago di Matemale. 4 camere da letto con letti matrimoniali. Area relax con sauna e spa/jacuzzi. Ampia terrazza con vista sul lago e sulle piste. 2 posti auto privati...."
    ],
    "fr": [
      "Chalet Spa et Sauna privatif. Vue panoramique sur le Lac de Matemale. 4 chambres avec lits doubles. Espace détente avec sauna et spa/jacuzzi. Grande terrasse avec vue sur le lac, les pistes. 2 places de parking privatives...."
    ],
    "es": [
      "Chalet Spa y Sauna privada. Vista panorámica del lago de Matemale. 4 habitaciones con camas dobles. Zona de relax con sauna y spa/jacuzzi. Gran terraza con vistas al lago y a las pistas. 2 plazas de aparcamiento privado...."
    ],
    "nl": [
      "Chalet Spa en privé Sauna. Panoramisch uitzicht op het meer van Matemale. 4 slaapkamers met tweepersoonsbedden. Ontspanningsruimte met sauna en spa/jacuzzi. Groot terras met uitzicht op het meer en de pistes. 2 privé parkeerplaatsen...."
    ]
  },
