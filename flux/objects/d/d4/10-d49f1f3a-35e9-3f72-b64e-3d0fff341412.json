  "rdfs:comment": {
    "de": [
      "6. Marsch der Freunde des Châtelet - Samstag, 26. August 2023; 9-15 Uhr; Start vom Fallières-Stadion in Saint-Nabord. Teilnahmegebühr mindestens 3 ? zugunsten der Freiwilligenorganisation und der Bewohner des EHPAD. Markierte Strecke von 5 km oder 10 km, Kontrolle, kostenloser Imbiss\nGetränke und kleine Snacks vor Ort."
    ],
    "en": [
      "6th walk of the Friends of the Châtelet - Saturday, August 26, 2023; 9am-3pm; departure from Fallières stadium in Saint-Nabord. Participation 3? minimum to benefit the association of volunteers, residents of the EHPAD. Course 5km or 10km marked out, control, free snack\nRefreshment bar and snacks on site."
    ],
    "it": [
      "6a passeggiata degli Amici dello Châtelet - sabato 26 agosto 2023; ore 9-15; partenza dallo stadio Fallières di Saint-Nabord. Partecipazione minima di 3 persone a beneficio dell'associazione di volontari, residenti nell'EHPAD. Percorso di 5 o 10 km tracciato, controllo, spuntino gratuito\nRistoro e merenda sul posto."
    ],
    "fr": [
      "6ème marche des Amis du Châtelet - Samedi 26 août 2023; 9h-15h; départ du stade de Fallières à Saint-Nabord. Participation 3€ minimum au profit de l'association des bénévoles, des résidents de l'EHPAD. Parcours 5km ou 10km balisé, contrôle, collation gratuite\nBuvette et petite restauration sur place."
    ],
    "nl": [
      "6e wandeling van de Vrienden van het Châtelet - zaterdag 26 augustus 2023; 9.00-3.00 uur; vertrek vanaf het Fallières-stadion in Saint-Nabord. Deelname 3? minimum ten voordele van de vereniging van vrijwilligers, bewoners van de EHPAD. Parcours van 5 of 10 km uitgezet, controle, gratis snack\nVersnaperingen en snacks ter plaatse."
    ],
    "es": [
      "6ª marcha de los Amigos del Châtelet - Sábado 26 de agosto de 2023; de 9:00 a 15:00 horas; salida del estadio Fallières en Saint-Nabord. Participación 3? mínimo a beneficio de la asociación de voluntarios, residentes del EHPAD. Recorrido de 5 km o 10 km balizado, control, refrigerio gratuito\nRefrescos y tentempiés in situ."
    ]
  },
