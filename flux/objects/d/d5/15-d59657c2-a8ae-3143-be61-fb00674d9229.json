  "rdfs:comment": {
    "de": [
      "Der Jardin aux Baies bietet Ihnen Bio-Erdbeeren, die zu 100 % natürlich und unbehandelt sind. Charlotte und Mara des Bois erwarten Sie in der Rue des Rochers Saint-Jean 12 in Argenton-sur-Creuse."
    ],
    "en": [
      "Le Jardin aux Baies offers you organic strawberries, 100% natural and untreated. Charlotte and Mara des Bois are waiting for you at 12 rue des Rochers Saint-Jean in Argenton-sur-Creuse."
    ],
    "it": [
      "Le Jardin aux Baies vi offre fragole biologiche, naturali al 100% e non trattate. Charlotte e Mara des Bois vi aspettano al 12 di rue des Rochers Saint-Jean ad Argenton-sur-Creuse."
    ],
    "fr": [
      "Le Jardin aux Baies vous propose des fraises bio, 100% naturelles et non traitées. Charlotte et Mara des Bois vous attendent au 12 rue des Rochers Saint-Jean à Argenton-sur-Creuse."
    ],
    "es": [
      "Le Jardin aux Baies le ofrece fresas biológicas, 100% naturales y no tratadas. Charlotte y Mara des Bois le esperan en el 12 rue des Rochers Saint-Jean, en Argenton-sur-Creuse."
    ],
    "nl": [
      "Le Jardin aux Baies biedt u biologische, 100% natuurlijke en onbehandelde aardbeien. Charlotte en Mara des Bois wachten op u in 12 rue des Rochers Saint-Jean in Argenton-sur-Creuse."
    ]
  },
