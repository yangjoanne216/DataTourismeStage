  "rdfs:comment": {
    "de": [
      "Imkerei in Warré-Bienenstöcken mit Bio-Zertifizierung\nProdukte aus dem Bienenstock, Honig, frischer Pollen, schwarzer Nougat und Lebkuchen\nVerkauf im Honigraum am Mittwochnachmittag von 14 bis 18 Uhr oder nach Vereinbarung."
    ],
    "en": [
      "Beekeeping in certified organic Warré hives\nProducts of the hive, honeys, fresh pollen, black nougat and gingerbread\nSales at the honey house on Wednesday afternoon from 2 to 6 pm or by appointment."
    ],
    "it": [
      "Apicoltura in arnie Warré certificate biologiche\nProdotti dell'alveare, mieli, polline fresco, torrone nero e pan di zenzero\nVendita presso la casa del miele il mercoledì pomeriggio dalle 14.00 alle 18.00 o su appuntamento."
    ],
    "fr": [
      "Apiculture en ruche Warré certifiée Bio\nProduits de la ruche, miels, pollen frais, nougat noir et pains d'épices\nVente à la miellerie le mercredi après-midi de 14h à 18h ou sur rendez-vous."
    ],
    "nl": [
      "Bijenteelt in gecertificeerde biologische bijenkasten van Warré\nProducten van de bijenkorf, honing, verse pollen, zwarte noga en peperkoek\nVerkoop in het honinghuis op woensdagmiddag van 14 tot 18 uur of op afspraak."
    ],
    "es": [
      "Apicultura en colmenas Warré certificadas como ecológicas\nProductos de la colmena, mieles, polen fresco, turrón negro y pan de especias\nVenta en la casa de la miel los miércoles por la tarde de 14 a 18 horas o con cita previa."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Miellerie du Peuple Zélé"
    ]
  },
