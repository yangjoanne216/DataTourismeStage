  "rdfs:comment": {
    "de": [
      "Marzili-Mahlzeiten, traditionelle Küche, Gerichte auf der Grundlage unserer Käse und Wurstwaren. Verkauf von Produkten unseres Bauernhofs. Blick auf die elsässische Ebene."
    ],
    "en": [
      "Marcaire meal, traditional cooking, dishes based on our cheeses and cold cuts. Sale of products from our farm. View on the plain of Alsace."
    ],
    "it": [
      "Cucina tradizionale, piatti a base dei nostri formaggi e salumi. Vendita di prodotti della nostra azienda agricola. Vista sulla pianura alsaziana."
    ],
    "fr": [
      "Repas marcaire, cuisine traditionnelle, plats à base de nos fromages et charcuteries. Vente de produits de notre ferme. Vue sur la plaine d'Alsace."
    ],
    "nl": [
      "Traditionele keuken, gerechten op basis van onze kazen en vleeswaren. Verkoop van producten van onze boerderij. Uitzicht op de vlakte van de Elzas."
    ],
    "es": [
      "Cocina tradicional, platos a base de nuestros quesos y embutidos. Venta de productos de nuestra granja. Vista de la llanura alsaciana."
    ]
  },
