  "rdfs:comment": {
    "de": [
      "Das Foyer Sainte-Claire liegt 5 Gehminuten vom Stadtzentrum von Aix en Provence entfernt und beherbergt Schüler (Jungen und Mädchen) im Alter von 18 bis 25 Jahren."
    ],
    "en": [
      "Located 5 minutes walk from downtown Aix en Provence, the Sainte-Claire hostel welcomes students (boys and girls) aged 18 to 25."
    ],
    "it": [
      "Situato a 5 minuti a piedi dal centro di Aix en Provence, l'ostello Sainte-Claire accoglie studenti (ragazzi e ragazze) dai 18 ai 25 anni."
    ],
    "fr": [
      "Situé à 5 minutes à pied du centre-ville d'Aix en Provence, le foyer Sainte-Claire accueille des étudiants (garçons et filles) de 18 à 25 ans."
    ],
    "es": [
      "Situado a 5 minutos a pie del centro de Aix en Provence, el albergue Sainte-Claire acoge a estudiantes (chicos y chicas) de 18 a 25 años."
    ],
    "nl": [
      "De herberg Sainte-Claire ligt op 5 minuten lopen van het centrum van Aix en Provence en verwelkomt studenten (jongens en meisjes) van 18 tot 25 jaar."
    ]
  },
