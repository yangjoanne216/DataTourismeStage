  "rdfs:comment": {
    "de": [
      "Das Familienrestaurant Le Mas des Armieux bietet Ihnen einfache und schmackhafte Gerichte, wobei wir dank unserer lokalen Produzenten frische Produkte aus unserer Region bevorzugen."
    ],
    "en": [
      "The Mas des Armieux, a family restaurant, offers you simple and tasty dishes, using fresh products from our region, thanks to our local producers."
    ],
    "it": [
      "Il Mas des Armieux, ristorante a conduzione familiare, vi offre piatti semplici e gustosi, utilizzando prodotti freschi locali, grazie ai nostri produttori locali."
    ],
    "fr": [
      "Le Mas des Armieux, restaurant familial, vous propose des plats simples et savoureux, en privilégiant les produits frais de notre terroir, grâce à nos producteurs locaux."
    ],
    "es": [
      "El Mas des Armieux, restaurante familiar, le ofrece platos sencillos y sabrosos, elaborados con productos frescos de la tierra, gracias a nuestros productores locales."
    ],
    "nl": [
      "De Mas des Armieux, een familierestaurant, biedt u eenvoudige en smakelijke gerechten, met verse streekproducten, dankzij onze lokale producenten."
    ]
  },
