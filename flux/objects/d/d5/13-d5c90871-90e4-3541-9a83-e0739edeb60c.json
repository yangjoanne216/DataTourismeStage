  "rdfs:comment": {
    "de": [
      "La Baume d'Hostun erstreckt sich von den Ufern der Isère bis zu den Kämmen der Monts du Matin."
    ],
    "en": [
      "La Baume d'Hostun extends from the banks of the Isère to the crests of the Monts du Matin."
    ],
    "it": [
      "La Baume d'Hostun si estende dalle rive dell'Isère alle creste dei Monts du Matin."
    ],
    "fr": [
      "La Baume d'Hostun s’étend des berges de l’Isère aux crêtes des Monts du Matin."
    ],
    "es": [
      "La Baume d'Hostun se extiende desde las orillas del Isère hasta las crestas de los Montes de Matin."
    ],
    "nl": [
      "Dit dorp strekt zich uit van de oevers van de Isère tot de kammen van de Monts du Matin. Hier zijn een twintigtal vogelsoorten geobserveerd."
    ]
