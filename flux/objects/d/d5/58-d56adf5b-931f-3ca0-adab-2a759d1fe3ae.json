      "dc:description": {
        "de": [
          "2 Frühstücksangebote\r\nSüßes Angebot 8€: Heißes Getränk, gepresste Orange, Gebäck, Toast\r\n12€: Rührei, Wurst, Käse, Butterbrot, gepresste Orange"
        ],
        "en": [
          "2 breakfast formulas\r\nSweet formula 8€: hot drink, squeezed orange, pastry, toast\r\nSavory formula 12€: scrambled eggs, cold cuts, bread and butter, squeezed orange"
        ],
        "it": [
          "2 formule per la colazione\r\nFormula dolce 8?: bevanda calda, spremuta d'arancia, pasticcino, pane tostato\r\nFormula salata 12?: uova strapazzate, salumi, pane e burro, formaggio, spremuta d'arancia"
        ],
        "fr": [
          "2 formules de petit déjeuner\r\nFormule sucrée 8€: boisson chaude, orange pressée, viennoiserie, tartine \r\nFormule salée 12€: œufs brouillés, charcuterie, fromage pain beurre, orange pressée"
        ],
        "es": [
          "2 opciones de desayuno\r\nOpción dulce 8€: bebida caliente, naranja exprimida, bollería, tostadas\r\nOpción salada 12€: huevos revueltos, embutidos, pan y mantequilla, naranja exprimida"
        ],
        "nl": [
          "2 ontbijtformules\r\nZoete formule 8€: warme drank, geperste sinaasappel, gebak, toast\r\nHartige formule 12€: roerei, vleeswaren, kaas, brood en boter, geperste sinaasappel"
        ]
      },
