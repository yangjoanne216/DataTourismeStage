      "dc:description": {
        "de": [
          "19.00 Uhr: Clément Bouscarel, Geschichtenerzähler\n21.00 Uhr: traditionelle Musikgruppe Cal camina\nGetränke und belegte Brote werden angeboten\"\nkeine Reservierung, Kartenverkauf vor Ort am Tag der Aufführung"
        ],
        "en": [
          "7:00 pm: Clément Bouscarel, storyteller\n9:00 pm: trad music group Cal camina\nDrinks and sandwiches available\"\nno reservations, tickets on sale the day of the show"
        ],
        "it": [
          "19.00: Clément Bouscarel, cantastorie\nore 21.00: gruppo di musica tradizionale Cal camina\nBevande e panini a disposizione\"\nnessuna prenotazione, biglietti in vendita il giorno dello spettacolo"
        ],
        "fr": [
          "19h00: Clément Bouscarel, conteur\n21h00: groupe de musique trad Cal camina\nBoissons et tartines proposées\"\npas de réservation, billetterie sur place le jour du spectacle"
        ],
        "nl": [
          "19.00 uur: Clément Bouscarel, verhalenverteller\n21.00 uur: traditionele muziekgroep Cal camina\nDrankjes en broodjes verkrijgbaar\"\ngeen reservering, kaarten te koop op de dag van de voorstelling"
        ],
        "es": [
          "19.00 h: Clément Bouscarel, cuentacuentos\n21.00 h: grupo de música tradicional Cal camina\nBebidas y bocadillos disponibles\"\nsin reserva, venta de entradas el día del espectáculo"
        ]
      },
