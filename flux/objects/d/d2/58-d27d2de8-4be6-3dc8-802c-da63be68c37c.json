  "rdfs:comment": {
    "de": [
      "Verleih von Fahrrädern und Elektrofahrrädern im Herzen des Dorfes. Entdecken Sie die Camargue mit einem umweltfreundlichen Transportmittel."
    ],
    "en": [
      "Bicycle and electric bike rental located in the heart of the village. Come and discover the Camargue with an ecological means of transport."
    ],
    "it": [
      "Noleggio di biciclette e bici elettriche nel cuore del villaggio. Venite a scoprire la Camargue con un mezzo di trasporto ecologico."
    ],
    "fr": [
      "Location de vélos et vélos électriques située au cœur du village. Venez découvrir la Camargue avec un moyen de transport écologique."
    ],
    "nl": [
      "Verhuur van fietsen en elektrische fietsen in het hart van het dorp. Kom en ontdek de Camargue met een ecologisch vervoermiddel."
    ],
    "es": [
      "Alquiler de bicicletas y bicicletas eléctricas en el centro del pueblo. Venga a descubrir la Camarga con un medio de transporte ecológico."
    ]
  },
