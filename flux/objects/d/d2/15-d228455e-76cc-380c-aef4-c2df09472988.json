  "rdfs:comment": {
    "de": [
      "Sehr schöner Campingplatz mit Kinderspielplatz und Minigolf in Neuvy-Saint-Sépulchre, einer Gemeinde mit einer Nachbildung des Heiligen Sepulchre in Jerusalem."
    ],
    "en": [
      "Very pleasant campsite with children's playground and a mini-golf course, located in Neuvy-Saint-Sépulchre, home to a replica of Jerusalem’s Holy Sepulchre."
    ],
    "it": [
      "Un campeggio molto piacevole, con parco giochi per bambini e mini-golf, situato a Neuvy-Saint-Sépulchre, una città che ospita la replica della Saint-Sépulchre di Gerusalemme."
    ],
    "fr": [
      "Très agréable camping avec jeux pour enfants et mini-golf situé à Neuvy-Saint-Sépulchre, communune qui abrite la réplique du Saint-Sépulchre de Jérusalem."
    ],
    "es": [
      "Un camping muy agradable con parque infantil y minigolf situado en Neuvy-Saint-Sépulchre, localidad que alberga la réplica de Saint-Sépulchre de Jerusalén."
    ],
    "nl": [
      "Een zeer aangename camping met speeltuin en minigolf gelegen in Neuvy-Saint-Sépulchre, een stad waar de replica van de Saint-Sépulchre van Jeruzalem staat."
    ]
  },
