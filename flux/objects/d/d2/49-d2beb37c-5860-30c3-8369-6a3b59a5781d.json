  "rdfs:comment": {
    "de": [
      "Aufenthalt im Herzen eines Gemüsebauernhofs, der 1 km vom Ufer der Loire entfernt in der Gemeinde Le Cellier liegt."
    ],
    "en": [
      "Stay in the heart of a market gardening farm located 1km from the banks of the Loire in the commune of Le Cellier."
    ],
    "it": [
      "Soggiornate nel cuore di una fattoria-orto situata a 1 km dalle rive della Loira nel comune di Le Cellier."
    ],
    "fr": [
      "Séjour au cœur d'une ferme maraîchère située à 1km des bords de Loire sur la commune du Cellier."
    ],
    "nl": [
      "Verblijf in het hart van een tuinderij op 1 km van de oevers van de Loire in de gemeente Le Cellier."
    ],
    "es": [
      "Alójese en el corazón de una granja de huerta situada a 1 km de las orillas del Loira en la comuna de Le Cellier."
    ]
  },
