  "rdfs:comment": {
    "de": [
      "Zugang bei jedem Wetter, Plein Est durch den großen Pass. Tiefgang: Vorhafen (5 bis 7 m m) - Becken (5 bis 7 m)."
    ],
    "en": [
      "Access by all weather, due east by the big pass. Draft : fore-port (5 to 7m m) - basin (5 to 7m)."
    ],
    "it": [
      "Accesso per tutte le stagioni, verso est attraverso il canale principale. Pescaggio: porto esterno (da 5 a 7 m) - bacino (da 5 a 7 m)."
    ],
    "fr": [
      "Accès par tous temps, plein est par la grande passe. Tirant d'eau : avant-port (5 à 7m m) - bassin (5 à 7m)."
    ],
    "nl": [
      "Toegang bij alle weersomstandigheden, naar het oosten via het hoofdkanaal. Diepgang: voorhaven (5 tot 7 m) - bassin (5 tot 7 m)."
    ],
    "es": [
      "Acceso para todo tipo de clima, hacia el este a través del canal principal. Calado: puerto exterior (5 a 7 m) - dársena (5 a 7 m)."
    ]
  },
