  "rdfs:comment": {
    "de": [
      "Herstellung von hausgemachten Burgern, Pommes frites und Desserts aus frischen und lokalen Produkten: Brot vom Bäcker, Gemüse vom Markt, Käse vom Bauernhof, Fleisch vom Metzger der Rasse Limousin."
    ],
    "en": [
      "Elaboration of home-made burgers, fries and desserts, made with fresh and local products: bread from the baker, vegetables from the market, farm cheese; meat from the Limousin butcher."
    ],
    "it": [
      "Hamburger, patatine e dessert fatti in casa con prodotti freschi locali: pane del fornaio, verdure del mercato, formaggio di fattoria, carne del macellaio del Limousin."
    ],
    "fr": [
      "Élaboration de burgers, frites et desserts maison, issus de produits frais et locaux : pain du boulanger, légumes du marché, fromage fermier; viande du boucher race limousine."
    ],
    "nl": [
      "Huisgemaakte hamburgers, frietjes en desserts gemaakt met verse streekproducten: brood van de bakker, groenten van de markt, boerenkaas, vlees van de Limousin slager."
    ],
    "es": [
      "Hamburguesas caseras, patatas fritas y postres elaborados con productos locales frescos: pan del panadero, verduras del mercado, queso de granja, carne del carnicero de Limousin."
    ]
  },
