  "rdfs:comment": {
    "de": [
      "Einzigartiger und unverbaubarer Meerblick, gegenüber dem Strand von La Courtade. Brasserie, Pizzas aus dem Holzofen, vor Ort oder zum Mitnehmen. Service nonstop 13h-21h. Keine Reservierung."
    ],
    "en": [
      "Unique and unobstructed sea view, facing La Courtade beach. Brasserie, wood-fired pizzas, on the spot or to take away. Non-stop service 1-9pm. No reservation required."
    ],
    "it": [
      "Unica e mozzafiato vista mare, di fronte alla spiaggia della Courtade. Brasserie, pizze a legna, in loco o da asporto. Servizio non-stop 13:00-21:00. Nessuna prenotazione."
    ],
    "fr": [
      "Unique et imprenable vue mer, face à la plage de la Courtade. Brasserie, pizzas au feu de bois, sur place ou à emporter. Service non-stop 13h-21h. Pas de réservation."
    ],
    "es": [
      "Vista única y despejada al mar, frente a la playa de La Courtade. Brasserie, pizzas a la leña, in situ o para llevar. Servicio ininterrumpido de 13.00 a 21.00 h. No es necesario reservar."
    ],
    "nl": [
      "Uniek en vrij uitzicht op zee, tegenover het strand van La Courtade. Brasserie, houtgestookte pizza's, ter plaatse of om mee te nemen. Non-stop service van 13.00 tot 21.00 uur. Geen reservering nodig."
    ]
  },
