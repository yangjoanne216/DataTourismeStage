  "rdfs:comment": {
    "de": [
      "Im Herzen der Diois-Berge gelegen, mit einem meisterhaften Tiefblick auf das Roanne-Tal, ein Feinschmeckergasthof mit regionaler Küche zwischen Drôme und Provence."
    ],
    "en": [
      "Located at the heart of the Diois Mountains, with a remarquable bird's eye view of the Roanne Valley, our gourmet inn offers traditional cooking with Drôme and Provençal influences."
    ],
    "it": [
      "Situato nel cuore delle montagne di Diois, con una vista mozzafiato sulla Valle di Roanne, una locanda gourmet e locale tra Drôme e Provenza."
    ],
    "fr": [
      "Située au cœur des montagnes du Diois, offrant une vue magistrale et plongeante sur la Vallée de la Roanne, une auberge gourmande et terroir entre Drôme et Provence."
    ],
    "es": [
      "Situado en el corazón de las montañas de Diois, con una vista impresionante sobre el valle de Roanne, una posada gourmet y local entre Drôme y Provenza."
    ],
    "nl": [
      "Een gulle, traditionele herberg tussen Drôme en Provence. Fantastische ligging in de bergen in de omgeving van Die met uitzicht op het dal van de Roanne."
    ]
  },
