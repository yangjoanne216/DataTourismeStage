  "rdfs:comment": {
    "de": [
      "Entdecken Sie Fontanges, ein Dorf mit schönen Häusern in typisch auvergnatischer Architektur, seiner monolithischen Kapelle, die 1896 in den Felsen gegraben wurde, seiner Kirche aus dem 15."
    ],
    "en": [
      "Discover Fontanges, a village with beautiful houses with typical Auvergne architecture, its monolithic chapel dug into the rock in 1896, its 15th century church."
    ],
    "it": [
      "Scoprite Fontanges, un villaggio con belle case dalla tipica architettura alverniate, la sua cappella monolitica scavata nella roccia nel 1896, la sua chiesa del XV secolo."
    ],
    "fr": [
      "Découvrez Fontanges, village aux belles maisons à l’architecture typiquement auvergnate, sa chapelle monolithe creusée dans le rocher en 1896, son église de XVème s."
    ],
    "nl": [
      "Ontdek Fontanges, een dorp met prachtige huizen van typische Auvergne architectuur, zijn monolithische kapel gegraven in de rots in 1896, zijn 15e eeuwse kerk."
    ],
    "es": [
      "Descubra Fontanges, un pueblo con hermosas casas de arquitectura típica de Auvernia, su capilla monolítica excavada en la roca en 1896, su iglesia del siglo XV."
    ]
  },
