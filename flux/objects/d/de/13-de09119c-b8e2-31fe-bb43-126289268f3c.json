  "rdfs:comment": {
    "de": [
      "La Maison B, Restaurant Bourg-en-Bresse empfängt Sie täglich von 11:30 bis 14:30 Uhr und von 19:00 bis 22:00 Uhr (außer sonntags). Am Wochenende und auf Reservierung 22:30 Uhr mit Freunden, Familie, Verliebten."
    ],
    "en": [
      "La Maison B, restaurant Bourg-en-Bresse welcomes you every day from 11:30 am to 2:30 pm and from 7 pm to 10 pm (except on Sundays), on weekends and on reservation 10:30 pm with friends, family, lovers."
    ],
    "it": [
      "La Maison B, ristorante di Bourg-en-Bresse vi accoglie tutti i giorni dalle 11.30 alle 14.30 e dalle 19.00 alle 22.00 (tranne la domenica), nei fine settimana e su prenotazione alle 22.30 con amici, familiari o amanti."
    ],
    "fr": [
      "La Maison B, restaurant Bourg-en-Bresse vous accueille tous les jours de 11h30 à 14h30 et de 19h à 22h (sauf le dimanche), le week-end et sur réservation 22h30 entre amis, en famille, en amoureux."
    ],
    "es": [
      "La Maison B, restaurante de Bourg-en-Bresse le acoge todos los días de 11.30 a 14.30 y de 19 a 22 horas (excepto los domingos), los fines de semana y con reserva a las 22.30 horas con amigos, familia o amantes."
    ],
    "nl": [
      "La Maison B, restaurant Bourg-en-Bresse ontvangt u dagelijks van 11.30 tot 14.30 uur en van 19.00 tot 22.00 uur (behalve op zondag), in het weekend en op reservering om 22.30 uur met vrienden, familie of geliefden."
    ]
  },
