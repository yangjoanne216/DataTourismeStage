  "rdfs:comment": {
    "de": [
      "Gasthaus in Familienbesitz. Traditionelles Restaurant mit authentischer Küche, in dem das einzige Stichwort hausgemachte und gesellige Gerichte sind. Herzliche Atmosphäre. Gruppe akzeptiert."
    ],
    "en": [
      "Friendly hotel. Traditional restaurant with authentic cooking where the only watchword is convivial home made cooking. Warm atmosphere. Groups accepted."
    ],
    "it": [
      "Locanda a conduzione familiare. Ristorante tradizionale con una cucina autentica dove l'unica parola d'ordine è cucina casalinga e convivialità. Atmosfera calda. Gruppi accettati."
    ],
    "fr": [
      "Auberge familial. Restaurant traditionnel avec une cuisine authentique où le seul mot d'ordre est une cuisine maison et conviviale. Ambiance chaleureuse. Groupe accepté."
    ],
    "es": [
      "Hotel familiar. Restaurante tradicional con una cocina auténtica, casera y distendida. Ambiente cálido. Se aceptan grupos. Sugerencia de plato regional."
    ],
    "nl": [
      "Familiale herberg. Traditioneel restaurant met een authentieke keuken waar het enige sleutelwoord thuis koken en gezelligheid is. Warme sfeer. Groepen geaccepteerd."
    ]
  },
