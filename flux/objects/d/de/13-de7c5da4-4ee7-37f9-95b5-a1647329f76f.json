  "rdfs:comment": {
    "de": [
      "Entdecken Sie Crapahut Parc Aventure, einen originellen Naturraum. Machen Sie einen Spaziergang, klettern Sie und genießen Sie den Wald von Fontestorbes auf einer Fläche von über 4 Hektar!"
    ],
    "en": [
      "Discover Crapahut Parc Aventure, an original nature area. Come and walk, climb and enjoy the forest of Fontestorbes on a site of more than 4 hectares!"
    ],
    "it": [
      "Scoprite il Crapahut Parc Aventure, un'area naturale originale. Venite a camminare, ad arrampicarvi e a godervi la foresta di Fontestorbes su un sito di oltre 4 ettari!"
    ],
    "fr": [
      "Découvrez Crapahut Parc Aventure, un espace nature original. Venez vous promener, grimper et profiter de la forêt de Fontestorbes sur un site de plus de 4 hectares !"
    ],
    "nl": [
      "Ontdek Crapahut Parc Aventure, een origineel natuurgebied. Kom wandelen, klimmen en genieten van het bos van Fontestorbes op een terrein van meer dan 4 hectare!"
    ],
    "es": [
      "Descubra Crapahut Parc Aventure, un espacio natural original. ¡Venga a pasear, escalar y disfrutar del bosque de Fontestorbes en un terreno de más de 4 hectáreas!"
    ]
  },
