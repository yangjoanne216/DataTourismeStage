  "rdfs:comment": {
    "de": [
      "Food Truck mit Spezialitäten aus Réunion, die auf lokalen, frischen und hochwertigen Produkten basieren. Rougail saucisse, Cari poulet, Civet zourite (Oktopus) und viele andere traditionelle Spezialitäten warten auf Sie!"
    ],
    "en": [
      "Food Truck of Reunionese specialities made with local, fresh and quality products. Rougail sausage, Chicken curry, Civet zourite (octopus), and many other traditional specialties await you!"
    ],
    "it": [
      "Food Truck di specialità reunionesi a base di prodotti locali, freschi e di qualità. Salsiccia di Rougail, pollo al curry, zourite (polpo) in umido e molte altre specialità tradizionali vi aspettano!"
    ],
    "fr": [
      "Food Truck de spécialités Réunionnaises à base de produits locaux, frais et de qualité. Rougail saucisse, Cari poulet, Civet zourite (poulpe), et bien d'autres spécialités traditionnelles vous attendent !"
    ],
    "nl": [
      "Food Truck van Reunionese specialiteiten op basis van lokale, verse en kwaliteitsproducten. Rougail worst, kip kerrie, zourite (octopus) stoofpot, en vele andere traditionele specialiteiten wachten op u!"
    ],
    "es": [
      "Food Truck de especialidades reunionenses a base de productos locales, frescos y de calidad. La salchicha Rougail, el pollo al curry, el guiso de zourite (pulpo) y muchas otras especialidades tradicionales le esperan"
    ]
  },
  "rdfs:label": {
    "fr": [
      "Foodtruck Chez Jojo"
    ]
  },
