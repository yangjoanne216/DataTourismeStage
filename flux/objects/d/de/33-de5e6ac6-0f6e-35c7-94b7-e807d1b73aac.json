  "rdfs:comment": {
    "de": [
      "Lernen Sie Lilian und Viviane kennen, die Erzeuger und Verfechter des violetten Knoblauchs mit geschützter Ursprungsbezeichnung (AOP) aus Cadours."
    ],
    "en": [
      "Meet Lilian and Viviane, producers and defenders of Cadours PDO Violet Garlic."
    ],
    "it": [
      "Vi presentiamo Lilian e Viviane, produttrici e difensori dell'Ail Violet AOP de Cadours."
    ],
    "fr": [
      "Rencontrez Lilian et Viviane, producteurs et défenseurs de l'Ail Violet AOP de Cadours."
    ],
    "nl": [
      "Maak kennis met Lilian en Viviane, producenten en verdedigers van de Ail Violet AOP de Cadours."
    ],
