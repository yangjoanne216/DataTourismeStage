  "rdfs:comment": {
    "de": [
      "Wanderung zum Entdeckung der Hochebenen von Vercors mit Skilanglauf. Zelt-Übernachtung. Zugänglich ab 12 Jahren. Mit Ski oder Schneeschuhe. Mahlzeit inklusive."
    ],
    "en": [
      "Immerse yourself in the middle of Nature and experience a winter bivouac on the Vercors High Plateaux. A two days trek. Booking is essential."
    ],
    "it": [
      "Immersione nel cuore della natura, scoperta del bivacco invernale e degli Altipiani del Vercors. 2 giorni. Su prenotazione."
    ],
    "fr": [
      "Immersion en pleine nature, découverte du bivouac hivernal et des Hauts Plateaux du Vercors. 2 jours. Sur réservation."
    ],
    "nl": [
      "Langlauftocht op de Hauts Plateaux du Vercors. Overnachting in tent. Toegankelijk vanaf 12 jaar. Op ski's of sneeuwschoenen. Inclusief maaltijd."
    ],
    "es": [
      "Inmersión en plena naturaleza, descubrimiento del vivac de invierno y de las Tierras Altas del Vercors. 2 días. En reserva."
    ]
  },
