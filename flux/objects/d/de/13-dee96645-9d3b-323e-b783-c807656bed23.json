  "rdfs:comment": {
    "de": [
      "Big Fernand bietet Ihnen Hamburgés aus hochwertigen Produkten (Fleisch aus Frankreich, Rohmilchkäse usw.). Ein authentisches und französisches Ambiente mit Musik aus den 80er Jahren"
    ],
    "en": [
      "Big Fernand serves Hamburgers made using top-quality ingredients (meat of French origin, raw milk cheese, etc.). An authentic, traditional French atmosphere with music from the 80s."
    ],
    "it": [
      "Big Fernand vi offre hamburger preparati con prodotti di qualità (carni francesi, formaggi a latte crudo, ecc.). un'atmosfera autentica e franchista, il tutto al ritmo di musica anni '80"
    ],
    "fr": [
      "Big Fernand vous propose des Hamburgés élaborés à partir de produits de qualités (viandes d'origine française, fromages au lait cru, etc.). une ambiance authentique et franchouillarde le tout aux rythmes des musiques des 80's"
    ],
    "es": [
      "Big Fernand le ofrece hamburguesas elaboradas con productos de calidad (carnes francesas, quesos de leche cruda, etc.). un ambiente auténtico y franchouillarde, todo al ritmo de la música de los 80"
    ],
    "nl": [
      "Big Fernand biedt u hamburgers van kwaliteitsproducten (Frans vlees, kazen van rauwe melk, enz.). Een authentieke en franchouillarde sfeer, alles op het ritme van 80's muziek"
    ]
  },
