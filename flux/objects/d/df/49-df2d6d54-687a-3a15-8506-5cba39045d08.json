  "rdfs:comment": {
    "de": [
      "In Treillières wurde die Kapelle Notre Dame des Dons um 1460 von François II., dem Herzog der Bretagne und Vater der Herzogin Anne, restauriert."
    ],
    "en": [
      "In Treillières, the chapel Notre Dame des Dons was restored around 1460 by François II, Duke of Brittany and father of Duchess Anne."
    ],
    "it": [
      "A Treillières, la cappella di Notre Dame des Dons fu restaurata intorno al 1460 da Francesco II, duca di Bretagna e padre della duchessa Anna."
    ],
    "fr": [
      "À Treillières, la chapelle Notre Dame des Dons fut restaurée vers 1460 par François II, Duc de Bretagne et père de la Duchesse Anne."
    ],
    "nl": [
      "In Treillières werd de kapel Notre Dame des Dons rond 1460 gerestaureerd door François II, hertog van Bretagne en vader van hertogin Anne."
    ],
    "es": [
      "En Treillières, la capilla de Notre Dame des Dons fue restaurada hacia 1460 por Francisco II, duque de Bretaña y padre de la duquesa Ana."
