      "dc:description": {
        "de": [
          "Ferienhaus Bambus. Fläche: 90m2. Erdgeschoss: Wohnzimmer, Küche - CH1: 1 Bett 140, Bad + WC, separates WC. Etage: CH2: 2 Betten 90 - CH3: 1 Bett 140, Dusche + WC. Telefon im Wohnzimmer - Kurtaxe extra."
        ],
        "en": [
          "Bamboo Gîte. Surface: 90m2. Ground floor: Living room/kitchen area - CH1: 1 bed 140, s.d'eau + wc, independent wc. Floor : CH2 : 2 beds 90 - CH3 : 1 bed 140, shower room + wc. Telephone téléséjour - Visitor's tax in supplement."
        ],
        "it": [
          "Gîte Bambou. Superficie: 90m2. Piano terra: soggiorno/cucina - CH1: 1 letto 140, bagno con doccia + wc, wc indipendente. Piano : CH2 : 2 letti 90 - CH3 : 1 letto 140, bagno con doccia + wc. Telefono, soggiorno - Tassa di soggiorno in supplemento."
        ],
        "fr": [
          "Gîte Bambou. Surface : 90m2. RDC : Séjour salon/Coin cuisine - CH1 : 1 lit 140, s.d'eau + wc, wc indépendant. Etage : CH2 : 2 lits 90 - CH3 : 1 lit 140, s. d'eau + wc. Téléphone téléséjour - Taxe de séjour en supplément."
        ],
        "es": [
          "Gîte Bambou. Superficie: 90m2. Planta baja: Sala de estar/cocina - CH1: 1 cama 140, cuarto de ducha + wc, wc independiente. Planta : CH2 : 2 camas 90 - CH3 : 1 cama 140, cuarto de ducha + wc. Teléfono, sala de estar - Tasa turística en suplemento."
        ],
        "nl": [
          "Gîte Bambou. Oppervlakte: 90m2. Begane grond: Woonkamer/keuken - CH1: 1 bed van 140, doucheruimte + wc, aparte wc. Verdieping : CH2 : 2 bedden 90 - CH3 : 1 bed 140, douchekamer + wc. Telefoon en TV - Toeristenbelasting niet inbegrepen."
        ]
      },
