  "rdfs:comment": {
    "de": [
      "Möglichkeit, die Hoteldienstleistungen des Lion d'Or zu nutzen, Mittag- und Abendessen, Frühstücksservice, Zugang zum beheizten Hallenbad, zur Sauna und zum Whirlpool."
    ],
    "en": [
      "Possibility to use the hotel services of the Lion d'Or, lunch and dinner, breakfast service, access to the heated indoor swimming pool, sauna and whirlpool bath."
    ],
    "it": [
      "Possibilità di usufruire dei servizi alberghieri del Lion d'Or, pranzo e cena, servizio di prima colazione, accesso alla piscina interna riscaldata, sauna e idromassaggio."
    ],
    "fr": [
      "Possible d'utiliser les services hôteliers du Lion d'Or, repas du midi et du soir, service du petit-déjeuner, accès à la piscine intérieure chauffée, au sauna et au bain boullonnant."
    ],
    "es": [
      "Posibilidad de utilizar los servicios del hotel Lion d'Or, comidas y cenas, servicio de desayuno, acceso a la piscina cubierta climatizada, sauna y jacuzzi."
    ],
    "nl": [
      "Mogelijkheid om gebruik te maken van de hoteldiensten van de Lion d'Or, lunch en diner, ontbijtservice, toegang tot het verwarmde binnenzwembad, sauna en whirlpool."
    ]
  },
