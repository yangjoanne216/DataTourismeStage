  "rdfs:comment": {
    "de": [
      "Typischer Bauernhof im Périgord Noir mit Stopfenten, Körnermais für die Stopfmast.\nVerkauf von Produkten des Bauernhofs (ganze Stopfleber, halbgekochte Konserve...) das ganze Jahr über von 11 bis 18 Uhr."
    ],
    "en": [
      "Typical farm of the Périgord Noir with fat ducks, corn for force-feeding.\nSale of farm products (whole foie gras, preserved half-cooked...) all year long from 11am to 6pm."
    ],
    "it": [
      "Tipica fattoria del Périgord Noir con anatre all'ingrasso, mais per l'alimentazione forzata.\nVendita di prodotti di fattoria (foie gras intero, conserve a metà cottura...) tutto l'anno dalle 11.00 alle 18.00."
    ],
    "fr": [
      "Ferme typique du Périgord Noir avec canards gras, maïs grain pour le gavage.\nVente de produits de la ferme (foie gras entier, conserve mi-cuit...) toute l'année de 11h à 18h."
    ],
    "nl": [
      "Typische Périgord Noir boerderij met vetgemeste eenden, maïs voor dwangvoeding.\nVerkoop van boerderijproducten (hele foie gras, geconserveerde halfgare...) het hele jaar door van 11 tot 18 uur."
    ],
    "es": [
      "Granja típica del Périgord Noir con patos de engorde, maíz para alimentación forzada.\nVenta de productos de granja (foie gras entero, conservas a medio cocer...) todo el año de 11.00 a 18.00 h."
    ]
  },
