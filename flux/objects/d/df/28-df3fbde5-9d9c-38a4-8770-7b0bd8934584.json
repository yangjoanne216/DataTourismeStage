  "rdfs:comment": {
    "de": [
      "Mit der Familie oder unter Freunden kommen Sie zu einer Partie Minigolf mit einer herrlichen Aussicht auf den See und das Schloss von Pierrefonds!"
    ],
    "en": [
      "With family or friends, come and play a game of mini-golf with a superb view of the lake and Pierrefonds castle!"
    ],
    "it": [
      "Con la famiglia o gli amici, venite a giocare una partita di minigolf con una splendida vista sul lago e sul castello di Pierrefonds!"
    ],
    "fr": [
      "En famille ou entre-amis venez faire une partie de mini-golf avec une superbe vue sur le lac et le château de Pierrefonds !"
    ],
    "nl": [
      "Kom met uw gezin of vrienden een partijtje minigolf spelen met een prachtig uitzicht op het meer en het kasteel van Pierrefonds!"
    ],
    "es": [
      "Con su familia o amigos, venga a jugar una partida de minigolf con una magnífica vista del lago y del castillo de Pierrefonds"
    ]
  },
