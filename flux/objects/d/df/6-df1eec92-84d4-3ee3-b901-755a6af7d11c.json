  "rdfs:comment": {
    "de": [
      "Begleitete Ausritte zu Pferd. Reitkurse und -praktika im Steinbruch mit Ausritten in den Wald das ganze Jahr über. Vermietung von gesattelten Eseln. Taufe von Ponys."
    ],
    "en": [
      "Accompanied horseback riding. Equestrian courses and training courses in quarry with stroll in forest all the year. Donkey rental. Pony baptism."
    ],
    "it": [
      "Passeggiate a cavallo accompagnate. Lezioni e corsi di equitazione nell'arena e passeggiate nella foresta durante tutto l'anno. Noleggio di asini. Battesimo dei pony."
    ],
    "fr": [
      "Balades à cheval accompagnées. Cours et stages équestres en carrière avec balade en forêt toute l'année. Location d'âne bâté. Baptême poney."
    ],
    "es": [
      "Paseos a caballo acompañados. Clases de equitación y cursos en la pista con paseos por el bosque durante todo el año. Alquiler de burros. Bautismo en poni."
    ],
    "nl": [
      "Begeleide paardrijtochten. Paardenlessen en cursussen in de manege met ritten in het bos het hele jaar door. Verhuur van ezels. Pony doop."
    ]
  },
