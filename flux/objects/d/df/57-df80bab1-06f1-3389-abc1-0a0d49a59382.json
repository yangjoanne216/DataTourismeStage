  "rdfs:comment": {
    "de": [
      "Fest des Esels und des Pferdes. Verschiedene Animationen während des Tages: Eselrennen, Gespannvorführung, Pferdeshow, Countrya-Tanz. Verkaufsstand auf dem Dachboden. Feuerwerk."
    ],
    "en": [
      "Celebration of the donkey and the horse. Various animations during the day: Donkey races, demonstration of harnessing, equestrian show, country dance. Flea market. Fireworks."
    ],
    "it": [
      "Festa degli asini e dei cavalli. Varie attività durante la giornata: corse di asini, dimostrazione di carrozze trainate da cavalli, spettacolo di cavalli, ballo country. Mercato delle pulci. Spettacolo pirotecnico."
    ],
    "fr": [
      "Fête de l'âne et du cheval. Diverses animations durant la journée : Courses d'ânes, Démonstration d'attelage, spectacle équestre, danse countrya. Vide grenier. Feu d'artifice."
    ],
    "es": [
      "Fiesta del burro y del caballo. Diversas actividades durante el día: carreras de burros, demostración de coches de caballos, espectáculo ecuestre, baile country. Mercadillo. Castillo de fuegos artificiales."
    ],
    "nl": [
      "Ezel- en paardenfeest. Diverse activiteiten gedurende de dag: ezelraces, demonstratie van paardenkoets, paardenshow, country dance. Vlooienmarkt. Vuurwerk."
    ]
  },
