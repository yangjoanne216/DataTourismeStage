  "rdfs:comment": {
    "de": [
      "Hotel auf zwei Etagen mit Blick auf das Meer, im Herzen des Dorfes. Vollständig klimatisiertes Etablissement. Panoramarestaurant, Fischspezialitäten. Wifi-Verbindung und Pay-TV."
    ],
    "en": [
      "Hotel on two floors overlooking the sea, in the heart of the village. Fully air-conditioned establishment. Panoramic restaurant, fish specialities. Wifi and Pay-TV connection."
    ],
    "it": [
      "Hotel a due piani con vista sul mare, nel cuore del villaggio. Completamente climatizzato. Ristorante panoramico, specialità di pesce. Connessione Wifi e Pay-TV."
    ],
    "fr": [
      "Hôtel sur deux étages surplombant la mer, au coeur du village. Etablissement entièrement climatisé. Restaurant panoramique, spécialités de poissons. Connexion Wifi et Pay-TV."
    ],
    "nl": [
      "Hotel met twee verdiepingen met uitzicht op zee, in het hart van het dorp. Volledig geklimatiseerd. Panoramisch restaurant, visspecialiteiten. Wifi aansluiting en Pay-TV."
    ],
    "es": [
      "Hotel de dos plantas con vistas al mar, en el corazón del pueblo. Con aire acondicionado. Restaurante panorámico, especialidades de pescado. Conexión wifi y televisión de pago."
    ]
  },
