  "rdfs:comment": {
    "de": [
      "Camping CALME FAMILIAL SANS ANIMATIONS situé face à la plage des huttes pour le confort des grands et des petits, cadre magnifique, baignade, détente repos, balade, dépaysement total."
    ],
    "en": [
      "A QUIET FAMILY CAMPING WITHOUT ANIMATIONS located in front of the beach of the huts for the comfort of the big and the small ones, splendid framework, bathing, relaxation, stroll, total change of scenery."
    ],
    "it": [
      "Un campeggio familiare e tranquillo, senza animazione, situato di fronte alla spiaggia delle capanne per il comfort di grandi e piccini, un ambiente magnifico, bagni, relax, passeggiate, un cambio totale di scenario."
    ],
    "fr": [
      "Camping CALME FAMILIAL SANS ANIMATIONS situé face à la plage des huttes pour le confort des grands et des petits , cadre magnifique, baignade , détente repos, balade , dépaysement total."
    ],
    "es": [
      "Un camping familiar, tranquilo, sin animación, situado frente a la playa de las cabañas para la comodidad de grandes y pequeños, un marco magnífico, baño, relajación, paseos, un cambio total de aires."
    ],
    "nl": [
      "Een rustige familiecamping zonder animatie, gelegen tegenover het strand van de hutten voor het comfort van jong en oud, een prachtige omgeving, zwemmen, ontspanning, wandelingen, een totale verandering van omgeving."
    ]
  },
