        "de": [
          "La Gerbe d'Or, traditionsreiche Bäckerei und Konditorei.\nRestaurantbereich: Sandwiches, Salate... mit frischen und qualitativ hochwertigen Produkten."
        ],
        "en": [
          "La Gerbe d'Or, traditional bakery and pastry shop.\nCatering area: sandwiches, salads... with fresh and quality products."
        ],
        "it": [
          "La Gerbe d'Or, panetteria e pasticceria tradizionale.\nArea catering: panini, insalate, con prodotti freschi e di qualità."
        ],
        "fr": [
          "La Gerbe d'Or, boulangerie pâtisserie de tradition.\nEspace restauration : sandwichs, salades… avec des produits frais et de qualité."
        ],
        "nl": [
          "La Gerbe d'Or, traditionele bakkerij en banketbakkerij.\nCateringruimte: broodjes, salades... met verse kwaliteitsproducten."
        ],
        "es": [
          "La Gerbe d'Or, panadería y pastelería tradicional.\nZona de restauración: sándwiches, ensaladas... con productos frescos y de calidad."
        ]
