  "rdfs:comment": {
    "de": [
      "Paninis, hausgemachte Pommes frites, Croque Monsieur, hausgemachte Waffeln, hausgemachte Eiscreme?\nIn der Saison von Dienstagmittag bis Sonntagabend geöffnet."
    ],
    "en": [
      "Paninis, homemade French fries, croque-monsieur, homemade waffles, homemade ice creams?\nOpen seasonally from Tuesday lunchtime to Sunday evening inclusive."
    ],
    "it": [
      "Panini, patatine fritte fatte in casa, panini tostati, cialde fatte in casa, gelati fatti in casa?\nAperto stagionalmente dal martedì a pranzo alla domenica sera compresa."
    ],
    "fr": [
      "Paninis, frites maison, croque-monsieur, gaufres maison, glaces maison… \nOuvert en saison du mardi midi au dimanche soir inclus."
    ],
    "es": [
      "Paninis, patatas fritas caseras, sándwiches tostados, gofres caseros, helados caseros..\nAbierto de martes a mediodía a domingo por la noche, ambos inclusive."
    ],
    "nl": [
      "Panini's, zelfgemaakte friet, tosti's, zelfgemaakte wafels, zelfgemaakt ijs?\nSeizoensgebonden geopend van dinsdagmiddag tot en met zondagavond."
    ]
  },
