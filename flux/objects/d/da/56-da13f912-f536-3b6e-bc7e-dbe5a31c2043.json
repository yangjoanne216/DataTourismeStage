  "rdfs:comment": {
    "de": [
      "Ein geselliger Kochworkshop, bei dem Sie lernen, Tricks und Kniffe eines Profis beherrschen, Rezepte, Produkte oder sogar neue Geschmacksrichtungen entdecken."
    ],
    "en": [
      "A friendly cooking workshop, where you will learn, master tips and tricks of the trade, discover recipes, products and even new flavors."
    ],
    "it": [
      "Un laboratorio di cucina conviviale, dove imparerete a conoscere i trucchi del mestiere, a scoprire ricette, prodotti e persino nuovi sapori."
    ],
    "fr": [
      "Un atelier de cuisine convivial, où vous allez apprendre, maîtriser des trucs et astuces de pro, découvrir des recettes, produits voire de nouvelles saveurs."
    ],
    "es": [
      "Un taller de cocina amigable, donde aprenderá, dominará consejos y trucos del oficio, descubrirá recetas, productos e incluso nuevos sabores."
    ],
    "nl": [
      "Een gezellige kookworkshop, waar u tips en trucs van het vak leert, recepten, producten en zelfs nieuwe smaken ontdekt."
    ]
  },
