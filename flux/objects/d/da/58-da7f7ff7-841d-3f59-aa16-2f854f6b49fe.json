  "rdfs:comment": {
    "de": [
      "La Confiserie d'Antan, ein zu 100 % biologischer Süßwarenhersteller in Puyricard nördlich von Aix-en-Provence, bietet an, die Produkte der Provence wiederzuentdecken."
    ],
    "en": [
      "Revisit Provencal produce at the 100% organic La Confiserie d'Antan in Puyricard north of Aix-en-Provence."
    ],
    "it": [
      "Pasticceria 100% biologica, La Confiserie d'Antan, situata a Puyricard, a nord di Aix-en-Provence, vi propone di riscoprire i prodotti provenzali."
    ],
    "fr": [
      "Confiseur 100% bio, La Confiserie d'Antan située à Puyricard au Nord d'Aix-en-Provence, propose de faire redécouvrir les produits provençaux."
    ],
    "es": [
      "La Confiserie d'Antan, una pastelería 100% ecológica, situada en Puyricard, al norte de Aix-en-Provence, le propone redescubrir los productos provenzales."
    ],
    "nl": [
      "La Confiserie d'Antan, een 100% biologische banketbakker, gevestigd in Puyricard, ten noorden van Aix-en-Provence, helpt u de Provençaalse producten te herontdekken."
    ]
  },
