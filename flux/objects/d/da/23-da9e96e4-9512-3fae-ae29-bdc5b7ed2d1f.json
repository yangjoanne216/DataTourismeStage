  "rdfs:comment": {
    "de": [
      "Unabhängiges Haus auf einer Ebene, alt, modernisiert, 25 km von Périgueux (Condat-sur-Trincou) entfernt. Gemeinschaftsraum mit Küchenzeile, 2 Schlafzimmer (1 Bett 90 + 2 Betten 140), Duschbad."
    ],
    "en": [
      "Single storey older, modernised self-contained house, 25 kms from Perigueux (Condat-sur-Trincout). Open plan kitchen-living room, 2 bedrooms, shower-room, small dogs welcomed."
    ],
    "it": [
      "Casa indipendente su un unico livello, antica, modernizzata, situata a 25 km da Périgueux (Condat-sur-Trincou). Sala comune con angolo cottura, 2 camere (1 letto 90 + 2 letti 140), bagno con doccia."
    ],
    "fr": [
      "Maison indépendante de plain-pied, ancienne, modernisée, située à 25 km de Périgueux (Condat-sur-Trincou). Salle commune coin cuisine, 2 chambres (1 lit 90 + 2 lits 140), salle d'eau."
    ],
    "nl": [
      "Gelijkvloerse woning, oud, gemoderniseerd, gelegen op 25 km van Périgueux (Condat-sur-Trincou). Gemeenschappelijke ruimte met kitchenette, 2 slaapkamers (1 bed van 90 + 2 bedden van 140), doucheruimte."
    ],
    "es": [
      "Casa independiente de una sola planta, antigua, modernizada, situada a 25 km de Périgueux (Condat-sur-Trincou). Sala común con cocina americana, 2 dormitorios (1 cama de 90 + 2 camas de 140), cuarto de ducha."
    ]
