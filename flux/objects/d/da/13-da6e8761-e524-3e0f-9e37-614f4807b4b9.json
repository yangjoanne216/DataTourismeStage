  "rdfs:comment": {
    "de": [
      "Frischer Landfisch, Wolfsbarsch in Salz, Bouillabaisse, Seeteufel-Bourride mit Safran, Saint-Pierre mit Morcheln, Hummer, Meeresfrüchte. Alle Produkte sind frisch und werden vor Ort zubereitet."
    ],
    "en": [
      "Fresh local fish, sea bass in salt, bouillabaisse, monkfish “bourride” with saffron, Saint Pierre fish served with morels, lobsters, seafood. All our dishes are prepared in our premises, we only use fresh products."
    ],
    "it": [
      "Pesce fresco locale, branzino al sale, bouillabaisse, rana pescatrice allo zafferano, Saint-Pierre con spugnole, aragosta, frutti di mare. Tutti i prodotti sono freschi e cucinati al momento."
    ],
    "fr": [
      "Poissons de pays frais, loup au sel, bouillabaisse, bourride de lotte au safran, Saint-Pierre aux morilles, homard, fruits de mer. Tous les produits sont frais et cuisinés sur place."
    ],
    "nl": [
      "Verse lokale vis, zeebaars met zout, bouillabaisse, zeeduivel met saffraan, Saint-Pierre met morilles, kreeft, zeevruchten. Alle producten zijn vers en worden ter plekke bereid."
    ],
    "es": [
      "Pescado fresco de la zona, lubina a la sal, bullabesa, rape al azafrán, Saint-Pierre con morillas, langosta, marisco. Todos los productos son frescos y se cocinan en el momento."
    ]
  },
  "rdfs:label": {
    "fr": [
      "Le Sirocco"
    ]
  },
