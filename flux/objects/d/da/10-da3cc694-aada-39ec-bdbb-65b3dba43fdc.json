  "rdfs:comment": {
    "de": [
      "Möbliertes Studio mit 6 Schlafplätzen für 6 Personen: 2 Doppelbetten und 2 Einzelbetten. Eine ausgestattete Küche und ein Wohnzimmer. Preis für 2 Erwachsene und Kinder: 150€ pro Nacht. Zuschlag: 25€/Erwachsener."
    ],
    "en": [
      "Furnished studio with 6 beds for 6 people: 2 double beds and 2 single beds. An equipped kitchen and a living room. Price for 2 adults and children: 150€ per night. Supplement : 25€/adult."
    ],
    "it": [
      "Monolocale arredato con 6 posti letto per 6 persone: 2 letti matrimoniali e 2 letti singoli. Una cucina completamente attrezzata e un soggiorno. Prezzo per 2 adulti e bambini: 150? a notte. Supplemento: 25?/adulto."
    ],
    "fr": [
      "Studio meublé composé de 6 couchages pour 6 personnes : 2 lits doubles et 2 lits simples. Une cuisine équipée et un salon. Prix pour 2 adultes et enfants : 150€ la nuitée. Supplément : 25€/adulte."
    ],
    "nl": [
      "Gemeubileerde studio met 6 bedden voor 6 personen: 2 tweepersoonsbedden en 2 eenpersoonsbedden. Een volledig uitgeruste keuken en een woonkamer. Prijs voor 2 volwassenen en kinderen: 150€ per nacht. Supplement: 25€/volwassene."
    ],
    "es": [
      "Estudio amueblado con 6 camas para 6 personas: 2 camas dobles y 2 individuales. Una cocina totalmente equipada y una sala de estar. Precio para 2 adultos y niños : 150? por noche. Suplemento: 25€/adulto."
    ]
  },
