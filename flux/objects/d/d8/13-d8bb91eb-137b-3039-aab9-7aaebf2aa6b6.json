  "rdfs:comment": {
    "de": [
      "Atelier d'art, verrier sur la commune de Talmont-sur-Gironde: \"Véronique et Marc, vous proposent une balade merveilleuse dans le monde lumineux et coloré de leurs créations en verre\" (Véronique und Marc, schlagen Ihnen einen wunderbaren Spaziergang durch die leuchtende und bunte Welt ihrer Glaskreationen vor)."
    ],
    "en": [
      "Art workshop, glassmaker on the commune of Talmont-sur-Gironde: \"Véronique and Marc, propose to you a marvellous stroll in the luminous and coloured world of their creations out of glass\"."
    ],
    "it": [
      "Laboratorio d'arte, vetraio nel comune di Talmont-sur-Gironde: \"Véronique e Marc vi offrono un viaggio meraviglioso nel mondo luminoso e colorato delle loro creazioni in vetro\"."
    ],
    "fr": [
      "Atelier d'art, verrier sur la commune de Talmont-sur-Gironde : \"Véronique et Marc, vous proposent une balade merveilleuse dans le monde lumineux et coloré de leurs créations en verre\"."
    ],
    "es": [
      "Taller de arte, vidriero en la comuna de Talmont-sur-Gironde: \"Véronique y Marc, le ofrecen un maravilloso viaje al mundo luminoso y colorido de sus creaciones en vidrio\"."
    ],
    "nl": [
      "Kunstatelier, glasblazer in de gemeente Talmont-sur-Gironde: \"Véronique en Marc, bieden u een prachtige reis in de lichtgevende en kleurrijke wereld van hun glascreaties\"."
    ]
  },
