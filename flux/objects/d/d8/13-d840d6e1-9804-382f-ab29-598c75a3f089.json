  "rdfs:comment": {
    "de": [
      "Italienische, traditionelle Küchen, Pizzen, bruschetta und Produkte der Gegend."
    ],
    "en": [
      "Come and taste authentic vegan, vegetarian and Italian cuisine, with fresh products, local producers and shops, Italian products from Pesaro, Parma and Napoli."
    ],
    "it": [
      "Venite a gustare l'autentica cucina vegana, vegetariana e italiana, con prodotti freschi provenienti da produttori e negozi locali, prodotti italiani di Pesaro, Parma e Napoli."
    ],
    "fr": [
      "Venez déguster une authentique cuisine végétalienne, végétarienne et italienne, avec des produits frais, de producteurs et commerces locaux, des produits italiens en provenance de Pesaro, Parma et de Napoli."
    ],
    "es": [
      "Venga a disfrutar de la auténtica cocina vegana, vegetariana e italiana, con productos frescos de productores y tiendas locales, productos italianos de Pesaro, Parma y Nápoles."
    ],
    "nl": [
      "Kom genieten van de authentieke veganistische, vegetarische en Italiaanse keuken, met verse producten van lokale producenten en winkels, Italiaanse producten uit Pesaro, Parma en Napels."
    ]
  },
