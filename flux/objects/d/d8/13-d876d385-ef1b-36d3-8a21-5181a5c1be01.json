  "rdfs:comment": {
    "de": [
      "Freie Besichtigung mit Dokument und Informationstafel. Romanische Kirche (ISMH) vom Ende des 11. Jahrhunderts. Jh. Sehenswert: quadratischer Turm (ehemaliger Bergfried), Kirchenschiff und 4 historisierende Kapitelle."
    ],
    "en": [
      "Free visit with a leaflet and information board. Romanesque church from the 11th century. To see : square tower (ancient keep), nave and 4 capitals."
    ],
    "it": [
      "Visita gratuita con pannello informativo e documenti. Chiesa romanica (ISMH) della fine dell'XI secolo. Da vedere: torre quadrata (ex mastio), navata centrale e 4 capitelli istoriati."
    ],
    "fr": [
      "Visite libre avec document et panneau d'information. Église romane (ISMH) de la fin du XIe siècle. A voir : tour carrée (ancien donjon), nef et 4 chapiteaux historiés."
    ],
    "es": [
      "Visita gratuita con documento y panel informativo. Iglesia románica (ISMH) de finales del siglo XI. Para ver: torre cuadrada (antigua torre del homenaje), nave y 4 capiteles historiados."
    ],
    "nl": [
      "Gratis bezoek met document en informatiepaneel. Romaanse kerk (ISMH) uit het einde van de 11e eeuw. Te zien: vierkante toren (voormalige donjon), schip en 4 historische kapitelen."
    ]
  },
