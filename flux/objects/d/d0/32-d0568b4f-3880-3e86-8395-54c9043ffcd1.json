  "rdfs:comment": {
    "de": [
      "KONZERT FLIF'HANGER\nIm Restaurant Lou Regalido - Pégairolles de Buèges\nAm 08. August 2023 um 19.00 Uhr\n\nPopkultur im Reißwolf, Flif?Hanger klettert in die Höhe!\nEs gräbt nach Rock, E?lectro und Hip-Hop... Haarwirbel zu erwarten!"
    ],
    "en": [
      "FLIF'HANGER CONCERT\nAt Restaurant Lou Regalido - Pégairolles de Buèges\nAugust 08, 2023 at 19h00\n\nPop culture in the crusher, Flif'Hanger climbs to the top!\nIt digs into rock, e?lectro and hip-hop... Hair twists and turns to be expected!"
    ],
    "it": [
      "CONCERTO DI FLIF'HANGER\nAl Ristorante Lou Regalido - Pégairolles de Buèges\n08 agosto 2023 alle 19.00\n\nCultura pop nel frantoio, Flif'Hanger sale in cima!\nRock, e?lectro e hip-hop... Previsti colpi di scena per i capelli!"
    ],
    "fr": [
      "CONCERT FLIF'HANGER\nAu Restaurant Lou Regalido - Pégairolles de Buèges\nLe 08 août 2023 à 19h00\n\nCulture pop au broyeur, Flif’Hanger grimpe dans les hauteurs ! \nÇa creuse rock, électro et hip-hop... Remous capillaires à prévoir !"
    ],
    "nl": [
      "FLIF'HANGER CONCERT\nIn het Restaurant Lou Regalido - Pégairolles de Buèges\n08 augustus 2023 om 19 uur\n\nPopcultuur in de breker, Flif'Hanger klimt naar de top!\nRock, e?lectro en hip-hop... Haarkloverijen te verwachten!"
    ],
    "es": [
      "CONCIERTO DE FLIF'HANGER\nEn el Restaurante Lou Regalido - Pégairolles de Buèges\n08 de agosto 2023 a las 19h\n\nCultura pop en la trituradora, ¡Flif'Hanger sube a lo más alto!\nRock, e?lectro y hip-hop... ¡Giros y giros de pelo a la expectativa!"
    ]
  },
