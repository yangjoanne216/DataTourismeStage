  "rdfs:comment": {
    "de": [
      "Herstellung von handwerklich gebrauten Bieren. Der gesamte Herstellungsprozess vom Malz bis zum fertigen Bier wird erklärt und es gibt eine Verkostung. Besuche nur nach Vereinbarung um 11:00 und 14:30 Uhr."
    ],
    "en": [
      "Craft beer factory. The entire manufacturing process explained, from malt to finished beer and tasting, visits by appointment only at 11am and 2.30pm."
    ],
    "it": [
      "Fabbrica di birra artigianale. Viene spiegato l'intero processo di produzione, dal malto alla birra finita e alla degustazione, con visite solo su appuntamento alle 11.00 e alle 14.30."
    ],
    "fr": [
      "Fabrique de bières artisanales. Tout le processus de fabrication expliqué, du malt à la bière finie et dégustation, visites uniquement sur rendez-vous à 11h et 14h30."
    ],
    "es": [
      "Fábrica de cerveza artesanal. Se explica todo el proceso de producción, desde la malta hasta la cerveza terminada y la degustación, visitas con cita previa a las 11:00 y 14:30 horas."
    ],
    "nl": [
      "Craft bier fabriek. Het hele productieproces wordt uitgelegd, van mout tot afgewerkt bier en proeven, bezoeken alleen op afspraak om 11 uur en 14.30 uur."
    ]
  },
