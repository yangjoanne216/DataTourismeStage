  "rdfs:comment": {
    "de": [
      "Produzent von Salers-Rindfleisch: Direktverkauf vor Ort bei Besuchen auf dem Bauernhof (nach Voranmeldung) oder in den Erzeugerläden in Le Mazet St Voy, Tence\r\nUnterkunft auf dem Bauernhof."
    ],
    "en": [
      "Producer of Salers beef : direct sale on the spot during visits to the farm (on reservation) or in producer's stores in Mazet St Voy, in Tence\r\nGite at the farm."
    ],
    "it": [
      "Produttore di carne bovina di Salers: vendita diretta sul posto durante le visite alla fattoria (su prenotazione) o nel negozio del produttore a Mazet St Voy, a Tence\r\nGite in fattoria."
    ],
    "fr": [
      "Producteur de viande de bœuf Salers : vente directe sur place lors de visites à la ferme (sur réservation) ou en magasins de producteurs au Mazet St Voy, à Tence \r\nGite à la ferme."
    ],
    "es": [
      "Productor de carne de vacuno de Salers: venta directa in situ durante las visitas a la granja (previa reserva) o en la tienda del productor en Mazet St Voy, en Tence\r\nCasa rural en la granja."
    ],
    "nl": [
      "Producent van rundvlees uit Salers: directe verkoop ter plaatse tijdens bezoeken aan de boerderij (op reservering) of in de winkel van de producent in Mazet St Voy, in Tence\r\nGite bij de boerderij."
    ]
  },
