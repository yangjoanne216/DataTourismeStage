  "rdfs:comment": {
    "de": [
      "Kirche im gotischen Stil (M.H.): Kapelle aus dem 16. Jh., Turmspitze und Glockenturm aus dem 19. Schöne Kreuzrippengewölbe - Besichtigungen für Gruppen nach Vereinbarung."
    ],
    "en": [
      "Gothic style church (M.H.): 16th century chapel, 19th century spire and bell tower. Beautiful vaults with cross vaults - Visits for groups by appointment."
    ],
    "it": [
      "Chiesa in stile gotico (M.H.): cappella del XVI secolo, guglia e campanile del XIX secolo. Bellissime volte a crociera - Visite di gruppo su appuntamento."
    ],
    "fr": [
      "Eglise de style gothique (M.H.) : chapelle du XVIe, flèche et clocher du XIXe. Belles voûtes sur croisées d'ogives - Visites pour groupes sur rendez-vous."
    ],
    "nl": [
      "Kerk in gotische stijl (M.H.): 16e eeuwse kapel, 19e eeuwse torenspits en klokkentoren. Mooie kruisgewelven - Groepsbezoeken op afspraak."
    ],
    "es": [
      "Iglesia de estilo gótico (M.H.): capilla del siglo XVI, aguja y campanario del siglo XIX. Preciosas bóvedas de crucería - Visitas en grupo con cita previa."
    ]
  },
