  "rdfs:comment": {
    "de": [
      "Wunderschöner botanischer Park von 12 Hektar, oberhalb der Calanque du Mugel gelegen. Unzählige tropische und mediterrane Pflanzenarten. Picknick- und Spielbereich für Kinder. Belvedere auf 82 m Höhe mit Panoramablick."
    ],
    "en": [
      "Splendid botanical park of 12 hectares, located above the Calanque du Mugel. Countless tropical and Mediterranean species. Picnic area and playground for children. Belvedere at an altitude of 82 metres with panoramic views."
    ],
    "it": [
      "Splendido parco botanico di 12 ettari, situato sopra il Calanque du Mugel. Innumerevoli specie tropicali e mediterranee. Area picnic e parco giochi per bambini. Belvedere a 82 metri di altitudine con vista panoramica."
    ],
    "fr": [
      "Splendide parc botanique de 12 hectares, situé au-dessus de la Calanque du Mugel. Innombrables essences tropicales et méditerranéennes. Aire de pique-nique et de jeux pour enfants. Belvédère à 82 mètres d'altitude avec vue panoramique."
    ],
    "nl": [
      "Prachtig botanisch park van 12 hectare, gelegen boven de Calanque du Mugel. Talloze tropische en mediterrane soorten. Picknickplaats en speelplaats voor kinderen. Belvedere op 82 meter hoogte met panoramisch uitzicht."
    ],
    "es": [
      "Espléndido parque botánico de 12 hectáreas, situado sobre el Calanque du Mugel. Innumerables especies tropicales y mediterráneas. Zona de picnic y parque infantil. Belvedere a 82 metros de altitud con vistas panorámicas."
    ]
  },
