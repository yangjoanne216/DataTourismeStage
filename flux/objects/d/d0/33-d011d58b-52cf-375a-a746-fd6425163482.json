  "rdfs:comment": {
    "de": [
      "Handi'TourGuide schlägt vor, Menschen mit Behinderungen die Möglichkeit zu bieten, den Reichtum der Welt zu entdecken."
    ],
    "en": [
      "Handi'TourGuide offers people with disabilities the opportunity to discover the world's treasures, according to their individual needs."
    ],
    "it": [
      "Handi'TourGuide offre alle persone con disabilità l'opportunità di scoprire i tesori del mondo, in base alle loro esigenze individuali."
    ],
    "fr": [
      "Handi'TourGuide propose d'offrir l'opportunité aux personnes en situation de handicap de découvrir les richesses du monde, en fonction des besoins de chacun."
    ],
    "es": [
      "Handi'TourGuide ofrece a las personas con discapacidad la oportunidad de descubrir los tesoros del mundo, según sus necesidades individuales."
    ],
    "nl": [
      "Handi'TourGuide biedt mensen met een handicap de mogelijkheid om de schatten van de wereld te ontdekken, volgens hun individuele behoeften."
    ]
  },
