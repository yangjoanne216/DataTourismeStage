  "rdfs:comment": {
    "de": [
      "Schönes Steinhaus im Herzen der normannischen Bocage gegenüber von L'Abbaye d'Hambye. 25 Minuten von den Stränden und 5 Minuten von den Geschäften entfernt. Grüne Umgebung und unbeschwerte Ferien garantiert."
    ],
    "en": [
      "Beautiful stone house located in the heart of the Normandy bocage opposite Hambye Abbey. 25 minutes from the beaches and 5 minutes from the shops. A green setting and a holiday in complete serenity guaranteed."
    ],
    "it": [
      "Bella casa in pietra situata nel cuore del bocage normanno di fronte all'Abbazia di Hambye. A 25 minuti dalle spiagge e a 5 minuti dai negozi. Un ambiente verde e una vacanza tranquilla garantiti."
    ],
    "fr": [
      "Belle maison en pierre située au cœur du bocage normand en face de L'Abbaye d'Hambye. A 25 minutes des plages et 5 minutes des commerces. Ecrin de verdure et vacances en toute sérénité garanties."
    ],
    "nl": [
      "Mooi stenen huis gelegen in het hart van de Normandische bocage tegenover de abdij van Hambye. 25 minuten van de stranden en 5 minuten van de winkels. Een groene omgeving en een rustige vakantie gegarandeerd."
    ],
    "es": [
      "Hermosa casa de piedra situada en el corazón del bocage de Normandía, frente a la abadía de Hambye. A 25 minutos de las playas y a 5 minutos de las tiendas. Un entorno verde y unas vacaciones tranquilas garantizadas."
    ]
  },
