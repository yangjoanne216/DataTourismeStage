  "rdfs:comment": {
    "de": [
      "Sich in den Bäumen bewegen. Auf mehreren Hektar Wald bietet Ihnen Aventure Parc mehr als 80 Spiele, die in den Bäumen eingerichtet sind."
    ],
    "en": [
      "Climb in the trees. Covering several hectares of forest, Aventure Parc offers more than 80 games set up in the trees."
    ],
    "it": [
      "Muoversi tra gli alberi. Su diversi ettari di foresta, l'Aventure Parc vi offre più di 80 giochi tra gli alberi."
    ],
    "fr": [
      "Se déplacer dans les arbres. Sur plusieurs hectares de forêt, Aventure Parc vous propose plus de 80 jeux aménagés dans les arbres."
    ],
    "es": [
      "Moviéndose entre los árboles. En varias hectáreas de bosque, Aventure Parc le ofrece más de 80 juegos en los árboles."
    ],
    "nl": [
      "Bewegend door de bomen. Op verschillende hectaren bos biedt Aventure Parc u meer dan 80 spelen in de bomen."
    ]
  },
