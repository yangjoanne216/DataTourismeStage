  "rdfs:comment": {
    "de": [
      "Les Terrasses d'Aurons empfängt Sie in der Nähe dieses typisch provenzalischen Dorfes in einem geschützten Naturgebiet inmitten eines Pinienwaldes."
    ],
    "en": [
      "Les Terrasses d'Aurons welcomes you near this typically Provencal village, in a protected natural area, in the heart of a pine forest."
    ],
    "it": [
      "Les Terrasses d'Aurons vi accoglie nei pressi di questo tipico villaggio provenzale, in un'area naturale protetta, nel cuore di una pineta."
    ],
    "fr": [
      "Les Terrasses d'Aurons vous accueille à proximité de ce village typiquement provençal, dans un espace naturel protégé, au cœur d'une pinède."
    ],
    "nl": [
      "Les Terrasses d'Aurons ontvangt u in de buurt van dit typisch Provençaalse dorp, in een beschermd natuurgebied, in het hart van een dennenbos."
    ],
    "es": [
      "Les Terrasses d'Aurons le da la bienvenida cerca de este típico pueblo provenzal, en un área natural protegida, en el corazón de un bosque de pinos."
    ]
  },
