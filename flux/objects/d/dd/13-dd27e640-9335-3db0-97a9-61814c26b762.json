  "rdfs:comment": {
    "de": [
      "Haben Sie Lust, dem Alltag zu entfliehen? Dann begeben Sie sich an Bord eines Segelkatamarans auf ein Abenteuer und fahren Sie einen Tag lang zu den kleinen Inseln in der Nähe von Nouméa!"
    ],
    "en": [
      "Want to get away from it all? Go on an adventure aboard a sailing catamaran for a day around the islets near Noumea!"
    ],
    "it": [
      "Volete allontanarvi da tutto? Partite all'avventura a bordo di un catamarano a vela per una giornata intorno agli isolotti vicino a Noumea!"
    ],
    "fr": [
      "Envie de vous évader ? Partez à l'aventure à bord d'un catamaran à voile pour une journée aux abords des îlots proches de Nouméa !"
    ],
    "es": [
      "¿Quiere alejarse de todo? Embárquese en una aventura a bordo de un catamarán de vela durante un día por los islotes cercanos a Numea"
    ],
    "nl": [
      "Wil je er even tussenuit? Ga op avontuur aan boord van een zeilcatamaran voor een dag rond de eilandjes bij Noumea!"
    ]
  },
