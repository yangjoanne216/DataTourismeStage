  "rdfs:comment": {
    "de": [
      "Unter der Anleitung von Gaëlle können Sie Yoga auf eine ganz neue Art und Weise praktizieren, im Herzen des Weinbergs!"
    ],
    "en": [
      "Guided by Gaëlle, practice yoga in a new way, in the heart of the vineyard!"
    ],
    "it": [
      "Guidati da Gaëlle, praticate lo yoga in modo nuovo, nel cuore del vigneto!"
    ],
    "fr": [
      "Guidés par Gaëlle, pratiquez le yoga de manière inédite, au cœur du vignoble !"
    ],
    "es": [
      "Guiado por Gaëlle, practique yoga de una forma nueva, ¡en el corazón del viñedo!"
    ],
    "nl": [
      "Begeleid door Gaëlle, beoefen yoga op een nieuwe manier, in het hart van de wijngaard!"
    ]
