  "rdfs:comment": {
    "de": [
      "Besuchen Sie auf ungewöhnliche Weise ein kleines Stück Haute-Garonne mit geländegängigen elektrischen Tretrollern!"
    ],
    "en": [
      "Visit a small part of Haute-Garonne in an unusual way with all-terrain electric scooters!"
    ],
    "it": [
      "Visitate una piccola parte dell'Alta Garonna in modo insolito con gli scooter elettrici!"
    ],
    "fr": [
      "Visitez de façon insolite un petit bout de Haute-Garonne avec des Trottinettes électriques tout terrain !"
    ],
    "es": [
      "Visite una pequeña parte de la Haute-Garonne de una manera inusual con scooters eléctricos todo terreno"
    ],
    "nl": [
      "Bezoek een klein deel van de Haute-Garonne op een ongewone manier met all-terrain elektrische scooters!"
    ]
  },
