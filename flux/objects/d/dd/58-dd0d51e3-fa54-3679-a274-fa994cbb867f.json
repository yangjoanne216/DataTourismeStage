  "rdfs:comment": {
    "de": [
      "Künstleratelier Carol on the roof im Herzen des historischen Zentrums von Aix-en-Provence. Hier werden Gemälde und Skulpturen des Künstlers ausgestellt."
    ],
    "en": [
      "Artist's studio Carol on the roof in the heart of the historic center of Aix-en-Provence. Paintings and sculptures of the artist are exhibited there."
    ],
    "it": [
      "Studio dell'artista Carol sul tetto nel cuore del centro storico di Aix-en-Provence. Sono esposti i dipinti e le sculture dell'artista."
    ],
    "fr": [
      "Atelier d'artiste Carol on the roof au cœur du centre historique d’Aix-en-Provence. Toiles et sculptures de l'artiste y sont exposées."
    ],
    "nl": [
      "Kunstenaarsatelier Carol op het dak in het hart van het historische centrum van Aix-en-Provence. De schilderijen en beeldhouwwerken van de kunstenaar worden tentoongesteld."
    ],
    "es": [
      "Estudio del artista Carol en la azotea, en pleno centro histórico de Aix-en-Provence. Se exponen pinturas y esculturas de la artista."
    ]
